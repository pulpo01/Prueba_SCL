/* LIBERA MEMORIA USADA EN LA LISTA DE HABILITADOS                           */
/*---------------------------------------------------------------------------*/
void vLiberaDatosHabilitado(stHabilitado * taux)
{
    stHabilitado * saux;
    if (taux!=NULL)
    {
        saux = taux->sgte;
        while (saux!=NULL)
        {
            free(taux);
            taux = saux;
            saux = taux->sgte;
        }
        if(taux!=NULL) free(taux);
    }
}
