    	     	INTO  :sFechaDesde,:sFechaHasta
    	      	FROM  DUAL;	
    	      	
    	if( SQLCODE != SQLOK  ) 
    	{
    	        ifnTrazasLog(modulo,"\n Error Fechas ingresadas invalidas\n", LOG01);	
    		return -1;
    	}
        return 0;
} /* ifnCargaParametros */

