DECLARE
  X NUMBER;
BEGIN
  SYS.DBMS_JOB.SUBMIT
  ( job       => X 
   ,what      => 'DECLARE
VP_FEC_PROC DATE;
COD_RETORNO NUMBER(1);
COD_SQLCODE VARCHAR2(255);
COD_SQLERRM VARCHAR2(255);

BEGIN
