CREATE TABLE CO_AVISOS_VENCIMIENTO_TD(
    COD_ACCION VARCHAR2(5) NOT NULL 
 ,  COD_MENSAJE VARCHAR2(5) NOT NULL 
 ,  NUM_DIAS NUMBER(2,0) NOT NULL 
 ,  COD_CATEGORIA VARCHAR2(50) NOT NULL 
 ,  IND_PAGOPARCIAL CHAR(1) NOT NULL 
 ,  FEC_ULTIMAMODIFICACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_AVISOS_VENCIMIENTO_TD IS 'Parametros fijos para proceso Avisos de Vencimineto'
/
COMMENT ON COLUMN  CO_AVISOS_VENCIMIENTO_TD.COD_ACCION IS 'Accion de Mensaje a generar'
/
COMMENT ON COLUMN  CO_AVISOS_VENCIMIENTO_TD.COD_MENSAJE IS 'Mensaje a enviar'
/
COMMENT ON COLUMN  CO_AVISOS_VENCIMIENTO_TD.NUM_DIAS IS 'Dias antes del vencimiento en que se enviara el mensaje'
/
COMMENT ON COLUMN  CO_AVISOS_VENCIMIENTO_TD.COD_CATEGORIA IS 'Categorias a las que se les envia el mensaje'
/
COMMENT ON COLUMN  CO_AVISOS_VENCIMIENTO_TD.IND_PAGOPARCIAL IS 'Indica si se deben considerar las facturas con pagos parciales'
/
COMMENT ON COLUMN  CO_AVISOS_VENCIMIENTO_TD.FEC_ULTIMAMODIFICACION IS 'Fecha de ultima modificacion de los datos de la tabla'
/
COMMENT ON COLUMN  CO_AVISOS_VENCIMIENTO_TD.NOM_USUARIO IS 'Username del usuario que hizo los cambios'
/
