CREATE TABLE GAD_SERVSUP_PLAN(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_PLANTARIF VARCHAR2(3) NOT NULL 
 ,  COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  TIP_RELACION VARCHAR2(5) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_MENSAJE NUMBER(3,0) NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAD_SERVSUP_PLAN IS 'Servicios Asociados A Planes.'
/
COMMENT ON COLUMN  GAD_SERVSUP_PLAN.COD_PRODUCTO IS 'Codigo del producto.'
/
COMMENT ON COLUMN  GAD_SERVSUP_PLAN.COD_PLANTARIF IS 'Codigo del plan tarifario.'
/
COMMENT ON COLUMN  GAD_SERVSUP_PLAN.COD_SERVICIO IS 'Codigo del servicio.'
/
COMMENT ON COLUMN  GAD_SERVSUP_PLAN.TIP_RELACION IS 'Tipo de relacion entre plantarifario y servicio.'
/
COMMENT ON COLUMN  GAD_SERVSUP_PLAN.FEC_DESDE IS 'Fecha Inicio Vigencia.'
/
COMMENT ON COLUMN  GAD_SERVSUP_PLAN.FEC_HASTA IS 'Fecha termino vigencia.'
/
COMMENT ON COLUMN  GAD_SERVSUP_PLAN.NOM_USUARIO IS 'Usuario Responsable de creacion de relacion.'
/
COMMENT ON COLUMN  GAD_SERVSUP_PLAN.COD_MENSAJE IS 'Codigo de mensaje asociado a la activacion del servicio.'
/
COMMENT ON COLUMN  GAD_SERVSUP_PLAN.FEC_ULTMOD IS 'Fecha ultima modificacion.'
/
