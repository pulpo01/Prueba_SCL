CREATE TABLE CET_DETCLAVACT(
    NUM_PROCESO NUMBER(10,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(16) NOT NULL 
 ,  NUM_CLAVE VARCHAR2(16) NOT NULL 
 ,  COD_TIPCLAVE VARCHAR2(5) NOT NULL 
 ,  COD_ESTADO VARCHAR2(5) NOT NULL 
 ,  FEC_ULTMOD DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CET_DETCLAVACT IS 'Detalle Activacion  de claves.'
/
COMMENT ON COLUMN  CET_DETCLAVACT.NUM_PROCESO IS 'Numero de proceso Activacion'
/
COMMENT ON COLUMN  CET_DETCLAVACT.NUM_SERIE IS 'Fecha de Proceso'
/
COMMENT ON COLUMN  CET_DETCLAVACT.NUM_CLAVE IS 'Numero de claves Activadas'
/
COMMENT ON COLUMN  CET_DETCLAVACT.COD_TIPCLAVE IS 'Tipo de clave ( tipos de tarjeta )'
/
COMMENT ON COLUMN  CET_DETCLAVACT.COD_ESTADO IS 'Estado de la clave  A: Activada     B: Baja'
/
COMMENT ON COLUMN  CET_DETCLAVACT.FEC_ULTMOD IS 'Fecha ultima modificacion'
/
COMMENT ON COLUMN  CET_DETCLAVACT.NOM_USUARIO IS 'Responsable ultima modificacion'
/
