 ,  COD_OPERADORA VARCHAR2(5) NOT NULL 
 ,  COD_PLAZA VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_RECEXT_PLAZA IS 'Tabla con Importes por Operadora y Plaza.'
/
COMMENT ON COLUMN  CO_RECEXT_PLAZA.COD_RECEXT IS 'Codigo de Recaudacion Externa.'
/
COMMENT ON COLUMN  CO_RECEXT_PLAZA.IMP_VALOR IS 'Importe de Pago.'
/
COMMENT ON COLUMN  CO_RECEXT_PLAZA.COD_OPERADORA IS 'Codigo de Operadora.'
