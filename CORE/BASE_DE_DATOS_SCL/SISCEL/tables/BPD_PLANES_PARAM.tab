CREATE TABLE BPD_PLANES_PARAM(
    COD_PLAN VARCHAR2(5) NOT NULL 
 ,  COD_QUERY VARCHAR2(5) NOT NULL 
 ,  TIP_QUERY CHAR(1) NOT NULL 
 ,  SEC_PARAM NUMBER(3,0) NOT NULL 
 ,  VAL_PARAM VARCHAR2(50) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  DES_PARAM VARCHAR2(30) NULL 
 ,  FEC_DESDEAPLI DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  BPD_PLANES_PARAM IS 'Bpd_Planes_Param.'
/
COMMENT ON COLUMN  BPD_PLANES_PARAM.COD_PLAN IS 'Codigo que identifica el Plan.'
/
COMMENT ON COLUMN  BPD_PLANES_PARAM.COD_QUERY IS 'Codigo del Query.'
/
COMMENT ON COLUMN  BPD_PLANES_PARAM.TIP_QUERY IS 'Tipo de Query.'
/
COMMENT ON COLUMN  BPD_PLANES_PARAM.SEC_PARAM IS 'Secuencia del parametro.'
/
COMMENT ON COLUMN  BPD_PLANES_PARAM.VAL_PARAM IS 'Valor del parametro.'
/
COMMENT ON COLUMN  BPD_PLANES_PARAM.FEC_ULTMOD IS 'Fecha ultima actualizacion.'
/
COMMENT ON COLUMN  BPD_PLANES_PARAM.NOM_USUARIO IS 'Nombre usuario responsable.'
/
COMMENT ON COLUMN  BPD_PLANES_PARAM.DES_PARAM IS 'descripcisn del parametro.'
/
COMMENT ON COLUMN  BPD_PLANES_PARAM.FEC_DESDEAPLI IS 'Desde que fecha se puede empezar a aplicar el Beneficio'
/
