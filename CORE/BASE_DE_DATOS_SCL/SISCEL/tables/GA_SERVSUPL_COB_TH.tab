CREATE TABLE GA_SERVSUPL_COB_TH(
    COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  COD_SERVSUPL NUMBER(2,0) NOT NULL 
 ,  DES_SERVICIO VARCHAR2(50) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  FEC_MODIFICA DATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(50) NOT NULL 
 ,  IND_COBRO NUMBER(1,0) NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_SERVSUPL_COB_TH IS '[ SETH ] HISTORICO DE COBROS DE SERV.SUPLEMENTARIOS POR SUSP. PROGRAMADAHISTORICO DE COBROS DE SERV.SUPLEMENTARIOS POR SUSP. PROGRAMADA'
/
COMMENT ON COLUMN  GA_SERVSUPL_COB_TH.COD_SERVICIO IS 'Codigo de Servicio'
/
COMMENT ON COLUMN  GA_SERVSUPL_COB_TH.COD_SERVSUPL IS 'Codigo de Servicio Suplementario'
/
COMMENT ON COLUMN  GA_SERVSUPL_COB_TH.DES_SERVICIO IS 'Descripcion del Servicio '
/
COMMENT ON COLUMN  GA_SERVSUPL_COB_TH.FEC_INGRESO IS 'Fecha de Ingreso'
/
COMMENT ON COLUMN  GA_SERVSUPL_COB_TH.FEC_MODIFICA IS 'Fecha de Modificacion'
/
COMMENT ON COLUMN  GA_SERVSUPL_COB_TH.COD_USUARIO IS 'Usuario BD'
/
COMMENT ON COLUMN  GA_SERVSUPL_COB_TH.IND_COBRO IS 'Indicador de Cobro  0:No Cobra 1:Cobra'
/
