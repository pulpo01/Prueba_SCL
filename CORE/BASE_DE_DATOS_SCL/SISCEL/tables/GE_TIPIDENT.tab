CREATE TABLE GE_TIPIDENT(
    COD_TIPIDENT VARCHAR2(2) NOT NULL 
 ,  DES_TIPIDENT VARCHAR2(20) NOT NULL 
 ,  IND_PERTRIB VARCHAR2(1) NOT NULL 
 ,  IND_SALEFACT NUMBER(1,0) NOT NULL 
 ,  ORDEN_DISPLAY NUMBER(2,0) NULL 
 ,  IND_TIPIDENT NUMBER(1,0) NULL 
 ,  COD_TIPIDENTDIAN VARCHAR2(2) NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_TIPIDENT IS 'Tabla de tipos de identificacion- Retrofitted'
/
COMMENT ON COLUMN  GE_TIPIDENT.COD_TIPIDENT IS 'Codigo del tipo de identificacion - Retrofitted'
/
COMMENT ON COLUMN  GE_TIPIDENT.DES_TIPIDENT IS 'Descripcion del tipo de identificacion - Retrofitted'
/
COMMENT ON COLUMN  GE_TIPIDENT.IND_PERTRIB IS 'Indicativo de tipo de identificacion Personal o Tributaria (P/T) - Retrofitted'
/
COMMENT ON COLUMN  GE_TIPIDENT.IND_SALEFACT IS 'Indicativo de si sale la identificacion o no en la factura (S/N) (1/0) - Retrofitted'
/
COMMENT ON COLUMN  GE_TIPIDENT.ORDEN_DISPLAY IS 'Orden es despliegue.'
/
COMMENT ON COLUMN  GE_TIPIDENT.IND_TIPIDENT IS 'Caracteristica del tipo de identificador: principal=<pendiente>|secundario=<pendiente>.'
/
