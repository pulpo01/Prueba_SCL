CREATE TABLE TA_CTLLIMFRAROA(
    NUM_BLOQUE VARCHAR2(9) NOT NULL 
 ,  NUM_TICKETS NUMBER(9,0) NOT NULL 
 ,  NUM_EXITOS NUMBER(5,0) NOT NULL 
 ,  NUM_NEXITOS NUMBER(5,0) NOT NULL 
 ,  NUM_FRAUDE NUMBER(5,0) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_CTLLIMFRAROA IS 'Control de limite de roaming'
/
COMMENT ON COLUMN  TA_CTLLIMFRAROA.NUM_BLOQUE IS 'Numero de bloque de ejecucion'
/
COMMENT ON COLUMN  TA_CTLLIMFRAROA.NUM_TICKETS IS 'Numero de tickets ejecutado'
/
COMMENT ON COLUMN  TA_CTLLIMFRAROA.NUM_EXITOS IS 'Numero de exitos'
/
COMMENT ON COLUMN  TA_CTLLIMFRAROA.NUM_NEXITOS IS 'Numero de no exitos'
/
COMMENT ON COLUMN  TA_CTLLIMFRAROA.NUM_FRAUDE IS 'Numero de tickets en presunto fraude'
/
