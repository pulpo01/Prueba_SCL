CREATE TABLE GE_FACTORCLASIF_TD (  
   COD_CLASIF                    VARCHAR2(5)      NOT NULL   
 , DIAS_MORA_INI                 NUMBER(3)        NOT NULL   
 , DIAS_MORA_FIN                 NUMBER(3)        NOT NULL   
 , PORC_PRE_AUT                  NUMBER(3)            NULL   
 , FAC_LIM_CRE                   NUMBER(4,1)          NULL 
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  GE_FACTORCLASIF_TD                      IS '[ GE_FACL ] parámetros actualizacion Clasif. Cred - Mto PreAut.parámetros actualizacion Clasif. Cred - Mto PreAut.'
/

COMMENT ON COLUMN GE_FACTORCLASIF_TD.COD_CLASIF                     IS 'clasificacion crediticia'
/
COMMENT ON COLUMN GE_FACTORCLASIF_TD.DIAS_MORA_INI                  IS 'dias mínimos de mora'
/
COMMENT ON COLUMN GE_FACTORCLASIF_TD.DIAS_MORA_FIN                  IS 'días máximos de mora'
/
COMMENT ON COLUMN GE_FACTORCLASIF_TD.PORC_PRE_AUT                   IS 'porcentaje a aplicar para Momto Pre Autorizado'
/
COMMENT ON COLUMN GE_FACTORCLASIF_TD.FAC_LIM_CRE                    IS 'factor a aplicar para limite credito'
/

