CREATE TABLE AL_ACUM_PMP_AFIJO(
    NUM_ACTIVO NUMBER(10,0) NOT NULL 
 ,  FEC_CALCULO DATE NOT NULL 
 ,  IMP_CORRECCION NUMBER(14,4) NOT NULL 
 ,  IMP_DEPRECIACION NUMBER(14,4) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_ACUM_PMP_AFIJO IS 'Acumulados calculos P.M.P. Activo Fijo'
/
COMMENT ON COLUMN  AL_ACUM_PMP_AFIJO.NUM_ACTIVO IS 'Numero de Activo Fijo'
/
COMMENT ON COLUMN  AL_ACUM_PMP_AFIJO.FEC_CALCULO IS 'Fecha en la que se realizo el calculo.'
/
COMMENT ON COLUMN  AL_ACUM_PMP_AFIJO.IMP_CORRECCION IS 'Importe correspondiente al calculo de la correccion monetaria.'
/
COMMENT ON COLUMN  AL_ACUM_PMP_AFIJO.IMP_DEPRECIACION IS 'Importe correspondiente al calculo de la depreciacion monetaria.'
/
