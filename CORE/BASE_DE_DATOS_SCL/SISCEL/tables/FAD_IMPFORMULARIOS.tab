CREATE TABLE FAD_IMPFORMULARIOS(
    COD_FORMULARIO NUMBER(3,0) NOT NULL 
 ,  DES_FORMULARIO VARCHAR2(40) NOT NULL 
 ,  NUM_LINEAS NUMBER(3,0) NOT NULL 
 ,  NUM_COLUMNAS NUMBER(3,0) NOT NULL 
 ,  NUM_LARGOGLS NUMBER(3,0) NOT NULL 
 ,  COL_TOTALES NUMBER(3,0) NOT NULL 
 ,  IND_MENSAJES CHAR(1) NOT NULL 
 ,  IND_LEGAL CHAR(1) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAD_IMPFORMULARIOS IS 'Formularios De Impresion.'
/
COMMENT ON COLUMN  FAD_IMPFORMULARIOS.COD_FORMULARIO IS 'Codigo de formulario de impresion..'
/
COMMENT ON COLUMN  FAD_IMPFORMULARIOS.DES_FORMULARIO IS 'Descripcion del formulario..'
/
COMMENT ON COLUMN  FAD_IMPFORMULARIOS.NUM_LINEAS IS 'Numero de lineas fisicas del archivo representativo del formulario para 1 cliente (cuando proceda)..'
/
COMMENT ON COLUMN  FAD_IMPFORMULARIOS.NUM_COLUMNAS IS 'Numero de columnas fisicas del archivo representativo del formulario para 1 cliente (cuando proceda)..'
/
COMMENT ON COLUMN  FAD_IMPFORMULARIOS.NUM_LARGOGLS IS 'Largo de Glosa de Grupos.'
/
COMMENT ON COLUMN  FAD_IMPFORMULARIOS.COL_TOTALES IS 'Columna de alineacion de totales.'
/
COMMENT ON COLUMN  FAD_IMPFORMULARIOS.IND_MENSAJES IS 'Indicador de Adminsion de mensajes en formulario.'
/
COMMENT ON COLUMN  FAD_IMPFORMULARIOS.IND_LEGAL IS 'Indicador de Legalidad del Documento.'
/
