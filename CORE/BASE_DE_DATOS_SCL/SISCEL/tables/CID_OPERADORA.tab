CREATE TABLE CID_OPERADORA(
    COD_OPERADORA NUMBER(5,0) NOT NULL 
 ,  DES_OPERADORA VARCHAR2(60) NULL 
 ,  COD_VIGENCIA CHAR(1) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CID_OPERADORA IS 'Tabla De Parametros Operadoras.'
/
COMMENT ON COLUMN  CID_OPERADORA.COD_OPERADORA IS 'Codigo de Identificacion de un Operador.'
/
COMMENT ON COLUMN  CID_OPERADORA.DES_OPERADORA IS 'Descripcion o nombre del operador.'
/
COMMENT ON COLUMN  CID_OPERADORA.COD_VIGENCIA IS 'Indicador de si el registro esta vigente o no esta vigente..'
/
