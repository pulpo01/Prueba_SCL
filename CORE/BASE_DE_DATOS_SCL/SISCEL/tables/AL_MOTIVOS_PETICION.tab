CREATE TABLE AL_MOTIVOS_PETICION(
    COD_MOTIVO NUMBER(2,0) NOT NULL 
 ,  DES_MOTIVO VARCHAR2(20) NULL 
 ,  IND_URGENCIA NUMBER(1,0) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_MOTIVOS_PETICION IS 'Motivos Peticion Devoluciones'
/
COMMENT ON COLUMN  AL_MOTIVOS_PETICION.COD_MOTIVO IS 'Codigo del motivo'
/
COMMENT ON COLUMN  AL_MOTIVOS_PETICION.DES_MOTIVO IS 'Descripcion del motivo'
/
COMMENT ON COLUMN  AL_MOTIVOS_PETICION.IND_URGENCIA IS 'Urgente Si / No'
/
