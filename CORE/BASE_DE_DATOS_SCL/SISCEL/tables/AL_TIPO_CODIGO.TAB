CREATE TABLE AL_TIPO_CODIGO(
    TIP_CODIGO VARCHAR2(10) NOT NULL 
 ,  NOM_CODIGO VARCHAR2(40) NOT NULL 
 ,  NOM_TABLA VARCHAR2(30) NOT NULL 
 ,  CAMPO_CODIGO VARCHAR2(20) NOT NULL 
 ,  CAMPO_DESCRIPCION VARCHAR2(20) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  AL_TIPO_CODIGO.TIP_CODIGO IS 'Tipo de Codigo de Conversion'
/
COMMENT ON COLUMN  AL_TIPO_CODIGO.NOM_CODIGO IS 'Tipo de Entidad a Homologar'
/
COMMENT ON COLUMN  AL_TIPO_CODIGO.NOM_TABLA IS 'Nombre de Tabla de codigo a homologar'
/
COMMENT ON COLUMN  AL_TIPO_CODIGO.CAMPO_CODIGO IS 'Campo codigo de la tabla'
/
COMMENT ON COLUMN  AL_TIPO_CODIGO.CAMPO_DESCRIPCION IS 'Campo descripcion del codigo en Tabla '
/
COMMENT ON COLUMN  AL_TIPO_CODIGO.NOM_USUARIO IS 'Usuario ultima alteracion'
/
COMMENT ON COLUMN  AL_TIPO_CODIGO.FEC_ULTMOD IS 'Fecha ultima alteracion'
/
