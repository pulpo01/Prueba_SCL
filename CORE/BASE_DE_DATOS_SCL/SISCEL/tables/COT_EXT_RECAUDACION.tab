CREATE TABLE COT_EXT_RECAUDACION(
    COD_ENTIDAD VARCHAR2(6) NOT NULL 
 ,  NUM_TRANSACCION NUMBER(10,0) NOT NULL 
 ,  NUM_IDENT VARCHAR2(20) NOT NULL 
 ,  IMP_TOTAL NUMBER(14,4) NOT NULL 
 ,  FEC_PAGO DATE NOT NULL 
 ,  NUM_EJERCICIO NUMBER(12,0) NOT NULL 
 ,  IND_RECHAZO NUMBER(1,0) NOT NULL 
 ,  IND_PROCESADO NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  COT_EXT_RECAUDACION IS 'Documentacion Externa.'
/
COMMENT ON COLUMN  COT_EXT_RECAUDACION.COD_ENTIDAD IS 'Codigo de Entidad de Cobranza.'
/
COMMENT ON COLUMN  COT_EXT_RECAUDACION.NUM_TRANSACCION IS 'NUMERO DE TRANSACCION.'
/
COMMENT ON COLUMN  COT_EXT_RECAUDACION.NUM_IDENT IS 'Rut Cliente.'
/
COMMENT ON COLUMN  COT_EXT_RECAUDACION.IMP_TOTAL IS 'Importe Total.'
/
COMMENT ON COLUMN  COT_EXT_RECAUDACION.FEC_PAGO IS 'Fecha de Pago.'
/
COMMENT ON COLUMN  COT_EXT_RECAUDACION.NUM_EJERCICIO IS 'Numero de Ejercicio.'
/
COMMENT ON COLUMN  COT_EXT_RECAUDACION.IND_RECHAZO IS 'Indicador de Rechazo.'
/
COMMENT ON COLUMN  COT_EXT_RECAUDACION.IND_PROCESADO IS 'Indicador de Procesado.'
/
