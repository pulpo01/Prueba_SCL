CREATE TABLE AL_GRUPO_TECNOLOGIA_TD(
    COD_GRUPO VARCHAR2(7) NOT NULL 
 ,  DES_GRUPO VARCHAR2(50) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(50) DEFAULT USER NOT NULL 
 ,  FEC_ULTIMOD DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_GRUPO_TECNOLOGIA_TD IS 'TABLA QUE CONTIENE LOS GRUPOS DE TECNOLOGIA'
/
COMMENT ON COLUMN  AL_GRUPO_TECNOLOGIA_TD.COD_GRUPO IS 'CODIGO DE GRUPO'
/
COMMENT ON COLUMN  AL_GRUPO_TECNOLOGIA_TD.DES_GRUPO IS 'DESCRIPCION GRUPO'
/
COMMENT ON COLUMN  AL_GRUPO_TECNOLOGIA_TD.NOM_USUARIO IS 'USUARIO'
/
COMMENT ON COLUMN  AL_GRUPO_TECNOLOGIA_TD.FEC_ULTIMOD IS 'ULTIMA FECHA DE MODIFICACION'
/
