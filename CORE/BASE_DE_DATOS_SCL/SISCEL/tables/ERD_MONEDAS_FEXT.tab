CREATE TABLE ERD_MONEDAS_FEXT(
    COD_MONEDA_SISCEL VARCHAR2(3) NOT NULL 
 ,  COD_MONEDA_EXT VARCHAR2(15) NOT NULL 
 ,  COD_OPERADOR VARCHAR2(20) NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERD_MONEDAS_FEXT IS 'Traduccion De Monedas De Fuentes Externas.'
/
COMMENT ON COLUMN  ERD_MONEDAS_FEXT.COD_MONEDA_SISCEL IS 'Codigo moneda Siscel. .'
/
COMMENT ON COLUMN  ERD_MONEDAS_FEXT.COD_MONEDA_EXT IS 'Codigo moneda entregado por alguna de las fuentes externas (DICOM, Camara de Comercio, etc.).'
/
COMMENT ON COLUMN  ERD_MONEDAS_FEXT.COD_OPERADOR IS 'Codigo de la fuentes externa. Ver en tabla ERD_PARAMETROS..'
/
