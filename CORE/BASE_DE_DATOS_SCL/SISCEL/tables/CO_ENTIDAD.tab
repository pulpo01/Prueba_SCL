CREATE TABLE CO_ENTIDAD(
    COD_ENTIDAD NUMBER(8,0) NOT NULL 
 ,  NOM_ENTIDAD VARCHAR2(30) NOT NULL 
 ,  NOM_APE1ENTIDAD VARCHAR2(30) NOT NULL 
 ,  NOM_APE2ENTIDAD VARCHAR2(30) NOT NULL 
 ,  FECHA_ALTA DATE NOT NULL 
 ,  IND_INTERNA NUMBER(1,0) NOT NULL 
 ,  COD_RIESGO NUMBER(1,0) NULL 
 ,  COD_DISPER NUMBER(1,0) NULL 
 ,  COD_TIPCOMISION NUMBER(2,0) NULL 
 ,  NUM_CELULAR NUMBER(15,0) NULL 
 ,  NUM_IDENT VARCHAR2(20) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_ENTIDAD IS 'Tabla de mantenimientos de las entidades del sistema'
/
COMMENT ON COLUMN  CO_ENTIDAD.COD_ENTIDAD IS 'Csdigo de la entidad ( Secuencia )'
/
COMMENT ON COLUMN  CO_ENTIDAD.NOM_ENTIDAD IS 'Nombre de la entidad'
/
COMMENT ON COLUMN  CO_ENTIDAD.NOM_APE1ENTIDAD IS 'Apellido 1 de la entidad'
/
COMMENT ON COLUMN  CO_ENTIDAD.NOM_APE2ENTIDAD IS 'Apellido 2 de la entidad'
/
COMMENT ON COLUMN  CO_ENTIDAD.FECHA_ALTA IS 'Fecha de alta de la entidad'
/
COMMENT ON COLUMN  CO_ENTIDAD.IND_INTERNA IS 'Indicador para saber si la entidad es interna (1) o externa (0)'
/
COMMENT ON COLUMN  CO_ENTIDAD.COD_RIESGO IS 'FACTOR DE RIESGO'
/
COMMENT ON COLUMN  CO_ENTIDAD.COD_DISPER IS 'FACTOR DE DISPERSION'
/
COMMENT ON COLUMN  CO_ENTIDAD.COD_TIPCOMISION IS 'CODIGO DE COMISION DEL COBRADOR'
/
COMMENT ON COLUMN  CO_ENTIDAD.NUM_CELULAR IS 'NUMERO DE CELULAR - Retrofitted'
/
