CREATE TABLE SP_ESTADOS_ORDEN(
    COD_ESTADO_ORDEN NUMBER(2,0) NOT NULL 
 ,  DES_ESTADO_ORDEN VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SP_ESTADOS_ORDEN IS 'Diferentes estados por los que puede pasar una Orden'
/
