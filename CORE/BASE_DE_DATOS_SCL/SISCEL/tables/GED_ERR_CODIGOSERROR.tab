CREATE TABLE GED_ERR_CODIGOSERROR(
    COD_ERROR NUMBER(6,0) NOT NULL 
 ,  DES_ERROR VARCHAR2(60) NOT NULL 
 ,  COD_TRANSACCION NUMBER(6,0) NOT NULL 
 ,  TIP_ERROR VARCHAR2(8) NOT NULL 
 ,  COD_DERIVACION VARCHAR2(6) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GED_ERR_CODIGOSERROR IS 'Define Los Errores Que Pueden Ocurrir Durante Una Transaccion.'
/
COMMENT ON COLUMN  GED_ERR_CODIGOSERROR.COD_ERROR IS 'Codigo de error..'
/
COMMENT ON COLUMN  GED_ERR_CODIGOSERROR.DES_ERROR IS 'Descripcion del error.'
/
COMMENT ON COLUMN  GED_ERR_CODIGOSERROR.COD_TRANSACCION IS 'Transaccion sobre la que se define el error.'
/
COMMENT ON COLUMN  GED_ERR_CODIGOSERROR.TIP_ERROR IS 'tipo de error (Grave-Warning.Leve)....'
/
COMMENT ON COLUMN  GED_ERR_CODIGOSERROR.COD_DERIVACION IS 'Derivacion inicial del error, Quien debe hacerse cargo..'
/
COMMENT ON COLUMN  GED_ERR_CODIGOSERROR.FEC_ULTMOD IS 'Fecha de ultima modificacion.'
/
COMMENT ON COLUMN  GED_ERR_CODIGOSERROR.NOM_USUARIO IS 'Usuario de creacion/modificacion.'
/
