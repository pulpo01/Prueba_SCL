CREATE TABLE AL_PLANTILLAS_KIT(
    COD_KIT NUMBER(6,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  CAN_ARTICULO NUMBER(9,0) NOT NULL 
 ,  FEC_CREACION DATE NULL 
 ,  COD_USUA VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_PLANTILLAS_KIT IS 'Tabla De Definicion De Kit.'
/
COMMENT ON COLUMN  AL_PLANTILLAS_KIT.COD_KIT IS 'Codigo de Articulo KIT..'
/
COMMENT ON COLUMN  AL_PLANTILLAS_KIT.COD_ARTICULO IS 'Codigo de Articulo de composicion..'
/
COMMENT ON COLUMN  AL_PLANTILLAS_KIT.CAN_ARTICULO IS 'Cantidad de Articulos..'
/
COMMENT ON COLUMN  AL_PLANTILLAS_KIT.FEC_CREACION IS 'Fecha de creacion de registro..'
/
COMMENT ON COLUMN  AL_PLANTILLAS_KIT.COD_USUA IS 'Usuario de creacion o modificacion de registro..'
/
