CREATE TABLE VE_OFIMATRIZ_TH(
    COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  COD_MATRIZ VARCHAR2(2) NOT NULL 
 ,  USU_CREACION VARCHAR2(100) DEFAULT 'USER' NOT NULL 
 ,  FEC_REGISTRO DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_ACCION NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_OFIMATRIZ_TH IS '[ VE_OFIMAT_TH ] TABLA HISTORICA DE CREACION Y ELIMINACION DE RELACION OFIMATRIZTABLA HISTORICA DE CREACION Y ELIMINACION DE RELACION OFIMATRIZ'
/
COMMENT ON COLUMN  VE_OFIMATRIZ_TH.COD_OFICINA IS 'CODIGO DE OFICINA'
/
COMMENT ON COLUMN  VE_OFIMATRIZ_TH.COD_MATRIZ IS 'CODIGO DE MATRIZ'
/
COMMENT ON COLUMN  VE_OFIMATRIZ_TH.USU_CREACION IS 'USUARIO CREADOR'
/
COMMENT ON COLUMN  VE_OFIMATRIZ_TH.FEC_REGISTRO IS 'FECHA DE CREACION'
/
COMMENT ON COLUMN  VE_OFIMATRIZ_TH.COD_ACCION IS 'ACCION REALIZADA (0 =INSERT; 1 = DELETE)'
/
