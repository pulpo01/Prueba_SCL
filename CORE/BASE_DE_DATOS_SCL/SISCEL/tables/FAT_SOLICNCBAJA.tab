CREATE TABLE FAT_SOLICNCBAJA(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_CICLFACT NUMBER(6,0) NOT NULL 
 ,  FEC_SOLICITUD DATE NOT NULL 
 ,  NUM_PROCESO NUMBER(8,0) DEFAULT 0 NOT NULL 
 ,  FEC_PROCESO DATE NULL 
 ,  FLG_EMISION NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  GLS_EMISION VARCHAR2(60) NULL 
 ,  NOM_USUARIO_SOLIC VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAT_SOLICNCBAJA IS 'Tabla De Solicitud De Notas De Credito (Pendientes).'
/
COMMENT ON COLUMN  FAT_SOLICNCBAJA.COD_CLIENTE IS 'CODIGO DEL CLIENTE.'
/
COMMENT ON COLUMN  FAT_SOLICNCBAJA.NUM_ABONADO IS 'NUMERO DE L ABONADO.'
/
COMMENT ON COLUMN  FAT_SOLICNCBAJA.COD_CICLFACT IS 'CICLO DE FATURACION.'
/
COMMENT ON COLUMN  FAT_SOLICNCBAJA.FEC_SOLICITUD IS 'FECHA DE LA BAJA O CAMBIO DE PLAN INMEDIATO.'
/
COMMENT ON COLUMN  FAT_SOLICNCBAJA.NUM_PROCESO IS 'NUERO DE PROCESO EN LA INTERFAZ DE FACTURACION.'
/
COMMENT ON COLUMN  FAT_SOLICNCBAJA.FEC_PROCESO IS 'FECHA DE PROCESO DE LA SOLICITUD..'
/
COMMENT ON COLUMN  FAT_SOLICNCBAJA.FLG_EMISION IS 'IDICADOR DE EMISION DE NC O NO EMISION (1:SI 0:NO).'
/
COMMENT ON COLUMN  FAT_SOLICNCBAJA.GLS_EMISION IS 'GLOSA DEL PROCESO DE  EMISION DE LA NOTA DE CREDITO..'
/
COMMENT ON COLUMN  FAT_SOLICNCBAJA.NOM_USUARIO_SOLIC IS 'USUARIO QUE ENCOLA LA SOLICITUD.'
/
COMMENT ON COLUMN  FAT_SOLICNCBAJA.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION DEL REGISTRO.'
/
COMMENT ON COLUMN  FAT_SOLICNCBAJA.NOM_USUARIO IS 'USUARIO QUE REALIZA EL ULTIMO MOVIMIENTO.'
/
