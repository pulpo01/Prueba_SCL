CREATE TABLE CO_VALRETRUT(
    COD_RUTINA VARCHAR2(5) NOT NULL 
 ,  TIP_RETORNO VARCHAR2(5) NOT NULL 
 ,  VAL_RETORNO VARCHAR2(10) NULL 
 ,  COD_CATEGORIA VARCHAR2(5) NULL 
 ,  NUM_DIAS NUMBER(3,0) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  CO_VALRETRUT.COD_CATEGORIA IS 'Categoria asociada.'
/
COMMENT ON COLUMN  CO_VALRETRUT.NUM_DIAS IS 'Dias de Vencida la Deuda.'
/
