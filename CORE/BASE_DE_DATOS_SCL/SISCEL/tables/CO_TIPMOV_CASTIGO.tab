CREATE TABLE CO_TIPMOV_CASTIGO(
    TIP_MOVIMIEN NUMBER(2,0) NOT NULL 
 ,  DES_MOVIMIEN VARCHAR2(40) DEFAULT ' ' NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_TIPMOV_CASTIGO IS 'Tipo De Movimientos Para Castigos Contables.'
/
COMMENT ON COLUMN  CO_TIPMOV_CASTIGO.TIP_MOVIMIEN IS 'Tipo de Movimiento 1:Castigo Contable ; 2:Castigo Recuperado ; 3:Castigo Eliminado.'
/
COMMENT ON COLUMN  CO_TIPMOV_CASTIGO.DES_MOVIMIEN IS 'Descripción del movimiento (verbal).'
/
