CREATE TABLE CMT_KIT_PREPAGO(
    COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_COMISIONISTA NUMBER(6,0) NOT NULL 
 ,  NUM_KITS NUMBER(6,0) NOT NULL 
 ,  COD_ESTADO VARCHAR2(3) DEFAULT 'ING' NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMT_KIT_PREPAGO IS 'Detalle De Kits Prepago Vendidos Por Comisionista.'
/
COMMENT ON COLUMN  CMT_KIT_PREPAGO.COD_PERIODO IS 'Periodo sobre el cual se aplica la meta..'
/
COMMENT ON COLUMN  CMT_KIT_PREPAGO.COD_TIPCOMIS IS 'Cdigo del canal por el cual se hizo la venta (10, 26, 29, ...)..'
/
COMMENT ON COLUMN  CMT_KIT_PREPAGO.COD_COMISIONISTA IS 'Cdigo del comisionista que hizo la venta (Master dealer o vendedor raz)..'
/
COMMENT ON COLUMN  CMT_KIT_PREPAGO.NUM_KITS IS 'Cantidad de Kits Amistar vendidos.'
/
COMMENT ON COLUMN  CMT_KIT_PREPAGO.COD_ESTADO IS 'Estado del registro. INGresado/LIQuidado.'
/
COMMENT ON COLUMN  CMT_KIT_PREPAGO.FEC_ULTMOD IS 'Fecha de ultima modificacion.'
/
COMMENT ON COLUMN  CMT_KIT_PREPAGO.NOM_USUARIO IS 'Usuario de ultima modificacion.'
/
