CREATE TABLE CO_PAC_COLASBATCH_TD(
    FEC_VALOR DATE NOT NULL 
 ,  COD_BANCO VARCHAR2(15) NOT NULL 
 ,  IND_PROCESADO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  COD_ESTADO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  COD_RETORNO NUMBER(8,0) NULL 
 ,  DESC_RETORNO VARCHAR2(500) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_PAC_COLASBATCH_TD IS 'Utilizada para proceso PAC en modo Batch'
/
COMMENT ON COLUMN  CO_PAC_COLASBATCH_TD.FEC_VALOR IS 'Fecha envio Banco'
/
COMMENT ON COLUMN  CO_PAC_COLASBATCH_TD.COD_BANCO IS 'Codigo de Banco'
/
COMMENT ON COLUMN  CO_PAC_COLASBATCH_TD.IND_PROCESADO IS 'Indicador de Proceso'
/
COMMENT ON COLUMN  CO_PAC_COLASBATCH_TD.COD_ESTADO IS 'Codigo indica estado'
/
COMMENT ON COLUMN  CO_PAC_COLASBATCH_TD.COD_RETORNO IS 'Codigo retorno CO_APLICAPAGO_UNIVERSAL'
/
COMMENT ON COLUMN  CO_PAC_COLASBATCH_TD.DESC_RETORNO IS 'Descripcion retorno CO_APLICAPAGO_UNIVERSAL'
/
