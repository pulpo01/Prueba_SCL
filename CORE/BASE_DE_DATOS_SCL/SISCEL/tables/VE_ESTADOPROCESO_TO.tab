CREATE TABLE VE_ESTADOPROCESO_TO(
    COD_PROCESO VARCHAR2(60) NOT NULL 
 ,  NUM_PROGRESOACTUAL NUMBER(3,0) NOT NULL 
 ,  NUM_SUBPROCACTUAL NUMBER(2,0) NOT NULL 
 ,  NUM_TOTALSUBPROC NUMBER(2,0) NOT NULL 
 ,  COD_ERROR NUMBER(3,0) NULL 
 ,  DES_ERROR VARCHAR2(250) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_ESTADOPROCESO_TO IS '[ ESTA ] Registro con estadísticas sobre el estado de avance de los subprocesos de registro de productos para el sistema SCL Web Activaciones Masivas'
/
COMMENT ON COLUMN  VE_ESTADOPROCESO_TO.COD_PROCESO IS 'Proceso asociado a session para control de barra de progreso'
/
COMMENT ON COLUMN  VE_ESTADOPROCESO_TO.NUM_PROGRESOACTUAL IS 'Porcentaje de avance de subproceso de registro de productos'
/
COMMENT ON COLUMN  VE_ESTADOPROCESO_TO.NUM_SUBPROCACTUAL IS 'Subproceso de regostro de productos en ejecución.Tarea de validación (1), Creación de líneas (2)'
/
COMMENT ON COLUMN  VE_ESTADOPROCESO_TO.NUM_TOTALSUBPROC IS 'Número total de productos a procesar en subproceso actual'
/
COMMENT ON COLUMN  VE_ESTADOPROCESO_TO.COD_ERROR IS 'Código de error en manejo de estado de avance'
/
COMMENT ON COLUMN  VE_ESTADOPROCESO_TO.DES_ERROR IS 'Descripción de error en manejo de estado de avance'
/
