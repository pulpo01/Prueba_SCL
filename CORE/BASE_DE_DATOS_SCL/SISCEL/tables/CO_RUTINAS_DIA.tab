CREATE TABLE CO_RUTINAS_DIA(
    COD_RUTINA VARCHAR2(5) NOT NULL 
 ,  FEC_RUTINA DATE NOT NULL 
 ,  NUM_EJECUTADAS NUMBER(8,0) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_RUTINAS_DIA IS 'Entidades De Cobranza.'
/
COMMENT ON COLUMN  CO_RUTINAS_DIA.COD_RUTINA IS 'Codigo Rutina Asociada.'
/
COMMENT ON COLUMN  CO_RUTINAS_DIA.FEC_RUTINA IS 'Fecha de Ejecucion de la Rutina.'
/
COMMENT ON COLUMN  CO_RUTINAS_DIA.NUM_EJECUTADAS IS 'Cantidad de ejecuciones disponibles para la Fecha de Ejecucion.'
/
