CREATE TABLE GE_DIRPARAOPERAD(
    COD_OPERAD VARCHAR2(5) NOT NULL 
 ,  TIP_SUJETO NUMBER(3,0) NOT NULL 
 ,  COD_DISPLAY NUMBER(3,0) NOT NULL 
 ,  DIR_PARA VARCHAR2(1000) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_DIRPARAOPERAD IS 'Direccion Parametrica De La Operadora.'
/
COMMENT ON COLUMN  GE_DIRPARAOPERAD.COD_OPERAD IS 'Codigo de la operadora.'
/
COMMENT ON COLUMN  GE_DIRPARAOPERAD.TIP_SUJETO IS 'Tipo de sujeto (1:Cliente, 2:Usuario).'
/
COMMENT ON COLUMN  GE_DIRPARAOPERAD.COD_DISPLAY IS 'Despliegue de direccion (1:Dir. Completa, 2:Identificando los param).'
/
COMMENT ON COLUMN  GE_DIRPARAOPERAD.DIR_PARA IS 'Query para la obtencion de la direccion.'
/
