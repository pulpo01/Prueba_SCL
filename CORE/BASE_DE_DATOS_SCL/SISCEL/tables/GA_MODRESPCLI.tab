CREATE TABLE GA_MODRESPCLI(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(1,0) NOT NULL 
 ,  FEC_MODIFICA DATE NOT NULL 
 ,  COD_TIPIDENT VARCHAR2(2) NOT NULL 
 ,  NUM_IDENT VARCHAR2(20) NOT NULL 
 ,  NOM_RESPONSABLE VARCHAR2(50) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_MODRESPCLI IS 'Modificacion de Responsables Legales de Clientes'
/
COMMENT ON COLUMN  GA_MODRESPCLI.COD_CLIENTE IS 'Codigo de Cliente'
/
COMMENT ON COLUMN  GA_MODRESPCLI.NUM_ORDEN IS 'Numero de Orden del Responsable'
/
COMMENT ON COLUMN  GA_MODRESPCLI.FEC_MODIFICA IS 'Fecha de Modificacion'
/
COMMENT ON COLUMN  GA_MODRESPCLI.COD_TIPIDENT IS 'Codigo Tipo Identificacion'
/
COMMENT ON COLUMN  GA_MODRESPCLI.NUM_IDENT IS 'Numero de Identificacion'
/
COMMENT ON COLUMN  GA_MODRESPCLI.NOM_RESPONSABLE IS 'Nombre de Responsable'
/
COMMENT ON COLUMN  GA_MODRESPCLI.NOM_USUARORA IS 'Nombre de Usuario Oracle'
/
