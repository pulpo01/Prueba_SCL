CREATE TABLE BPD_PARAMETROS(
    COD_QUERY VARCHAR2(5) NOT NULL 
 ,  TIP_QUERY CHAR(1) NOT NULL 
 ,  SEC_PARAM NUMBER(2,0) NOT NULL 
 ,  DES_PARAM VARCHAR2(30) NOT NULL 
 ,  TIP_PARAM VARCHAR2(5) NOT NULL 
 ,  MAX_LARGO NUMBER(2,0) NULL 
 ,  OBS_PARAM VARCHAR2(30) NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  TIP_DATO VARCHAR2(5) NULL 
 ,  COD_QUERYREL VARCHAR2(5) NULL 
 ,  FLG_OBLIGATORIO CHAR(1) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  BPD_PARAMETROS IS 'Parametros De Beneficios.'
/
COMMENT ON COLUMN  BPD_PARAMETROS.COD_QUERY IS 'Codigo que identifica al Query.'
/
COMMENT ON COLUMN  BPD_PARAMETROS.TIP_QUERY IS 'Tipo de Query a aplicar M: masivo, P: puntual.'
/
COMMENT ON COLUMN  BPD_PARAMETROS.SEC_PARAM IS 'Secuencia del parametro.'
/
COMMENT ON COLUMN  BPD_PARAMETROS.DES_PARAM IS 'Descripcion del parametro.'
/
COMMENT ON COLUMN  BPD_PARAMETROS.TIP_PARAM IS 'Tipo de Datoa sociado al parametro.'
/
COMMENT ON COLUMN  BPD_PARAMETROS.MAX_LARGO IS 'Maximo Tama?o asociado al Tipo de Dato.'
/
COMMENT ON COLUMN  BPD_PARAMETROS.OBS_PARAM IS 'Observaciones o comentarios acerca del parametro.'
/
COMMENT ON COLUMN  BPD_PARAMETROS.FEC_ULTMOD IS 'Fecha ultima actualizacion.'
/
COMMENT ON COLUMN  BPD_PARAMETROS.NOM_USUARIO IS 'Nombre usuario responsable.'
/
COMMENT ON COLUMN  BPD_PARAMETROS.TIP_DATO IS 'Tipo de Dato relacionado.'
/
COMMENT ON COLUMN  BPD_PARAMETROS.COD_QUERYREL IS 'Query relacionado.'
/
COMMENT ON COLUMN  BPD_PARAMETROS.FLG_OBLIGATORIO IS 'indica si el parametro es obligatorio.'
/
