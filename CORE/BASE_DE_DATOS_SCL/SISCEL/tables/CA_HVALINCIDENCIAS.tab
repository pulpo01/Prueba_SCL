CREATE TABLE CA_HVALINCIDENCIAS(
    NUM_INCIDENCIA NUMBER(9,0) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  COD_CAMPO NUMBER(3,0) NOT NULL 
 ,  VAL_ALFA VARCHAR2(2000) NULL 
 ,  VAL_NUMER NUMBER(31,9) NULL 
 ,  VAL_FECHA DATE NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_HVALINCIDENCIAS IS 'Historico Valores Campos de Incidencias'
/
COMMENT ON COLUMN  CA_HVALINCIDENCIAS.NUM_INCIDENCIA IS 'Numero incidencia'
/
COMMENT ON COLUMN  CA_HVALINCIDENCIAS.FEC_HISTORICO IS 'Fecha Historico'
/
COMMENT ON COLUMN  CA_HVALINCIDENCIAS.COD_CAMPO IS 'Campo'
/
COMMENT ON COLUMN  CA_HVALINCIDENCIAS.VAL_ALFA IS 'Valor alfanumerico'
/
COMMENT ON COLUMN  CA_HVALINCIDENCIAS.VAL_NUMER IS 'Valor numerico'
/
COMMENT ON COLUMN  CA_HVALINCIDENCIAS.VAL_FECHA IS 'Valor fecha'
/
