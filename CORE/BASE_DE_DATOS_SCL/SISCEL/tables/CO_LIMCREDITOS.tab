CREATE TABLE CO_LIMCREDITOS(
    COD_CREDMOR NUMBER(3,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  DES_CREDMOR VARCHAR2(40) NOT NULL 
 ,  COD_CALCLIEN VARCHAR2(2) NOT NULL 
 ,  IMP_MOROSIDAD NUMBER(14,4) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_LIMCREDITOS IS 'LIMITE DE CREDITO'
/
COMMENT ON COLUMN  CO_LIMCREDITOS.COD_CREDMOR IS 'CODIGO DE CREDITO DE MOROSIDAD'
/
COMMENT ON COLUMN  CO_LIMCREDITOS.COD_PRODUCTO IS 'CODIGO DE PRODUCTO'
/
COMMENT ON COLUMN  CO_LIMCREDITOS.DES_CREDMOR IS 'DESCRIPCION DE CODIGO DE CREDITO DE MOROSIDAD'
/
COMMENT ON COLUMN  CO_LIMCREDITOS.COD_CALCLIEN IS 'CALIDAD DE CLIENTE'
/
COMMENT ON COLUMN  CO_LIMCREDITOS.IMP_MOROSIDAD IS 'IMPORTE DE MOROSIDAD'
/
