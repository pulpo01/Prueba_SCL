CREATE TABLE SP_HSERIES_REEMPLAZO(
    NUM_ORDEN NUMBER(9,0) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  NUM_LINEA NUMBER(2,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  NUM_SERIEMEC VARCHAR2(20) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SP_HSERIES_REEMPLAZO IS 'Historiico de Series de los accesorios seriados entregados en prestamo al clientepara ordenes finalizadas'
/
COMMENT ON COLUMN  SP_HSERIES_REEMPLAZO.NUM_ORDEN IS 'Numero de Orden de Reparacion a que pertenece el Numero de serie deaccesorio de reemplazo seriado'
/
COMMENT ON COLUMN  SP_HSERIES_REEMPLAZO.FEC_HISTORICO IS 'Fecha en que la orden paso al historico'
/
COMMENT ON COLUMN  SP_HSERIES_REEMPLAZO.NUM_LINEA IS 'Numero de linea de accesorios reemplazo'
/
COMMENT ON COLUMN  SP_HSERIES_REEMPLAZO.NUM_SERIE IS 'Numero de serie de accesorio de reemplazo seriado'
/
COMMENT ON COLUMN  SP_HSERIES_REEMPLAZO.NUM_SERIEMEC IS 'numero de serie mecanico de la serie de reemplazo'
/
