CREATE TABLE VE_VENTASERV(
    COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  FEC_ACEPTVENTA DATE NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_NIVEL NUMBER(4,0) NOT NULL 
 ,  IMP_COMISION NUMBER(12,4) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_MDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_VENTASERV IS 'Venta de Servicios Suplementarios'
/
COMMENT ON COLUMN  VE_VENTASERV.COD_SERVICIO IS 'Codigo de Servicio'
/
COMMENT ON COLUMN  VE_VENTASERV.NUM_ABONADO IS 'Numero de Abonado'
/
COMMENT ON COLUMN  VE_VENTASERV.FEC_ACEPTVENTA IS 'Fecha de aceptacion de la venta'
/
COMMENT ON COLUMN  VE_VENTASERV.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  VE_VENTASERV.COD_TIPCOMIS IS 'Codigo de Tipo de Comisionista'
/
COMMENT ON COLUMN  VE_VENTASERV.COD_VENDEDOR IS 'Codigo de Vendedor'
/
COMMENT ON COLUMN  VE_VENTASERV.COD_NIVEL IS 'Codigo de Nivel de Servicio'
/
COMMENT ON COLUMN  VE_VENTASERV.IMP_COMISION IS 'Importe de la comision'
/
