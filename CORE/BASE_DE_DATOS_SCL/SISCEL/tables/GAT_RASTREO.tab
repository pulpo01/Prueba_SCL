CREATE TABLE GAT_RASTREO(
    NUM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_OOSS DATE NOT NULL 
 ,  CLASE_SERVICIO_ACT VARCHAR2(255) NOT NULL 
 ,  PERFIL_ABONADO_ACT VARCHAR2(255) NOT NULL 
 ,  CLASE_SERVICIO_GEN VARCHAR2(255) NULL 
 ,  COD_OOSS VARCHAR2(5) NOT NULL 
 ,  NUM_MOV_CENTRAL NUMBER(9,0) NULL 
 ,  IND_ENTRADA VARCHAR2(1) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAT_RASTREO IS 'Tabla Log Para Activacion de Servicios.'
/
COMMENT ON COLUMN  GAT_RASTREO.NUM_USUARIO IS 'USUARIO siscel.'
/
COMMENT ON COLUMN  GAT_RASTREO.FEC_OOSS IS 'Fecha OOSS.'
/
COMMENT ON COLUMN  GAT_RASTREO.CLASE_SERVICIO_ACT IS 'Clase servicio antes de OOSS.'
/
COMMENT ON COLUMN  GAT_RASTREO.PERFIL_ABONADO_ACT IS 'Perfil del abonado antes de OOSS.'
/
COMMENT ON COLUMN  GAT_RASTREO.CLASE_SERVICIO_GEN IS 'Clase de Servicio posterior a OOSS.'
/
COMMENT ON COLUMN  GAT_RASTREO.COD_OOSS IS 'Codigo de la OOSS.'
/
COMMENT ON COLUMN  GAT_RASTREO.NUM_MOV_CENTRAL IS 'Numero de Movimiento en central'
/
COMMENT ON COLUMN  GAT_RASTREO.IND_ENTRADA IS 'Indicativo de origen del rastreo.'
/
