CREATE TABLE GC_CODIGOS_TD(
    NOM_TABLA VARCHAR2(30) NOT NULL 
 ,  NOM_COLUMNA VARCHAR2(50) NOT NULL 
 ,  COD_VALOR VARCHAR2(5) NOT NULL 
 ,  DES_VALOR VARCHAR2(50) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) DEFAULT USER NOT NULL 
 ,  FEC_ULTMOD DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GC_CODIGOS_TD IS 'Codigos de GC'
/
COMMENT ON COLUMN  GC_CODIGOS_TD.FEC_ULTMOD IS 'Fecha de ultima modificacion'
/
COMMENT ON COLUMN  GC_CODIGOS_TD.NOM_TABLA IS 'Nombre de la tabla a la que pertenece el campo'
/
COMMENT ON COLUMN  GC_CODIGOS_TD.NOM_COLUMNA IS 'Nombre de campo al que se asigna el codigo'
/
COMMENT ON COLUMN  GC_CODIGOS_TD.COD_VALOR IS 'Valor del codigo'
/
COMMENT ON COLUMN  GC_CODIGOS_TD.DES_VALOR IS 'Descripcion del codigo'
/
COMMENT ON COLUMN  GC_CODIGOS_TD.NOM_USUARORA IS 'Usuario Oracle'
/
