 ,  IND_SOLICITANTE VARCHAR2(1) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_SEG_USUPROG IS 'USUARIOS PERMITIDOS PARA TRANSMITIR UN EJECUTABLE'
/
