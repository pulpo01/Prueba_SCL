CREATE TABLE CO_TIPVALOR(
    TIP_VALOR NUMBER(2,0) NOT NULL 
 ,  DES_TIPVALOR VARCHAR2(20) NOT NULL 
 ,  IND_EFECTIVO NUMBER(1,0) NOT NULL 
 ,  IND_OC NUMBER(1,0) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  IND_ACTIVO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  IND_DEPOSITO NUMBER(1,0) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_TIPVALOR IS 'Tipos de Valores para Pagos'
/
COMMENT ON COLUMN  CO_TIPVALOR.TIP_VALOR IS 'Tipo de Valor para Pago'
/
COMMENT ON COLUMN  CO_TIPVALOR.DES_TIPVALOR IS 'Descripcion de Tipo de Valor'
/
COMMENT ON COLUMN  CO_TIPVALOR.IND_EFECTIVO IS 'Indicador de Valor Efectivo'
/
COMMENT ON COLUMN  CO_TIPVALOR.IND_OC IS 'Indicador de Tipo Valor = Orden de Compra'
/
COMMENT ON COLUMN  CO_TIPVALOR.COD_MONEDA IS 'Código de moneda'
/
COMMENT ON COLUMN  CO_TIPVALOR.IND_ACTIVO IS 'Indicador de Valor Activo para Alta Facturacion'
/
COMMENT ON COLUMN  CO_TIPVALOR.IND_DEPOSITO IS 'indicador de deposito'
/
