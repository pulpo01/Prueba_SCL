CREATE TABLE CO_CLIENTE_PROVEEDOR(
    NUM_IDENT VARCHAR2(20) NOT NULL 
 ,  FEC_DESDE DATE DEFAULT sysdate NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_CLIENTE_PROVEEDOR IS 'CLIENTES QUE TAMBIEN SON PROVEEDORES'
/
COMMENT ON COLUMN  CO_CLIENTE_PROVEEDOR.NUM_IDENT IS 'RUT DEL CLIENTE-PROVEEDOR'
/
COMMENT ON COLUMN  CO_CLIENTE_PROVEEDOR.FEC_DESDE IS 'FECHA DESDE LA CUAL PASA A SER CLIENTE-PROVEEDOR'
/
COMMENT ON COLUMN  CO_CLIENTE_PROVEEDOR.FEC_HASTA IS 'FECHA HASTA LA CUAL ESTA HABILITADO PARA HACER CANJES (NULL-> HABILITADO)'
/
