CREATE TABLE CM_PERMANENCIARET_TD(
    COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) NOT NULL 
 ,  COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  TIP_BONIFICACION CHAR(1) NOT NULL 
 ,  IMP_BONIFICACION NUMBER(14,4) NOT NULL 
 ,  NUM_PERIODOS NUMBER(8,0) NOT NULL 
 ,  COD_CONCEPTO_ASOC NUMBER(3,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_PERMANENCIARET_TD IS 'SE CONFIGURA LA PERMANENCIA PARA LAS COMISIONES DE RETENCIONES'
/
COMMENT ON COLUMN  CM_PERMANENCIARET_TD.COD_TIPORED IS 'CSDIGO DEL TIPO DE RED'
/
COMMENT ON COLUMN  CM_PERMANENCIARET_TD.COD_PLANCOMIS IS 'CSDIGO DEL PLAN DE COMISIONES'
/
COMMENT ON COLUMN  CM_PERMANENCIARET_TD.COD_CONCEPTO IS 'CSDIGO DEL CONCEPTO'
/
COMMENT ON COLUMN  CM_PERMANENCIARET_TD.TIP_BONIFICACION IS 'TIPO DE BONIFICACISN'
/
COMMENT ON COLUMN  CM_PERMANENCIARET_TD.IMP_BONIFICACION IS 'IMPORTE DE BONIFICACISN'
/
COMMENT ON COLUMN  CM_PERMANENCIARET_TD.NUM_PERIODOS IS 'NZMERO DE PERMODOS'
/
COMMENT ON COLUMN  CM_PERMANENCIARET_TD.COD_CONCEPTO_ASOC IS 'CONCEPTO ASOCIADO'
/
COMMENT ON COLUMN  CM_PERMANENCIARET_TD.FEC_ULTMOD IS 'FECHA DE ZLTIMA MODIFICACISN'
/
COMMENT ON COLUMN  CM_PERMANENCIARET_TD.NOM_USUARIO IS 'USUARIO QUE REALIZS ZLTIMA MODIFICACISN'
/
