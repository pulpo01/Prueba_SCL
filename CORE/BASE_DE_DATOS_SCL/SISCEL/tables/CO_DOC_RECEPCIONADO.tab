CREATE TABLE CO_DOC_RECEPCIONADO(
    NUM_IDENT VARCHAR2(20) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_TIPDOCUM NUMBER(2,0) NOT NULL 
 ,  NUM_FOLIO NUMBER(9,0) NOT NULL 
 ,  FEC_EMISION DATE NOT NULL 
 ,  SALDO_IMPAGO NUMBER(14,4) NOT NULL 
 ,  IND_NETEO VARCHAR2(1) NOT NULL 
 ,  MONTO_NETEADO NUMBER(14,4) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_DOC_RECEPCIONADO IS 'RECEPCION DE ARCHIVOS OFA, PREVIO A ACEPTACION'
/
COMMENT ON COLUMN  CO_DOC_RECEPCIONADO.NUM_IDENT IS 'RUT DEL CLIENTE-PROVEEDOR'
/
COMMENT ON COLUMN  CO_DOC_RECEPCIONADO.COD_CLIENTE IS 'CODIGO DE CLIENTE ASOCIADO AL RUT'
/
COMMENT ON COLUMN  CO_DOC_RECEPCIONADO.COD_TIPDOCUM IS 'TIPO DE DOCUMENTO'
/
COMMENT ON COLUMN  CO_DOC_RECEPCIONADO.NUM_FOLIO IS 'NUMERO DE FOLIO'
/
COMMENT ON COLUMN  CO_DOC_RECEPCIONADO.FEC_EMISION IS 'FECHA DE EMISION'
/
COMMENT ON COLUMN  CO_DOC_RECEPCIONADO.SALDO_IMPAGO IS 'SALDO IMPAGO'
/
COMMENT ON COLUMN  CO_DOC_RECEPCIONADO.IND_NETEO IS 'INDICADOR DE NETEO : Y:TOTAL; P:PARCIAL; N:NO NETEADO'
/
COMMENT ON COLUMN  CO_DOC_RECEPCIONADO.MONTO_NETEADO IS 'MONTO NETEADO EFECTIVAMENTE'
/
