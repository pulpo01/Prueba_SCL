CREATE TABLE GE_BANCOS(
    COD_BANCO VARCHAR2(15) NOT NULL 
 ,  DES_BANCO VARCHAR2(40) NOT NULL 
 ,  COD_BCOI NUMBER(1,0) NULL 
 ,  IND_PAC NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  COD_DICOM VARCHAR2(3) NULL 
 ,  COD_BANCO_ON2 VARCHAR2(15) DEFAULT '1' NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_BANCOS IS 'Tabla de bancos'
/
COMMENT ON COLUMN  GE_BANCOS.COD_BANCO IS 'Codigo de banco'
/
COMMENT ON COLUMN  GE_BANCOS.DES_BANCO IS 'Descripcion del banco'
/
COMMENT ON COLUMN  GE_BANCOS.COD_DICOM IS 'CODIGO DICOM DEL BANCO.'
/
COMMENT ON COLUMN  GE_BANCOS.COD_BANCO_ON2 IS 'Codigo interno del ON2.'
/
