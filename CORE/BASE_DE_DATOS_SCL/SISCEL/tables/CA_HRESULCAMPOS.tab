CREATE TABLE CA_HRESULCAMPOS(
    NUM_LLAMADA NUMBER(9,0) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  NUM_SECUENCIA NUMBER(3,0) NOT NULL 
 ,  COD_CUESTIONARIO VARCHAR2(6) NOT NULL 
 ,  NUM_PREGUNTA NUMBER(2,0) NOT NULL 
 ,  NUM_RESPUESTA NUMBER(2,0) NOT NULL 
 ,  COD_CAMPO NUMBER(3,0) NOT NULL 
 ,  VAL_ALFA VARCHAR2(2000) NULL 
 ,  VAL_NUMER NUMBER(31,9) NULL 
 ,  VAL_FECHA DATE NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_HRESULCAMPOS IS 'Historico Resultados Campos'
/
COMMENT ON COLUMN  CA_HRESULCAMPOS.NUM_LLAMADA IS 'Numero llamada'
/
COMMENT ON COLUMN  CA_HRESULCAMPOS.FEC_HISTORICO IS 'Fecha historico'
/
COMMENT ON COLUMN  CA_HRESULCAMPOS.NUM_SECUENCIA IS 'Numero secuencia'
/
COMMENT ON COLUMN  CA_HRESULCAMPOS.COD_CUESTIONARIO IS 'Codigo cuestionario'
/
COMMENT ON COLUMN  CA_HRESULCAMPOS.NUM_PREGUNTA IS 'Numero pregunta'
/
COMMENT ON COLUMN  CA_HRESULCAMPOS.NUM_RESPUESTA IS 'Numero respuesta'
/
COMMENT ON COLUMN  CA_HRESULCAMPOS.COD_CAMPO IS 'Campo'
/
COMMENT ON COLUMN  CA_HRESULCAMPOS.VAL_ALFA IS 'Valor alfanumerico'
/
COMMENT ON COLUMN  CA_HRESULCAMPOS.VAL_NUMER IS 'Valor numerico'
/
COMMENT ON COLUMN  CA_HRESULCAMPOS.VAL_FECHA IS 'Valor fecha'
/
