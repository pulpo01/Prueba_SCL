CREATE TABLE SISCEL.AL_BODEGAS
(
  COD_BODEGA        NUMBER(6)                   NOT NULL,
  DES_BODEGA        VARCHAR2(50 BYTE)           NOT NULL,
  TIP_BODEGA        NUMBER(2)                   NOT NULL,
  COD_DIRECCION     NUMBER(20)                  NOT NULL,
  IND_ASIGNADA      NUMBER(1)                   DEFAULT 0                     NOT NULL,
  COD_BODEGA_PADRE  NUMBER(6)                   NOT NULL,
  NUM_TELEFONO1     VARCHAR2(15 BYTE)               NULL,
  NUM_TELEFONO2     VARCHAR2(15 BYTE)               NULL,
  NUM_FAX           VARCHAR2(15 BYTE)               NULL,
  NOM_RESPONSABLE   VARCHAR2(40 BYTE)               NULL,
  COD_GRPCONCEPTO   NUMBER(6)                       NULL
)
TABLESPACE TS_LOGISTICA_SDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          760K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING
/

COMMENT ON TABLE SISCEL.AL_BODEGAS IS 'Bodegas'
/

COMMENT ON COLUMN SISCEL.AL_BODEGAS.COD_BODEGA IS 'Csdigo identificativo de la bodega'
/

COMMENT ON COLUMN SISCEL.AL_BODEGAS.DES_BODEGA IS 'Descripcion Bodega'
/

COMMENT ON COLUMN SISCEL.AL_BODEGAS.TIP_BODEGA IS 'Tipo de Bodega'
/

COMMENT ON COLUMN SISCEL.AL_BODEGAS.COD_DIRECCION IS 'Codigo de Direccion'
/

COMMENT ON COLUMN SISCEL.AL_BODEGAS.IND_ASIGNADA IS 'Indicativo de asignada: 0=No, 1=Si'
/

COMMENT ON COLUMN SISCEL.AL_BODEGAS.COD_BODEGA_PADRE IS 'Bsdega de la que depende'
/

COMMENT ON COLUMN SISCEL.AL_BODEGAS.NUM_TELEFONO1 IS 'Numero de telefono'
/

COMMENT ON COLUMN SISCEL.AL_BODEGAS.NUM_TELEFONO2 IS 'Numero de telefono'
/

COMMENT ON COLUMN SISCEL.AL_BODEGAS.NUM_FAX IS 'Numero de fax'
/

COMMENT ON COLUMN SISCEL.AL_BODEGAS.NOM_RESPONSABLE IS 'Nombre del responsable de la bodega'
/

COMMENT ON COLUMN SISCEL.AL_BODEGAS.COD_GRPCONCEPTO IS 'CODIGO DE CONCEPTO DE GRUPO'
/

