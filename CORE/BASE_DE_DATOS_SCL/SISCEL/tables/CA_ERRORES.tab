CREATE TABLE CA_ERRORES(
    NUM_ERROR NUMBER(3,0) NOT NULL 
 ,  COD_CUESTIONARIO VARCHAR2(6) NOT NULL 
 ,  NUM_PREGUNTA NUMBER(2,0) NOT NULL 
 ,  NUM_RESPUESTA NUMBER(2,0) NOT NULL 
 ,  COD_CUESTIONARIO_NEW VARCHAR2(6) NULL 
 ,  NUM_PREGUNTA_NEW NUMBER(2,0) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_ERRORES IS 'Errores en  cuestionarios'
/
COMMENT ON COLUMN  CA_ERRORES.NUM_ERROR IS 'Numero de error'
/
COMMENT ON COLUMN  CA_ERRORES.COD_CUESTIONARIO IS 'Codigo cuestionario'
/
COMMENT ON COLUMN  CA_ERRORES.NUM_PREGUNTA IS 'Numero pregunta'
/
COMMENT ON COLUMN  CA_ERRORES.NUM_RESPUESTA IS 'Numero respuesta'
/
COMMENT ON COLUMN  CA_ERRORES.COD_CUESTIONARIO_NEW IS 'Nuevo cuestionario'
/
COMMENT ON COLUMN  CA_ERRORES.NUM_PREGUNTA_NEW IS 'Nueva pregunta'
/
