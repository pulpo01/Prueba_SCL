CREATE TABLE CMT_DETALLE_RECHAZOS(
    NUM_GENERAL NUMBER(10,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_COMISIONISTA NUMBER(6,0) NOT NULL 
 ,  COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  ID_PERIODO VARCHAR2(10) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  IMP_EQUIPO NUMBER(11,4) NOT NULL 
 ,  IMP_RECHAZO NUMBER(11,4) NOT NULL 
 ,  IMP_TRAFICO NUMBER(11,4) NULL 
 ,  MIN_SALIDA NUMBER(6,0) NULL 
 ,  VAL_PLAN NUMBER(7,0) NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMT_DETALLE_RECHAZOS IS 'Detalle De Penalizaciones Por Rechazo De Ventas.'
/
COMMENT ON COLUMN  CMT_DETALLE_RECHAZOS.NUM_GENERAL IS 'Correlativo identificador unico de registro de rechazos.'
/
COMMENT ON COLUMN  CMT_DETALLE_RECHAZOS.COD_TIPCOMIS IS 'Codigo del tipo de comisionista (canal de ventas).'
/
COMMENT ON COLUMN  CMT_DETALLE_RECHAZOS.COD_COMISIONISTA IS 'Codigo del comisionista (master dealer).'
/
COMMENT ON COLUMN  CMT_DETALLE_RECHAZOS.COD_PERIODO IS 'Codigo del periodo comisional.'
/
COMMENT ON COLUMN  CMT_DETALLE_RECHAZOS.ID_PERIODO IS 'Codigo del periodo en formato largo.'
/
COMMENT ON COLUMN  CMT_DETALLE_RECHAZOS.COD_CONCEPTO IS 'Codigo del concepto de comision (rechazos normales o rechazos por fraude).'
/
COMMENT ON COLUMN  CMT_DETALLE_RECHAZOS.IMP_EQUIPO IS 'Penalizacion por no devolucion del equipo.'
/
COMMENT ON COLUMN  CMT_DETALLE_RECHAZOS.IMP_RECHAZO IS 'Penalizacion por el rechazo en si (de acuerdo a la actuacion).'
/
COMMENT ON COLUMN  CMT_DETALLE_RECHAZOS.IMP_TRAFICO IS 'Penalizacion por el trafico generado.'
/
COMMENT ON COLUMN  CMT_DETALLE_RECHAZOS.MIN_SALIDA IS 'Cantidad de minutos de trafico de salida.'
/
COMMENT ON COLUMN  CMT_DETALLE_RECHAZOS.VAL_PLAN IS 'Valor en $ del minuto considerado para la penalizacion.'
/
COMMENT ON COLUMN  CMT_DETALLE_RECHAZOS.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
