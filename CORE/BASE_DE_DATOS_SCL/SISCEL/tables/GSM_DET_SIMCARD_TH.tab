CREATE TABLE GSM_DET_SIMCARD_TH(
    COD_CAMPO VARCHAR2(10) NOT NULL 
 ,  NUM_SIMCARD VARCHAR2(20) NOT NULL 
 ,  VAL_CAMPO VARCHAR2(50) NOT NULL 
 ,  FEC_ACTUALIZACION DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_DET_SIMCARD_TH IS 'Tabla de detalle de SIMCARD, con la configuracion de la simcard.'
/
COMMENT ON COLUMN  GSM_DET_SIMCARD_TH.COD_CAMPO IS 'Codigo de Campo.'
/
COMMENT ON COLUMN  GSM_DET_SIMCARD_TH.NUM_SIMCARD IS 'Numero de SIMcard.'
/
COMMENT ON COLUMN  GSM_DET_SIMCARD_TH.VAL_CAMPO IS 'Valor del Campo.'
/
COMMENT ON COLUMN  GSM_DET_SIMCARD_TH.FEC_ACTUALIZACION IS 'Fecha de actualizacion.'
/
COMMENT ON COLUMN  GSM_DET_SIMCARD_TH.COD_USUARIO IS 'Usuario que actualizo.'
/
