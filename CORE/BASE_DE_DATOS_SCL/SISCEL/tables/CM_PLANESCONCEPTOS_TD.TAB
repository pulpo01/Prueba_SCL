CREATE TABLE CM_PLANESCONCEPTOS_TD(
    COD_PLANCOMIS VARCHAR2(6) NOT NULL 
 ,  COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  IND_CRITICO CHAR(1) NOT NULL 
 ,  NIV_CRITICO NUMBER(14,4) NULL 
 ,  COD_OPERADOR VARCHAR2(10) NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_PLANESCONCEPTOS_TD IS 'CONCEPTOS ASOCIADOS A PLANES DE COMISIONES'
/
COMMENT ON COLUMN  CM_PLANESCONCEPTOS_TD.COD_PLANCOMIS IS 'CODIGO DEL PLAN DE COMISIONES '
/
COMMENT ON COLUMN  CM_PLANESCONCEPTOS_TD.COD_CONCEPTO IS 'CODIGO DEL CONCEPTO '
/
COMMENT ON COLUMN  CM_PLANESCONCEPTOS_TD.IND_CRITICO IS 'INDICADOR DE CRITICIDAD DEL CONCEPTO DENTRO DEL PLAN DE COMISION '
/
COMMENT ON COLUMN  CM_PLANESCONCEPTOS_TD.NIV_CRITICO IS 'NIVEL DE CRITICIDAD'
/
COMMENT ON COLUMN  CM_PLANESCONCEPTOS_TD.COD_OPERADOR IS 'OPERADOR DE COMPARACION ENTRE NIVEL DE CRITICIDAD Y EL OBTENIDO '
/
COMMENT ON COLUMN  CM_PLANESCONCEPTOS_TD.FEC_ULTMOD IS 'FECHA ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  CM_PLANESCONCEPTOS_TD.NOM_USUARIO IS 'USUARIO ULTIMA MODIFICACION'
/
