CREATE TABLE FAT_PRESUPTEMP(
    NUM_PROCESO NUMBER(8,0) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COLUMNA NUMBER(6,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  FEC_EFECTIVIDAD DATE NOT NULL 
 ,  IMP_CONCEPTO NUMBER(14,4) DEFAULT 0 NOT NULL 
 ,  IMP_FACTURABLE NUMBER(14,4) DEFAULT 0 NOT NULL 
 ,  COD_TIPCONCE NUMBER(2,0) NOT NULL 
 ,  COD_CONCEREL NUMBER(4,0) NULL 
 ,  COLUMNA_REL NUMBER(6,0) NULL 
 ,  FLAG_IMPUES NUMBER(1,0) NULL 
 ,  PRC_IMPUESTO NUMBER(5,2) NULL 
 ) TABLESPACE    TS_FACTURACION_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAT_PRESUPTEMP IS 'Tabla Temporal De Presupuestos.'
/
COMMENT ON COLUMN  FAT_PRESUPTEMP.NUM_PROCESO IS 'Numero de Proceso.'
/
COMMENT ON COLUMN  FAT_PRESUPTEMP.COD_CONCEPTO IS 'Codigo de concepto.'
/
COMMENT ON COLUMN  FAT_PRESUPTEMP.COLUMNA IS 'Numero de columna.'
/
COMMENT ON COLUMN  FAT_PRESUPTEMP.COD_CLIENTE IS 'codigo de cliente.'
/
COMMENT ON COLUMN  FAT_PRESUPTEMP.FEC_EFECTIVIDAD IS 'fecha de efectividad.'
/
COMMENT ON COLUMN  FAT_PRESUPTEMP.IMP_CONCEPTO IS 'importe de cncepto.'
/
COMMENT ON COLUMN  FAT_PRESUPTEMP.IMP_FACTURABLE IS 'importe facturable.'
/
COMMENT ON COLUMN  FAT_PRESUPTEMP.COD_TIPCONCE IS 'Tipo de concepto.'
/
COMMENT ON COLUMN  FAT_PRESUPTEMP.COD_CONCEREL IS 'concepto relacionado.'
/
COMMENT ON COLUMN  FAT_PRESUPTEMP.COLUMNA_REL IS 'columna relacionada.'
/
COMMENT ON COLUMN  FAT_PRESUPTEMP.FLAG_IMPUES IS 'flag de impuestos.'
/
COMMENT ON COLUMN  FAT_PRESUPTEMP.PRC_IMPUESTO IS 'porcentaje de impuesto.'
/
