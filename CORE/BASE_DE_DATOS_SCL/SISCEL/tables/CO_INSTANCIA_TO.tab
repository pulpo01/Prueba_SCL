CREATE TABLE CO_INSTANCIA_TO(
    COD_PROCESO VARCHAR2(5) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  ESTADO VARCHAR2(1) NOT NULL 
 ,  FEC_ESTADO DATE NOT NULL 
 ,  CNT_INSTANCIA_PROP NUMBER(2,0) NOT NULL 
 ,  CNT_INSTANCIA_USR NUMBER(2,0) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  CO_INSTANCIA_TO.COD_PROCESO IS 'Identificador del Proceso'
/
COMMENT ON COLUMN  CO_INSTANCIA_TO.FEC_INGRESO IS 'Fecha de Ingreso'
/
COMMENT ON COLUMN  CO_INSTANCIA_TO.ESTADO IS 'Estado de vigencia de configuracion. V: Vigente, H: Historico.'
/
COMMENT ON COLUMN  CO_INSTANCIA_TO.FEC_ESTADO IS 'Fecha del estado'
/
COMMENT ON COLUMN  CO_INSTANCIA_TO.CNT_INSTANCIA_PROP IS 'Cantidad de instancias propuestas por sistema.'
/
COMMENT ON COLUMN  CO_INSTANCIA_TO.CNT_INSTANCIA_USR IS 'Cantidad de instancias configuradas por el usuario.'
/
