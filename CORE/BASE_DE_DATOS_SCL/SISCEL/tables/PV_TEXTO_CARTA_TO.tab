CREATE TABLE PV_TEXTO_CARTA_TO(
    COD_TEXTO VARCHAR2(2) NOT NULL 
 ,  DES_TEXTO VARCHAR2(2000) NOT NULL 
 ,  QUERY VARCHAR2(500) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_IDIOMA VARCHAR2(5) DEFAULT '1' NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_TEXTO_CARTA_TO IS 'Texto De Orden De Servicio.'
/
COMMENT ON COLUMN  PV_TEXTO_CARTA_TO.COD_TEXTO IS 'Codigo Texto.'
/
COMMENT ON COLUMN  PV_TEXTO_CARTA_TO.DES_TEXTO IS 'Descripcion Texto.'
/
COMMENT ON COLUMN  PV_TEXTO_CARTA_TO.QUERY IS 'Query asociada a la orden de servicio..'
/
COMMENT ON COLUMN  PV_TEXTO_CARTA_TO.FEC_ULTMOD IS 'Fecha Modificacion.'
/
COMMENT ON COLUMN  PV_TEXTO_CARTA_TO.NOM_USUARIO IS 'Nombre de Usuario.'
/
COMMENT ON COLUMN  PV_TEXTO_CARTA_TO.COD_IDIOMA IS 'Codigo Idioma.'
/
