CREATE TABLE GA_DIST_LCRED_TH(
    COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  MONTO_CREDITO NUMBER(14,4) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_DIST_LCRED_TH IS 'Limite de credito para distribuidores'
/
COMMENT ON COLUMN  GA_DIST_LCRED_TH.COD_VENDEDOR IS 'Codigo de vendedor.'
/
COMMENT ON COLUMN  GA_DIST_LCRED_TH.COD_CLIENTE IS 'Codigo de cliente.'
/
COMMENT ON COLUMN  GA_DIST_LCRED_TH.FEC_DESDE IS 'Fecha de inicio de vigencia del monto.'
/
COMMENT ON COLUMN  GA_DIST_LCRED_TH.FEC_HASTA IS 'Fecha hasta de vigencia del monto.'
/
COMMENT ON COLUMN  GA_DIST_LCRED_TH.MONTO_CREDITO IS 'Monto limite de credito.'
/
COMMENT ON COLUMN  GA_DIST_LCRED_TH.COD_MONEDA IS 'Moneda del monto.'
/
COMMENT ON COLUMN  GA_DIST_LCRED_TH.NOM_USUARIO IS 'Usuario que modifica el registro.'
/
