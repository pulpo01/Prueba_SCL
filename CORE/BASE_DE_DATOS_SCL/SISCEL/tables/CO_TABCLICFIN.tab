CREATE TABLE CO_TABCLICFIN(
    NRT_CLIE VARCHAR2(20) NOT NULL 
 ,  DGV_CLIE VARCHAR2(1) NOT NULL 
 ,  DIR_CALLE VARCHAR2(50) NOT NULL 
 ,  COD_COMU VARCHAR2(10) NULL 
 ,  COD_CIUD VARCHAR2(10) NULL 
 ,  FLG_CLIE VARCHAR2(20) NULL 
 ,  NUM_FONO VARCHAR2(20) NULL 
 ,  FECHA_MIGRACION DATE NULL 
 ,  COD_CUENTA_MIGRA NUMBER(8,0) NULL 
 ,  COD_CLIENTE_MIGRA NUMBER(8,0) NULL 
 ,  TOT_FACT_MIGRADO NUMBER(14,4) NULL 
 ,  TOT_CAST_MIGRADO NUMBER(14,4) NULL 
 ,  NOM_USUA_MIGRA VARCHAR2(30) NULL 
 ,  NOM_CLIE VARCHAR2(50) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_TABCLICFIN IS 'Tabclicfin.'
/
COMMENT ON COLUMN  CO_TABCLICFIN.NRT_CLIE IS 'NUMERO DE CLIENTE(RUT).'
/
