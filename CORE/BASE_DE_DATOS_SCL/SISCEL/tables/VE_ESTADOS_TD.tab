CREATE TABLE VE_ESTADOS_TD(
    COD_ESTADO NUMBER(2,0) NOT NULL 
 ,  DES_ESTADO VARCHAR2(30) NOT NULL 
 ,  IND_INVALIDA_VENTA NUMBER(1,0) NOT NULL 
 ,  IND_INVALIDA_COMIS NUMBER(1,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 NOLOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_ESTADOS_TD IS 'ESTADOS DE LOS AGENTES DE VENTAS (VE_VENDEDORES)'
/
COMMENT ON COLUMN  VE_ESTADOS_TD.COD_ESTADO IS 'CÓDIGO DE ESTADO'
/
COMMENT ON COLUMN  VE_ESTADOS_TD.DES_ESTADO IS 'DESCRIPCIÓN DE ESTADO'
/
COMMENT ON COLUMN  VE_ESTADOS_TD.IND_INVALIDA_VENTA IS 'INDICATIVO DE INHABILIDAD DE LA VENTA (1:SI, 0: NO)'
/
COMMENT ON COLUMN  VE_ESTADOS_TD.IND_INVALIDA_COMIS IS 'INDICATIVO DE INHABILIDAD DE COMISIONES (1:SI, 0: NO)'
/
COMMENT ON COLUMN  VE_ESTADOS_TD.FEC_DESDE IS 'FECHA INICIAL DE VIGENCIA'
/
COMMENT ON COLUMN  VE_ESTADOS_TD.FEC_HASTA IS 'FECHA FINAL DE VIGENCIA'
/
COMMENT ON COLUMN  VE_ESTADOS_TD.FEC_ULTMOD IS 'FECHA ÚLTIMA MODIFICACIÓN'
/
COMMENT ON COLUMN  VE_ESTADOS_TD.NOM_USUARIO IS 'USUARIO ÚLTIMA MODIFICACIÓN'
/
