CREATE TABLE FA_IMPGRUPOS(
    COD_GRUPO NUMBER(3,0) NOT NULL 
 ,  DES_GRUPO VARCHAR2(40) NULL 
 ,  DES_GLOSA VARCHAR2(50) NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_FORMULARIO NUMBER(3,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(3,0) NOT NULL 
 ,  IND_EXPANSION NUMBER(1,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_IMPGRUPOS IS 'Definicion  de  Grupos  de  Impresion  (Asociados a cada Formulario).'
/
COMMENT ON COLUMN  FA_IMPGRUPOS.COD_GRUPO IS 'Codigo de grupo de impresion. [PK 1/1]'
/
COMMENT ON COLUMN  FA_IMPGRUPOS.DES_GRUPO IS 'Descripcion del codigo de grupo de impresion.'
/
COMMENT ON COLUMN  FA_IMPGRUPOS.DES_GLOSA IS 'Glosa que aparecera fisicamente en el formulario.'
/
COMMENT ON COLUMN  FA_IMPGRUPOS.COD_PRODUCTO IS 'Codigo de producto del grupo de impresion.'
/
COMMENT ON COLUMN  FA_IMPGRUPOS.COD_FORMULARIO IS 'Formulario donde se usara el grupo de impresion.'
/
COMMENT ON COLUMN  FA_IMPGRUPOS.NUM_ORDEN IS 'Orden de aparicion del grupo dentro del formulario.'
/
COMMENT ON COLUMN  FA_IMPGRUPOS.IND_EXPANSION IS 'Indicador de expansion de grupo.'
/
COMMENT ON COLUMN  FA_IMPGRUPOS.NOM_USUARIO IS 'Ultimo usuario que realizo algun cambio en el registro.'
/
COMMENT ON COLUMN  FA_IMPGRUPOS.FEC_ULTMOD IS 'Fecha de ultima modificacion.'
/
