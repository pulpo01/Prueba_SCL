CREATE TABLE ERD_DAT_ADICIONALES_TD(
    TIP_REGISTRO VARCHAR2(4) NOT NULL 
 ,  NOM_CAMPO VARCHAR2(25) NOT NULL 
 ,  LEN_CAMPO NUMBER(4,0) NOT NULL 
 ,  POS_INICIAL NUMBER(4,0) NOT NULL 
 ,  POS_FINAL NUMBER(4,0) NOT NULL 
 ,  TIP_CAMPO VARCHAR2(1) NOT NULL 
 ,  DES_CAMPO VARCHAR2(50) NOT NULL 
 ,  IND_ACTIVO VARCHAR2(1) NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERD_DAT_ADICIONALES_TD IS '[ DAAD ] Tabla que contiene los campos de burs a desplegar en la evaluacion de riesgo.Tabla que contiene los campos de burs a desplegar en la evaluacion de riesgo.'
/
COMMENT ON COLUMN  ERD_DAT_ADICIONALES_TD.TIP_REGISTRO IS 'Tipo de registro '
/
COMMENT ON COLUMN  ERD_DAT_ADICIONALES_TD.NOM_CAMPO IS 'nombre del campo'
/
COMMENT ON COLUMN  ERD_DAT_ADICIONALES_TD.LEN_CAMPO IS 'longitud del campo'
/
COMMENT ON COLUMN  ERD_DAT_ADICIONALES_TD.POS_INICIAL IS 'posicion inicial donde se ubica el campo'
/
COMMENT ON COLUMN  ERD_DAT_ADICIONALES_TD.POS_FINAL IS 'posicion final  donde se ubica el campo'
/
COMMENT ON COLUMN  ERD_DAT_ADICIONALES_TD.TIP_CAMPO IS 'tipo del campo (A:alfanumerico/ N:numerico)'
/
COMMENT ON COLUMN  ERD_DAT_ADICIONALES_TD.DES_CAMPO IS 'descripcion del campo'
/
COMMENT ON COLUMN  ERD_DAT_ADICIONALES_TD.IND_ACTIVO IS 'indicador de activo(S:si/N:no)'
/
