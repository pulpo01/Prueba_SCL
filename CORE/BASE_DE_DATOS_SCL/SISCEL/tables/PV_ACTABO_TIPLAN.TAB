CREATE TABLE PV_ACTABO_TIPLAN(
    COD_TIPMODI VARCHAR2(2) NOT NULL 
 ,  COD_ACTABO VARCHAR2(2) NOT NULL 
 ,  COD_TIPLAN VARCHAR2(5) NOT NULL 
 ,  COD_OS VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_ACTABO_TIPLAN IS 'Actuaciones asociadas a la OOSS'
/
COMMENT ON COLUMN  PV_ACTABO_TIPLAN.COD_TIPMODI IS 'Codigo Actuacion OOSS'
/
COMMENT ON COLUMN  PV_ACTABO_TIPLAN.COD_ACTABO IS 'Codigo de Actuacion de Abonados'
/
COMMENT ON COLUMN  PV_ACTABO_TIPLAN.COD_TIPLAN IS 'Tipo de Plan (1=prepago,2=pospago,3=hibrido)'
/
COMMENT ON COLUMN  PV_ACTABO_TIPLAN.COD_OS IS 'Codigo de OOSS'
/
