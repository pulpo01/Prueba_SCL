CREATE TABLE ERD_DETALLE_EXCEPCIONES(
    TIP_EXCEPCION VARCHAR2(2) NOT NULL 
 ,  COD_EXCEPCION VARCHAR2(5) NOT NULL 
 ,  DES_EXCEPCION VARCHAR2(60) NOT NULL 
 ,  FEC_ULTMOD DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERD_DETALLE_EXCEPCIONES IS 'Clasificacion Excepciones en la Evaluacion de Riesgo.'
/
COMMENT ON COLUMN  ERD_DETALLE_EXCEPCIONES.TIP_EXCEPCION IS 'Tipo de Excepcion.'
/
COMMENT ON COLUMN  ERD_DETALLE_EXCEPCIONES.COD_EXCEPCION IS 'Codigo Excepcion.'
/
COMMENT ON COLUMN  ERD_DETALLE_EXCEPCIONES.DES_EXCEPCION IS 'Descripcion de Codigo Excepcion.'
/
COMMENT ON COLUMN  ERD_DETALLE_EXCEPCIONES.FEC_ULTMOD IS 'Fecha de ultima modificacion.'
/
COMMENT ON COLUMN  ERD_DETALLE_EXCEPCIONES.NOM_USUARIO IS 'Usuario responsable.'
/
