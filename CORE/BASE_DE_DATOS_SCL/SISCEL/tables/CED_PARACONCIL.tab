CREATE TABLE CED_PARACONCIL(
    COD_EMPRSERV VARCHAR2(5) NOT NULL 
 ,  COD_PATH NUMBER(10,0) NULL 
 ,  COD_ARCHIVO NUMBER(10,0) NULL 
 ,  COD_AJUSTE NUMBER(10,0) NULL 
 ,  FEC_EJECUCION DATE NULL 
 ,  COD_PROCESO_CONCIL NUMBER(10,0) NULL 
 ,  COD_PROCESO_AJUSTE NUMBER(10,0) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CED_PARACONCIL IS 'Procesos Gce.'
/
COMMENT ON COLUMN  CED_PARACONCIL.COD_EMPRSERV IS 'CODIGO DE LA EMPRESA DE SERVICIO.'
/
COMMENT ON COLUMN  CED_PARACONCIL.COD_PATH IS 'PATH DE UBICACION DEL ARCHIVO.'
/
COMMENT ON COLUMN  CED_PARACONCIL.COD_ARCHIVO IS 'NOMBRE DE ARCHIVO ENVIADO POR LA EMPRESA.'
/
COMMENT ON COLUMN  CED_PARACONCIL.COD_AJUSTE IS 'VALOR PARAMETRO ARCHIVO DE AJUSTE.'
/
COMMENT ON COLUMN  CED_PARACONCIL.FEC_EJECUCION IS 'FECHA DE EJECUCION DEL PROCESO DE CONCILIACION.'
/
COMMENT ON COLUMN  CED_PARACONCIL.COD_PROCESO_CONCIL IS 'CODIGO DE PROCESO ASOCIADO A LA CONCILIACION.'
/
COMMENT ON COLUMN  CED_PARACONCIL.COD_PROCESO_AJUSTE IS 'CODIGO DE PROCESO ASOCIADO AL AJUSTE.'
/
