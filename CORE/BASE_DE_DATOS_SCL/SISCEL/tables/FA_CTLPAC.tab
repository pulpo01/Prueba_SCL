CREATE TABLE FA_CTLPAC(
    SEQ_PAC NUMBER(12,0) NOT NULL 
 ,  NOM_FICHERO VARCHAR2(40) NOT NULL 
 ,  FEC_PROCESO DATE NOT NULL 
 ,  NUM_REGISTROS NUMBER(9,0) NOT NULL 
 ,  COD_ZONA NUMBER(2,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(2,0) NOT NULL 
 ,  COD_CICLOBCO NUMBER(2,0) NOT NULL 
 ,  COD_BANCOINT NUMBER(3,0) NOT NULL 
 ,  TOT_IMPORTE NUMBER(15,4) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_CTLPAC IS 'Tabla de Control para el Proceso PACC'
/
COMMENT ON COLUMN  FA_CTLPAC.COD_BANCOINT IS 'Codigo de Banco Interno'
/
COMMENT ON COLUMN  FA_CTLPAC.TOT_IMPORTE IS 'Total Importe Procesado'
/
COMMENT ON COLUMN  FA_CTLPAC.SEQ_PAC IS 'Secuencia PACC'
/
COMMENT ON COLUMN  FA_CTLPAC.NOM_FICHERO IS 'Nombre de Fichero PACC'
/
COMMENT ON COLUMN  FA_CTLPAC.FEC_PROCESO IS 'Fecha en la que se efectuo el Proceso'
/
COMMENT ON COLUMN  FA_CTLPAC.NUM_REGISTROS IS 'Numero de Registros Procesados'
/
COMMENT ON COLUMN  FA_CTLPAC.COD_ZONA IS 'Codigo de Zona'
/
COMMENT ON COLUMN  FA_CTLPAC.COD_CENTRAL IS 'Codigo de Central'
/
COMMENT ON COLUMN  FA_CTLPAC.COD_CICLOBCO IS 'Codigo de Ciclo Banco'
/
