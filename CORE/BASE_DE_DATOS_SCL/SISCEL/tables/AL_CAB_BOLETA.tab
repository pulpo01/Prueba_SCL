CREATE TABLE SISCEL.AL_CAB_BOLETA
(
  NUM_BOLETA       NUMBER(9)                    NOT NULL,
  COD_BODEGA       NUMBER(6)                    NOT NULL,
  FEC_BOLETA       DATE                         NOT NULL,
  COD_ESTADO       VARCHAR2(2 BYTE)             NOT NULL,
  TIP_MOVIMIENTO   NUMBER(2)                    NOT NULL,
  COD_TIPCOMIS     VARCHAR2(2 BYTE)             NOT NULL,
  COD_VENDEDOR     NUMBER(6)                    NOT NULL,
  COD_OFICINA      VARCHAR2(2 BYTE)             NOT NULL,
  NOM_USUARIO      VARCHAR2(30 BYTE)            NOT NULL,
  DES_OBSERVACION  VARCHAR2(100 BYTE),
  NUM_FOLIO        NUMBER(9),
  PRC_TOTAL        NUMBER(14,4),
  COD_OPERADORA    VARCHAR2(5 BYTE),
  PREF_PLAZA       VARCHAR2(25 BYTE)
)  TABLESPACE TS_LOGISTICA_SDAT
/

COMMENT ON TABLE SISCEL.AL_CAB_BOLETA IS 'Cabecera de venta Amistar por medio de boleta';

COMMENT ON COLUMN SISCEL.AL_CAB_BOLETA.NUM_BOLETA IS 'Nzmero de Boleta';

COMMENT ON COLUMN SISCEL.AL_CAB_BOLETA.COD_BODEGA IS 'Bsdega de la que se efectua la venta con boleta';

COMMENT ON COLUMN SISCEL.AL_CAB_BOLETA.FEC_BOLETA IS 'Fecha de venta con boleta';

COMMENT ON COLUMN SISCEL.AL_CAB_BOLETA.COD_ESTADO IS 'Estado en el que se encuentra la venta con boleta';

COMMENT ON COLUMN SISCEL.AL_CAB_BOLETA.TIP_MOVIMIENTO IS 'Tipo de Movimiento Asociado';

COMMENT ON COLUMN SISCEL.AL_CAB_BOLETA.COD_TIPCOMIS IS 'Codigo de Tipo de Comisionista';

COMMENT ON COLUMN SISCEL.AL_CAB_BOLETA.COD_VENDEDOR IS 'Codigo de vendedor';

COMMENT ON COLUMN SISCEL.AL_CAB_BOLETA.COD_OFICINA IS 'Codigo de Oficina relacionada';

COMMENT ON COLUMN SISCEL.AL_CAB_BOLETA.NOM_USUARIO IS 'Usuario que realizs la venta con boleta';

COMMENT ON COLUMN SISCEL.AL_CAB_BOLETA.DES_OBSERVACION IS 'Observaciones o Comentarios';

COMMENT ON COLUMN SISCEL.AL_CAB_BOLETA.NUM_FOLIO IS 'Numero de folio de la venta';

COMMENT ON COLUMN SISCEL.AL_CAB_BOLETA.PRC_TOTAL IS 'Monto total de la venta';

COMMENT ON COLUMN SISCEL.AL_CAB_BOLETA.COD_OPERADORA IS 'Codigo de Operadora';

COMMENT ON COLUMN SISCEL.AL_CAB_BOLETA.PREF_PLAZA IS 'Prefijo de folio de la plaza';

