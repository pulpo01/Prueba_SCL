CREATE TABLE CA_AVISOSECTORES(
    NUM_INCIDENCIA NUMBER(9,0) NOT NULL 
 ,  FEC_GRABACION DATE NOT NULL 
 ,  FEC_INCIDENCIA DATE NOT NULL 
 ,  FEC_ESTIMADA DATE NOT NULL 
 ,  DES_INCIDENCIA VARCHAR2(30) NOT NULL 
 ,  DES_SECTOR VARCHAR2(30) NOT NULL 
 ,  FEC_DERIVACION DATE NOT NULL 
 ,  NOM_RESPONSABLE VARCHAR2(40) NOT NULL 
 ,  DES_INTER VARCHAR2(20) NOT NULL 
 ,  COD_INTERLOCUTOR NUMBER(10,0) NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NULL 
 ,  NOM_INTERLOCUTOR VARCHAR2(50) NOT NULL 
 ,  PER_CONTACTO VARCHAR2(40) NOT NULL 
 ,  NOM_IMPRESORA VARCHAR2(40) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_ESTADO VARCHAR2(1) DEFAULT 'P' NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
