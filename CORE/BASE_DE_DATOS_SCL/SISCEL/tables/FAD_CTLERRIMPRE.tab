CREATE TABLE FAD_CTLERRIMPRE(
    NUM_SECUINFO NUMBER(8,0) NOT NULL 
 ,  COD_TIPIMPRES NUMBER(2,0) NOT NULL 
 ,  NOM_ARCHIVO VARCHAR2(254) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  GLS_ERROR VARCHAR2(150) NOT NULL 
 ,  TOT_CORRECTO NUMBER(14,4) NULL 
 ,  TOT_ERRONEO NUMBER(14,4) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAD_CTLERRIMPRE IS 'Informe De Errores De Impresion.'
/
COMMENT ON COLUMN  FAD_CTLERRIMPRE.NUM_SECUINFO IS 'Numero secuencial del informe.'
/
COMMENT ON COLUMN  FAD_CTLERRIMPRE.COD_TIPIMPRES IS 'Codigo de tipo de impresion: facturas/boletas, detalle de pago, resumen de consumo. Co_codigos ().'
/
COMMENT ON COLUMN  FAD_CTLERRIMPRE.NOM_ARCHIVO IS 'Nombre del archivo.'
/
COMMENT ON COLUMN  FAD_CTLERRIMPRE.COD_CLIENTE IS 'Codigo de Clientes.'
/
COMMENT ON COLUMN  FAD_CTLERRIMPRE.GLS_ERROR IS 'Glosa del Error.'
/
COMMENT ON COLUMN  FAD_CTLERRIMPRE.TOT_CORRECTO IS 'Total correcto.'
/
COMMENT ON COLUMN  FAD_CTLERRIMPRE.TOT_ERRONEO IS 'Total erroneo.'
/
