CREATE TABLE CO_DET_CONVENIO
(
  NUM_CONVENIO        NUMBER(8)                 NOT NULL,
  NUM_SECUENCI        NUMBER(8)                 NOT NULL,
  TIPO_CONVENIO       VARCHAR2(5 BYTE)          NOT NULL,
  NUM_PAGARE          NUMBER(8),
  NUM_SECUENCI_CA     NUMBER(10)                NOT NULL,
  COD_TIPDOCUM_CA     NUMBER(2)                 NOT NULL,
  COD_VEND_AGENTE_CA  NUMBER(6),
  LETRA_CA            VARCHAR2(1 BYTE),
  COD_CLIENTE         NUMBER(8)                 NOT NULL,
  NUM_FOLIO_CA        NUMBER(9)                 NOT NULL,
  NUM_CUOTA_CA        NUMBER(8),
  SEC_CUOTA_CA        NUMBER(3),
  IMPORTE_CA          NUMBER(14,4)              NOT NULL,
  PREF_PLAZA          VARCHAR2(25 BYTE),
  COD_CONCEPTO_CA     NUMBER(4),
  COLUMNA_CA          NUMBER(4),
  NUM_ABONADO_CA      NUMBER(8),
  COD_CENTREMI_CA     NUMBER(4)
)
TABLESPACE TS_RECYCOB_SDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          8M
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE CO_DET_CONVENIO IS 'Detalle Del Convenio.';

COMMENT ON COLUMN CO_DET_CONVENIO.NUM_CONVENIO IS 'Numero del Convenio.';

COMMENT ON COLUMN CO_DET_CONVENIO.NUM_SECUENCI IS 'Numero de Secuencia .';

COMMENT ON COLUMN CO_DET_CONVENIO.TIPO_CONVENIO IS 'Tipo de Convenio.';

COMMENT ON COLUMN CO_DET_CONVENIO.NUM_PAGARE IS 'Numero del Pagare.';

COMMENT ON COLUMN CO_DET_CONVENIO.NUM_SECUENCI_CA IS 'Numero de Secuencia de la CO_CARTERA.';

COMMENT ON COLUMN CO_DET_CONVENIO.COD_TIPDOCUM_CA IS 'Tipo de Documento de la CO_CARTERA.';

COMMENT ON COLUMN CO_DET_CONVENIO.COD_VEND_AGENTE_CA IS 'Codigo Vendedor Agente de la CO_CARTERA.';

COMMENT ON COLUMN CO_DET_CONVENIO.LETRA_CA IS 'Letra de la CO_CARTERA.';

COMMENT ON COLUMN CO_DET_CONVENIO.COD_CLIENTE IS 'Codigo de Cliente.';

COMMENT ON COLUMN CO_DET_CONVENIO.NUM_FOLIO_CA IS 'Numero de Folio de la CO_CARTERA.';

COMMENT ON COLUMN CO_DET_CONVENIO.NUM_CUOTA_CA IS 'Numero de Cuota de la CO_CARTERA.';

COMMENT ON COLUMN CO_DET_CONVENIO.SEC_CUOTA_CA IS 'Secuencia de Cuota de la CO_CARTERA.';

COMMENT ON COLUMN CO_DET_CONVENIO.IMPORTE_CA IS 'Importe de la CO_CARTERA.';

COMMENT ON COLUMN CO_DET_CONVENIO.PREF_PLAZA IS 'Prefijo Plaza.';
