CREATE TABLE CMD_PLANESTARIF(
    COD_PLANTARIF VARCHAR2(3) NOT NULL 
 ,  TIP_PLAN VARCHAR2(5) NOT NULL 
 ,  FEC_CREACION DATE NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMD_PLANESTARIF IS 'Definicion De Planes Tarifarios Y Sus Categorias De Comision.'
/
COMMENT ON COLUMN  CMD_PLANESTARIF.COD_PLANTARIF IS 'Codigo de plan tarifario..'
/
COMMENT ON COLUMN  CMD_PLANESTARIF.TIP_PLAN IS 'Tipo (categoria) de plan tarifario desde el punto de vista comisional..'
/
COMMENT ON COLUMN  CMD_PLANESTARIF.FEC_CREACION IS 'Fecha de insercion del registro en la tabla..'
/
COMMENT ON COLUMN  CMD_PLANESTARIF.FEC_ULTMOD IS 'Fecha de ultima modificacion del registro..'
/
COMMENT ON COLUMN  CMD_PLANESTARIF.NOM_USUARIO IS 'Usuario que hizo la ultima modificacion del registro en la tabla..'
/
