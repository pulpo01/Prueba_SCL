CREATE TABLE CMT_FONDOS_COAP(
    COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_COMISIONISTA NUMBER(6,0) NOT NULL 
 ,  IMP_FONDO NUMBER(14,4) NOT NULL 
 ,  NUM_PERIODO NUMBER(3,0) NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) DEFAULT 'C' NOT NULL 
 ,  COD_CONCEPTO NUMBER(3,0) DEFAULT 0 NOT NULL 
 ,  ID_PERIODO VARCHAR2(10) DEFAULT 'NN' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMT_FONDOS_COAP IS 'Tabla De Fondos Para Aportes Coap.'
/
COMMENT ON COLUMN  CMT_FONDOS_COAP.COD_PERIODO IS 'Codigo del Periodo donde se incorpora el fondo.'
/
COMMENT ON COLUMN  CMT_FONDOS_COAP.COD_TIPCOMIS IS 'Codigo del tipo de comisiionista (Canal de Ventas).'
/
COMMENT ON COLUMN  CMT_FONDOS_COAP.COD_COMISIONISTA IS 'Codigo del Comisionista.'
/
COMMENT ON COLUMN  CMT_FONDOS_COAP.IMP_FONDO IS 'Monto del fondo. Valor proveniente de la acumulacion..'
/
COMMENT ON COLUMN  CMT_FONDOS_COAP.NUM_PERIODO IS 'Numero correlativo del periodo del fondo para ese comisionista..'
/
COMMENT ON COLUMN  CMT_FONDOS_COAP.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
COMMENT ON COLUMN  CMT_FONDOS_COAP.COD_PLANCOMIS IS 'PLAN DE COMISIONES QUE CONTIENEN AL CONCEPTO.'
/
COMMENT ON COLUMN  CMT_FONDOS_COAP.COD_CONCEPTO IS 'CONCEPTO DE BONIFICACION DE ARRIENDO.'
/
COMMENT ON COLUMN  CMT_FONDOS_COAP.ID_PERIODO IS 'IDENTIFICADOR DEL CICLO DE COMISIONES'
/
