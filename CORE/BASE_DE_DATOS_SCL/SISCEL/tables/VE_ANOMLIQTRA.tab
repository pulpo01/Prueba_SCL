CREATE TABLE VE_ANOMLIQTRA(
    NUM_ANOMALIA NUMBER(8,0) NOT NULL 
 ,  NUM_LIQUIDACION NUMBER(8,0) NOT NULL 
 ,  DES_MENSAJE VARCHAR2(120) NOT NULL 
 ,  NOM_PROCEDURE VARCHAR2(30) NOT NULL 
 ,  NOM_TABLA VARCHAR2(30) NOT NULL 
 ,  IND_ACCION CHAR(1) NOT NULL 
 ,  COD_SQLCODE VARCHAR2(10) NULL 
 ,  DES_SQLERRM VARCHAR2(70) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_ANOMLIQTRA IS 'Anomalias del calculo bono por trafico'
/
COMMENT ON COLUMN  VE_ANOMLIQTRA.NUM_ANOMALIA IS 'Numero de anomalia'
/
COMMENT ON COLUMN  VE_ANOMLIQTRA.NUM_LIQUIDACION IS 'Numero de liquidacion'
/
COMMENT ON COLUMN  VE_ANOMLIQTRA.DES_MENSAJE IS 'Mensaje'
/
COMMENT ON COLUMN  VE_ANOMLIQTRA.NOM_PROCEDURE IS 'Nombre del procedimiento'
/
COMMENT ON COLUMN  VE_ANOMLIQTRA.NOM_TABLA IS 'Nombre de la tabla'
/
COMMENT ON COLUMN  VE_ANOMLIQTRA.IND_ACCION IS 'Accion efectuada sobre la tabla'
/
COMMENT ON COLUMN  VE_ANOMLIQTRA.COD_SQLCODE IS 'Codigo de error sql'
/
COMMENT ON COLUMN  VE_ANOMLIQTRA.DES_SQLERRM IS 'Descripcion de error SQL'
/
