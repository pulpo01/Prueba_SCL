CREATE TABLE CO_QUERYS(
    COD_RUTINA VARCHAR2(25) NOT NULL 
 ,  SEC_QUERY NUMBER(3,0) NOT NULL 
 ,  TXT_QUERY VARCHAR2(50) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  CO_QUERYS.COD_RUTINA IS 'Rutina Asociada al Query'
/
COMMENT ON COLUMN  CO_QUERYS.SEC_QUERY IS 'Secuencia de Lectura del Query'
/
COMMENT ON COLUMN  CO_QUERYS.TXT_QUERY IS 'Texto que conforma el QUery'
/
