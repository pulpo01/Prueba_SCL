CREATE TABLE CO_MOVDEP(
    COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  COD_CAJA NUMBER(4,0) NOT NULL 
 ,  NUM_EJERCICIO VARCHAR2(12) NOT NULL 
 ,  NUM_SECUMOV NUMBER(8,0) NOT NULL 
 ,  FEC_DEPOSITO DATE NOT NULL 
 ,  COD_BANCO VARCHAR2(15) NOT NULL 
 ,  NUM_DEPOSITO VARCHAR2(20) NOT NULL 
 ,  CTA_CORRIENTE VARCHAR2(18) NOT NULL 
 ,  IMPORTE NUMBER(14,4) NOT NULL 
 ,  IND_DEPOSITO NUMBER(1,0) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NULL 
 ,  COD_OPERADORA_SCL VARCHAR2(5) NULL 
 ,  COD_PLAZA VARCHAR2(5) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_MOVDEP IS 'ALMACENAMIENTO DE LOS MOVIMIENTOS DE DEPOSITO DE LAS CAJAS'
/
COMMENT ON COLUMN  CO_MOVDEP.COD_OFICINA IS 'CODIGO DE OFICINA'
/
COMMENT ON COLUMN  CO_MOVDEP.COD_CAJA IS 'CODIGO DE CAJA'
/
COMMENT ON COLUMN  CO_MOVDEP.NUM_EJERCICIO IS 'Numero de ejercicio - Retrofitted'
/
COMMENT ON COLUMN  CO_MOVDEP.NUM_SECUMOV IS 'NUMERO DE SECUENCIA'
/
COMMENT ON COLUMN  CO_MOVDEP.FEC_DEPOSITO IS 'FECHA DE DEPOSITO'
/
COMMENT ON COLUMN  CO_MOVDEP.COD_BANCO IS 'CODIGO DEL BANCO'
/
COMMENT ON COLUMN  CO_MOVDEP.NUM_DEPOSITO IS 'Numero de deposito - Retrofitted'
/
COMMENT ON COLUMN  CO_MOVDEP.CTA_CORRIENTE IS 'CUENTA CORRIENTE'
/
COMMENT ON COLUMN  CO_MOVDEP.IMPORTE IS 'IMPORTE'
/
COMMENT ON COLUMN  CO_MOVDEP.IND_DEPOSITO IS 'INDICA SI EL HA SIDO DEPOSITADO (1) O  NO (0)'
/
COMMENT ON COLUMN  CO_MOVDEP.COD_MONEDA IS 'Codigo de moneda'
/
COMMENT ON COLUMN  CO_MOVDEP.COD_OPERADORA_SCL IS 'Codigo de operadora'
/
COMMENT ON COLUMN  CO_MOVDEP.COD_PLAZA IS 'Codigo de plaza'
/
