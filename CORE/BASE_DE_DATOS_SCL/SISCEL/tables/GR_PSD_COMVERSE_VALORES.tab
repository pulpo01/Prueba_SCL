CREATE TABLE GR_PSD_COMVERSE_VALORES(
    SUBSCRIBER_ID NUMBER(8,0) NOT NULL 
 ,  BALANCE_TYPE VARCHAR2(1) NOT NULL 
 ,  CALL_DURATION NUMBER(8,0) NULL 
 ,  TOTAL_CHARGE NUMBER(8,0) NULL 
 ,  CANTIDAD NUMBER(6,0) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GR_PSD_COMVERSE_VALORES IS 'Temporal Para Abonados De Prepago Con Trafico.'
/
COMMENT ON COLUMN  GR_PSD_COMVERSE_VALORES.SUBSCRIBER_ID IS 'Identificador de suscriptor.'
/
COMMENT ON COLUMN  GR_PSD_COMVERSE_VALORES.BALANCE_TYPE IS 'Tipo de transaccion.'
/
COMMENT ON COLUMN  GR_PSD_COMVERSE_VALORES.CALL_DURATION IS 'Cantidad de minutos.'
/
COMMENT ON COLUMN  GR_PSD_COMVERSE_VALORES.TOTAL_CHARGE IS 'Monto de la transaccion.'
/
COMMENT ON COLUMN  GR_PSD_COMVERSE_VALORES.CANTIDAD IS 'Cantidad de transacciones.'
/
