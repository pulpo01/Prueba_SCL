CREATE TABLE ADN_CAB_PLAN_STATUS_TO(
    NUM_INFORME NUMBER(8,0) NOT NULL 
 ,  FEC_INFORME DATE NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  CNT_CANDIDATOS NUMBER(8,0) DEFAULT 0 NULL 
 ,  CNT_TOTAL NUMBER(8,0) DEFAULT 0 NULL 
 ,  CNT_CONSUMIDOS NUMBER(8,0) DEFAULT 0 NULL 
 ,  CNT_LIMPIOS NUMBER(8,0) DEFAULT 0 NULL 
 ,  COD_ESTADO NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ADN_CAB_PLAN_STATUS_TO IS 'Almacena el resumen de los bloques del sistema de numeracion'
/
COMMENT ON COLUMN  ADN_CAB_PLAN_STATUS_TO.NUM_INFORME IS 'Numero de Informe'
/
COMMENT ON COLUMN  ADN_CAB_PLAN_STATUS_TO.FEC_INFORME IS 'Fecha de Informe'
/
COMMENT ON COLUMN  ADN_CAB_PLAN_STATUS_TO.COD_CLIENTE IS 'Codigo del Cliente'
/
COMMENT ON COLUMN  ADN_CAB_PLAN_STATUS_TO.NOM_USUARIO IS 'Nombre del Usuario'
/
COMMENT ON COLUMN  ADN_CAB_PLAN_STATUS_TO.CNT_CANDIDATOS IS 'Cantidad de Numeros Candidatos'
/
COMMENT ON COLUMN  ADN_CAB_PLAN_STATUS_TO.CNT_TOTAL IS 'Cantidad Total de Numeros'
/
COMMENT ON COLUMN  ADN_CAB_PLAN_STATUS_TO.CNT_CONSUMIDOS IS 'Cantidad de Numeros Consumidos'
/
COMMENT ON COLUMN  ADN_CAB_PLAN_STATUS_TO.CNT_LIMPIOS IS 'Cantidad de Numeros Limpios'
/
