CREATE TABLE ICC_COMPROC_MOVPIN(
    NUM_MOVIMIENTO NUMBER(9,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(3,0) NOT NULL 
 ,  COD_LENGUAJE NUMBER(3,0) NOT NULL 
 ,  FEC_INGRESO DATE DEFAULT SYSDATE NOT NULL 
 ,  IND_BLOQUEO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  FEC_EJECUCION DATE NULL 
 ,  DES_COMANDO VARCHAR2(512) NOT NULL 
 ,  DES_RESPUESTA VARCHAR2(80) NULL 
 ,  COD_COMANDO NUMBER(3,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  COD_SISTEMA NUMBER(2,0) NOT NULL 
 ,  TIP_RESPUESTA NUMBER(1,0) DEFAULT -1 NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICC_COMPROC_MOVPIN IS 'Comandos Procesados Celular.'
/
COMMENT ON COLUMN  ICC_COMPROC_MOVPIN.FEC_INGRESO IS 'Fecha de ingreso del comando en la tabla.'
/
COMMENT ON COLUMN  ICC_COMPROC_MOVPIN.IND_BLOQUEO IS 'Indicador de bloqueo del comando'
/
COMMENT ON COLUMN  ICC_COMPROC_MOVPIN.FEC_EJECUCION IS 'Ultima fecha de ejecucion del comando.'
/
COMMENT ON COLUMN  ICC_COMPROC_MOVPIN.DES_COMANDO IS 'Comando a ejecutar en la central.'
/
COMMENT ON COLUMN  ICC_COMPROC_MOVPIN.DES_RESPUESTA IS 'Respuesta de la central a la ejecucion del comando.'
/
COMMENT ON COLUMN  ICC_COMPROC_MOVPIN.COD_COMANDO IS 'Codigo de comando a ejecutar.'
/
COMMENT ON COLUMN  ICC_COMPROC_MOVPIN.COD_CENTRAL IS 'Codigo de central en la que se ejecuta el comando.'
/
COMMENT ON COLUMN  ICC_COMPROC_MOVPIN.COD_SISTEMA IS 'Codigo de sistema donde se ejecuta el comando.'
/
COMMENT ON COLUMN  ICC_COMPROC_MOVPIN.TIP_RESPUESTA IS 'Respuesta de la central correcta o incorrecta.'
/
COMMENT ON COLUMN  ICC_COMPROC_MOVPIN.NUM_MOVIMIENTO IS 'Numero de movimiento.'
/
COMMENT ON COLUMN  ICC_COMPROC_MOVPIN.NUM_ORDEN IS 'Numero de orden del comando.'
/
COMMENT ON COLUMN  ICC_COMPROC_MOVPIN.COD_LENGUAJE IS 'Codigo de lenguaje del comando.'
/
