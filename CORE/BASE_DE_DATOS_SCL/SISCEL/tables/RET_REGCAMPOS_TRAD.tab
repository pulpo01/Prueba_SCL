CREATE TABLE RET_REGCAMPOS_TRAD(
    COD_ENTIDAD VARCHAR2(15) NOT NULL 
 ,  COD_REGISTRO VARCHAR2(3) NOT NULL 
 ,  COD_CAMPO NUMBER(3,0) NOT NULL 
 ,  VAL_INICIAL NUMBER(3,0) DEFAULT 1 NULL 
 ,  VAL_LARGO NUMBER(3,0) DEFAULT 1 NULL 
 ,  VAL_DECIMALES NUMBER(1,0) DEFAULT 0 NULL 
 ,  GLS_DEFECTO VARCHAR2(30) NULL 
 ,  IND_CORRELATIVO NUMBER(1,0) DEFAULT 0 NULL 
 ,  GLS_FORMATO_FECHA VARCHAR2(21) DEFAULT '.' NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  RET_REGCAMPOS_TRAD IS 'Relaciona Los Campos Que Pertenecen Al Archivo.'
/
COMMENT ON COLUMN  RET_REGCAMPOS_TRAD.COD_ENTIDAD IS '.'
/
COMMENT ON COLUMN  RET_REGCAMPOS_TRAD.COD_REGISTRO IS '.'
/
COMMENT ON COLUMN  RET_REGCAMPOS_TRAD.COD_CAMPO IS '.'
/
COMMENT ON COLUMN  RET_REGCAMPOS_TRAD.VAL_INICIAL IS 'Posicion inicial del campo en el archivo.'
/
COMMENT ON COLUMN  RET_REGCAMPOS_TRAD.VAL_LARGO IS 'Largo total del campo en el archivo.'
/
COMMENT ON COLUMN  RET_REGCAMPOS_TRAD.VAL_DECIMALES IS 'indica la cantidad de decimales contando de derecha a izquierda en caso de ser necesario.'
/
COMMENT ON COLUMN  RET_REGCAMPOS_TRAD.GLS_DEFECTO IS 'Valor por defecto de los campos en el archivo.'
/
COMMENT ON COLUMN  RET_REGCAMPOS_TRAD.IND_CORRELATIVO IS '0: Indica valor en archivo; 1: Indica correlativo interno.'
/
COMMENT ON COLUMN  RET_REGCAMPOS_TRAD.GLS_FORMATO_FECHA IS '.'
