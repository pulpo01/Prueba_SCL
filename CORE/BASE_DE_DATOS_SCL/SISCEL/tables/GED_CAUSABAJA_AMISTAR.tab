CREATE TABLE GED_CAUSABAJA_AMISTAR(
    COD_CAUSABAJA CHAR(1) NOT NULL 
 ,  DES_CAUSABAJA VARCHAR2(60) NOT NULL 
 ,  IND_VIGENCIA CHAR(1) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GED_CAUSABAJA_AMISTAR IS 'Tabla de tipos de baja de registro de venta de Kits Amistar o reclamo'
/
COMMENT ON COLUMN  GED_CAUSABAJA_AMISTAR.COD_CAUSABAJA IS 'Codigo de baja de registro de venta de Kits Amistar o reclamo. No corresponde a la columna COD_CAUSABAJA de otros sussistemas.'
/
COMMENT ON COLUMN  GED_CAUSABAJA_AMISTAR.DES_CAUSABAJA IS 'Descripcion de la causa de la baja'
/
COMMENT ON COLUMN  GED_CAUSABAJA_AMISTAR.IND_VIGENCIA IS 'Indicador de vigencia: V=Vigente | N=No Vigente'
/
