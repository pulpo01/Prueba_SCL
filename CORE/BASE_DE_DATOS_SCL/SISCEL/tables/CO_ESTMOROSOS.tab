CREATE TABLE CO_ESTMOROSOS(
    FEC_ANALISIS DATE NOT NULL 
 ,  COD_CATEGORIA VARCHAR2(5) NOT NULL 
 ,  COD_GESTION VARCHAR2(5) NOT NULL 
 ,  CNT_CLIENTE NUMBER(8,0) NOT NULL 
 ,  CNT_ABOCELU NUMBER(8,0) NOT NULL 
 ,  CNT_ABOBEEP NUMBER(8,0) NOT NULL 
 ,  DEU_VENCIDA NUMBER(18,4) NOT NULL 
 ,  DEU_NOVENC NUMBER(18,4) NOT NULL 
 ,  DEU_INICIAL NUMBER(18,4) NOT NULL 
 ,  CNT_NUM_IDENT NUMBER(8,0) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_ESTMOROSOS IS 'Estadisticas de Clientes Morosos
'
/
COMMENT ON COLUMN  CO_ESTMOROSOS.FEC_ANALISIS IS 'Fecha de analisis'
/
COMMENT ON COLUMN  CO_ESTMOROSOS.COD_CATEGORIA IS 'Categorma del cliente'
/
COMMENT ON COLUMN  CO_ESTMOROSOS.COD_GESTION IS 'Gestion del cliente'
/
COMMENT ON COLUMN  CO_ESTMOROSOS.CNT_CLIENTE IS 'Cantidad de Clientes'
/
COMMENT ON COLUMN  CO_ESTMOROSOS.CNT_ABOCELU IS 'Cantidad de Abonados Celulares'
/
COMMENT ON COLUMN  CO_ESTMOROSOS.CNT_ABOBEEP IS 'Cantidad de Abonados Beeper'
/
COMMENT ON COLUMN  CO_ESTMOROSOS.DEU_VENCIDA IS 'Deuda Vencida'
/
COMMENT ON COLUMN  CO_ESTMOROSOS.DEU_NOVENC IS 'Deuda No Vencida'
/
COMMENT ON COLUMN  CO_ESTMOROSOS.DEU_INICIAL IS 'Deuda Inicial'
/
