CREATE TABLE CO_EXCLUIDOS_ACCIONES_TO(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  TIP_EXCLUSION NUMBER(2,0) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  FEC_INI_EXCLUSION DATE NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_EXCLUIDOS_ACCIONES_TO IS '[ EXCL ] Registro de clientes que deben ser excluidos de las acciones de cobranzaRegistro de clientes que deben ser excluidos de las acciones de cobranza'
/
COMMENT ON COLUMN  CO_EXCLUIDOS_ACCIONES_TO.COD_CLIENTE IS 'Código de cliente que debe ser excluido de las acciones de cobranza'
/
COMMENT ON COLUMN  CO_EXCLUIDOS_ACCIONES_TO.TIP_EXCLUSION IS 'Tipos de exclusiones (registrado en tabla CO_CODIGOS)'
/
COMMENT ON COLUMN  CO_EXCLUIDOS_ACCIONES_TO.FEC_INGRESO IS 'Fecha en que se registró el cliente en la tabla'
/
COMMENT ON COLUMN  CO_EXCLUIDOS_ACCIONES_TO.FEC_INI_EXCLUSION IS 'Fecha en que se realizó la primera omisión de las acciones'
/
