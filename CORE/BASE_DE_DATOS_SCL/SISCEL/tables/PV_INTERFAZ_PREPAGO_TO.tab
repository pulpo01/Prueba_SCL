CREATE TABLE PV_INTERFAZ_PREPAGO_TO
(
  NUM_ABONADO    NUMBER                         NOT NULL,
  FEC_EJECUCION  DATE                           NOT NULL,
  COD_ESTADO     VARCHAR2(1 BYTE),
  COD_ACTABO     VARCHAR2(2 BYTE)               NOT NULL,
  FEC_PROCESO    DATE,
  NOM_USUARIO    VARCHAR2(30 BYTE),
  NUM_OS         NUMBER(10),
  COD_CAUSA      VARCHAR2(2 BYTE),
  IND_CENTRAL    NUMBER(1)                      NOT NULL,
  COD_OS         VARCHAR2(5 BYTE)
) TABLESPACE INSTALL_DATA
/

COMMENT ON TABLE PV_INTERFAZ_PREPAGO_TO IS 'Interfaz para bloqueo y bajas masivas';

COMMENT ON COLUMN PV_INTERFAZ_PREPAGO_TO.COD_OS IS 'Código de Orden de Servicio';

COMMENT ON COLUMN PV_INTERFAZ_PREPAGO_TO.NUM_ABONADO IS 'Numero de abonado SCL.';

COMMENT ON COLUMN PV_INTERFAZ_PREPAGO_TO.FEC_EJECUCION IS 'Fecha que se ejecuta la ooss';

COMMENT ON COLUMN PV_INTERFAZ_PREPAGO_TO.COD_ESTADO IS 'Estado de la ejecucion de la OoSs';

COMMENT ON COLUMN PV_INTERFAZ_PREPAGO_TO.COD_ACTABO IS 'Codigo de actuacion de la ooss';

COMMENT ON COLUMN PV_INTERFAZ_PREPAGO_TO.NOM_USUARIO IS 'usuario que genero la ooss';

COMMENT ON COLUMN PV_INTERFAZ_PREPAGO_TO.NUM_OS IS 'numero secuencial de la ooss.';

COMMENT ON COLUMN PV_INTERFAZ_PREPAGO_TO.COD_CAUSA IS 'Causa de la baja o bloqueo ';

COMMENT ON COLUMN PV_INTERFAZ_PREPAGO_TO.IND_CENTRAL IS 'Indica si el registro se ejecuta en central 1 se jecuta 0 no se ejecuta';


		
