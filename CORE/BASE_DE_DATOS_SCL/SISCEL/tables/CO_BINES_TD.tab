CREATE TABLE CO_BINES_TD(
    NUM_BIN NUMBER(6,0) NOT NULL 
 ,  COD_BANCO VARCHAR2(15) NOT NULL 
 ,  DES_BIN VARCHAR2(50) NULL 
 ,  COD_TIPTARJETA VARCHAR2(3) NOT NULL 
 ,  TIP_CARGO VARCHAR2(1) NOT NULL 
 ,  PRO_TARJETA VARCHAR2(1) NOT NULL 
 ,  COD_BCO_FUSION VARCHAR2(15) NULL 
 ,  COD_ESTADO VARCHAR2(1) NOT NULL 
 ,  COD_BCO_DEBITO VARCHAR2(15) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_BINES_TD IS '[ BINE ] Contiene los Bines de las Tarjetas de CréditoContiene los Bines de las Tarjetas de Crédito'
/
COMMENT ON COLUMN  CO_BINES_TD.NUM_BIN IS 'Numero de Bin'
/
COMMENT ON COLUMN  CO_BINES_TD.COD_BANCO IS 'Codigo de Banco asociado al BIN'
/
COMMENT ON COLUMN  CO_BINES_TD.DES_BIN IS 'Descripción del Bin'
/
COMMENT ON COLUMN  CO_BINES_TD.COD_TIPTARJETA IS 'Tipo de tarjeta '
/
COMMENT ON COLUMN  CO_BINES_TD.TIP_CARGO IS 'Tipo de cargo , Valores : D o  C'
/
COMMENT ON COLUMN  CO_BINES_TD.PRO_TARJETA IS 'Procedencia tarjeta , Valores : N o E'
/
COMMENT ON COLUMN  CO_BINES_TD.COD_BCO_FUSION IS 'Código Banco Fusión'
/
COMMENT ON COLUMN  CO_BINES_TD.COD_ESTADO IS 'Codigo de estado , Valores: A o B'
/
COMMENT ON COLUMN  CO_BINES_TD.COD_BCO_DEBITO IS 'Codigo de Banco de Debito'
/
