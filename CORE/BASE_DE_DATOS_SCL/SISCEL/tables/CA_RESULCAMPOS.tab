CREATE TABLE CA_RESULCAMPOS(
    NUM_LLAMADA NUMBER(9,0) NOT NULL 
 ,  NUM_SECUENCIA NUMBER(3,0) NOT NULL 
 ,  NUM_SECURESP NUMBER(3,0) NOT NULL 
 ,  COD_CUESTIONARIO VARCHAR2(6) NOT NULL 
 ,  NUM_PREGUNTA NUMBER(2,0) NOT NULL 
 ,  NUM_RESPUESTA NUMBER(2,0) NOT NULL 
 ,  COD_CAMPO NUMBER(3,0) NOT NULL 
 ,  VAL_ALFA VARCHAR2(2000) NULL 
 ,  VAL_NUMER NUMBER(31,9) NULL 
 ,  VAL_FECHA DATE NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_RESULCAMPOS IS 'Resultados Campos'
/
COMMENT ON COLUMN  CA_RESULCAMPOS.NUM_LLAMADA IS 'Numero llamada'
/
COMMENT ON COLUMN  CA_RESULCAMPOS.NUM_SECUENCIA IS 'Numero secuencia'
/
COMMENT ON COLUMN  CA_RESULCAMPOS.NUM_SECURESP IS 'Numero de secuencia de respuesta'
/
COMMENT ON COLUMN  CA_RESULCAMPOS.COD_CUESTIONARIO IS 'Codigo cuestionario'
/
COMMENT ON COLUMN  CA_RESULCAMPOS.NUM_PREGUNTA IS 'Numero pregunta'
/
COMMENT ON COLUMN  CA_RESULCAMPOS.NUM_RESPUESTA IS 'Numero respuesta'
/
COMMENT ON COLUMN  CA_RESULCAMPOS.COD_CAMPO IS 'Campo'
/
COMMENT ON COLUMN  CA_RESULCAMPOS.VAL_ALFA IS 'Valor alfanumerico'
/
COMMENT ON COLUMN  CA_RESULCAMPOS.VAL_NUMER IS 'Valor numerico'
/
COMMENT ON COLUMN  CA_RESULCAMPOS.VAL_FECHA IS 'Valor fecha'
/
