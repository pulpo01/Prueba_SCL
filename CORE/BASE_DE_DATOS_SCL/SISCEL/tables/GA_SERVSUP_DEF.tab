CREATE TABLE GA_SERVSUP_DEF(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  COD_SERVDEF VARCHAR2(3) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  COD_SERVORIG VARCHAR2(3) NULL 
 ,  TIP_RELACION NUMBER(1,0) DEFAULT 1 NOT NULL 
 ,  COD_ACTABO VARCHAR2(2) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_SERVSUP_DEF IS 'Servicios suplementarios que se deben activar por defecto.'
/
COMMENT ON COLUMN  GA_SERVSUP_DEF.COD_PRODUCTO IS 'Codigo de producto.'
/
COMMENT ON COLUMN  GA_SERVSUP_DEF.COD_SERVICIO IS 'Codigo de servicio suplementario.'
/
COMMENT ON COLUMN  GA_SERVSUP_DEF.COD_SERVDEF IS 'Codigo de servicio suplementario activado por defecto.'
/
COMMENT ON COLUMN  GA_SERVSUP_DEF.FEC_DESDE IS 'Fecha inicial de activacion por defecto.'
/
COMMENT ON COLUMN  GA_SERVSUP_DEF.NOM_USUARIO IS 'Usuario que asocio la activacion por defecto al servicio.'
/
COMMENT ON COLUMN  GA_SERVSUP_DEF.FEC_HASTA IS 'Fecha final de activacion por defecto.'
/
COMMENT ON COLUMN  GA_SERVSUP_DEF.COD_SERVORIG IS 'Codigo de servicio suplementario origen.'
/
