CREATE TABLE PV_LABEL_REPORTES(
    COD_REPORTE NUMBER(6,0) NOT NULL 
 ,  COD_LABEL NUMBER(6,0) NOT NULL 
 ,  DES_LABEL VARCHAR2(80) NOT NULL 
 ,  TIP_LABEL NUMBER(4,0) NOT NULL 
 ,  IND_ORDEN NUMBER(4,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_LABEL_REPORTES IS 'Pv_Label_Reportes.'
/
COMMENT ON COLUMN  PV_LABEL_REPORTES.COD_REPORTE IS 'Codigo de reporte.'
/
COMMENT ON COLUMN  PV_LABEL_REPORTES.COD_LABEL IS 'Codigo del label.'
/
COMMENT ON COLUMN  PV_LABEL_REPORTES.DES_LABEL IS 'Descripcion del Label.'
/
COMMENT ON COLUMN  PV_LABEL_REPORTES.TIP_LABEL IS 'Tipo del label (1: datos gen., 2: cabecera, 3: detalles, ...).'
/
COMMENT ON COLUMN  PV_LABEL_REPORTES.IND_ORDEN IS 'Orden dentro de cada tipo de label.'
/
