CREATE TABLE FAD_IMPCONCEPTOS(
    COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_SUBGRUPO NUMBER(4,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(5,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  FEC_ULTMOD DATE NULL 
 ,  TIP_CONCEP NUMBER(1,0) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAD_IMPCONCEPTOS IS 'Conceptos De Sub-Grupos De Impresion.'
/
COMMENT ON COLUMN  FAD_IMPCONCEPTOS.TIP_CONCEP IS 'Tipo de concepto..'
/
COMMENT ON COLUMN  FAD_IMPCONCEPTOS.COD_CONCEPTO IS 'Codigo de concepto..'
/
COMMENT ON COLUMN  FAD_IMPCONCEPTOS.COD_SUBGRUPO IS 'Codigo de subgrupo de impresion..'
/
COMMENT ON COLUMN  FAD_IMPCONCEPTOS.NUM_ORDEN IS 'Orden de aparicion del concepto dentro del bloque correspondiente al subgrupo asociado..'
/
COMMENT ON COLUMN  FAD_IMPCONCEPTOS.NOM_USUARIO IS 'ultimo usuario que realizo algun cambio en el registro..'
/
COMMENT ON COLUMN  FAD_IMPCONCEPTOS.FEC_ULTMOD IS 'Fecha de ultima modificacion..'
/
