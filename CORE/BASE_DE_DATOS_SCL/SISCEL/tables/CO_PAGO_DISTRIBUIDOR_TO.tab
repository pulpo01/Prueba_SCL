CREATE TABLE CO_PAGO_DISTRIBUIDOR_TO
(
  COD_CLIENTE    NUMBER(8)                      NOT NULL,
  IMPORTE_PAGO   NUMBER(14,4)                   NOT NULL,
  NUM_FOLIO      NUMBER(9)                      NOT NULL,
  PREF_PLAZA     VARCHAR2(25 BYTE),
  IND_CANCELADO  NUMBER(1)                      DEFAULT 0,
  FECHA_PAGO     DATE,
  DESC_ERROR     VARCHAR2(200 BYTE),
  FECHA_ERROR    DATE
)
TABLESPACE TS_RECYCOB_SDAT
PCTUSED    0
PCTFREE    20
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          3360K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE CO_PAGO_DISTRIBUIDOR_TO IS 'TABLA ENTRADA APLICACION DE PAGOS';

COMMENT ON COLUMN CO_PAGO_DISTRIBUIDOR_TO.COD_CLIENTE IS 'CODIGO DEL CLIENTE';

COMMENT ON COLUMN CO_PAGO_DISTRIBUIDOR_TO.IMPORTE_PAGO IS 'IMPORTE DEL PAGO';

COMMENT ON COLUMN CO_PAGO_DISTRIBUIDOR_TO.NUM_FOLIO IS 'NPUMERO DEL FOLIO';

COMMENT ON COLUMN CO_PAGO_DISTRIBUIDOR_TO.PREF_PLAZA IS 'PLAZA';

COMMENT ON COLUMN CO_PAGO_DISTRIBUIDOR_TO.IND_CANCELADO IS 'INDICADOR DE CANDELADO';

COMMENT ON COLUMN CO_PAGO_DISTRIBUIDOR_TO.FECHA_PAGO IS 'FECHA DEL PAGO';

COMMENT ON COLUMN CO_PAGO_DISTRIBUIDOR_TO.DESC_ERROR IS 'DESCRICION DEL ERROR';

COMMENT ON COLUMN CO_PAGO_DISTRIBUIDOR_TO.FECHA_ERROR IS 'FECHA DE GENERACION DEL ERROR';
