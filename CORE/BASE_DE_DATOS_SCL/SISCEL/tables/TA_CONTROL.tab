CREATE TABLE TA_CONTROL(
    NUM_BLOQUE VARCHAR2(9) NOT NULL 
 ,  FEC_SISTEMA DATE NOT NULL 
 ,  TIP_EJECUCION NUMBER(1,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  NOM_FICHERO VARCHAR2(80) NOT NULL 
 ,  COD_TIPCENTRAL NUMBER(2,0) NULL 
 ,  MOD_EJECUCION NUMBER(1,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_CONTROL IS 'Ficheros de tickets introducidos al sistema'
/
COMMENT ON COLUMN  TA_CONTROL.NUM_BLOQUE IS 'Numero de Bloque'
/
COMMENT ON COLUMN  TA_CONTROL.FEC_SISTEMA IS 'Fecha de sistema'
/
COMMENT ON COLUMN  TA_CONTROL.TIP_EJECUCION IS 'Tipo de ejecucion'
/
COMMENT ON COLUMN  TA_CONTROL.COD_PRODUCTO IS 'Codigo de producto'
/
COMMENT ON COLUMN  TA_CONTROL.NOM_FICHERO IS 'Nombre del Fichero'
/
COMMENT ON COLUMN  TA_CONTROL.COD_TIPCENTRAL IS 'Codigo de tipo de central'
/
COMMENT ON COLUMN  TA_CONTROL.MOD_EJECUCION IS 'Modo de ejecucion'
/
COMMENT ON COLUMN  TA_CONTROL.COD_CENTRAL IS 'Codigo de Central'
/
