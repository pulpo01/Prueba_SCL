CREATE TABLE VE_PROCESOSOMITIDOS_TO(
    NUM_VENTA NUMBER NOT NULL 
 ,  ORDEN NUMBER NOT NULL 
 ,  PROCEDIMIENTO VARCHAR2(300) NOT NULL 
 ,  OBSERVACION VARCHAR2(100) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_PROCESOSOMITIDOS_TO IS 'PROCESOS OMITIDOS DE LAS VENTAS '
/
COMMENT ON COLUMN  VE_PROCESOSOMITIDOS_TO.NUM_VENTA IS 'Numero de la venta                                          '
/
COMMENT ON COLUMN  VE_PROCESOSOMITIDOS_TO.ORDEN IS 'Orden de ejecucion de procesos omitidos asociados a la venta'
/
COMMENT ON COLUMN  VE_PROCESOSOMITIDOS_TO.PROCEDIMIENTO IS 'Cadena que contiene pl a ejecutar y sus parametros          '
/
COMMENT ON COLUMN  VE_PROCESOSOMITIDOS_TO.OBSERVACION IS 'Observaciones en caso de error                              '
/
