CREATE TABLE RE_HRECLAMOS_ANTIDUPLI(
    NUM_TERMINAL NUMBER(15,0) NOT NULL 
 ,  TIP_RECLAMO NUMBER(3,0) NOT NULL 
 ,  FEC_RECLAMO DATE NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  RE_HRECLAMOS_ANTIDUPLI IS 'Reclamos cerrados de un mes y medio atras, para chequeo antiduplicidad.'
/
COMMENT ON COLUMN  RE_HRECLAMOS_ANTIDUPLI.NUM_TERMINAL IS 'Numero de terminal asociado al reclamo.'
/
COMMENT ON COLUMN  RE_HRECLAMOS_ANTIDUPLI.TIP_RECLAMO IS 'Tipo de reclamo ingresado para el terminal asociado.'
/
COMMENT ON COLUMN  RE_HRECLAMOS_ANTIDUPLI.FEC_RECLAMO IS 'Fecha de ingreso del reclamo.'
/
