CREATE TABLE GE_MTOPREAUTOCLI_TO (  
   COD_CLIENTE                   NUMBER(8)         NOT NULL
 , MTO_PREAUTO                   NUMBER(14,4)      NOT NULL
 , COD_CREDITICIA                VARCHAR2(5)       NOT NULL
 , FEC_MODIFICACION              DATE              NOT NULL
 , NOM_USUARORA                  VARCHAR2(30)      NOT NULL
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  GE_MTOPREAUTOCLI_TO                      IS '[ GA_MPA ] Monto Preautorizado de ClienteMonto Preautorizado de Cliente'
/

COMMENT ON COLUMN GE_MTOPREAUTOCLI_TO.COD_CLIENTE                    IS 'codigo cliente SCL'
/
COMMENT ON COLUMN GE_MTOPREAUTOCLI_TO.MTO_PREAUTO                    IS 'monto preautorizado'
/
COMMENT ON COLUMN GE_MTOPREAUTOCLI_TO.COD_CREDITICIA                 IS 'clasificacion crediticia'
/
COMMENT ON COLUMN GE_MTOPREAUTOCLI_TO.FEC_MODIFICACION               IS 'fecha modificacion'
/
COMMENT ON COLUMN GE_MTOPREAUTOCLI_TO.NOM_USUARORA                   IS 'usuario de modificacion'
/

