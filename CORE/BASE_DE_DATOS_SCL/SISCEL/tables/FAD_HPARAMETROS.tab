CREATE TABLE FAD_HPARAMETROS(
    COD_PARAMETRO NUMBER(10,0) NOT NULL 
 ,  DES_PARAMETRO VARCHAR2(1024) NOT NULL 
 ,  TIP_PARAMETRO VARCHAR2(32) NOT NULL 
 ,  VAL_NUMERICO NUMBER(10,0) NULL 
 ,  VAL_CARACTER VARCHAR2(512) NULL 
 ,  VAL_FECHA DATE NULL 
 ,  COD_MODULO VARCHAR2(2) DEFAULT 'FA' NOT NULL 
 ,  FEC_MODIFICACION DATE DEFAULT SYSDATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) DEFAULT 'OPS$MDVILLAG' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAD_HPARAMETROS IS 'Historial De Parametros.'
/
COMMENT ON COLUMN  FAD_HPARAMETROS.COD_PARAMETRO IS 'Codigo de Parmetro.'
/
COMMENT ON COLUMN  FAD_HPARAMETROS.DES_PARAMETRO IS 'Descripcion de Parametro.'
/
COMMENT ON COLUMN  FAD_HPARAMETROS.TIP_PARAMETRO IS 'Tipo de Parametro.'
/
COMMENT ON COLUMN  FAD_HPARAMETROS.VAL_NUMERICO IS 'Valor Numerico.'
/
COMMENT ON COLUMN  FAD_HPARAMETROS.VAL_CARACTER IS 'Valor Carcter.'
/
COMMENT ON COLUMN  FAD_HPARAMETROS.VAL_FECHA IS 'Valor Fecha.'
/
COMMENT ON COLUMN  FAD_HPARAMETROS.COD_MODULO IS 'Codigo Modulo.'
/
COMMENT ON COLUMN  FAD_HPARAMETROS.FEC_MODIFICACION IS 'Fecha Ultima Modificacion.'
/
COMMENT ON COLUMN  FAD_HPARAMETROS.NOM_USUARIO IS 'Nombre Usuario.'
/
