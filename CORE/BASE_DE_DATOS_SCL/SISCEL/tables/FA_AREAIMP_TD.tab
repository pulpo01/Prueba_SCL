CREATE TABLE FA_AREAIMP_TD(
    COD_AREA VARCHAR2(5) NOT NULL 
 ,  COD_AREA_IMP VARCHAR2(5) NOT NULL 
 ,  FEC_INI_VIG DATE NOT NULL 
 ,  FEC_TER_VIG DATE NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_AREAIMP_TD IS '[ AIMP ] RELACION DE AREAS DE IMPRESIÓN CON AREAS IMPOSITIVAS'
/
COMMENT ON COLUMN  FA_AREAIMP_TD.COD_AREA IS 'Area desde donde fue realizada la llamada'
/
COMMENT ON COLUMN  FA_AREAIMP_TD.COD_AREA_IMP IS 'Area Impositiva'
/
COMMENT ON COLUMN  FA_AREAIMP_TD.FEC_INI_VIG IS 'Fecha Inicio de Vigencia'
/
COMMENT ON COLUMN  FA_AREAIMP_TD.FEC_TER_VIG IS 'Fecha Terimino de Vigencia'
/
