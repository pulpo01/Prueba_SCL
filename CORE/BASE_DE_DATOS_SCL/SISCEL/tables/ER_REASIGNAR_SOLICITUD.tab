CREATE TABLE ER_REASIGNAR_SOLICITUD(
    NRO_SOLIC NUMBER(10,0) NOT NULL 
 ,  FEC_ASIGNACION_DH DATE NOT NULL 
 ,  NOM_USUAAPROBADOR VARCHAR2(30) NOT NULL 
 ,  NOM_USUANUEVO_APROBADOR VARCHAR2(30) NOT NULL 
 ,  OBSERVACION_APROBADOR VARCHAR2(80) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ER_REASIGNAR_SOLICITUD IS 'Solicitudes de Ventas reasignadas'
/
COMMENT ON COLUMN  ER_REASIGNAR_SOLICITUD.NRO_SOLIC IS 'Numero de Solicitud.'
/
COMMENT ON COLUMN  ER_REASIGNAR_SOLICITUD.FEC_ASIGNACION_DH IS 'Fecha de Asignacion ( Con hh:mm:ss).'
/
COMMENT ON COLUMN  ER_REASIGNAR_SOLICITUD.NOM_USUAAPROBADOR IS 'Usuario Aprobador.'
/
COMMENT ON COLUMN  ER_REASIGNAR_SOLICITUD.NOM_USUANUEVO_APROBADOR IS 'Nuevo Usuario para Aprobacion.'
/
COMMENT ON COLUMN  ER_REASIGNAR_SOLICITUD.OBSERVACION_APROBADOR IS 'Observacion ingresada por el Aprobador.'
/
