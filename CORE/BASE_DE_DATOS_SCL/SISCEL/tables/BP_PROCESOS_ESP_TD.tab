CREATE TABLE BP_PROCESOS_ESP_TD(
    COD_PROCESO VARCHAR2(5) NOT NULL 
 ,  DES_PROCESO VARCHAR2(100) NOT NULL 
 ,  DIR_PROCESO VARCHAR2(15) NOT NULL 
 ,  PAR_ENTRADA VARCHAR2(70) NULL 
 ,  PAR_SALIDA VARCHAR2(70) NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  BP_PROCESOS_ESP_TD.COD_PROCESO IS 'Codigo interno del proceso'
/
COMMENT ON COLUMN  BP_PROCESOS_ESP_TD.DES_PROCESO IS 'Descripcion breve del proceso'
/
COMMENT ON COLUMN  BP_PROCESOS_ESP_TD.DIR_PROCESO IS 'Ubicacion logica del proceso'
/
COMMENT ON COLUMN  BP_PROCESOS_ESP_TD.PAR_ENTRADA IS 'Parametros de entrada al proceso'
/
COMMENT ON COLUMN  BP_PROCESOS_ESP_TD.PAR_SALIDA IS 'Parametros de salida del proceso'
/
COMMENT ON COLUMN  BP_PROCESOS_ESP_TD.NOM_USUARIO IS 'Usuario Responsable'
/
COMMENT ON COLUMN  BP_PROCESOS_ESP_TD.FEC_ULTMOD IS 'Fecha de ultima modificacion'
/
