CREATE TABLE CI_PANTALLAS(
    COD_PANTALLA NUMBER(3,0) NOT NULL 
 ,  NOM_PANTALLA VARCHAR2(50) NOT NULL 
 ,  PAT_PANTALLA VARCHAR2(1000) NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CI_PANTALLAS IS 'Pantallas del SAC '
/
COMMENT ON COLUMN  CI_PANTALLAS.COD_PANTALLA IS 'Csdigo de pantalla/carpeta SAC'
/
COMMENT ON COLUMN  CI_PANTALLAS.NOM_PANTALLA IS 'Nombre de la pantalla'
/
COMMENT ON COLUMN  CI_PANTALLAS.PAT_PANTALLA IS 'Path de ejecucisn para llegar a la pantalla'
/
