CREATE TABLE FA_TRAZAFORDET(
    IND_ORDEN NUMBER(12,0) NOT NULL 
 ,  COD_CICLFACT NUMBER(8,0) NOT NULL 
 ,  COD_ESTADO VARCHAR2(3) NOT NULL 
 ,  CANT_REGISTROS NUMBER(6,0) NOT NULL 
 ,  CANT_MINUTOS NUMBER(6,0) NOT NULL 
 ,  MTO_NETO NUMBER(14,4) NOT NULL 
 ,  MTO_IVA NUMBER(14,4) NOT NULL 
 ,  MTO_TOTAL NUMBER(14,4) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  FA_TRAZAFORDET.IND_ORDEN IS 'ind_orden'
/
COMMENT ON COLUMN  FA_TRAZAFORDET.COD_CICLFACT IS 'ciclfact a facturar'
/
COMMENT ON COLUMN  FA_TRAZAFORDET.COD_ESTADO IS 'Estado ( ingresado, facturado,cerrado)'
/
COMMENT ON COLUMN  FA_TRAZAFORDET.CANT_REGISTROS IS 'total de registros por ciclo'
/
COMMENT ON COLUMN  FA_TRAZAFORDET.CANT_MINUTOS IS 'cantidad de minutos port ciclo'
/
COMMENT ON COLUMN  FA_TRAZAFORDET.MTO_NETO IS 'monto neto'
/
COMMENT ON COLUMN  FA_TRAZAFORDET.MTO_IVA IS 'monto iva'
/
COMMENT ON COLUMN  FA_TRAZAFORDET.MTO_TOTAL IS 'monto total bruto'
/
