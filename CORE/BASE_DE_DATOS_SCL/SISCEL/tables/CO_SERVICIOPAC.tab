CREATE TABLE CO_SERVICIOPAC(
    COD_BANCO VARCHAR2(15) NOT NULL 
 ,  COD_FORMATO VARCHAR2(6) NOT NULL 
 ,  COD_SERVICIO VARCHAR2(10) NULL 
 ,  COD_CONVENIO VARCHAR2(20) NULL 
 ,  COD_BANCO_GRP VARCHAR2(15) NOT NULL 
 ,  COD_OPERADORA_SCL VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_SERVICIOPAC IS 'Tabla Con Servicio de Formatos de Archivos Pac.'
/
COMMENT ON COLUMN  CO_SERVICIOPAC.COD_BANCO IS 'Codigo de Banco.'
/
COMMENT ON COLUMN  CO_SERVICIOPAC.COD_FORMATO IS 'Codigo de Formato.'
/
COMMENT ON COLUMN  CO_SERVICIOPAC.COD_SERVICIO IS 'Codigo de Servicio.'
/
COMMENT ON COLUMN  CO_SERVICIOPAC.COD_CONVENIO IS 'Codigo de Convenio.'
/
COMMENT ON COLUMN  CO_SERVICIOPAC.COD_BANCO_GRP IS 'Codigo de Banco Agrupado'
/
COMMENT ON COLUMN  CO_SERVICIOPAC.COD_OPERADORA_SCL IS 'Codigo Operadora'
/
