CREATE TABLE ICC_INTERFAZ_CONSULTAS_TO(
    NUM_MOVIMIENTO NUMBER(9,0) NOT NULL 
 ,  NUM_TRX NUMBER(10,0) NOT NULL 
 ,  COD_ESTADO NUMBER(1,0) DEFAULT 1 NOT NULL 
 ,  TIP_TECNOLOGIA VARCHAR2(7) NULL 
 ,  COD_MODULO VARCHAR2(2) NULL 
 ,  COD_PERIOCIDAD VARCHAR2(5) NULL 
 ,  TIP_ACTIVIDAD VARCHAR2(5) NULL 
 ,  COD_SERVICIOS VARCHAR2(255) DEFAULT NULL NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICC_INTERFAZ_CONSULTAS_TO IS 'NUMERO DE TRANSACCIONES ASOCIADAS AL MOVIMIENTO'
/
COMMENT ON COLUMN  ICC_INTERFAZ_CONSULTAS_TO.COD_SERVICIOS IS 'Servicios de creación'
/
COMMENT ON COLUMN  ICC_INTERFAZ_CONSULTAS_TO.NUM_MOVIMIENTO IS 'NUMERO DE MOVIMIENTO'
/
COMMENT ON COLUMN  ICC_INTERFAZ_CONSULTAS_TO.NUM_TRX IS 'NUMERO DE TRANSACCION'
/
COMMENT ON COLUMN  ICC_INTERFAZ_CONSULTAS_TO.COD_ESTADO IS 'ESTADO DE EJECUCION DE LA TRANSACCION'
/
COMMENT ON COLUMN  ICC_INTERFAZ_CONSULTAS_TO.TIP_TECNOLOGIA IS 'TIPO DE TECNOLOGIA'
/
COMMENT ON COLUMN  ICC_INTERFAZ_CONSULTAS_TO.COD_MODULO IS 'CODIGO DE MODULO'
/
COMMENT ON COLUMN  ICC_INTERFAZ_CONSULTAS_TO.COD_PERIOCIDAD IS 'Código de periodicidad'
/
COMMENT ON COLUMN  ICC_INTERFAZ_CONSULTAS_TO.TIP_ACTIVIDAD IS 'Tipo de actividad'
/
