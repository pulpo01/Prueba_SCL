CREATE TABLE VE_ARCHIVOS_TO(
    NOM_ARCHIVO VARCHAR2(30) NOT NULL 
 ,  CANT_REGISTROS NUMBER(4,0) DEFAULT 0 NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_CARGA DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_ARCHIVOS_TO IS 'Lista de archivos ejecutados correctamente en la carga'
/
COMMENT ON COLUMN  VE_ARCHIVOS_TO.NOM_ARCHIVO IS 'Nombre del archivo'
/
COMMENT ON COLUMN  VE_ARCHIVOS_TO.CANT_REGISTROS IS 'Cantidad de Registros'
/
COMMENT ON COLUMN  VE_ARCHIVOS_TO.NOM_USUARIO IS 'Nombre del Usuario responsable'
/
COMMENT ON COLUMN  VE_ARCHIVOS_TO.FEC_CARGA IS 'Fecha de ejecución'
/
