CREATE TABLE AL_AUDIT_CAB_NUMERACION_TO(
    DES_MODULO VARCHAR2(48) NULL 
 ,  COD_USUARIO VARCHAR2(30) NULL 
 ,  COD_OSUSUARIO VARCHAR2(30) NULL 
 ,  DES_PROGRAM VARCHAR2(48) NULL 
 ,  FEC_LOGIN DATE NULL 
 ,  NUM_NUMERACION NUMBER(9,0) NULL 
 ,  COD_ESTADO_ORI VARCHAR2(2) NULL 
 ,  COD_ESTADO_DES VARCHAR2(2) NULL 
 ,  COD_OPERACION VARCHAR2(1) NULL 
 ,  FEC_OPERACION DATE NULL 
 ) TABLESPACE    TS_LOGISTICA_MDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_AUDIT_CAB_NUMERACION_TO IS 'TABLA DE AUDITORIA DE NUMERACION'
/
COMMENT ON COLUMN  AL_AUDIT_CAB_NUMERACION_TO.DES_MODULO IS 'DESCRIPCION DEL MODULO'
/
COMMENT ON COLUMN  AL_AUDIT_CAB_NUMERACION_TO.COD_USUARIO IS 'USUARIO DEL MODULO'
/
COMMENT ON COLUMN  AL_AUDIT_CAB_NUMERACION_TO.COD_OSUSUARIO IS 'USUARIO MAQUINA'
/
COMMENT ON COLUMN  AL_AUDIT_CAB_NUMERACION_TO.DES_PROGRAM IS 'DESCRIPCION DEL PROGRAMA'
/
COMMENT ON COLUMN  AL_AUDIT_CAB_NUMERACION_TO.FEC_LOGIN IS 'FECHA DE LOGIN'
/
COMMENT ON COLUMN  AL_AUDIT_CAB_NUMERACION_TO.NUM_NUMERACION IS 'NUMERO DE LA ORDEN DE NUMERACION'
/
COMMENT ON COLUMN  AL_AUDIT_CAB_NUMERACION_TO.COD_ESTADO_ORI IS 'CODIGO ESTADO ORIGEN'
/
COMMENT ON COLUMN  AL_AUDIT_CAB_NUMERACION_TO.COD_ESTADO_DES IS 'CODIGO ESTADO FINAL'
/
COMMENT ON COLUMN  AL_AUDIT_CAB_NUMERACION_TO.COD_OPERACION IS 'TIPO DE OPERACION'
/
COMMENT ON COLUMN  AL_AUDIT_CAB_NUMERACION_TO.FEC_OPERACION IS 'FECHA DE LA OPERACION'
/
