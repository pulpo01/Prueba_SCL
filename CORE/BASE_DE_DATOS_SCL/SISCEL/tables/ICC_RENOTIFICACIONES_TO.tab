CREATE TABLE SISCEL.ICC_RENOTIFICACIONES_TO
(
  OS        NUMBER(10)                          NOT NULL,
  MENSAJE   VARCHAR2(512 BYTE)                  NOT NULL,
  ESTADO    NUMBER(4)                           NOT NULL,
  CONTADOR  NUMBER(4)                           NOT NULL
)  TABLESPACE TS_PROVISIONING_SDAT
/

COMMENT ON TABLE SISCEL.ICC_RENOTIFICACIONES_TO IS 'Tabla de asignacion de reproceso de respuestas';

COMMENT ON COLUMN SISCEL.ICC_RENOTIFICACIONES_TO.OS IS 'Numero de la Orden de servicio no procesada';

COMMENT ON COLUMN SISCEL.ICC_RENOTIFICACIONES_TO.MENSAJE IS 'Comando de la notioficacion no procesada';

COMMENT ON COLUMN SISCEL.ICC_RENOTIFICACIONES_TO.ESTADO IS 'Estado del Procesamiento';

COMMENT ON COLUMN SISCEL.ICC_RENOTIFICACIONES_TO.CONTADOR IS 'Cantidad de Reintentos';

