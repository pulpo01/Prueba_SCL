CREATE TABLE GED_PLANTILLARUTINAS(
    COD_OPERADORA VARCHAR2(5) NOT NULL 
 ,  COD_MODULO VARCHAR2(2) NOT NULL 
 ,  NUM_CORRELATIVO NUMBER(5,0) NOT NULL 
 ,  NOM_RUTINA VARCHAR2(50) NOT NULL 
 ,  COD_RUTINA VARCHAR2(5) NOT NULL 
 ,  FLG_ESTADO VARCHAR2(5) DEFAULT 'TRUE' NOT NULL 
 ,  DES_RUTINA VARCHAR2(100) NOT NULL 
 ,  FEC_ULTMOD_H DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GED_PLANTILLARUTINAS IS '.'
/
COMMENT ON COLUMN  GED_PLANTILLARUTINAS.COD_OPERADORA IS 'Código de la operadora actual.'
/
COMMENT ON COLUMN  GED_PLANTILLARUTINAS.COD_MODULO IS 'Código del módulo al que pertenece la rutina (GE_MODULOS.COD_MODULO).'
/
COMMENT ON COLUMN  GED_PLANTILLARUTINAS.NUM_CORRELATIVO IS 'Número entero positivo correlativo que parte de 1.'
/
COMMENT ON COLUMN  GED_PLANTILLARUTINAS.NOM_RUTINA IS 'Nombre de la Rutina que se utilizará para la operadora.'
/
COMMENT ON COLUMN  GED_PLANTILLARUTINAS.COD_RUTINA IS 'Tipo de Rutina (PL, ProC, etc.).'
/
COMMENT ON COLUMN  GED_PLANTILLARUTINAS.FLG_ESTADO IS 'Estado de la Rutina A=Activa, I=Inactiva.'
/
COMMENT ON COLUMN  GED_PLANTILLARUTINAS.DES_RUTINA IS 'Descripción de las acciones de la Rutina.'
/
COMMENT ON COLUMN  GED_PLANTILLARUTINAS.FEC_ULTMOD_H IS 'Fecha de modificación o inserción del registro.'
/
COMMENT ON COLUMN  GED_PLANTILLARUTINAS.NOM_USUARIO IS 'Identificador del usuario ("username" Oracle)..'
/
