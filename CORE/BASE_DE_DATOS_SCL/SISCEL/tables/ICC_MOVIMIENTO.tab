CREATE TABLE SISCEL.ICC_MOVIMIENTO
(
  NUM_MOVIMIENTO       NUMBER(9)                NOT NULL,
  NUM_ABONADO          NUMBER(8)                NOT NULL,
  COD_ESTADO           NUMBER(3)                DEFAULT 1                     NOT NULL,
  COD_ACTABO           VARCHAR2(2 BYTE)         DEFAULT 'XX'                  NOT NULL,
  COD_MODULO           VARCHAR2(2 BYTE)         NOT NULL,
  NUM_INTENTOS         NUMBER(3)                DEFAULT 0                     NOT NULL,
  COD_CENTRAL_NUE      NUMBER(3),
  DES_RESPUESTA        VARCHAR2(80 BYTE)        DEFAULT 'PENDIENTE'           NOT NULL,
  COD_ACTUACION        NUMBER(5)                NOT NULL,
  NOM_USUARORA         VARCHAR2(30 BYTE)        DEFAULT 'USER'                NOT NULL,
  FEC_INGRESO          DATE                     DEFAULT SYSDATE               NOT NULL,
  TIP_TERMINAL         VARCHAR2(1 BYTE)         NOT NULL,
  COD_CENTRAL          NUMBER(3)                NOT NULL,
  FEC_LECTURA          DATE,
  IND_BLOQUEO          NUMBER(1)                DEFAULT 0                     NOT NULL,
  FEC_EJECUCION        DATE,
  TIP_TERMINAL_NUE     VARCHAR2(1 BYTE),
  NUM_MOVANT           NUMBER(9),
  NUM_CELULAR          NUMBER(15)               NOT NULL,
  NUM_MOVPOS           NUMBER(9),
  NUM_SERIE            VARCHAR2(25 BYTE)        NOT NULL,
  NUM_PERSONAL         VARCHAR2(4 BYTE),
  NUM_CELULAR_NUE      NUMBER(15),
  NUM_SERIE_NUE        VARCHAR2(25 BYTE),
  NUM_PERSONAL_NUE     VARCHAR2(4 BYTE),
  NUM_MSNB             VARCHAR2(12 BYTE),
  NUM_MSNB_NUE         VARCHAR2(12 BYTE),
  COD_SUSPREHA         VARCHAR2(3 BYTE),
  COD_SERVICIOS        VARCHAR2(255 BYTE),
  NUM_MIN              VARCHAR2(3 BYTE)         DEFAULT NULL,
  NUM_MIN_NUE          VARCHAR2(3 BYTE),
  STA                  CHAR(1 BYTE),
  COD_MENSAJE          NUMBER(3),
  PARAM1_MENS          VARCHAR2(30 BYTE),
  PARAM2_MENS          VARCHAR2(30 BYTE),
  PARAM3_MENS          VARCHAR2(30 BYTE),
  PLAN                 VARCHAR2(3 BYTE),
  CARGA                NUMBER(14,4),
  VALOR_PLAN           NUMBER(14,4),
  PIN                  VARCHAR2(13 BYTE),
  FEC_EXPIRA           DATE,
  DES_MENSAJE          VARCHAR2(512 BYTE),
  COD_PIN              VARCHAR2(16 BYTE),
  COD_IDIOMA           VARCHAR2(5 BYTE),
  COD_ENRUTAMIENTO     NUMBER(4),
  TIP_ENRUTAMIENTO     NUMBER(2),
  DES_ORIGEN_PIN       VARCHAR2(8 BYTE),
  NUM_LOTE_PIN         NUMBER(9),
  NUM_SERIE_PIN        VARCHAR2(16 BYTE),
  TIP_TECNOLOGIA       VARCHAR2(7 BYTE)         DEFAULT 'TDMA',
  IMSI                 VARCHAR2(15 BYTE),
  IMSI_NUE             VARCHAR2(15 BYTE),
  IMEI                 VARCHAR2(20 BYTE),
  IMEI_NUE             VARCHAR2(20 BYTE),
  ICC                  VARCHAR2(25 BYTE),
  ICC_NUE              VARCHAR2(25 BYTE),
  FEC_ACTIVACION       DATE,
  COD_ESPEC_PROV       VARCHAR2(10 BYTE),
  COD_PROD_CONTRATADO  NUMBER(38),
  IND_BAJATRANS        NUMBER(2)
) TABLESPACE TS_PROVISIONING_SDAT
/

COMMENT ON TABLE SISCEL.ICC_MOVIMIENTO IS 'Movimientos Celulares';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.COD_PROD_CONTRATADO IS 'Código producto contratado';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.FEC_ACTIVACION IS 'Fecha activación.';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_MOVIMIENTO IS 'Numero de movimiento';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_ABONADO IS 'Numero de abonado';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.COD_ESTADO IS 'Estado de ejecucion del movimiento';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.COD_ACTABO IS 'Codigo de actuacion de abonados';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.COD_MODULO IS 'modulo que inserto el movimiento';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_INTENTOS IS 'numero de veces que se ha intentado el movimiento';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.COD_CENTRAL_NUE IS 'nueva central para el cambio de numero';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.DES_RESPUESTA IS 'Respuesta a la ejecucion del movimiento';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.COD_ACTUACION IS 'codigo de actuacion en central';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NOM_USUARORA IS 'nombre de usuario que solicita la ejecucion del movimiento';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.FEC_INGRESO IS 'fecha de ingreso en la tabla';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.TIP_TERMINAL IS 'tipo de terminal celular';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.COD_CENTRAL IS 'codigo de central a la que se envia el movimiento';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.FEC_LECTURA IS 'fecha de inicio de procesamiento del movimiento';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.IND_BLOQUEO IS 'indicador de bloqueo del movimiento';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.FEC_EJECUCION IS 'fecha en la que se ejecuta el movimiento';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.TIP_TERMINAL_NUE IS 'tipo de terminal nuevo para el cambio de terminal';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_MOVANT IS 'Numero anterior de movimiento encadenado';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_CELULAR IS 'numero de telefono';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_MOVPOS IS 'nuemro posterior de movimiento encadenado';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_SERIE IS 'numero de serie';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_PERSONAL IS 'PIN del abonado';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_CELULAR_NUE IS 'numero de telefono nuevo para el cambio de numero';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_SERIE_NUE IS 'numero de serie nuevo para el cambio de serie';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_PERSONAL_NUE IS 'PIN nuevo para el cambio de PIN';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_MSNB IS 'numero de telefono mundial del abonado';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_MSNB_NUE IS 'nuevo numero mundial del abonado';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.COD_SUSPREHA IS 'codigo de suspension/rehabilitacion';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.COD_SERVICIOS IS 'servicios a procesar en el movimiento';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_MIN IS 'Prefijo MIN';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.STA IS 'Estado suscriptor mensajeria';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.FEC_EXPIRA IS 'Fecha expiracion carga prepago';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.DES_MENSAJE IS 'Descripcisn del Mensaje.';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.COD_PIN IS 'Codigo secreto de tarjeta de prepago..';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.COD_IDIOMA IS 'Codigo de idioma del suscriptor.';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.COD_ENRUTAMIENTO IS 'Codigo de numero de enrutamiento.';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.TIP_ENRUTAMIENTO IS 'Tipo del numero de enrutamiento 0.Entrante 1.Saliente 2.Ambos.';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.DES_ORIGEN_PIN IS 'Proveedor de tarjeta de prepago..';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_LOTE_PIN IS 'Numero de lote de tarjeta de prepago..';

COMMENT ON COLUMN SISCEL.ICC_MOVIMIENTO.NUM_SERIE_PIN IS 'Numero de serie de tarjeta de prepago..';
