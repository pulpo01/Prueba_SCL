CREATE TABLE ADN_PORTADO_IN_TO(
    NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  NUM_MDN NUMBER(15,0) NOT NULL 
 ,  NUM_MIN NUMBER(10,0) NOT NULL 
 ,  COD_SUBALM VARCHAR2(5) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  COD_CAT NUMBER(2,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  ADN_PORTADO_IN_TO.NUM_CELULAR IS 'Numero de Celular'
/
COMMENT ON COLUMN  ADN_PORTADO_IN_TO.NUM_MDN IS 'Numero MDN'
/
COMMENT ON COLUMN  ADN_PORTADO_IN_TO.NUM_MIN IS 'Numero de Identificacion del Movil'
/
COMMENT ON COLUMN  ADN_PORTADO_IN_TO.COD_SUBALM IS 'Codigo de Subalm'
/
COMMENT ON COLUMN  ADN_PORTADO_IN_TO.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  ADN_PORTADO_IN_TO.COD_CENTRAL IS 'Codigo de Central'
/
COMMENT ON COLUMN  ADN_PORTADO_IN_TO.COD_CAT IS 'Codigo de Categoria'
/
COMMENT ON COLUMN  ADN_PORTADO_IN_TO.COD_USO IS 'Codigo de Uso'
/
COMMENT ON COLUMN  ADN_PORTADO_IN_TO.FEC_ALTA IS 'Fecha de Alta'
/
