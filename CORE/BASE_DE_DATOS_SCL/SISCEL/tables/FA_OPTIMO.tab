CREATE TABLE FA_OPTIMO(
    COD_PLANTARIF VARCHAR2(3) NOT NULL 
 ,  MIN_DESDE NUMBER(8,2) NOT NULL 
 ,  MIN_HASTA NUMBER(8,2) NULL 
 ,  PRC_ABONO NUMBER(5,2) NOT NULL 
 ,  PRC_NORMAL NUMBER(5,2) NOT NULL 
 ,  PRC_BAJO NUMBER(5,2) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_OPTIMO IS 'Tabla para la Aplicacion de PLAN OPTIMO'
/
COMMENT ON COLUMN  FA_OPTIMO.COD_PLANTARIF IS 'Plan Tarifario'
/
COMMENT ON COLUMN  FA_OPTIMO.MIN_DESDE IS 'Minutos Desde'
/
COMMENT ON COLUMN  FA_OPTIMO.MIN_HASTA IS 'Minutos Hasta'
/
COMMENT ON COLUMN  FA_OPTIMO.PRC_ABONO IS 'Porcentaje Descuento Cargo Basico'
/
COMMENT ON COLUMN  FA_OPTIMO.PRC_NORMAL IS 'Porcentaje Descuento en Horario Normal'
/
COMMENT ON COLUMN  FA_OPTIMO.PRC_BAJO IS 'Porcentaje Descuento en Franja Horaria Baja'
/
