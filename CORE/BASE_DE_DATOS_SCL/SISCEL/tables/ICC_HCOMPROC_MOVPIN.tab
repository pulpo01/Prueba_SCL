CREATE TABLE ICC_HCOMPROC_MOVPIN(
    NUM_MOVIMIENTO NUMBER(9,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(3,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  FEC_HISTORICO DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_LENGUAJE NUMBER(3,0) NOT NULL 
 ,  FEC_INGRESO DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_ACT VARCHAR2(2) NULL 
 ,  FEC_EJECUCION DATE NULL 
 ,  DES_COMANDO VARCHAR2(512) NOT NULL 
 ,  DES_RESPUESTA VARCHAR2(80) NULL 
 ,  COD_COMANDO NUMBER(3,0) NOT NULL 
 ,  COD_SISTEMA NUMBER(2,0) NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICC_HCOMPROC_MOVPIN IS 'Historico comandos procesados Celular.'
/
COMMENT ON COLUMN  ICC_HCOMPROC_MOVPIN.NUM_MOVIMIENTO IS 'Numero de movimiento.'
/
COMMENT ON COLUMN  ICC_HCOMPROC_MOVPIN.NUM_ORDEN IS 'Numero de orden del comando.'
/
COMMENT ON COLUMN  ICC_HCOMPROC_MOVPIN.COD_CENTRAL IS 'Codigo de central donde se ejecuto el comando.'
/
COMMENT ON COLUMN  ICC_HCOMPROC_MOVPIN.FEC_HISTORICO IS 'Fecha de entrada en la tabla.'
/
COMMENT ON COLUMN  ICC_HCOMPROC_MOVPIN.COD_LENGUAJE IS 'Codigo de lenguaje del comando.'
/
COMMENT ON COLUMN  ICC_HCOMPROC_MOVPIN.FEC_INGRESO IS 'Fecha de solicitud de ejecucion del comando.'
/
COMMENT ON COLUMN  ICC_HCOMPROC_MOVPIN.COD_ACT IS 'Usado para el paso a tablas mensuales de historicos.'
/
COMMENT ON COLUMN  ICC_HCOMPROC_MOVPIN.FEC_EJECUCION IS 'Fecha en que el comando se ejcuto correctamente.'
/
COMMENT ON COLUMN  ICC_HCOMPROC_MOVPIN.DES_COMANDO IS 'Comando que se ejecuto en la central.'
/
COMMENT ON COLUMN  ICC_HCOMPROC_MOVPIN.DES_RESPUESTA IS 'Respuesta de la central a la ejecucion del comando.'
/
COMMENT ON COLUMN  ICC_HCOMPROC_MOVPIN.COD_COMANDO IS 'Codigo de comando que se ejecuto.'
/
COMMENT ON COLUMN  ICC_HCOMPROC_MOVPIN.COD_SISTEMA IS 'Sistema donde se ejecuto el comando.'
/
