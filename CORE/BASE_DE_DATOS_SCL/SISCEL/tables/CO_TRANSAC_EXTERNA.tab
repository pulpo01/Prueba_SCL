CREATE TABLE CO_TRANSAC_EXTERNA
(
  EMP_RECAUDADORA  VARCHAR2(15 BYTE)            NOT NULL,
  FEC_EFECTIVIDAD  DATE                         NOT NULL,
  NUM_TRANSACCION  NUMBER(10)                   NOT NULL,
  NUM_COMPAGO      NUMBER(9)                    NOT NULL,
  PREF_PLAZA       VARCHAR2(25 BYTE)            NOT NULL,
  COD_RETORNO      NUMBER(2)                    NOT NULL,
  GLOSA_RETORNO    VARCHAR2(155 BYTE)           NOT NULL
)
TABLESPACE TS_RECYCOB_SDAT
PCTUSED    0
PCTFREE    5
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          360K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE CO_TRANSAC_EXTERNA IS 'Tabla de Paso de Transacciones de Pago.';

COMMENT ON COLUMN CO_TRANSAC_EXTERNA.EMP_RECAUDADORA IS 'Empresa Recaudadora.';

COMMENT ON COLUMN CO_TRANSAC_EXTERNA.FEC_EFECTIVIDAD IS 'Fecha de Efectividad.';

COMMENT ON COLUMN CO_TRANSAC_EXTERNA.NUM_TRANSACCION IS 'NUMERO DE TRANSACCION.';

COMMENT ON COLUMN CO_TRANSAC_EXTERNA.NUM_COMPAGO IS 'Numero de Comprobante de pago.';

COMMENT ON COLUMN CO_TRANSAC_EXTERNA.COD_RETORNO IS 'Codigo de Retorno de PL.';

COMMENT ON COLUMN CO_TRANSAC_EXTERNA.GLOSA_RETORNO IS 'Glosa  de Retorno de PL.';

ALTER TABLE CO_TRANSAC_EXTERNA MODIFY  ( PREF_PLAZA    VARCHAR2 (25)  )
/

