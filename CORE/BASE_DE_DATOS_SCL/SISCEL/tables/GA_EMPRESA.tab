CREATE TABLE GA_EMPRESA
(
  COD_EMPRESA    NUMBER(8)                      NOT NULL,
  DES_EMPRESA    VARCHAR2(50 BYTE)              NOT NULL,
  COD_PRODUCTO   NUMBER(1)                      NOT NULL,
  COD_PLANTARIF  VARCHAR2(3 BYTE)                   NULL,
  FEC_ALTA       DATE                           NOT NULL,
  NOM_USUARORA   VARCHAR2(30 BYTE)              NOT NULL,
  COD_CICLO      NUMBER(2)                      NOT NULL,
  COD_CLIENTE    NUMBER(8)                          NULL,
  TIP_TERMINAL   VARCHAR2(1 BYTE)                   NULL,
  NUM_ABONADOS   NUMBER(4)                          NULL,
  RAZON_SOCIAL   VARCHAR2(50 BYTE)                  NULL,
  NUM_PATENTE    VARCHAR2(90 BYTE)                  NULL
)
TABLESPACE TS_CRM_POSVENTA_SDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          2M
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE GA_EMPRESA IS 'Tabla de Empresas de Abonados';

COMMENT ON COLUMN GA_EMPRESA.RAZON_SOCIAL IS 'razón social';

COMMENT ON COLUMN GA_EMPRESA.NUM_PATENTE IS 'Patente comercio';

COMMENT ON COLUMN GA_EMPRESA.COD_EMPRESA IS 'Codigo de Empresa';

COMMENT ON COLUMN GA_EMPRESA.DES_EMPRESA IS 'Descripcion de Empresa';

COMMENT ON COLUMN GA_EMPRESA.COD_PRODUCTO IS 'Codigo de Producto';

COMMENT ON COLUMN GA_EMPRESA.COD_PLANTARIF IS 'Codigo de Plan Tarifario';

COMMENT ON COLUMN GA_EMPRESA.FEC_ALTA IS 'Fecha de Alta de Empresa';

COMMENT ON COLUMN GA_EMPRESA.NOM_USUARORA IS 'Usuario Creador de Empresa';

COMMENT ON COLUMN GA_EMPRESA.COD_CICLO IS 'Codigo de Ciclo de Facturacion';

COMMENT ON COLUMN GA_EMPRESA.COD_CLIENTE IS 'Codigo de Cliente';

COMMENT ON COLUMN GA_EMPRESA.TIP_TERMINAL IS 'Tipo de Terminal';

COMMENT ON COLUMN GA_EMPRESA.NUM_ABONADOS IS 'Numero de Abonados Incluidos en la Empresa';