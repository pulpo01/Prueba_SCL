CREATE TABLE CSH_REGCAMBIO(
    NOM_COMPONENTE VARCHAR2(60) NOT NULL 
 ,  FEC_REGCAMBIO DATE NOT NULL 
 ,  GLS_REGCAMBIO VARCHAR2(256) NOT NULL 
 ,  NUM_VERSION NUMBER(2,0) NOT NULL 
 ,  NUM_SUBVERSION NUMBER(3,0) NOT NULL 
 ,  NOM_USUASOLICI VARCHAR2(50) NOT NULL 
 ,  NOM_USUARESP VARCHAR2(50) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CSH_REGCAMBIO IS 'Registros De Cambio De Componentes.'
/
COMMENT ON COLUMN  CSH_REGCAMBIO.NOM_COMPONENTE IS 'Nombre de la Componente del Sistema.'
/
COMMENT ON COLUMN  CSH_REGCAMBIO.FEC_REGCAMBIO IS 'Fecha de Registro de Cambio.'
/
COMMENT ON COLUMN  CSH_REGCAMBIO.GLS_REGCAMBIO IS 'Descripcion del Cambio Solicitado.'
/
COMMENT ON COLUMN  CSH_REGCAMBIO.NUM_VERSION IS 'Numero de Version de la Componente.'
/
COMMENT ON COLUMN  CSH_REGCAMBIO.NUM_SUBVERSION IS 'Numero de Sub-Versin de la Componente.'
/
COMMENT ON COLUMN  CSH_REGCAMBIO.NOM_USUASOLICI IS 'Nombre del Usuario Solicitante.'
/
COMMENT ON COLUMN  CSH_REGCAMBIO.NOM_USUARESP IS 'Nombre del Usuario Desarrollo Responsable.'
/
