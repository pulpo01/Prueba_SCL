/
COMMENT ON COLUMN  AL_TIPOS_STOCK.DES_STOCK IS 'Descipricisn del tipo de stock'
/
COMMENT ON COLUMN  AL_TIPOS_STOCK.IND_VALORAR IS 'Tipo de Valoracion'
/
COMMENT ON COLUMN  AL_TIPOS_STOCK.COD_USO IS 'Uso por defecto de ingreso'
