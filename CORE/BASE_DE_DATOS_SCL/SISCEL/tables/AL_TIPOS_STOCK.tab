 ,  IND_VALORAR NUMBER(1,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_ESTADO NUMBER(2,0) NOT NULL 
 ,  IND_CONSIGNACION NUMBER(1,0) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_TIPOS_STOCK IS 'Tipos de Stock'
/
