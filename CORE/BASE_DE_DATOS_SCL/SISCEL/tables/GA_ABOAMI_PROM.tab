CREATE TABLE GA_ABOAMI_PROM(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_PROMOCION NUMBER(5,0) NOT NULL 
 ,  NUM_PERIODOS NUMBER(4,0) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ,  CARGA_TOTAL NUMBER(14,4) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NULL 
 ,  CARGA_PERIODO NUMBER(14,4) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_ABOAMI_PROM IS 'Abonados Amistar con Promociones.'
/
COMMENT ON COLUMN  GA_ABOAMI_PROM.NUM_ABONADO IS 'Numero de Abonado.'
/
COMMENT ON COLUMN  GA_ABOAMI_PROM.COD_PROMOCION IS 'Codigo Promocion.'
/
COMMENT ON COLUMN  GA_ABOAMI_PROM.NUM_PERIODOS IS 'Numero de periodos asignados.'
/
COMMENT ON COLUMN  GA_ABOAMI_PROM.FEC_ALTA IS 'Fecha de Alta Abo. con Promocion.'
/
COMMENT ON COLUMN  GA_ABOAMI_PROM.CARGA_TOTAL IS 'Carga Total Asignada.'
/
COMMENT ON COLUMN  GA_ABOAMI_PROM.FEC_ULTMOD IS 'Fecha ultima modificacion.'
/
COMMENT ON COLUMN  GA_ABOAMI_PROM.COD_MONEDA IS 'Codigo de moneda dolar, pesos, uf,etc.'
/
COMMENT ON COLUMN  GA_ABOAMI_PROM.CARGA_PERIODO IS 'Carga de celulares por periodos.'
/
