CREATE TABLE GAD_SEG_DEDUCIBLES(
    COD_TIPSEGU NUMBER(2,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CAUSA VARCHAR2(2) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  TIP_FRANQUICIA NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  IMP_FRANQUICIA NUMBER(14,4) DEFAULT 0 NOT NULL 
 ,  FEC_DESDE DATE DEFAULT sysdate NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAD_SEG_DEDUCIBLES IS 'Deducibles De Planes De Proteccion.'
/
COMMENT ON COLUMN  GAD_SEG_DEDUCIBLES.COD_TIPSEGU IS 'Codigo del Plan de Proteccion.'
/
COMMENT ON COLUMN  GAD_SEG_DEDUCIBLES.COD_PRODUCTO IS 'Codigo de Producto.'
/
COMMENT ON COLUMN  GAD_SEG_DEDUCIBLES.COD_CAUSA IS 'Codigo de Causa de Siniestro (Aplicado en efectividad de seguro).'
/
COMMENT ON COLUMN  GAD_SEG_DEDUCIBLES.COD_MONEDA IS 'Codigo de Moneda.'
/
COMMENT ON COLUMN  GAD_SEG_DEDUCIBLES.TIP_FRANQUICIA IS 'Tipo de Deducible (0= Monto, 1= Porcentaje).'
/
COMMENT ON COLUMN  GAD_SEG_DEDUCIBLES.IMP_FRANQUICIA IS 'Importe de Deducible.'
/
COMMENT ON COLUMN  GAD_SEG_DEDUCIBLES.FEC_DESDE IS 'Fecha de inicio comercializacion del importe.'
/
COMMENT ON COLUMN  GAD_SEG_DEDUCIBLES.FEC_HASTA IS 'Fecha final de comercializacion del importe.'
/
COMMENT ON COLUMN  GAD_SEG_DEDUCIBLES.NOM_USUARORA IS 'Responsable del ingreso de importes.'
/
