CREATE TABLE GC_TIPO_TD(
    COD_TIPO NUMBER(6,0) NOT NULL 
 ,  GLS_TIPO VARCHAR2(70) NOT NULL 
 ,  FEC_INI_VIGENCIA DATE NOT NULL 
 ,  FEC_FIN_VIGENCIA DATE NULL 
 ,  NOM_USUARORA VARCHAR2(30) DEFAULT USER NOT NULL 
 ,  FEC_ULTMOD DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GC_TIPO_TD IS 'Tipos de GC'
/
COMMENT ON COLUMN  GC_TIPO_TD.COD_TIPO IS 'Codigo de Tipo'
/
COMMENT ON COLUMN  GC_TIPO_TD.GLS_TIPO IS 'Descripcion de Tipo'
/
COMMENT ON COLUMN  GC_TIPO_TD.FEC_INI_VIGENCIA IS 'Fecha de inicio vigencia del tipo'
/
COMMENT ON COLUMN  GC_TIPO_TD.FEC_FIN_VIGENCIA IS 'Fecha de fin de vigencia del tipo'
/
COMMENT ON COLUMN  GC_TIPO_TD.NOM_USUARORA IS 'Usuario Oracle'
/
COMMENT ON COLUMN  GC_TIPO_TD.FEC_ULTMOD IS 'Fecha de ultima modificacion'
/
