CREATE TABLE CO_PROC_ACUM_CASCON(
    NUM_PROCESO NUMBER NOT NULL 
 ,  TIP_MOVIMIEN NUMBER(1,0) NOT NULL 
 ,  FEC_PROCESO DATE DEFAULT Sysdate NULL 
 ,  USUARIO_PROCESO VARCHAR2(40) NULL 
 ,  FEC_TERMINO DATE DEFAULT null NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_PROC_ACUM_CASCON IS 'Cabecera Proceso Acumulacion Castigos Contables.'
/
COMMENT ON COLUMN  CO_PROC_ACUM_CASCON.NUM_PROCESO IS 'IDENTIFICACION DEL PROCESO.'
/
COMMENT ON COLUMN  CO_PROC_ACUM_CASCON.TIP_MOVIMIEN IS 'TIPO DE MOVIMIENTO ( 1: CASTIGO, 2: RECUPERADO, 3 :ELIMINADO).'
/
COMMENT ON COLUMN  CO_PROC_ACUM_CASCON.FEC_PROCESO IS 'Fecha de ultimo procesamiento.'
/
COMMENT ON COLUMN  CO_PROC_ACUM_CASCON.USUARIO_PROCESO IS 'usuario responsable ultimo procesamiento.'
/
COMMENT ON COLUMN  CO_PROC_ACUM_CASCON.FEC_TERMINO IS 'fecha de termino del proceso.'
/
