CREATE TABLE VE_HISVENTASERV(
    COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  FEC_ACEPTVENTA DATE NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_NIVEL NUMBER(4,0) NOT NULL 
 ,  IMP_COMISION NUMBER(12,4) NOT NULL 
 ,  FEC_PASOHIST DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_HISVENTASERV IS 'Historico de ventas de Servicios Suplementarios'
/
COMMENT ON COLUMN  VE_HISVENTASERV.COD_SERVICIO IS 'Codigo de Servicio Suplementario'
/
COMMENT ON COLUMN  VE_HISVENTASERV.NUM_ABONADO IS 'Numero de Abonado'
/
COMMENT ON COLUMN  VE_HISVENTASERV.FEC_ACEPTVENTA IS 'Fecha de aceptacion de la venta'
/
COMMENT ON COLUMN  VE_HISVENTASERV.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  VE_HISVENTASERV.COD_TIPCOMIS IS 'Codigo de Tipo de Comisionista'
/
COMMENT ON COLUMN  VE_HISVENTASERV.COD_VENDEDOR IS 'Codigo de Vendedor'
/
COMMENT ON COLUMN  VE_HISVENTASERV.COD_NIVEL IS 'Codigo de Nivel del Servicio Suplementario'
/
COMMENT ON COLUMN  VE_HISVENTASERV.IMP_COMISION IS 'Importe de la comision'
/
COMMENT ON COLUMN  VE_HISVENTASERV.FEC_PASOHIST IS 'Fecha de paso a historico'
/
