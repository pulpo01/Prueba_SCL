CREATE TABLE CO_DEPOSITO_EXT(
    COD_RECEXT NUMBER(8,0) NOT NULL 
 ,  NOM_FICHERO VARCHAR2(80) NOT NULL 
 ,  COD_ORIPAGO NUMBER(2,0) NOT NULL 
 ,  FEC_VALOR DATE NOT NULL 
 ,  NUM_REGISTROS NUMBER(8,0) NOT NULL 
 ,  IMP_VALOR NUMBER(15,4) NOT NULL 
 ,  NUM_ANOMALIAS NUMBER(8,0) NOT NULL 
 ,  IMP_VALOR_ANOM NUMBER(15,4) NOT NULL 
 ,  FEC_IMPUTCONTABLE DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_DEPOSITO_EXT IS 'Procesos de Depositos de Recaudacion externa.'
/
COMMENT ON COLUMN  CO_DEPOSITO_EXT.COD_RECEXT IS 'Codigo de recaudacion (secuencia).'
/
COMMENT ON COLUMN  CO_DEPOSITO_EXT.NOM_FICHERO IS 'Nombre de archivo.'
/
COMMENT ON COLUMN  CO_DEPOSITO_EXT.COD_ORIPAGO IS 'Codigo de Origen de depositos.'
/
COMMENT ON COLUMN  CO_DEPOSITO_EXT.FEC_VALOR IS 'Fecha de proceso.'
/
COMMENT ON COLUMN  CO_DEPOSITO_EXT.NUM_REGISTROS IS 'Numero de Registros.'
/
COMMENT ON COLUMN  CO_DEPOSITO_EXT.IMP_VALOR IS 'Valor de registros Validos.'
/
COMMENT ON COLUMN  CO_DEPOSITO_EXT.NUM_ANOMALIAS IS 'Numero de Registros Anomalos.'
/
COMMENT ON COLUMN  CO_DEPOSITO_EXT.IMP_VALOR_ANOM IS 'Valor de registros Anomalos.'
/
COMMENT ON COLUMN  CO_DEPOSITO_EXT.FEC_IMPUTCONTABLE IS 'Fecha de Imputacion Contable.'
/
