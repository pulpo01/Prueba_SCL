 ,  IMP_DEBITO NUMBER(16,4) NOT NULL 
 ,  IMP_CREDITO NUMBER(16,4) NOT NULL 
 ,  DES_TRANSACCION VARCHAR2(30) NULL 
 ,  IND_DIR_CUENTA CHAR(1) NULL 
 ,  COD_OPERADORA_SCL VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
