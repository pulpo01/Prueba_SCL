COMMENT ON COLUMN  SC_ASIENTO_LOTE.COD_CUENTA IS 'CODIGO DE CUENTA CONTABLE'
/
COMMENT ON COLUMN  SC_ASIENTO_LOTE.IMP_DEBITO IS 'IMPORTE IMPUTACION AL DEBITO'
/
COMMENT ON COLUMN  SC_ASIENTO_LOTE.IMP_CREDITO IS 'IMPORTE IMPUTACION AL CREDITO'
/
COMMENT ON COLUMN  SC_ASIENTO_LOTE.DES_TRANSACCION IS 'Descripcisn Transaccion'
/
COMMENT ON COLUMN  SC_ASIENTO_LOTE.COD_OPERADORA_SCL IS 'CODIGO DE OPERADORA'
/
