CREATE TABLE GE_PARAMDIROPERAD(
    COD_OPERAD VARCHAR2(5) NOT NULL 
 ,  COD_PARAMDIR NUMBER(3,0) NOT NULL 
 ,  ORDEN NUMBER(3,0) NOT NULL 
 ,  IND_OBLIGATORIO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_PARAMDIROPERAD IS 'Parametros De Direccion Por Operadora.'
/
COMMENT ON COLUMN  GE_PARAMDIROPERAD.COD_OPERAD IS 'Codigo de la operadora.'
/
COMMENT ON COLUMN  GE_PARAMDIROPERAD.COD_PARAMDIR IS 'Codigo del parametro de direccion.'
/
COMMENT ON COLUMN  GE_PARAMDIROPERAD.ORDEN IS 'Orden dentro de la composicion de la direccion.'
/
COMMENT ON COLUMN  GE_PARAMDIROPERAD.IND_OBLIGATORIO IS 'Indicador de obligatorio.'
/
