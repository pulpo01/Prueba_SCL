CREATE TABLE GC_CLIENTE_TO(
    ID_CLIENTE_GC NUMBER(10,0) NOT NULL 
 ,  COD_TIPIDENT VARCHAR2(2) NOT NULL 
 ,  NUM_IDENT VARCHAR2(22) NOT NULL 
 ,  DES_IDENT VARCHAR2(50) NOT NULL 
 ,  NUM_FONO_CONTACTO VARCHAR2(15) NULL 
 ,  GLS_OBSERVACION VARCHAR2(100) NULL 
 ,  COD_CUENTA NUMBER(8,0) NULL 
 ,  NOM_USUARORA VARCHAR2(30) DEFAULT USER NOT NULL 
 ,  FEC_ULTMOD DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GC_CLIENTE_TO IS 'Clientes de GC'
/
COMMENT ON COLUMN  GC_CLIENTE_TO.ID_CLIENTE_GC IS 'Secuencial interno de clientes del GC'
/
COMMENT ON COLUMN  GC_CLIENTE_TO.COD_TIPIDENT IS 'Tipo de Numero de Identificacion del Cliente'
/
COMMENT ON COLUMN  GC_CLIENTE_TO.NUM_IDENT IS 'Numero de Identificacion del Cliente'
/
COMMENT ON COLUMN  GC_CLIENTE_TO.DES_IDENT IS 'Nombre del no Cliente o de la Cuenta SCL'
/
COMMENT ON COLUMN  GC_CLIENTE_TO.NUM_FONO_CONTACTO IS 'Numero de Telefono de contacto del Cliente'
/
COMMENT ON COLUMN  GC_CLIENTE_TO.GLS_OBSERVACION IS 'Observacion'
/
COMMENT ON COLUMN  GC_CLIENTE_TO.COD_CUENTA IS 'Codigo de Cuenta en SCL'
/
COMMENT ON COLUMN  GC_CLIENTE_TO.NOM_USUARORA IS 'Usuario Oracle'
/
COMMENT ON COLUMN  GC_CLIENTE_TO.FEC_ULTMOD IS 'Fecha de ultima modificacion'
/
