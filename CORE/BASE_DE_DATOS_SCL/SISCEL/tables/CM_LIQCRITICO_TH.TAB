CREATE TABLE CM_LIQCRITICO_TH(
    NUM_SECUENCIA NUMBER(11,0) NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_COMISIONISTA NUMBER(6,0) NOT NULL 
 ,  ID_PERIODO VARCHAR2(10) NOT NULL 
 ,  IND_CUMPLIMIENTO CHAR(1) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_LIQCRITICO_TH IS 'REGISTRA LOS CAMBIOS DE INDICADOR DE CUMPLIMIENTO A LA LIQUIDACION'
/
COMMENT ON COLUMN  CM_LIQCRITICO_TH.NUM_SECUENCIA IS 'NUMERO DE SECUENCIA'
/
COMMENT ON COLUMN  CM_LIQCRITICO_TH.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
COMMENT ON COLUMN  CM_LIQCRITICO_TH.COD_TIPCOMIS IS 'CODIGO DE LA CATEGORIA DE VENTA'
/
COMMENT ON COLUMN  CM_LIQCRITICO_TH.COD_COMISIONISTA IS 'CODIGO DEL AGENTE DE VENTA'
/
COMMENT ON COLUMN  CM_LIQCRITICO_TH.ID_PERIODO IS 'IDENTIFICADOR DEL CICLO DE COMISIONES '
/
COMMENT ON COLUMN  CM_LIQCRITICO_TH.IND_CUMPLIMIENTO IS 'INDICADOR DE CUMPLIMIENTO DEL CONCEPTO CRITICO'
/
COMMENT ON COLUMN  CM_LIQCRITICO_TH.FEC_ULTMOD IS 'FECHA ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  CM_LIQCRITICO_TH.NOM_USUARIO IS 'USUARIO ULTIMA MODIFICACION'
/
