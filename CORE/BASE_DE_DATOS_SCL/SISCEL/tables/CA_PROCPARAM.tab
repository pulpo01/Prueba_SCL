CREATE TABLE CA_PROCPARAM(
    COD_PROGRAMA VARCHAR2(3) NOT NULL 
 ,  NUM_VERSION NUMBER(2,0) NOT NULL 
 ,  COD_PROCESO VARCHAR2(7) NOT NULL 
 ,  COD_CAMPO NUMBER(3,0) NOT NULL 
 ,  FEC_DESDE_DH DATE NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_PROCPARAM IS 'Paramteros asociados a procesos'
/
COMMENT ON COLUMN  CA_PROCPARAM.COD_PROGRAMA IS 'Codigo de programa'
/
COMMENT ON COLUMN  CA_PROCPARAM.NUM_VERSION IS 'Numero de version'
/
COMMENT ON COLUMN  CA_PROCPARAM.COD_PROCESO IS 'Codigo de proceso'
/
COMMENT ON COLUMN  CA_PROCPARAM.COD_CAMPO IS 'Codigo de campo'
/
