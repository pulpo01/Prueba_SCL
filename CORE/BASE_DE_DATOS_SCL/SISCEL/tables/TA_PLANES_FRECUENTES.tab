CREATE TABLE TA_PLANES_FRECUENTES(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_PLANTARIF VARCHAR2(3) NOT NULL 
 ,  NUM_FREC_FIJOS NUMBER(5,0) NULL 
 ,  NUM_FREC_MOVIL NUMBER(5,0) NULL 
 ,  IND_FF NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  NOM_USUARIO VARCHAR2(40) NOT NULL 
 ,  FEC_ULTIMAMOD DATE NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_PLANES_FRECUENTES IS 'Planes Tarifarios Con Caracteristica De Numeros Frecuentes.'
/
COMMENT ON COLUMN  TA_PLANES_FRECUENTES.COD_PRODUCTO IS 'Codigo de Producto.'
/
COMMENT ON COLUMN  TA_PLANES_FRECUENTES.COD_PLANTARIF IS 'Codigo de plan tarifario con numeros frecuentes.'
/
COMMENT ON COLUMN  TA_PLANES_FRECUENTES.NUM_FREC_FIJOS IS 'Cantidad de Numeros Frecuentes de red fija.'
/
COMMENT ON COLUMN  TA_PLANES_FRECUENTES.NUM_FREC_MOVIL IS 'Cantidad de Numeros Frecuentes de Telefonia Movil.'
/
COMMENT ON COLUMN  TA_PLANES_FRECUENTES.IND_FF IS 'Indicador de  servicio F y F entre abonados de agrupacion (Nivel de Cliente).'
/
COMMENT ON COLUMN  TA_PLANES_FRECUENTES.FEC_DESDE IS 'Fecha de Alta del plan con atributo de frecuentes.'
/
COMMENT ON COLUMN  TA_PLANES_FRECUENTES.FEC_HASTA IS 'Fecha fin de vigencia de plan con atributo de frecuentes.'
/
COMMENT ON COLUMN  TA_PLANES_FRECUENTES.NOM_USUARIO IS 'Usuario que realizo la ultima modificacion.'
/
COMMENT ON COLUMN  TA_PLANES_FRECUENTES.FEC_ULTIMAMOD IS 'Fecha en que realizo la ultima modificacion.'
/
