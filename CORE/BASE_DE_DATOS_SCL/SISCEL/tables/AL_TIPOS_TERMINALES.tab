CREATE TABLE AL_TIPOS_TERMINALES(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  TIP_TERMINAL VARCHAR2(1) NOT NULL 
 ,  DES_TERMINAL VARCHAR2(15) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_TIPOS_TERMINALES IS 'Posibles terminales atribuibles a la definicion de un articulo definiendolos para cada uno de los diferentes productos.'
/
COMMENT ON COLUMN  AL_TIPOS_TERMINALES.COD_PRODUCTO IS 'Producto'
/
COMMENT ON COLUMN  AL_TIPOS_TERMINALES.TIP_TERMINAL IS 'Tipo de Terminal'
/
COMMENT ON COLUMN  AL_TIPOS_TERMINALES.DES_TERMINAL IS 'Descripcion tipo de terminal'
/
