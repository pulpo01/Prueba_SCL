 ,  COD_FORMULARIO NUMBER(3,0) NOT NULL 
 ,  TIP_CONCNOFACT VARCHAR2(5) NOT NULL 
 ,  DES_GLOSA VARCHAR2(30) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  FAD_IMPCONC_NOFACT.COD_SUBGRUPO IS 'Codigo de Subgrupo.'
/
COMMENT ON COLUMN  FAD_IMPCONC_NOFACT.COD_FORMULARIO IS 'Codigo de Formulario.'
/
COMMENT ON COLUMN  FAD_IMPCONC_NOFACT.TIP_CONCNOFACT IS 'Tipo de Concepto No Facturable.'
/
COMMENT ON COLUMN  FAD_IMPCONC_NOFACT.DES_GLOSA IS 'Descripcion del Concepto.'
/
COMMENT ON COLUMN  FAD_IMPCONC_NOFACT.NOM_USUARIO IS 'Nombre de Usuario.'
/
COMMENT ON COLUMN  FAD_IMPCONC_NOFACT.FEC_ULTMOD IS 'Fecha de Ultima Modificacion.'
/
