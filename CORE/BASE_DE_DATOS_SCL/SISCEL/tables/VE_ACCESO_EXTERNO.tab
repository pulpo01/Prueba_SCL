CREATE TABLE VE_ACCESO_EXTERNO(
    COD_USUARIO_TS VARCHAR2(30) NOT NULL 
 ,  PASS_USUARIO_TS VARCHAR2(30) NOT NULL 
 ,  COD_USUARIO_IN VARCHAR2(30) NOT NULL 
 ,  PASS_USUARIO_IN VARCHAR2(30) NOT NULL 
 ,  NOMBRE_USUARIO VARCHAR2(40) NOT NULL 
 ,  FECHA_INICIO DATE NOT NULL 
 ,  FECHA_TERMINO DATE NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_ACCESO_EXTERNO IS 'Control De Acceso Externo.'
/
COMMENT ON COLUMN  VE_ACCESO_EXTERNO.COD_USUARIO_TS IS 'Codigo usuario externo Terminal Service.'
/
COMMENT ON COLUMN  VE_ACCESO_EXTERNO.PASS_USUARIO_TS IS 'Password usuario externo Terminal Service.'
/
COMMENT ON COLUMN  VE_ACCESO_EXTERNO.COD_USUARIO_IN IS 'Codigo usuario externo INTERNET.'
/
COMMENT ON COLUMN  VE_ACCESO_EXTERNO.PASS_USUARIO_IN IS 'Password usuario externo INTERNET.'
/
COMMENT ON COLUMN  VE_ACCESO_EXTERNO.NOMBRE_USUARIO IS 'Nombre del usuario externo.'
/
COMMENT ON COLUMN  VE_ACCESO_EXTERNO.FECHA_INICIO IS 'Fecha de inicio de vigencia.'
/
COMMENT ON COLUMN  VE_ACCESO_EXTERNO.FECHA_TERMINO IS 'Fecha de termino de vigencia.'
/
COMMENT ON COLUMN  VE_ACCESO_EXTERNO.COD_VENDEDOR IS 'Codigo de vendedor.'
/
