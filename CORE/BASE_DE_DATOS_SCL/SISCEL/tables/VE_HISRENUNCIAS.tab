CREATE TABLE VE_HISRENUNCIAS(
    COD_RENUNCIA NUMBER(8,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  FEC_ALTA DATE NULL 
 ,  FEC_RENUNCIA DATE NOT NULL 
 ,  FEC_PASOHIST DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_HISRENUNCIAS IS 'Historico de las Renuncias de los abonados del sistema'
/
COMMENT ON COLUMN  VE_HISRENUNCIAS.COD_RENUNCIA IS 'Codigo de la renuncia'
/
COMMENT ON COLUMN  VE_HISRENUNCIAS.COD_TIPCOMIS IS 'Codigo de tipo de comisionista asociado al cliente'
/
COMMENT ON COLUMN  VE_HISRENUNCIAS.COD_VENDEDOR IS 'Codigo de vendedor asociado al cliente'
/
COMMENT ON COLUMN  VE_HISRENUNCIAS.COD_CLIENTE IS 'Codigo del cliente del que depende el abonado'
/
COMMENT ON COLUMN  VE_HISRENUNCIAS.NUM_ABONADO IS 'Numero del abonado que renuncia al sistema'
/
COMMENT ON COLUMN  VE_HISRENUNCIAS.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  VE_HISRENUNCIAS.COD_OFICINA IS 'Codigo de Oficina'
/
COMMENT ON COLUMN  VE_HISRENUNCIAS.FEC_ALTA IS 'Fecha de alta de la venta de un producto'
/
COMMENT ON COLUMN  VE_HISRENUNCIAS.FEC_RENUNCIA IS 'Fecha de la renuncia'
/
COMMENT ON COLUMN  VE_HISRENUNCIAS.FEC_PASOHIST IS 'Fecha de ingreso en historico'
/
