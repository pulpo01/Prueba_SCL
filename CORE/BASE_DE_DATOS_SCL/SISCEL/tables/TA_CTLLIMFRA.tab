CREATE TABLE TA_CTLLIMFRA(
    NUM_BLOQUE VARCHAR2(9) NOT NULL 
 ,  NUM_TICKETS NUMBER(9,0) NOT NULL 
 ,  NUM_EXITOS NUMBER(8,0) NOT NULL 
 ,  NUM_NEXITOS NUMBER(8,0) NOT NULL 
 ,  NUM_FRAUDE NUMBER(5,0) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_CTLLIMFRA IS 'Control ejecuciones de control de consumo y fraude'
/
COMMENT ON COLUMN  TA_CTLLIMFRA.NUM_BLOQUE IS 'Numero de Bloque'
/
COMMENT ON COLUMN  TA_CTLLIMFRA.NUM_TICKETS IS 'Numero de Tickets'
/
COMMENT ON COLUMN  TA_CTLLIMFRA.NUM_EXITOS IS 'Numero de Exitos'
/
COMMENT ON COLUMN  TA_CTLLIMFRA.NUM_NEXITOS IS 'Numero de No Exitos'
/
COMMENT ON COLUMN  TA_CTLLIMFRA.NUM_FRAUDE IS 'Numero de tickets de fraude'
/
