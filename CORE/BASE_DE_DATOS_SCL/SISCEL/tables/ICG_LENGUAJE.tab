CREATE TABLE ICG_LENGUAJE(
    COD_LENGUAJE NUMBER(3,0) NOT NULL 
 ,  DES_LENGUAJE VARCHAR2(50) NOT NULL 
 ,  TIP_LENGUAJE NUMBER(1,0) DEFAULT 0 NULL 
 ,  DES_HEADER VARCHAR2(256) NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICG_LENGUAJE IS 'Lenguajes de Comandos'
/
COMMENT ON COLUMN  ICG_LENGUAJE.COD_LENGUAJE IS 'Codigo de lenguaje - Retrofitted'
/
COMMENT ON COLUMN  ICG_LENGUAJE.DES_LENGUAJE IS 'Descripcion de lenguaje - Retrofitted'
/
COMMENT ON COLUMN  ICG_LENGUAJE.TIP_LENGUAJE IS 'Tipo de Lenguaje.'
/
COMMENT ON COLUMN  ICG_LENGUAJE.DES_HEADER IS 'Header de Comando.'
/
