CREATE TABLE FA_AJUSTECONC
(
  NUM_FOLIO        NUMBER(9)                    NOT NULL,
  COD_CLIENTE      NUMBER(8)                    DEFAULT 1                     NOT NULL,
  COD_CONCEPTO     NUMBER(4)                    NOT NULL,
  COD_MONEDA       VARCHAR2(3 BYTE)             NOT NULL,
  COLUMNA          NUMBER(6)                    NOT NULL,
  IMP_FACTURABLE   NUMBER(14,4)                 NOT NULL,
  COD_PRODUCTO     NUMBER(1)                    NOT NULL,
  FEC_EFECTIVIDAD  DATE                         NOT NULL,
  IMP_CONCEPTO     NUMBER(14,4)                 NOT NULL,
  COD_REGION       VARCHAR2(3 BYTE)             NOT NULL,
  COD_PROVINCIA    VARCHAR2(5 BYTE)             NOT NULL,
  COD_TIPCONCE     NUMBER(2)                    NOT NULL,
  COD_CIUDAD       VARCHAR2(5 BYTE)             NOT NULL,
  FEC_VALOR        DATE                         NOT NULL,
  COD_MODULO       VARCHAR2(2 BYTE)             NOT NULL,
  IMP_MONTOBASE    NUMBER(14,4)                 NOT NULL,
  IND_FACTUR       NUMBER(1)                    DEFAULT 1                     NOT NULL,
  NUM_UNIDADES     NUMBER(6),
  IND_SUPERTEL     NUMBER(1)                    DEFAULT 0                     NOT NULL,
  NUM_SERIEMEC     VARCHAR2(25 BYTE),
  NUM_ABONADO      NUMBER(8)                    DEFAULT 0                     NOT NULL,
  COD_PORTADOR     NUMBER(5)                    DEFAULT 0                     NOT NULL,
  DES_CONCEPTO     VARCHAR2(60 BYTE)            NOT NULL,
  SEQ_CUOTAS       NUMBER(8),
  NUM_CUOTAS       NUMBER(3),
  ORD_CUOTA        NUMBER(3),
  NUM_SERIELE      VARCHAR2(25 BYTE),
  MES_GARANTIA     NUMBER(2),
  NUM_GUIA         NUMBER(10),
  IND_ALTA         NUMBER(1),
  NUM_PAQUETE      NUMBER(3),
  NUM_VENTA        NUMBER(8),
  NUM_TRANSACCION  NUMBER(9),
  FLAG_IMPUES      NUMBER(1),
  FLAG_DTO         NUMBER(1),
  COD_TIPDOCUM     VARCHAR2(2 BYTE)             DEFAULT '2'                   NOT NULL,
  PREF_PLAZA       VARCHAR2(25 BYTE)            DEFAULT 'TMG'                 NOT NULL
)
TABLESPACE TS_FACTURACION_MDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          4M
            NEXT             4M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE FA_AJUSTECONC IS 'Conceptos que pertenecen a un documento que se modificado por una Nota D/C';

