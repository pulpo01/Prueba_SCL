CREATE TABLE VE_CATPLANTARIF(
    COD_CATEGORIA VARCHAR2(3) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_PLANTARIF VARCHAR2(3) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_EFECTIVIDAD DATE NOT NULL 
 ,  FEC_FINEFECTIVIDAD DATE NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CATPLANTARIF IS 'Categorias asociadas a Planes Tarifarios'
/
COMMENT ON COLUMN  VE_CATPLANTARIF.COD_CATEGORIA IS 'Codigo de Categoria de Plan Tarifario'
/
COMMENT ON COLUMN  VE_CATPLANTARIF.COD_PRODUCTO IS 'Codigo de Producto para Plan Tarifario'
/
COMMENT ON COLUMN  VE_CATPLANTARIF.COD_PLANTARIF IS 'Codigo de Plan Tarifario'
/
COMMENT ON COLUMN  VE_CATPLANTARIF.NOM_USUARIO IS 'Usuario que genera/elimina la asociacion'
/
COMMENT ON COLUMN  VE_CATPLANTARIF.FEC_EFECTIVIDAD IS 'Fecha de entrada en vigencia de la asociacion Plan/Categoria'
/
COMMENT ON COLUMN  VE_CATPLANTARIF.FEC_FINEFECTIVIDAD IS 'Fecha de expiracion de la asociacion Plan/Categoria'
/
