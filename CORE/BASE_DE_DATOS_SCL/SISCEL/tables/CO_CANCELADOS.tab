CREATE TABLE CO_CANCELADOS
(
  COD_CLIENTE          NUMBER(8)                NOT NULL,
  COD_TIPDOCUM         NUMBER(2)                NOT NULL,
  COD_CENTREMI         NUMBER(4)                NOT NULL,
  NUM_SECUENCI         NUMBER(8)                NOT NULL,
  COD_VENDEDOR_AGENTE  NUMBER(6)                NOT NULL,
  LETRA                VARCHAR2(1 BYTE)         NOT NULL,
  COD_CONCEPTO         NUMBER(4)                NOT NULL,
  COLUMNA              NUMBER(4)                DEFAULT 1                     NOT NULL,
  COD_PRODUCTO         NUMBER(1)                NOT NULL,
  IMPORTE_HABER        NUMBER(16,4)             NOT NULL,
  NUM_TRANSACCION      NUMBER(9),
  IMPORTE_DEBE         NUMBER(16,4)             NOT NULL,
  IND_CONTADO          NUMBER(1)                NOT NULL,
  IND_FACTURADO        NUMBER(1)                NOT NULL,
  FEC_EFECTIVIDAD      DATE                     NOT NULL,
  FEC_CANCELACION      DATE                     NOT NULL,
  IND_PORTADOR         NUMBER(1)                NOT NULL,
  FEC_PAGO             DATE                     NOT NULL,
  FEC_CADUCIDA         DATE                     NOT NULL,
  FEC_ANTIGUEDAD       DATE                     NOT NULL,
  FEC_VENCIMIE         DATE                     NOT NULL,
  NUM_CUOTA            NUMBER(8),
  SEC_CUOTA            NUMBER(3),
  NUM_VENTA            NUMBER(8),
  NUM_ABONADO          NUMBER(8)                NOT NULL,
  NUM_FOLIO            NUMBER(9)                NOT NULL,
  NUM_FOLIOCTC         VARCHAR2(11 BYTE)        DEFAULT '0',
  COD_OPERADORA_SCL    VARCHAR2(5 BYTE),
  COD_PLAZA            VARCHAR2(5 BYTE),
  PREF_PLAZA           VARCHAR2(25 BYTE)
)
TABLESPACE TS_RECYCOB_LDAT
PCTUSED    0
PCTFREE    5
INITRANS   4
MAXTRANS   255
STORAGE    (
            INITIAL          128M
            NEXT             128M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE CO_CANCELADOS IS 'Cartera de Cobros';

COMMENT ON COLUMN CO_CANCELADOS.COD_CLIENTE IS 'Codigo de Cliente';

COMMENT ON COLUMN CO_CANCELADOS.COD_TIPDOCUM IS 'CODIGO DE TIPO DE DOCUMENTO';

COMMENT ON COLUMN CO_CANCELADOS.COD_CENTREMI IS 'CODIGO DEL CENTRO EMISOR';

COMMENT ON COLUMN CO_CANCELADOS.NUM_SECUENCI IS 'Numero de secuencia del siguiente docume';

COMMENT ON COLUMN CO_CANCELADOS.COD_VENDEDOR_AGENTE IS 'Codigo de Agente';

COMMENT ON COLUMN CO_CANCELADOS.LETRA IS 'Letra de un documento';

COMMENT ON COLUMN CO_CANCELADOS.COD_CONCEPTO IS 'Codigo de Concepto';

COMMENT ON COLUMN CO_CANCELADOS.COLUMNA IS 'Secuencia';

COMMENT ON COLUMN CO_CANCELADOS.COD_PRODUCTO IS 'Codigo de producto';

COMMENT ON COLUMN CO_CANCELADOS.IMPORTE_HABER IS 'Importe haber';

COMMENT ON COLUMN CO_CANCELADOS.NUM_TRANSACCION IS 'Numero de transaccion';

COMMENT ON COLUMN CO_CANCELADOS.IMPORTE_DEBE IS 'Importe debe';

COMMENT ON COLUMN CO_CANCELADOS.IND_CONTADO IS '0 - Recurrente 1 - Contado';

COMMENT ON COLUMN CO_CANCELADOS.IND_FACTURADO IS 'Indicador de Facturado';

COMMENT ON COLUMN CO_CANCELADOS.FEC_EFECTIVIDAD IS 'Fecha de Efectividad';

COMMENT ON COLUMN CO_CANCELADOS.FEC_CANCELACION IS 'Fecha de cancelacion';

COMMENT ON COLUMN CO_CANCELADOS.IND_PORTADOR IS 'Indicador de Portador';

COMMENT ON COLUMN CO_CANCELADOS.FEC_PAGO IS 'Fecha de Pago';

COMMENT ON COLUMN CO_CANCELADOS.FEC_CADUCIDA IS 'Fecha de caducidad';

COMMENT ON COLUMN CO_CANCELADOS.FEC_ANTIGUEDAD IS 'Fecha de Antiguedad';

COMMENT ON COLUMN CO_CANCELADOS.FEC_VENCIMIE IS 'Fecha de vencimiento';

COMMENT ON COLUMN CO_CANCELADOS.NUM_CUOTA IS 'NUMERO DE LA CUOTA DE UN CLIENTE';

COMMENT ON COLUMN CO_CANCELADOS.SEC_CUOTA IS 'Secuencia de la cuota';

COMMENT ON COLUMN CO_CANCELADOS.NUM_VENTA IS 'Numero de Venta';

COMMENT ON COLUMN CO_CANCELADOS.NUM_ABONADO IS 'Numero del Terminal';

COMMENT ON COLUMN CO_CANCELADOS.NUM_FOLIO IS 'Numero de Folio';

COMMENT ON COLUMN CO_CANCELADOS.COD_OPERADORA_SCL IS 'Codigo de operadora';

COMMENT ON COLUMN CO_CANCELADOS.COD_PLAZA IS 'Codigo de plaza';

COMMENT ON COLUMN CO_CANCELADOS.PREF_PLAZA IS 'Prefijo Plaza.';
