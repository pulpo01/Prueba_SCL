CREATE TABLE GA_PERABORENT(
    NUM_ALQUILER NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ,  COD_PERIODO VARCHAR2(2) NOT NULL 
 ,  FEC_BAJA DATE NOT NULL 
 ,  NUM_MOVSS NUMBER(9,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_PERABORENT IS 'Periodos de Renta de Abonados Rent a Phone'
/
COMMENT ON COLUMN  GA_PERABORENT.NUM_ALQUILER IS 'Numero de Alquiler'
/
COMMENT ON COLUMN  GA_PERABORENT.NUM_ABONADO IS 'Numero de Abonado/Producto'
/
COMMENT ON COLUMN  GA_PERABORENT.FEC_ALTA IS 'Fecha de Alta'
/
COMMENT ON COLUMN  GA_PERABORENT.COD_PERIODO IS 'Codigo de Periodo Contratado'
/
COMMENT ON COLUMN  GA_PERABORENT.FEC_BAJA IS 'Fecha de Baja'
/
COMMENT ON COLUMN  GA_PERABORENT.NUM_MOVSS IS 'Numero de movimiento servicios suplementarios'
/
