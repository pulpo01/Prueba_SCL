CREATE TABLE GAT_SOLICLIQ_DEUDA
(
  NUM_VENTA        NUMBER(8)                    NOT NULL,
  COD_CLIENTE      NUMBER(8)                    NOT NULL,
  COD_TIPDOCUM     NUMBER(2)                    NOT NULL,
  NUM_FOLIO        NUMBER(9)                    NOT NULL,
  NUM_PROCESO      NUMBER(8),
  NUM_SEC_NCP      NUMBER(8),
  FEC_INGRESO_SOL  DATE,
  FEC_PROCESO      DATE,
  PREF_PLAZA       VARCHAR2(25 BYTE)
)
TABLESPACE TS_CRM_POSVENTA_SDAT
PCTUSED    0
PCTFREE    4
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          40K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE GAT_SOLICLIQ_DEUDA IS '.';

COMMENT ON COLUMN GAT_SOLICLIQ_DEUDA.NUM_VENTA IS 'Numero de Venta.';

COMMENT ON COLUMN GAT_SOLICLIQ_DEUDA.COD_CLIENTE IS 'Codigo de Cliente.';

COMMENT ON COLUMN GAT_SOLICLIQ_DEUDA.COD_TIPDOCUM IS 'Tipo de Documento.';

COMMENT ON COLUMN GAT_SOLICLIQ_DEUDA.NUM_FOLIO IS 'Numero de Folio.';

COMMENT ON COLUMN GAT_SOLICLIQ_DEUDA.NUM_PROCESO IS 'Numero de Proceso de Facturacion.';

COMMENT ON COLUMN GAT_SOLICLIQ_DEUDA.NUM_SEC_NCP IS 'Numero de Secuencia Nota Credito Parcial.';

COMMENT ON COLUMN GAT_SOLICLIQ_DEUDA.FEC_INGRESO_SOL IS 'Fecha ingreso Solicitud.';

COMMENT ON COLUMN GAT_SOLICLIQ_DEUDA.FEC_PROCESO IS 'Fecha de Proceso.';

COMMENT ON COLUMN GAT_SOLICLIQ_DEUDA.PREF_PLAZA IS 'PREFIJO PLAZA DEL FOLIO (NUM_GUIA)';
