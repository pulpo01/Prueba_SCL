CREATE TABLE FA_DOCIMPRE(
    COD_DOCUMENTO NUMBER(2,0) NOT NULL 
 ,  COD_ORDEN NUMBER(3,0) NOT NULL 
 ,  COD_FORMULARIO NUMBER(3,0) NOT NULL 
 ,  COD_NIVEL NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_DOCIMPRE IS 'Niveles de un Docuemento'
/
COMMENT ON COLUMN  FA_DOCIMPRE.COD_DOCUMENTO IS 'Codigo de Documento Impresion'
/
COMMENT ON COLUMN  FA_DOCIMPRE.COD_ORDEN IS 'Orden del Formulario dentro del Documento'
/
COMMENT ON COLUMN  FA_DOCIMPRE.COD_FORMULARIO IS 'Codigo de Formulario (Plantilla)'
/
COMMENT ON COLUMN  FA_DOCIMPRE.COD_NIVEL IS 'Nivel de la Imformacion a Imprimir dentro del Formulario'
/
