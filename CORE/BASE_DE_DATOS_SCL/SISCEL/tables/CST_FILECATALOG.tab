CREATE TABLE CST_FILECATALOG(
    NOM_COMPONENTE VARCHAR2(60) NOT NULL 
 ,  COD_PLATAFORMA VARCHAR2(5) NOT NULL 
 ,  PATH_CLASS VARCHAR2(60) NOT NULL 
 ,  NOM_FILEREGSYS VARCHAR2(60) NOT NULL 
 ,  NOM_FILEUNREGSYS VARCHAR2(60) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CST_FILECATALOG IS 'Catalogo De Componentes Por Plataforma.'
/
COMMENT ON COLUMN  CST_FILECATALOG.NOM_COMPONENTE IS 'Nombre de la Componente del Sistema.'
/
COMMENT ON COLUMN  CST_FILECATALOG.COD_PLATAFORMA IS 'Nombre de la Aplicacion que Requiere de la Componente.'
/
COMMENT ON COLUMN  CST_FILECATALOG.PATH_CLASS IS 'Path Origen de la Clase en el Refistro de Sistema HKEY.'
/
COMMENT ON COLUMN  CST_FILECATALOG.NOM_FILEREGSYS IS 'Nombre del Archivo de Registro del Sistema.'
/
COMMENT ON COLUMN  CST_FILECATALOG.NOM_FILEUNREGSYS IS 'Nombre del Archivo de para Des-Registrar la Componente del Sistema.'
/
