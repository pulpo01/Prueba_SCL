CREATE TABLE GC_BITACORA_TO(
    ID_BITACORA NUMBER(15,0) NOT NULL 
 ,  NOM_TABLA VARCHAR2(30) NOT NULL 
 ,  NOM_CAMPO VARCHAR2(30) NOT NULL 
 ,  ID_TABLA NUMBER(10,0) NOT NULL 
 ,  VAL_NUEVO VARCHAR2(50) NULL 
 ,  VAL_ANTERIOR VARCHAR2(50) NULL 
 ,  GLS_OBSERVACIONES VARCHAR2(100) NULL 
 ,  USU_MODIF VARCHAR2(30) NULL 
 ,  FEC_MODIF DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_MDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GC_BITACORA_TO IS 'Bitacora de cambios en GC'
/
COMMENT ON COLUMN  GC_BITACORA_TO.ID_BITACORA IS 'Identificacion unica de la modificacion'
/
COMMENT ON COLUMN  GC_BITACORA_TO.NOM_TABLA IS 'Nombre de la tabla en que se produce el cambio'
/
COMMENT ON COLUMN  GC_BITACORA_TO.NOM_CAMPO IS 'Nombre del campo de la tabla en que se produce el cambio'
/
COMMENT ON COLUMN  GC_BITACORA_TO.ID_TABLA IS 'Identificador (secuencial) del campo en la tabla a la cual pertence'
/
COMMENT ON COLUMN  GC_BITACORA_TO.VAL_NUEVO IS 'Nuevo valor del dato cambiado'
/
COMMENT ON COLUMN  GC_BITACORA_TO.VAL_ANTERIOR IS 'Valor anterior al cambio realizado'
/
COMMENT ON COLUMN  GC_BITACORA_TO.GLS_OBSERVACIONES IS 'Descripcion del cambio realizado'
/
COMMENT ON COLUMN  GC_BITACORA_TO.USU_MODIF IS 'Usuario que efectua el cambio'
/
COMMENT ON COLUMN  GC_BITACORA_TO.FEC_MODIF IS 'Fecha en que se efectua el cambio'
/
