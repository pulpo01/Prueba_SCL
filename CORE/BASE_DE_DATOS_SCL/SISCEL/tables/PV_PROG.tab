CREATE TABLE PV_PROG(
    NUM_OS NUMBER(10,0) NOT NULL 
 ,  F_PRORROGA DATE NOT NULL 
 ,  USUARIO VARCHAR2(30) NOT NULL 
 ,  OBSERVACION VARCHAR2(30) NULL 
 ,  FEC_PROG DATE DEFAULT sysdate NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_PROG IS 'Registro De Prorrogas Por Ooss.'
/
COMMENT ON COLUMN  PV_PROG.NUM_OS IS 'Identificador Orden de servicio.'
/
COMMENT ON COLUMN  PV_PROG.F_PRORROGA IS 'Fecha y Hora de ocurrecia de la prorroga.'
/
COMMENT ON COLUMN  PV_PROG.USUARIO IS 'Usuario que realiza la prorroga.'
/
COMMENT ON COLUMN  PV_PROG.OBSERVACION IS 'Observaciones.'
/
COMMENT ON COLUMN  PV_PROG.FEC_PROG IS 'Fecha a la cual prorrogo.'
/
