CREATE TABLE CO_DESAPAGOS
(
  COD_TIPDOCUM         NUMBER(2)                NOT NULL,
  COD_CENTREMI         NUMBER(4)                NOT NULL,
  NUM_SECUENCI         NUMBER(8)                NOT NULL,
  COD_VENDEDOR_AGENTE  NUMBER(6)                NOT NULL,
  LETRA                VARCHAR2(1 BYTE)         NOT NULL,
  COD_CLIENTE          NUMBER(8)                NOT NULL,
  IMP_PAGO             NUMBER(14,4)             NOT NULL,
  FEC_EFECTIVIDAD      DATE                     NOT NULL,
  FEC_VALOR            DATE                     NOT NULL,
  NOM_USUARORA         VARCHAR2(30 BYTE)        NOT NULL,
  COD_FORPAGO          NUMBER(1)                NOT NULL,
  COD_SISPAGO          NUMBER(2)                NOT NULL,
  COD_ORIPAGO          NUMBER(2)                NOT NULL,
  COD_CAUPAGO          NUMBER(2)                NOT NULL,
  FEC_DESAPLICA        DATE                     NOT NULL,
  NOM_USUARIOSDESAP    VARCHAR2(30 BYTE)        NOT NULL,
  TIP_DESAPLICA        NUMBER(3),
  COD_BANCO            VARCHAR2(15 BYTE),
  COD_TIPTARJETA       VARCHAR2(3 BYTE),
  COD_CAJA             VARCHAR2(4 BYTE),
  COD_SUCURSAL         VARCHAR2(4 BYTE),
  CTA_CORRIENTE        VARCHAR2(18 BYTE),
  NUM_TARJETA          VARCHAR2(20 BYTE),
  DES_PAGO             VARCHAR2(40 BYTE),
  PREF_PLAZA           VARCHAR2(25 BYTE)
)
TABLESPACE TS_RECYCOB_SDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          400K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE CO_DESAPAGOS IS 'PAGOS DESAPLICADOS';

COMMENT ON COLUMN CO_DESAPAGOS.IMP_PAGO IS 'IMPORTE DEL PAGO';

COMMENT ON COLUMN CO_DESAPAGOS.FEC_EFECTIVIDAD IS 'FECHA DE EFECTIVIDAD';

COMMENT ON COLUMN CO_DESAPAGOS.FEC_VALOR IS 'FECHA VALOR';

COMMENT ON COLUMN CO_DESAPAGOS.NOM_USUARORA IS 'NOMBRE DE USUARIO';

COMMENT ON COLUMN CO_DESAPAGOS.COD_FORPAGO IS 'FORMA DE PAGO';

COMMENT ON COLUMN CO_DESAPAGOS.COD_SISPAGO IS 'CODIGO DE SISTEMA DE PAGO';

COMMENT ON COLUMN CO_DESAPAGOS.COD_ORIPAGO IS 'CODIGO DE ORIGEN DE PAGO';

COMMENT ON COLUMN CO_DESAPAGOS.COD_CAUPAGO IS 'CODIGO DE CAUSA DE PAGO';

COMMENT ON COLUMN CO_DESAPAGOS.FEC_DESAPLICA IS 'FECHA DE EN LA QUE SE DESAPLICA EL PAGO';

COMMENT ON COLUMN CO_DESAPAGOS.NOM_USUARIOSDESAP IS 'NOMBRE USUARIO QUE DESAPLICA';

COMMENT ON COLUMN CO_DESAPAGOS.TIP_DESAPLICA IS 'TIPO DE DESAPLICACION';

COMMENT ON COLUMN CO_DESAPAGOS.COD_BANCO IS 'CODIGO DE BANCO';

COMMENT ON COLUMN CO_DESAPAGOS.COD_TIPTARJETA IS 'TIPO DE TARJETA';

COMMENT ON COLUMN CO_DESAPAGOS.COD_CAJA IS 'Codigo de Caja - Retrofitted';

COMMENT ON COLUMN CO_DESAPAGOS.COD_SUCURSAL IS 'SUCURSAL';

COMMENT ON COLUMN CO_DESAPAGOS.CTA_CORRIENTE IS 'CUENTA CORRIENTE';

COMMENT ON COLUMN CO_DESAPAGOS.NUM_TARJETA IS 'NUMERO DE TARJETA';

COMMENT ON COLUMN CO_DESAPAGOS.DES_PAGO IS 'DESCRIPCION DEL PAGO';

COMMENT ON COLUMN CO_DESAPAGOS.PREF_PLAZA IS 'Prefijo Plaza.';

COMMENT ON COLUMN CO_DESAPAGOS.COD_TIPDOCUM IS 'TIPO DE DOCUMENTO';

COMMENT ON COLUMN CO_DESAPAGOS.COD_CENTREMI IS 'CODIGO DEL CENTRO EMISOR';

COMMENT ON COLUMN CO_DESAPAGOS.NUM_SECUENCI IS 'NUMERO DE SECUENCIA';

COMMENT ON COLUMN CO_DESAPAGOS.COD_VENDEDOR_AGENTE IS 'CODIGO DE AGENTE';

COMMENT ON COLUMN CO_DESAPAGOS.LETRA IS 'LETRA';

COMMENT ON COLUMN CO_DESAPAGOS.COD_CLIENTE IS 'CODIGO DE CLIENTE';
