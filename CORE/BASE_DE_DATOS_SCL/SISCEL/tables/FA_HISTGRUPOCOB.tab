CREATE TABLE FA_HISTGRUPOCOB(
    COD_GRUPO VARCHAR2(2) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_CICLO NUMBER(2,0) NOT NULL 
 ,  TIP_COBRO NUMBER(1,0) DEFAULT 1 NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  TIP_COBROANT NUMBER(1,0) NOT NULL 
 ,  USUARIO VARCHAR2(30) DEFAULT 'user ' NOT NULL 
 ,  FECHA_HIST DATE DEFAULT sysdate NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_HISTGRUPOCOB IS 'Tipo de cobro Anticipado o Vencido de cargos por grupo'
/
COMMENT ON COLUMN  FA_HISTGRUPOCOB.COD_GRUPO IS 'Codigo de grupo'
/
COMMENT ON COLUMN  FA_HISTGRUPOCOB.COD_PRODUCTO IS 'Producto al que pertenece el concepto'
/
COMMENT ON COLUMN  FA_HISTGRUPOCOB.COD_CONCEPTO IS 'Codigo de conceptos de Facturacion'
/
COMMENT ON COLUMN  FA_HISTGRUPOCOB.COD_CICLO IS 'Ciclo de Facturacion'
/
COMMENT ON COLUMN  FA_HISTGRUPOCOB.TIP_COBRO IS 'Tipo de Cobro 1=Mes por Adelantado, 0=Mes Vencido'
/
COMMENT ON COLUMN  FA_HISTGRUPOCOB.FEC_DESDE IS 'Fecha inicio validez del tipo de cobro'
/
COMMENT ON COLUMN  FA_HISTGRUPOCOB.FEC_HASTA IS 'Fecha final de validez del tipo de cobro'
/
COMMENT ON COLUMN  FA_HISTGRUPOCOB.TIP_COBROANT IS 'Forma de Cobro anterior'
/
COMMENT ON COLUMN  FA_HISTGRUPOCOB.USUARIO IS 'Usuario'
/
COMMENT ON COLUMN  FA_HISTGRUPOCOB.FECHA_HIST IS 'Fecha de Paso Historico'
/
