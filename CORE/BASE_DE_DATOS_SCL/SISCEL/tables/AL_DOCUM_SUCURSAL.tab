CREATE TABLE AL_DOCUM_SUCURSAL(
    COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  COD_TIPDOCUM NUMBER(2,0) NOT NULL 
 ,  COD_CENTREMI NUMBER(4,0) NOT NULL 
 ,  NUM_MAX NUMBER(9,0) NULL 
 ,  NUM_MIN NUMBER(9,0) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_DOCUM_SUCURSAL IS 'Definicion de minimos y maximos aconsejables por sucursal'
/
COMMENT ON COLUMN  AL_DOCUM_SUCURSAL.COD_OFICINA IS 'Codigo identificativo de la sucursal.'
/
COMMENT ON COLUMN  AL_DOCUM_SUCURSAL.COD_TIPDOCUM IS 'Tipo de Documento'
/
COMMENT ON COLUMN  AL_DOCUM_SUCURSAL.COD_CENTREMI IS 'Centro emisor'
/
COMMENT ON COLUMN  AL_DOCUM_SUCURSAL.NUM_MAX IS 'Maximo de folios'
/
COMMENT ON COLUMN  AL_DOCUM_SUCURSAL.NUM_MIN IS 'Minimo folios aconsejado.'
/
