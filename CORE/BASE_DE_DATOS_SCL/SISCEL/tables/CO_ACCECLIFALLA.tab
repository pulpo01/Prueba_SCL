CREATE TABLE CO_ACCECLIFALLA(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  DIR_IP VARCHAR2(15) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_INICIO DATE NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_ACCECLIFALLA IS 'FALLOS QUE SE PRODUCEN SOBRE EL CONTROL DE UN CLIENTE'
/
COMMENT ON COLUMN  CO_ACCECLIFALLA.COD_CLIENTE IS 'CODIGO DE CLIENTE'
/
COMMENT ON COLUMN  CO_ACCECLIFALLA.DIR_IP IS 'DIRECCION IP DE LA MAQUINA'
/
COMMENT ON COLUMN  CO_ACCECLIFALLA.NOM_USUARORA IS 'NOMBRE DE USUARIO'
/
COMMENT ON COLUMN  CO_ACCECLIFALLA.FEC_INICIO IS 'FECHA DE INICIO'
/
