CREATE TABLE CM_DETCATEGVENTAS_TD(
    COD_CATEGVENTA VARCHAR2(10) NOT NULL 
 ,  COD_CATEGCLIENTE VARCHAR2(10) NOT NULL 
 ,  TIP_PLAN VARCHAR2(5) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_DETCATEGVENTAS_TD IS 'DETALLE DE CATEGORIAS DE VENTA'
/
COMMENT ON COLUMN  CM_DETCATEGVENTAS_TD.COD_CATEGVENTA IS 'CODIGO DE LA CATEGORIA DE VENTA'
/
COMMENT ON COLUMN  CM_DETCATEGVENTAS_TD.COD_CATEGCLIENTE IS 'CODIGO DE LA CATEGORIA DEL CLIENTE'
/
COMMENT ON COLUMN  CM_DETCATEGVENTAS_TD.TIP_PLAN IS 'TIPO (CATEGORIA) DE PLAN TARIFARIO DESDE EL PUNTO DE VISTA COMISIONAL '
/
COMMENT ON COLUMN  CM_DETCATEGVENTAS_TD.NOM_USUARIO IS 'USUARIO QUE REALIZO LA ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  CM_DETCATEGVENTAS_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION'
/
