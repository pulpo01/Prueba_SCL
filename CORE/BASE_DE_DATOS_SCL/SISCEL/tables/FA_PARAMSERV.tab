CREATE TABLE FA_PARAMSERV(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  IND_ABONADO0 NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  IND_CRITERIO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  FEC_MODIFICA DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_PARAMSERV IS 'Tabla Que Almacena Informacion De Los Servicios Que Poseen Criterios De Aplicacion.'
/
COMMENT ON COLUMN  FA_PARAMSERV.COD_PRODUCTO IS 'CODIGO DEL PRODUCTO.'
/
COMMENT ON COLUMN  FA_PARAMSERV.COD_SERVICIO IS 'CODIGO DEL SERVICIO A PARAMETRIZAR.'
/
COMMENT ON COLUMN  FA_PARAMSERV.IND_ABONADO0 IS 'INDICADOR SI EL SERVICIO SE APLICA A ABONADO CERO.'
/
COMMENT ON COLUMN  FA_PARAMSERV.IND_CRITERIO IS 'INDICARDOR SI EL SERVICIO TIENE CRITERIOS DE APLICACION.'
/
COMMENT ON COLUMN  FA_PARAMSERV.FEC_MODIFICA IS 'FECHA DE MODIFICACION DEL REGISTRO.'
/
COMMENT ON COLUMN  FA_PARAMSERV.NOM_USUARIO IS 'NOMBRE USUARIO QUE MODIFICA EL REGISTRO.'
/
