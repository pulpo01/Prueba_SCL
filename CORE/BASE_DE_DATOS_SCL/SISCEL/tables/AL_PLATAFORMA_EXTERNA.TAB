CREATE TABLE AL_PLATAFORMA_EXTERNA(
    COD_PLATAFORMA VARCHAR2(10) NOT NULL 
 ,  DES_PLATAFORMA VARCHAR2(30) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  AL_PLATAFORMA_EXTERNA.COD_PLATAFORMA IS 'Codigo de Plataforma'
/
COMMENT ON COLUMN  AL_PLATAFORMA_EXTERNA.DES_PLATAFORMA IS 'Nombre de Plataforma'
/
COMMENT ON COLUMN  AL_PLATAFORMA_EXTERNA.NOM_USUARIO IS 'Usuario ultima alteracion'
/
COMMENT ON COLUMN  AL_PLATAFORMA_EXTERNA.FEC_ULTMOD IS 'Fecha de Ultima alteracion'
/
