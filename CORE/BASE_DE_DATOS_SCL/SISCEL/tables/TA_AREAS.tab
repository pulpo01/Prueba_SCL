CREATE TABLE TA_AREAS(
    COD_AREA VARCHAR2(5) NOT NULL 
 ,  DES_AREA VARCHAR2(30) NOT NULL 
 ,  IND_SUPERTEL NUMBER(1,0) NULL 
 ,  COD_ALM VARCHAR2(5) NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_AREAS IS 'Regiones interurbanas o areas de otras redes'
/
COMMENT ON COLUMN  TA_AREAS.COD_AREA IS 'Codigo del Area'
/
COMMENT ON COLUMN  TA_AREAS.DES_AREA IS 'Descripcion del Area'
/
COMMENT ON COLUMN  TA_AREAS.IND_SUPERTEL IS 'Validez de codigo de area para supertelefono: 1, si es valido. 0 o nulo si n
o lo es'
/
