CREATE TABLE GA_TIP_OPERADORES_TD (  
   COD_DOPE                      VARCHAR2(5)          NULL
 , COD_TOPE                      VARCHAR2(5)          NULL
 , TIP_RED                       VARCHAR2(1)          NULL
 , TIP_LLAM                      NUMBER(1)            NULL
 , FEC_MODIFIC                   DATE                 NULL
 , NOM_USUAORA                   VARCHAR2(30)         NULL 
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  GA_TIP_OPERADORES_TD                      IS '[ GTOP ] Tipo de operadoresTipo de operadores'
/

COMMENT ON COLUMN GA_TIP_OPERADORES_TD.COD_DOPE                       IS 'Código Dope'
/
COMMENT ON COLUMN GA_TIP_OPERADORES_TD.COD_TOPE                       IS 'Código Tope'
/
COMMENT ON COLUMN GA_TIP_OPERADORES_TD.TIP_RED                        IS 'Tipo Red (0-Movil; 1-Fijo)'
/
COMMENT ON COLUMN GA_TIP_OPERADORES_TD.TIP_LLAM                       IS 'Tipo Llamada (0-ONN NET;1-OFF NET)'
/
COMMENT ON COLUMN GA_TIP_OPERADORES_TD.FEC_MODIFIC                    IS 'Fecha de modificación'
/
COMMENT ON COLUMN GA_TIP_OPERADORES_TD.NOM_USUAORA                    IS 'Nombre Usuario'
/

