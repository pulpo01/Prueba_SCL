CREATE TABLE CM_DIFDOCUMENTACION_TD(
    COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  TIP_PLAN VARCHAR2(5) NOT NULL 
 ,  COD_CATEGCLIENTE VARCHAR2(10) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  IMP_DIFERENCIADO NUMBER(14,4) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_DIFDOCUMENTACION_TD IS 'VALORACION DIFERENCIADA CONCEPTOS DE DOCUMENTACION'
/
COMMENT ON COLUMN  CM_DIFDOCUMENTACION_TD.COD_TIPORED IS 'TIPO DE RED SOBRE EL QUE SE APLICA EL CONCEPTO.'
/
COMMENT ON COLUMN  CM_DIFDOCUMENTACION_TD.COD_PLANCOMIS IS 'PLAN DE COMISIONES QUE CONTIENE EL CONCEPTO.'
/
COMMENT ON COLUMN  CM_DIFDOCUMENTACION_TD.COD_CONCEPTO IS 'CONCEPTO DE COMISION DE DOCUMENTACION.'
/
COMMENT ON COLUMN  CM_DIFDOCUMENTACION_TD.TIP_PLAN IS 'TIPO DE PLAN A COMISIONAR.'
/
COMMENT ON COLUMN  CM_DIFDOCUMENTACION_TD.COD_CATEGCLIENTE IS 'CATEGORIA DE CLIENTE A COMISIONAR'
/
COMMENT ON COLUMN  CM_DIFDOCUMENTACION_TD.COD_VENDEDOR IS 'VENDEDOR QUE VERA SUS VENTAS DIFERENCIADAS.'
/
COMMENT ON COLUMN  CM_DIFDOCUMENTACION_TD.IMP_DIFERENCIADO IS 'IMPORTE DE COMISION DIFERENCIADO PARA VENDEDOR'
/
COMMENT ON COLUMN  CM_DIFDOCUMENTACION_TD.NOM_USUARIO IS 'USUARIO QUE HIZO LA ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  CM_DIFDOCUMENTACION_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION'
/
