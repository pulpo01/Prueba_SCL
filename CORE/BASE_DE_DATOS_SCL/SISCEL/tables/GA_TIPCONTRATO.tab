CREATE TABLE GA_TIPCONTRATO(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_TIPCONTRATO VARCHAR2(3) NOT NULL 
 ,  DES_TIPCONTRATO VARCHAR2(30) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  IND_CONTSEG VARCHAR2(3) NULL 
 ,  IND_CONTCEL VARCHAR2(1) NULL 
 ,  IND_COMODATO NUMBER(1,0) NULL 
 ,  CANAL_VTA NUMBER(1,0) NULL 
 ,  MESES_MINIMO NUMBER(2,0) NULL 
 ,  IND_PROCEQUI VARCHAR2(1) DEFAULT 'I' NOT NULL 
 ,  IND_PRECIOLISTA VARCHAR2(1) DEFAULT '1' NOT NULL 
 ,  IND_GMC VARCHAR2(3) DEFAULT '0' NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_TIPCONTRATO IS 'Tipos de Contrato por Producto'
/
COMMENT ON COLUMN  GA_TIPCONTRATO.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_TIPCONTRATO.COD_TIPCONTRATO IS 'Codigo de Tipo de Contrato'
/
COMMENT ON COLUMN  GA_TIPCONTRATO.DES_TIPCONTRATO IS 'Descripcion de Tipo de Contrato'
/
COMMENT ON COLUMN  GA_TIPCONTRATO.FEC_DESDE IS 'Fecha Desde Vigencia Contrato'
/
COMMENT ON COLUMN  GA_TIPCONTRATO.FEC_HASTA IS 'Fecha Hasta Vigencia Contrato'
/
COMMENT ON COLUMN  GA_TIPCONTRATO.IND_CONTSEG IS 'Indicativo de Tipo de Contrato (P)roducto,(S)eguro Cobertura'
/
COMMENT ON COLUMN  GA_TIPCONTRATO.IND_CONTCEL IS 'Indicativo de Tipo de Contrato Celular ; (V)enta, (A)lquiler'
/
COMMENT ON COLUMN  GA_TIPCONTRATO.IND_COMODATO IS 'Contrato comodato si o no'
/
COMMENT ON COLUMN  GA_TIPCONTRATO.CANAL_VTA IS 'Canal de venta: 0=Canal interno | 1=Canal externo | 2=Ambos canales, externo e interno.'
/
COMMENT ON COLUMN  GA_TIPCONTRATO.MESES_MINIMO IS 'Numero  de meses minimo.'
/
COMMENT ON COLUMN  GA_TIPCONTRATO.IND_PROCEQUI IS 'Determina si el contrato es para equipo interno o externo  E=Externo I=interno.'
/
COMMENT ON COLUMN  GA_TIPCONTRATO.IND_PRECIOLISTA IS 'Determina si un contrato es precio de lista o politica  0=P/L; 1=P/P;.'
/
COMMENT ON COLUMN  GA_TIPCONTRATO.IND_GMC IS 'Indicativo de contrato para GMC.'
/
