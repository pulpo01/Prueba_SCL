CREATE TABLE CO_PAGOSCONCCC
(
  COD_TIPDOCUM         NUMBER(2)                NOT NULL,
  COD_CENTREMI         NUMBER(4)                NOT NULL,
  NUM_SECUENCI         NUMBER(8)                NOT NULL,
  COD_VENDEDOR_AGENTE  NUMBER(6)                NOT NULL,
  NUM_SECUREL          NUMBER(8),
  LETRA                VARCHAR2(1 BYTE)         NOT NULL,
  IMP_CONCEPTO         NUMBER(14,4)             NOT NULL,
  COD_PRODUCTO         NUMBER(1)                NOT NULL,
  COD_TIPDOCREL        NUMBER(2),
  COD_AGENTEREL        NUMBER(6),
  NUM_TRANSACCION      NUMBER(9),
  LETRA_REL            VARCHAR2(1 BYTE),
  COD_CENTRREL         NUMBER(4),
  COD_CONCEPTO         NUMBER(4),
  COLUMNA              NUMBER(4),
  NUM_ABONADO          NUMBER(8),
  NUM_FOLIO            NUMBER(9),
  NUM_CUOTA            NUMBER(8),
  SEC_CUOTA            NUMBER(3),
  NUM_VENTA            NUMBER(8),
  NUM_FOLIOCTC         VARCHAR2(11 BYTE)        DEFAULT '0',
  PREF_PLAZA           VARCHAR2(25 BYTE)
)
TABLESPACE TS_RECYCOB_SDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          40K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE CO_PAGOSCONCCC IS 'RELACION DE PAGOS CON CONCEPTOS';

COMMENT ON COLUMN CO_PAGOSCONCCC.COD_TIPDOCUM IS 'CODIGO DE TIPO DE DOCUMENTO - Retrofitted';

COMMENT ON COLUMN CO_PAGOSCONCCC.COD_CENTREMI IS 'CODIGO DE CENTRO EMISOR';

COMMENT ON COLUMN CO_PAGOSCONCCC.NUM_SECUENCI IS 'NUMERO DE SECUENCIA';

COMMENT ON COLUMN CO_PAGOSCONCCC.COD_VENDEDOR_AGENTE IS 'CODIGO DE AGENTE';

COMMENT ON COLUMN CO_PAGOSCONCCC.NUM_SECUREL IS 'Secuencia de documento Relacionado - Retrofitted';

COMMENT ON COLUMN CO_PAGOSCONCCC.LETRA IS 'LETRA';

COMMENT ON COLUMN CO_PAGOSCONCCC.IMP_CONCEPTO IS 'IMPORTE DEL CONCEPTO';

COMMENT ON COLUMN CO_PAGOSCONCCC.COD_PRODUCTO IS 'CODIGO DE PRODUCTO';

COMMENT ON COLUMN CO_PAGOSCONCCC.COD_TIPDOCREL IS 'DOCUMENTO RELACIONAL';

COMMENT ON COLUMN CO_PAGOSCONCCC.COD_AGENTEREL IS 'AGENTE RELACIONAL';

COMMENT ON COLUMN CO_PAGOSCONCCC.NUM_TRANSACCION IS 'Numero de transaccion - Retrofitted';

COMMENT ON COLUMN CO_PAGOSCONCCC.LETRA_REL IS 'LETRA RELACIONAL';

COMMENT ON COLUMN CO_PAGOSCONCCC.COD_CENTRREL IS 'CENTRO EMISOR RELACIONAL';

COMMENT ON COLUMN CO_PAGOSCONCCC.COD_CONCEPTO IS 'CODIGO DE CONCEPTO';

COMMENT ON COLUMN CO_PAGOSCONCCC.COLUMNA IS 'CODIGO DE COLUMNA';

COMMENT ON COLUMN CO_PAGOSCONCCC.NUM_ABONADO IS 'NUMERO DEL ABONADO';

COMMENT ON COLUMN CO_PAGOSCONCCC.NUM_FOLIO IS 'NUMERO DE FOLIO';

COMMENT ON COLUMN CO_PAGOSCONCCC.NUM_CUOTA IS 'NUMERO DE LA CUOTA';

COMMENT ON COLUMN CO_PAGOSCONCCC.SEC_CUOTA IS 'SECUENCIA DE LA CUOTA';

COMMENT ON COLUMN CO_PAGOSCONCCC.NUM_VENTA IS 'NUMERO DE LA VENTA';

COMMENT ON COLUMN CO_PAGOSCONCCC.NUM_FOLIOCTC IS 'Numero de folio CTC';

COMMENT ON COLUMN CO_PAGOSCONCCC.PREF_PLAZA IS 'Prefijo Plaza';
