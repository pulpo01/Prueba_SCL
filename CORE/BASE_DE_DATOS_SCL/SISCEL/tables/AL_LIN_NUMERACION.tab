CREATE TABLE AL_LIN_NUMERACION(
    NUM_NUMERACION NUMBER(9,0) NOT NULL 
 ,  LIN_NUMERACION NUMBER(3,0) NOT NULL 
 ,  TIP_STOCK NUMBER(2,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  COD_ESTADO NUMBER(2,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  NUM_DESDE NUMBER(9,0) DEFAULT 0 NOT NULL 
 ,  CAN_NUMERA NUMBER(9,0) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_LIN_NUMERACION IS 'Contiene las lineas de las ordenes de numeracion.- Retrofitted'
/
COMMENT ON COLUMN  AL_LIN_NUMERACION.NUM_NUMERACION IS 'numero de orden de numeracion'
/
COMMENT ON COLUMN  AL_LIN_NUMERACION.LIN_NUMERACION IS 'Numero de linea'
/
COMMENT ON COLUMN  AL_LIN_NUMERACION.TIP_STOCK IS 'Tipo de stock al que corresponde el articulo'
/
COMMENT ON COLUMN  AL_LIN_NUMERACION.COD_ARTICULO IS 'Codigo de articulo a traspasar.'
/
COMMENT ON COLUMN  AL_LIN_NUMERACION.COD_ESTADO IS 'Estado del articulo'
/
COMMENT ON COLUMN  AL_LIN_NUMERACION.COD_USO IS 'Codigo de uso'
/
COMMENT ON COLUMN  AL_LIN_NUMERACION.NUM_DESDE IS 'desde folio'
/
COMMENT ON COLUMN  AL_LIN_NUMERACION.CAN_NUMERA IS 'Cantidad a numerar'
/
