CREATE TABLE GA_HSUSCGRP(
    COD_GRUPO NUMBER(8,0) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  TS VARCHAR2(1) DEFAULT 'G' NOT NULL 
 ,  ID NUMBER(7,0) NOT NULL 
 ,  COB VARCHAR2(3) NOT NULL 
 ,  NOM VARCHAR2(50) NOT NULL 
 ,  STA VARCHAR2(1) DEFAULT 'S' NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_HSUSCGRP IS 'Historico Suscriptores Grupos Beeper soporte Plataforma'
/
COMMENT ON COLUMN  GA_HSUSCGRP.COD_GRUPO IS 'Suscripcion Grupo'
/
COMMENT ON COLUMN  GA_HSUSCGRP.FEC_HISTORICO IS 'Suscripcion Grupo'
/
COMMENT ON COLUMN  GA_HSUSCGRP.TS IS 'Tipo de suscripcion; (I)ndividual,(G)rupo'
/
COMMENT ON COLUMN  GA_HSUSCGRP.ID IS 'Numero de Beeper'
/
COMMENT ON COLUMN  GA_HSUSCGRP.COB IS 'Codigo de Cobertura'
/
COMMENT ON COLUMN  GA_HSUSCGRP.NOM IS 'Nombre de Usuario'
/
COMMENT ON COLUMN  GA_HSUSCGRP.STA IS 'Estatus Suscripcion; (S)Activo,(N)Desactivado'
/
