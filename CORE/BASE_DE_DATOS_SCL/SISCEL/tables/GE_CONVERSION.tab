CREATE TABLE GE_CONVERSION(
    COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  CAMBIO NUMBER(10,5) NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_CONVERSION IS 'Cambios Monetarios'
/
COMMENT ON COLUMN  GE_CONVERSION.COD_MONEDA IS 'Codigo de Moneda'
/
COMMENT ON COLUMN  GE_CONVERSION.FEC_DESDE IS 'Fecha de Inicio de Conversion'
/
COMMENT ON COLUMN  GE_CONVERSION.CAMBIO IS 'Valor del Cambio'
/
COMMENT ON COLUMN  GE_CONVERSION.FEC_HASTA IS 'Fecha de Final de Conversion'
/
COMMENT ON COLUMN  GE_CONVERSION.COD_USUARIO IS 'Usuario'
/
