CREATE TABLE EV_ESTADOSSCORING_TO (  
   NUM_SOLSCORING                NUMBER(10)        NOT NULL
 , COD_ESTADO                    VARCHAR2(2)       NOT NULL
 , FECHA_INICIO                  DATE              NOT NULL
 , FECHA_TERMINO                 DATE                  NULL
 , COD_VENDEDOR                  NUMBER(6)         NOT NULL 
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  EV_ESTADOSSCORING_TO                      IS '[ ESTC ] Estados de la solicitud scoring.Estados de la solicitud scoring.'
/

COMMENT ON COLUMN EV_ESTADOSSCORING_TO.NUM_SOLSCORING                 IS 'Numero de Solicitud Scoring Asociado'
/
COMMENT ON COLUMN EV_ESTADOSSCORING_TO.COD_ESTADO                     IS 'Código Estado'
/
COMMENT ON COLUMN EV_ESTADOSSCORING_TO.FECHA_INICIO                   IS 'Fecha Inicio Estado'
/
COMMENT ON COLUMN EV_ESTADOSSCORING_TO.FECHA_TERMINO                  IS 'Fecha Fin Estado'
/
COMMENT ON COLUMN EV_ESTADOSSCORING_TO.COD_VENDEDOR                   IS 'Código Vendedor'
/

