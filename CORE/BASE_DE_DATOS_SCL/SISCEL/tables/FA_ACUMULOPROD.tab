CREATE TABLE FA_ACUMULOPROD(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  IND_ORDENTOTAL NUMBER(12,0) NOT NULL 
 ,  TOT_PRODUCTO NUMBER(12,0) NOT NULL 
 ,  DES_TOTALPROD VARCHAR2(60) NOT NULL 
 ,  ACUM_CARGO NUMBER(12,0) NOT NULL 
 ,  DES_ACUMCARGO VARCHAR2(60) NOT NULL 
 ,  ACUM_DTO NUMBER(12,0) NOT NULL 
 ,  DES_ACUMDTO VARCHAR2(60) NOT NULL 
 ,  ACUM_IVA NUMBER(12,0) NOT NULL 
 ,  DES_ACUMIVA VARCHAR2(60) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_ACUMULOPROD IS 'Tabla de Acumulados por Producto'
/
COMMENT ON COLUMN  FA_ACUMULOPROD.COD_PRODUCTO IS 'Producto de los acumulados'
/
COMMENT ON COLUMN  FA_ACUMULOPROD.IND_ORDENTOTAL IS 'Campo que relaciona acumulados con facturas'
/
COMMENT ON COLUMN  FA_ACUMULOPROD.TOT_PRODUCTO IS 'Total Importe por Producto'
/
COMMENT ON COLUMN  FA_ACUMULOPROD.DES_TOTALPROD IS 'Descripcion segun Producto'
/
COMMENT ON COLUMN  FA_ACUMULOPROD.ACUM_CARGO IS 'Acumulados de cargos'
/
COMMENT ON COLUMN  FA_ACUMULOPROD.DES_ACUMCARGO IS 'Descripcion del acumulado de cargos'
/
COMMENT ON COLUMN  FA_ACUMULOPROD.ACUM_DTO IS 'Acumulados de descuentos'
/
COMMENT ON COLUMN  FA_ACUMULOPROD.DES_ACUMDTO IS 'Descripcion del acumulado de descuentos'
/
COMMENT ON COLUMN  FA_ACUMULOPROD.ACUM_IVA IS 'Acumulados de Impuestos'
/
COMMENT ON COLUMN  FA_ACUMULOPROD.DES_ACUMIVA IS 'Descripcion del acumulado de descuentos'
/
