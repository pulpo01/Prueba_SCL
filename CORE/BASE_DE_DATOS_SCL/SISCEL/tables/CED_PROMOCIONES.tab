CREATE TABLE CED_PROMOCIONES(
    COD_SERVICIO VARCHAR2(5) NOT NULL 
 ,  COD_EMPRESA VARCHAR2(5) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  COD_TIPO VARCHAR2(2) NOT NULL 
 ,  COD_TIPVALOR VARCHAR2(2) NULL 
 ,  NUM_VALOR NUMBER(14,4) NOT NULL 
 ,  FEC_ULTMOD DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  IND_CARGASERV VARCHAR2(1) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CED_PROMOCIONES IS 'Promociones por servicios '
/
COMMENT ON COLUMN  CED_PROMOCIONES.COD_SERVICIO IS 'Codigo servicio en promocion '
/
COMMENT ON COLUMN  CED_PROMOCIONES.COD_EMPRESA IS 'Codigo empresa '
/
COMMENT ON COLUMN  CED_PROMOCIONES.FEC_DESDE IS 'Fecha inicio promocion '
/
COMMENT ON COLUMN  CED_PROMOCIONES.FEC_HASTA IS 'Fecha termino promocion '
/
COMMENT ON COLUMN  CED_PROMOCIONES.COD_TIPO IS 'Tipo de promocion : D: Descuento, A: Aumento carga '
/
COMMENT ON COLUMN  CED_PROMOCIONES.COD_TIPVALOR IS 'Tipo de valor asociado a la promocion M: monto fijo P: porcentaje '
/
COMMENT ON COLUMN  CED_PROMOCIONES.NUM_VALOR IS 'Valor promocion '
/
COMMENT ON COLUMN  CED_PROMOCIONES.FEC_ULTMOD IS 'Fecha ultima modificacion '
/
COMMENT ON COLUMN  CED_PROMOCIONES.NOM_USUARIO IS 'Responsable ultima modificacion '
/
COMMENT ON COLUMN  CED_PROMOCIONES.IND_CARGASERV IS 'Indicador de tipo de promocisn'
/
