CREATE TABLE AL_LIN_TIMBRADO(
    NUM_ORDEN NUMBER(9,0) NOT NULL 
 ,  NUM_LINEA NUMBER(2,0) NOT NULL 
 ,  TIP_STOCK NUMBER(2,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_ESTADO NUMBER(2,0) NOT NULL 
 ,  NUM_DESDE NUMBER(9,0) NOT NULL 
 ,  NUM_HASTA NUMBER(9,0) NOT NULL 
 ,  COD_PLAZA VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_LIN_TIMBRADO IS 'Lineas de Ordenes de Timbrado de Documentos'
/
COMMENT ON COLUMN  AL_LIN_TIMBRADO.NUM_ORDEN IS 'Numero de orden'
/
COMMENT ON COLUMN  AL_LIN_TIMBRADO.NUM_LINEA IS 'Numero de linea'
/
COMMENT ON COLUMN  AL_LIN_TIMBRADO.TIP_STOCK IS 'Tipo de stock'
/
COMMENT ON COLUMN  AL_LIN_TIMBRADO.COD_ARTICULO IS 'Codigo de articulo'
/
COMMENT ON COLUMN  AL_LIN_TIMBRADO.COD_USO IS 'Codigo de uso'
/
COMMENT ON COLUMN  AL_LIN_TIMBRADO.COD_ESTADO IS 'Codigo de Estado'
/
COMMENT ON COLUMN  AL_LIN_TIMBRADO.NUM_DESDE IS 'Desde Folio'
/
COMMENT ON COLUMN  AL_LIN_TIMBRADO.NUM_HASTA IS 'Hasta Folio'
/
COMMENT ON COLUMN  AL_LIN_TIMBRADO.COD_PLAZA IS 'Codigo de Plaza'
/
