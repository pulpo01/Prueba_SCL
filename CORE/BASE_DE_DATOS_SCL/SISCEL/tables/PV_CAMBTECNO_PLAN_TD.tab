CREATE TABLE PV_CAMBTECNO_PLAN_TD(
    COD_PRODUCTO NUMBER(1,0) DEFAULT 1 NOT NULL 
 ,  COD_TECNOLOGIA_ORI VARCHAR2(7) NOT NULL 
 ,  COD_PLANTARIF_ORI VARCHAR2(3) NOT NULL 
 ,  COD_TECNOLOGIA_DES VARCHAR2(7) NOT NULL 
 ,  COD_PLANTARIF_DES VARCHAR2(3) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_CAMBTECNO_PLAN_TD IS 'Planes homologos en cambio de tecnologia CDMA/TDMA a GSM'
/
COMMENT ON COLUMN  PV_CAMBTECNO_PLAN_TD.COD_PRODUCTO IS 'Codigo Tipo de Plan tarifario 1:prepago 2:postpago 3:hibrido'
/
COMMENT ON COLUMN  PV_CAMBTECNO_PLAN_TD.COD_TECNOLOGIA_ORI IS 'Tecnologia Origen'
/
COMMENT ON COLUMN  PV_CAMBTECNO_PLAN_TD.COD_PLANTARIF_ORI IS 'Plan Origen'
/
COMMENT ON COLUMN  PV_CAMBTECNO_PLAN_TD.COD_TECNOLOGIA_DES IS 'Tecnologia Destino'
/
COMMENT ON COLUMN  PV_CAMBTECNO_PLAN_TD.COD_PLANTARIF_DES IS 'Plan Destino'
/
