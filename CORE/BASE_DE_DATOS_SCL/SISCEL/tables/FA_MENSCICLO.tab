CREATE TABLE FA_MENSCICLO(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_FORMULARIO NUMBER(2,0) NOT NULL 
 ,  COD_BLOQUE NUMBER(1,0) NOT NULL 
 ,  CORR_MENSAJE NUMBER(6,0) NOT NULL 
 ,  COD_ORIGEN VARCHAR2(2) NOT NULL 
 ,  NUM_LINEAS NUMBER(2,0) NOT NULL 
 ,  COD_PRIORIDAD NUMBER(3,0) NULL 
 ,  IND_FACTURADO CHAR(1) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_MENSCICLO IS 'Mensajes por cliente por ciclo.'
/
COMMENT ON COLUMN  FA_MENSCICLO.COD_CLIENTE IS 'Codigo de cliente. [PK 1/5]'
/
COMMENT ON COLUMN  FA_MENSCICLO.COD_FORMULARIO IS 'Codigo de documento de destino. [PK 2/5]'
/
COMMENT ON COLUMN  FA_MENSCICLO.COD_BLOQUE IS 'Codigo de bloque destino mensaje. [PK 3/5]'
/
COMMENT ON COLUMN  FA_MENSCICLO.CORR_MENSAJE IS 'Correlativo creacion mensaje. [PK 4/5]'
/
COMMENT ON COLUMN  FA_MENSCICLO.COD_ORIGEN IS 'Codigo origen del mensaje. [PK 5/5]'
/
COMMENT ON COLUMN  FA_MENSCICLO.NUM_LINEAS IS 'Numero de lineas del mensaje.'
/
COMMENT ON COLUMN  FA_MENSCICLO.COD_PRIORIDAD IS 'Prioridad de mensaje.'
/
COMMENT ON COLUMN  FA_MENSCICLO.IND_FACTURADO IS 'Indicador incluido en la factura: I=ingresado | F=facturado incluido | N=facturado no incluido.'
/
