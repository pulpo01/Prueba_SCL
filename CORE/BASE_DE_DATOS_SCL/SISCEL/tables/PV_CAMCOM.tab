CREATE TABLE PV_CAMCOM
(
  NUM_OS              NUMBER(10)                NOT NULL,
  NUM_ABONADO         NUMBER(8)                 NOT NULL,
  NUM_CELULAR         NUMBER(15)                NOT NULL,
  COD_CLIENTE         NUMBER(8)                 NOT NULL,
  COD_CUENTA          NUMBER(8)                 NOT NULL,
  COD_PRODUCTO        NUMBER(1)                 NOT NULL,
  BDATOS              VARCHAR2(15 BYTE)         NOT NULL,
  NUM_VENTA           NUMBER(8),
  NUM_TRANSACCION     NUMBER(9),
  NUM_FOLIO           NUMBER(9),
  NUM_CUOTAS          NUMBER(2),
  NUM_PROCESO         NUMBER(8),
  COD_ACTABO          VARCHAR2(2 BYTE),
  FH_ANULA            DATE,
  USER_ANULA          VARCHAR2(30 BYTE),
  CAT_TRIBUT          VARCHAR2(1 BYTE),
  COD_ESTADOC         NUMBER(3),
  COD_CAUSAELIM       VARCHAR2(5 BYTE),
  FEC_VENCIMIENTO     DATE,
  ABONADOS            VARCHAR2(1024 BYTE),
  TABLA               VARCHAR2(30 BYTE),
  CLASE_SERVICIO_ACT  VARCHAR2(255 BYTE),
  CLASE_SERVICIO_DES  VARCHAR2(255 BYTE),
  IND_MSG             NUMBER(2),
  NUM_CARGO_OS        NUMBER(14,4),
  IMP_CARGO_SER       NUMBER(14,4),
  IND_CENTRAL_SS      NUMBER(1)                 DEFAULT 1,
  IND_PORTABLE        NUMBER(1)                 DEFAULT 0                     NOT NULL,
  TIP_TERMINAL        VARCHAR2(1 BYTE),
  TIP_SUSP_AVSINIE    VARCHAR2(1 BYTE),
  PREF_PLAZA          VARCHAR2(25 BYTE)
)
TABLESPACE TS_CRM_POSVENTA_SDAT
PCTUSED    0
PCTFREE    5
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          4M
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE PV_CAMCOM IS 'Campos Comunes Ooss.';

COMMENT ON COLUMN PV_CAMCOM.NUM_OS IS 'Identificador Orden de servicio.';

COMMENT ON COLUMN PV_CAMCOM.NUM_ABONADO IS 'Numero de Abonado como sujeto de una OOSS.';

COMMENT ON COLUMN PV_CAMCOM.NUM_CELULAR IS 'Numero de Celular.';

COMMENT ON COLUMN PV_CAMCOM.COD_CLIENTE IS 'Codigo Cliente como sujeto de una OOSS.';

COMMENT ON COLUMN PV_CAMCOM.COD_CUENTA IS 'Codigo Cliente como sujeto de una OOSS.';

COMMENT ON COLUMN PV_CAMCOM.COD_PRODUCTO IS 'Tipo de Producto 1 = celular 2 = Beeper 3= .';

COMMENT ON COLUMN PV_CAMCOM.BDATOS IS 'Base de Datos informada por SAC.';

COMMENT ON COLUMN PV_CAMCOM.NUM_VENTA IS 'Numero de Venta generado por Postventa.';

COMMENT ON COLUMN PV_CAMCOM.NUM_TRANSACCION IS 'Numero de Transaccion asociado a la venta.';

COMMENT ON COLUMN PV_CAMCOM.NUM_FOLIO IS 'Numero de Folio del Documento de Cobro.';

COMMENT ON COLUMN PV_CAMCOM.NUM_CUOTAS IS 'Numero de Cuotas.';

COMMENT ON COLUMN PV_CAMCOM.NUM_PROCESO IS 'Numero de Proceso para Facturacion.';

COMMENT ON COLUMN PV_CAMCOM.COD_ACTABO IS 'Codigo de Actuacion de la OOSS.';

COMMENT ON COLUMN PV_CAMCOM.FH_ANULA IS 'Fecha y Hora en que se anula la OOSS.';

COMMENT ON COLUMN PV_CAMCOM.USER_ANULA IS 'Usuario que anula la OOSS.';

COMMENT ON COLUMN PV_CAMCOM.CAT_TRIBUT IS 'Categoria Tributaria del sujeto de la OOSS.';

COMMENT ON COLUMN PV_CAMCOM.COD_ESTADOC IS 'Estado del documento.';

COMMENT ON COLUMN PV_CAMCOM.COD_CAUSAELIM IS 'Codigo causa de eliminacion E: Error de digitacion; D: duplicidad.';

COMMENT ON COLUMN PV_CAMCOM.FEC_VENCIMIENTO IS 'Fecha de Vencmiento del documento.';

COMMENT ON COLUMN PV_CAMCOM.ABONADOS IS 'abonaos si TIP_SUBSUJETO = AL.';

COMMENT ON COLUMN PV_CAMCOM.TABLA IS 'Fecha de Vencmiento del documento.';

COMMENT ON COLUMN PV_CAMCOM.CLASE_SERVICIO_ACT IS 'Servicios a activar.';

COMMENT ON COLUMN PV_CAMCOM.CLASE_SERVICIO_DES IS 'Servicios a desactivar.';

COMMENT ON COLUMN PV_CAMCOM.IND_MSG IS 'Indicador de envio de mensaje via SMS (1) envio , (0) no envio.';

COMMENT ON COLUMN PV_CAMCOM.NUM_CARGO_OS IS 'Cargo de la solicitud.';

COMMENT ON COLUMN PV_CAMCOM.IMP_CARGO_SER IS 'Cargo por el total del servicio asociado a la solicitud.';

COMMENT ON COLUMN PV_CAMCOM.IND_CENTRAL_SS IS 'valor = 1 significa que envia mov. A central 0 = no envia mov. A central.';

COMMENT ON COLUMN PV_CAMCOM.IND_PORTABLE IS 'Indica si la causa permite portababilidad de numero 0=no portable 1= es portable.';

COMMENT ON COLUMN PV_CAMCOM.TIP_TERMINAL IS 'Tipo Terminal';

COMMENT ON COLUMN PV_CAMCOM.TIP_SUSP_AVSINIE IS 'Hay suspension del servicio.0:no;1:Susp.Bi-direcc';

COMMENT ON COLUMN PV_CAMCOM.PREF_PLAZA IS 'Prefijo Plaza asociado al folio.';

