CREATE TABLE FA_NUMFOLIOSCICLO(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_CICLFACT NUMBER(6,0) NOT NULL 
 ,  NOM_FACTURA VARCHAR2(40) NOT NULL 
 ,  NUM_FOLIO NUMBER(9,0) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_NUMFOLIOSCICLO IS 'Asignacion de numeros de folios a documentos de ciclo-'
/
COMMENT ON COLUMN  FA_NUMFOLIOSCICLO.COD_CLIENTE IS 'Codigo de Cliente'
/
COMMENT ON COLUMN  FA_NUMFOLIOSCICLO.COD_CICLFACT IS 'Codigo de ciclo de facturacion'
/
COMMENT ON COLUMN  FA_NUMFOLIOSCICLO.NOM_FACTURA IS 'Nombre de fichero de factura'
/
COMMENT ON COLUMN  FA_NUMFOLIOSCICLO.NUM_FOLIO IS 'Numero de folio'
/
