CREATE TABLE FA_REGISIMPRESION_TH(
    COD_TIPDOCUM VARCHAR2(5) NOT NULL 
 ,  COD_REGISTRO VARCHAR2(5) NOT NULL 
 ,  FEC_ACTUALIZA DATE NOT NULL 
 ,  IND_IMPRESION NUMBER(1,0) NOT NULL 
 ,  USU_INGRESO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_REGISIMPRESION_TH IS 'Historico de modimiento de registro.'
/
COMMENT ON COLUMN  FA_REGISIMPRESION_TH.COD_TIPDOCUM IS 'Tipo de Documento.'
/
COMMENT ON COLUMN  FA_REGISIMPRESION_TH.COD_REGISTRO IS 'Tipo de Registro.'
/
COMMENT ON COLUMN  FA_REGISIMPRESION_TH.FEC_ACTUALIZA IS 'Fecha del movimiento.'
/
COMMENT ON COLUMN  FA_REGISIMPRESION_TH.IND_IMPRESION IS 'Indicador si el registro es generado en el archivo.'
/
COMMENT ON COLUMN  FA_REGISIMPRESION_TH.USU_INGRESO IS 'Usuario que origino el movimiento.'
/
COMMENT ON COLUMN  FA_REGISIMPRESION_TH.FEC_ULTMOD IS 'Fecha Que fue traspaso al historico.'
/
COMMENT ON COLUMN  FA_REGISIMPRESION_TH.NOM_USUARIO IS 'Usuario que realiza el traspaso.'
/
