CREATE TABLE TA_RUTAS(
    COD_TIPCENTRAL NUMBER(2,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  COD_RUTA VARCHAR2(7) NOT NULL 
 ,  COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  COD_ALOPERADOR NUMBER(5,0) NULL 
 ,  IND_RUTA NUMBER(1,0) NOT NULL 
 ,  COD_ALM VARCHAR2(3) NULL 
 ,  COD_CELDA VARCHAR2(7) NULL 
 ,  NOM_USUARIO VARCHAR2(40) DEFAULT 'TARIFICA' NOT NULL 
 ,  FEC_ULTIMAMOD DATE DEFAULT '01-jan-99' NOT NULL 
 ,  COD_SUBALM VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_RUTAS IS 'Rutas o vinculos de conexion a centrales'
/
COMMENT ON COLUMN  TA_RUTAS.COD_TIPCENTRAL IS 'Tipo de Central'
/
COMMENT ON COLUMN  TA_RUTAS.COD_CENTRAL IS 'Csdigo de Central'
/
COMMENT ON COLUMN  TA_RUTAS.COD_RUTA IS 'Csdigo de Ruta'
/
COMMENT ON COLUMN  TA_RUTAS.COD_OPERADOR IS 'Csdigo de Operador'
/
COMMENT ON COLUMN  TA_RUTAS.COD_ALOPERADOR IS 'Operador por el que sale la ruta'
/
COMMENT ON COLUMN  TA_RUTAS.IND_RUTA IS 'Indicador de Ruta'
/
COMMENT ON COLUMN  TA_RUTAS.COD_ALM IS 'Csdigo de ALM'
/
COMMENT ON COLUMN  TA_RUTAS.COD_CELDA IS 'Csdigo de Celda'
/
COMMENT ON COLUMN  TA_RUTAS.NOM_USUARIO IS 'Almacena Nombre Usuario'
/
COMMENT ON COLUMN  TA_RUTAS.FEC_ULTIMAMOD IS 'Fecha Ultima Modificacion'
/
COMMENT ON COLUMN  TA_RUTAS.COD_SUBALM IS 'Codigo de subalm'
/
