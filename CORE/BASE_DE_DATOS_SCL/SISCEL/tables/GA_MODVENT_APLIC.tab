CREATE TABLE GA_MODVENT_APLIC(
    COD_PRODUCTO NUMBER(2,0) NOT NULL 
 ,  COD_MODVENTA NUMBER(2,0) NOT NULL 
 ,  TIP_STOCK NUMBER(2,0) NOT NULL 
 ,  COD_MODVENTA_NUE NUMBER(2,0) NULL 
 ,  COD_APLIC VARCHAR2(180) NULL 
 ,  COD_CANAL NUMBER(2,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  GA_MODVENT_APLIC.COD_PRODUCTO IS 'Codigo de producto.'
/
COMMENT ON COLUMN  GA_MODVENT_APLIC.COD_MODVENTA IS 'codigo de modalidad de venta.'
/
COMMENT ON COLUMN  GA_MODVENT_APLIC.TIP_STOCK IS 'Tipo de stock, del articulo.'
/
COMMENT ON COLUMN  GA_MODVENT_APLIC.COD_MODVENTA_NUE IS 'Modalidad de venta nueva.'
/
COMMENT ON COLUMN  GA_MODVENT_APLIC.COD_APLIC IS 'Codigo de aplicacion que puede visualizar la modalidad de venta.'
/
COMMENT ON COLUMN  GA_MODVENT_APLIC.COD_CANAL IS 'codigo de canal de venta asociado a la modalidad de venta.'
/
