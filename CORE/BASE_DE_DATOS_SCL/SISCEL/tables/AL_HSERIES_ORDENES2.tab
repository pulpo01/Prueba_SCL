CREATE TABLE AL_HSERIES_ORDENES2
(
  NUM_ORDEN      NUMBER(9)                      NOT NULL,
  TIP_ORDEN      NUMBER(1)                      NOT NULL,
  FEC_HISTORICO  DATE                           NOT NULL,
  NUM_LINEA      NUMBER(2)                      NOT NULL,
  NUM_SERIE      VARCHAR2(25 BYTE)              NOT NULL,
  NUM_SERIEMEC   VARCHAR2(20 BYTE)                  NULL,
  COD_PRODUCTO   NUMBER(1)                          NULL,
  COD_CENTRAL    NUMBER(3)                          NULL,
  CAP_CODE       NUMBER(7)                          NULL,
  NUM_TELEFONO   NUMBER(15)                         NULL,
  COD_SUBALM     VARCHAR2(5 BYTE)                   NULL,
  COD_CAT        NUMBER(2)                          NULL,
  IND_TELEFONO   NUMBER(1)                          NULL,
  PLAN           VARCHAR2(3 BYTE)                   NULL,
  CARGA          NUMBER(14,4)                       NULL,
  A_KEY          VARCHAR2(20 BYTE)                  NULL,
  CHK_DIGITS     VARCHAR2(6 BYTE)                   NULL,
  NUM_SEC_LOCA   NUMBER(10)                         NULL,
  COD_HLR        VARCHAR2(10 BYTE)                  NULL
)
TABLESPACE TS_LOGISTICA_MDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          200M
            NEXT             4M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE AL_HSERIES_ORDENES2 IS 'Historico de Series de Ordenes de Ingreso';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.NUM_SERIEMEC IS 'Numero serie mecanica';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.COD_PRODUCTO IS 'Producto';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.COD_CENTRAL IS 'Central';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.CAP_CODE IS 'Cap Code';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.NUM_TELEFONO IS 'Numero de telefono';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.COD_SUBALM IS 'Subalm';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.COD_CAT IS 'Codigo de Categoria';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.IND_TELEFONO IS 'Tipo de numeracion: 0,1,2';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.PLAN IS 'Plan de Prepago';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.CARGA IS 'Carga de prepago';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.A_KEY IS 'A-KEY Normal (20 primeros decimal)';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.CHK_DIGITS IS 'Codigos de chequeo (los 6 ultimos de la A-KEY decimal.)';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.NUM_SEC_LOCA IS 'Identificador de la Localizacion';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.COD_HLR IS 'Codigo de HLR';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.NUM_ORDEN IS 'Numero de orden';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.TIP_ORDEN IS 'Tipo de orden';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.FEC_HISTORICO IS 'Fecha Histsrico';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.NUM_LINEA IS 'Numero de linea de detalle';

COMMENT ON COLUMN AL_HSERIES_ORDENES2.NUM_SERIE IS 'Numero de serie';

