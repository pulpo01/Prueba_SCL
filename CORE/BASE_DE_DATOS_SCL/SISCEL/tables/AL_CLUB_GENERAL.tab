CREATE TABLE AL_CLUB_GENERAL(
    COD_CONCEPTO_REPOSICION NUMBER(4,0) NULL 
 ,  IMP_CARGO_REPOSICION NUMBER(14,4) NULL 
 ,  COD_CONCEPTO_DTO_REPOSICION NUMBER(4,0) NULL 
 ,  IMP_CARGO_DTO_REPOSICION NUMBER(14,4) NULL 
 ,  COD_MONEDA_REPOSICION VARCHAR2(3) DEFAULT '001' NULL 
 ,  DUR_VIGENCIA NUMBER(2,0) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_CLUB_GENERAL IS 'Tabla De Datos Generales Para Gestion Club Abonados.'
/
COMMENT ON COLUMN  AL_CLUB_GENERAL.COD_CONCEPTO_REPOSICION IS 'Codigo de concepto usado para el cargo de reposicion por robo o extravio.'
/
COMMENT ON COLUMN  AL_CLUB_GENERAL.IMP_CARGO_REPOSICION IS 'Cargo del concepto usado para el cargo de reposicion por robo o extravio, sin IVA.'
/
COMMENT ON COLUMN  AL_CLUB_GENERAL.COD_CONCEPTO_DTO_REPOSICION IS 'Codigo de concepto dto usado para el cargo de reposicion por robo o extravio.'
/
COMMENT ON COLUMN  AL_CLUB_GENERAL.IMP_CARGO_DTO_REPOSICION IS 'Cargo del concepto dto.usado para el cargo de reposicion por robo o extravio , sin IVA.'
/
COMMENT ON COLUMN  AL_CLUB_GENERAL.COD_MONEDA_REPOSICION IS 'codigo de moneda del cargo por reposicion.'
/
COMMENT ON COLUMN  AL_CLUB_GENERAL.DUR_VIGENCIA IS 'Duracion en meses de la vigencia de la tarjeta.'
/
