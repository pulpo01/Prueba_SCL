CREATE TABLE FA_REPORTES_EST_TO(
    COD_REPORTE VARCHAR2(5) NOT NULL 
 ,  FECHA_INICO DATE NOT NULL 
 ,  FECHA_TERMINO DATE NOT NULL 
 ,  CANT_CLIENTES NUMBER(12,0) NULL 
 ,  CANT_DOCUMENTOS NUMBER(12,0) NULL 
 ,  ULT_FECHA_EJECUCION DATE NULL 
 ,  TERMINO_NORMAL VARCHAR2(1) DEFAULT 'S' NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_REPORTES_EST_TO IS '[ REES ] Reporte estadistico'
/
COMMENT ON COLUMN  FA_REPORTES_EST_TO.COD_REPORTE IS 'Código del Reporte'
/
COMMENT ON COLUMN  FA_REPORTES_EST_TO.FECHA_INICO IS 'Fecha Inicio Reporte'
/
COMMENT ON COLUMN  FA_REPORTES_EST_TO.FECHA_TERMINO IS 'Fecha Termino Reporte'
/
COMMENT ON COLUMN  FA_REPORTES_EST_TO.CANT_CLIENTES IS 'Cantidad de clientes Procesados'
/
COMMENT ON COLUMN  FA_REPORTES_EST_TO.CANT_DOCUMENTOS IS 'Cantidad de Documentos'
/
COMMENT ON COLUMN  FA_REPORTES_EST_TO.ULT_FECHA_EJECUCION IS 'Fecha ultima Ejecucion'
/
COMMENT ON COLUMN  FA_REPORTES_EST_TO.TERMINO_NORMAL IS 'Termino Exitoso o no'
/
