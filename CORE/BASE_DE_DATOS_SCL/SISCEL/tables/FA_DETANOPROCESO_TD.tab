CREATE TABLE FA_DETANOPROCESO_TD(
    NUM_PROCESO NUMBER(8,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  SEQ_RECORRIDO NUMBER(3,0) NOT NULL 
 ,  DES_RECORRIDO VARCHAR2(256) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_DETANOPROCESO_TD IS '[ FA_DEAP_TD ] DETALLE ANOMALIAS FACTURACION'
/
COMMENT ON COLUMN  FA_DETANOPROCESO_TD.NUM_PROCESO IS 'Número del proceso de facturación'
/
COMMENT ON COLUMN  FA_DETANOPROCESO_TD.COD_CLIENTE IS 'Código del cliente'
/
COMMENT ON COLUMN  FA_DETANOPROCESO_TD.SEQ_RECORRIDO IS 'Secuencia propia del recorrido del error'
/
COMMENT ON COLUMN  FA_DETANOPROCESO_TD.DES_RECORRIDO IS 'Descripción del recorrido de la anomalía de facturación'
/
