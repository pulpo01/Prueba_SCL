CREATE TABLE GA_NUMROAOPER(
    NUM_DESDE NUMBER(15,0) NOT NULL 
 ,  NUM_HASTA NUMBER(15,0) NOT NULL 
 ,  COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  COD_PAIS VARCHAR2(3) NOT NULL 
 ,  COD_ZONA VARCHAR2(2) NOT NULL 
 ,  NUM_LIBRES NUMBER(8,0) NOT NULL 
 ,  NUM_SIGUIENTE NUMBER(15,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_NUMROAOPER IS 'Numeracion Operadoras Roaming'
/
COMMENT ON COLUMN  GA_NUMROAOPER.NUM_DESDE IS 'Numero desde del rango'
/
COMMENT ON COLUMN  GA_NUMROAOPER.NUM_HASTA IS 'Numero hasta del rango'
/
COMMENT ON COLUMN  GA_NUMROAOPER.COD_OPERADOR IS 'Codigo de Operador'
/
COMMENT ON COLUMN  GA_NUMROAOPER.COD_PAIS IS 'Codigo de Pais'
/
COMMENT ON COLUMN  GA_NUMROAOPER.COD_ZONA IS 'Codigo de Zona'
/
COMMENT ON COLUMN  GA_NUMROAOPER.NUM_LIBRES IS 'Numeros Libres en el Rango'
/
COMMENT ON COLUMN  GA_NUMROAOPER.NUM_SIGUIENTE IS 'Siguiente Numero a Asignar'
/
