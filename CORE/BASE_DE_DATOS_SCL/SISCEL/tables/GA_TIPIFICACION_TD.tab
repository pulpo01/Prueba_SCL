CREATE TABLE GA_TIPIFICACION_TD
(
  COD_TIPIFICACION   VARCHAR2(10 BYTE),
  COD_ARTICULO       NUMBER(6)                  NOT NULL,
  FLAG_CLIENTIZABLE  NUMBER(1),
  NOM_USUARIO        VARCHAR2(30 BYTE)          NOT NULL,
  DES_TIPIFICACION   VARCHAR2(50 BYTE)
)
TABLESPACE TS_CRM_POSVENTA_SDAT
/

COMMENT ON TABLE GA_TIPIFICACION_TD IS '[ TICION ] Tipificacion y Clientizacion de ArticulosTipificacion y Clientizacion de Articulos';

COMMENT ON COLUMN GA_TIPIFICACION_TD.COD_TIPIFICACION IS 'Codigo de Tipificacion';

COMMENT ON COLUMN GA_TIPIFICACION_TD.COD_ARTICULO IS 'Codigo de Articulo';

COMMENT ON COLUMN GA_TIPIFICACION_TD.FLAG_CLIENTIZABLE IS 'Flag Clientizable';

COMMENT ON COLUMN GA_TIPIFICACION_TD.NOM_USUARIO IS 'Nombre de Usuario Registro';

COMMENT ON COLUMN GA_TIPIFICACION_TD.DES_TIPIFICACION IS 'Descripcion de Tipificacion';
