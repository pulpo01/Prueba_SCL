CREATE TABLE GA_CAUSALESMODIFICACION_TO(
    COD_CAUSA NUMBER(4,0) NOT NULL 
 ,  NOMBRE_CAUSA VARCHAR2(40) NOT NULL 
 ,  FECHA_DESDE DATE NOT NULL 
 ,  FECHA_HASTA DATE NULL 
 ,  USUARIO VARCHAR2(40) NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_CAUSALESMODIFICACION_TO IS '[ CAMO ] Tabla que contiene todas las causales de modificación activas en SCL'
/
COMMENT ON COLUMN  GA_CAUSALESMODIFICACION_TO.COD_CAUSA IS 'Código de causa de Modificación'
/
COMMENT ON COLUMN  GA_CAUSALESMODIFICACION_TO.NOMBRE_CAUSA IS 'Texto que describe la causa de modificación'
/
COMMENT ON COLUMN  GA_CAUSALESMODIFICACION_TO.FECHA_DESDE IS 'Fecha de inicio de la vigencia de la causa de modificación'
/
COMMENT ON COLUMN  GA_CAUSALESMODIFICACION_TO.FECHA_HASTA IS 'Fecha de termino de la vigencia de la causa de modificación'
/
COMMENT ON COLUMN  GA_CAUSALESMODIFICACION_TO.USUARIO IS 'Usuario quien crea un registro de causa de modificación'
/
