CREATE TABLE GA_SERVSUPL_COB_TO(
    COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  COD_SERVSUPL NUMBER(2,0) NOT NULL 
 ,  DES_SERVICIO VARCHAR2(50) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(50) NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_SERVSUPL_COB_TO IS '[ SETO ] COBROS DE SERV SUPLEMENTARIOS POR SUSPENSION PROGRAMADACOBROS DE SERV SUPLEMENTARIOS POR SUSPENSION PROGRAMADA'
/
COMMENT ON COLUMN  GA_SERVSUPL_COB_TO.COD_SERVICIO IS 'Codigo de Servicio'
/
COMMENT ON COLUMN  GA_SERVSUPL_COB_TO.COD_SERVSUPL IS 'Codigo de Servicio Suplementario'
/
COMMENT ON COLUMN  GA_SERVSUPL_COB_TO.DES_SERVICIO IS 'Descripcion del Servicio '
/
COMMENT ON COLUMN  GA_SERVSUPL_COB_TO.FEC_INGRESO IS 'Fecha de Ingreso'
/
COMMENT ON COLUMN  GA_SERVSUPL_COB_TO.COD_USUARIO IS 'Usuario BD'
/
