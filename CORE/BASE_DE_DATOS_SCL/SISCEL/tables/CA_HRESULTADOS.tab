CREATE TABLE CA_HRESULTADOS(
    NUM_LLAMADA NUMBER(9,0) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_LLAMADA DATE NOT NULL 
 ,  COD_ATENCION NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_HRESULTADOS IS 'Historico Resultados Llamadas'
/
COMMENT ON COLUMN  CA_HRESULTADOS.NUM_LLAMADA IS 'Numero Llamada'
/
COMMENT ON COLUMN  CA_HRESULTADOS.FEC_HISTORICO IS 'Fecha Historico'
/
COMMENT ON COLUMN  CA_HRESULTADOS.NOM_USUARIO IS 'Usuario creador'
/
COMMENT ON COLUMN  CA_HRESULTADOS.FEC_LLAMADA IS 'Fecha Llamada'
/
COMMENT ON COLUMN  CA_HRESULTADOS.COD_ATENCION IS 'Tipo de atencion'
/
