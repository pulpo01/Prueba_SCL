CREATE TABLE EV_SS_SCORING_TO (  
   NUM_SOLSCORING                NUMBER(10)         NOT NULL 
 , NUM_LINEASCORING              NUMBER(8)          NOT NULL 
 , COD_SERVICIO                  VARCHAR2(3)        NOT NULL 
 , IMPORTE_MENSUAL               NUMBER(14,4)       NOT NULL 
 , COD_MONEDA_MENSUAL            VARCHAR2(10)           NULL 
 , IMPORTE_CONEXION              NUMBER(14,4)           NULL 
 , COD_MONEDA_CONEXION           VARCHAR2(10)           NULL 
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  EV_SS_SCORING_TO                      IS '[ ESSC ] Servicios suplementarios asociados a la línea scoringServicios suplementarios asociados a la línea scoring'
/

COMMENT ON COLUMN EV_SS_SCORING_TO.NUM_SOLSCORING                 IS 'Numero de Solicitud Scoring Asociado'
/
COMMENT ON COLUMN EV_SS_SCORING_TO.NUM_LINEASCORING               IS 'Numero de linea scoring'
/
COMMENT ON COLUMN EV_SS_SCORING_TO.COD_SERVICIO                   IS 'Código servicio'
/
COMMENT ON COLUMN EV_SS_SCORING_TO.IMPORTE_MENSUAL                IS 'Importe Mensual Servicio Suplementario'
/
COMMENT ON COLUMN EV_SS_SCORING_TO.COD_MONEDA_MENSUAL             IS 'Código moneda'
/
COMMENT ON COLUMN EV_SS_SCORING_TO.IMPORTE_CONEXION               IS 'Importe conexión'
/
COMMENT ON COLUMN EV_SS_SCORING_TO.COD_MONEDA_CONEXION            IS 'Código moneda conexión'
/

