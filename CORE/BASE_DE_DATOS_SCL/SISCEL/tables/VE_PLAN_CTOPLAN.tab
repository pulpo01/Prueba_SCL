CREATE TABLE VE_PLAN_CTOPLAN(
    COD_PLANCOM NUMBER(6,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CTOPLAN NUMBER(8,0) NOT NULL 
 ,  COD_CTOFAC NUMBER(4,0) NOT NULL 
 ,  FEC_EFECTIVIDAD DATE NOT NULL 
 ,  FEC_FINEFECTIVIDAD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_PLAN_CTOPLAN IS 'Asociacion de Conceptos de Descuento a Plan Comercial'
/
COMMENT ON COLUMN  VE_PLAN_CTOPLAN.COD_PLANCOM IS 'Codigo de Plan Comercial'
/
COMMENT ON COLUMN  VE_PLAN_CTOPLAN.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  VE_PLAN_CTOPLAN.COD_CTOPLAN IS 'Codigo de Concepto Descuento'
/
COMMENT ON COLUMN  VE_PLAN_CTOPLAN.COD_CTOFAC IS 'Codigo de concepto descuento de facturacion'
/
COMMENT ON COLUMN  VE_PLAN_CTOPLAN.FEC_EFECTIVIDAD IS 'Fecha Efectividad'
/
COMMENT ON COLUMN  VE_PLAN_CTOPLAN.FEC_FINEFECTIVIDAD IS 'Fecha Fin Efectividad'
/
COMMENT ON COLUMN  VE_PLAN_CTOPLAN.NOM_USUARIO IS 'Nombre del usuario que asocia conceptos'
/
