 ,  COD_TIPLAN VARCHAR2(8) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_ACTABO VARCHAR2(2) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_RUTINA_ACTABO_TD IS 'Asocia codigos de accion de cobranza con actuaciones de abonados.'
/
COMMENT ON COLUMN  CO_RUTINA_ACTABO_TD.COD_RUTINA IS 'Codigo accion.'
/
COMMENT ON COLUMN  CO_RUTINA_ACTABO_TD.COD_TIPLAN IS 'Tipo Plan de celular.'
/
COMMENT ON COLUMN  CO_RUTINA_ACTABO_TD.COD_PRODUCTO IS 'Codigo de producto.'
