CREATE TABLE GA_DIRECUSUAR(
    COD_USUARIO NUMBER(8,0) NOT NULL 
 ,  COD_TIPDIRECCION VARCHAR2(1) NOT NULL 
 ,  COD_DIRECCION NUMBER(20,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_DIRECUSUAR IS 'Tabla de Direcciones de Usuario'
/
COMMENT ON COLUMN  GA_DIRECUSUAR.COD_USUARIO IS 'Codigo de Usuario'
/
COMMENT ON COLUMN  GA_DIRECUSUAR.COD_TIPDIRECCION IS 'Codigo de Tipo de Direccion'
/
COMMENT ON COLUMN  GA_DIRECUSUAR.COD_DIRECCION IS 'Codigo de Direccion'
/
