CREATE TABLE CM_CATEGRESULTADOSRET_TD(
    COD_CATEGRESULTADO NUMBER(3,0) NOT NULL 
 ,  DES_CATEGRESULTADO VARCHAR2(50) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_CATEGRESULTADOSRET_TD IS 'CATEGORIA DE LOS RESULTADOS DE LAS RETENCIONES PARA COMISIONES'
/
COMMENT ON COLUMN  CM_CATEGRESULTADOSRET_TD.COD_CATEGRESULTADO IS 'CSDIGO DE CLASIFICACISN DE RESULTADO DE LAS RETENCIONES PARA COMISIONES'
/
COMMENT ON COLUMN  CM_CATEGRESULTADOSRET_TD.DES_CATEGRESULTADO IS 'DESCRIPCISN DE LA CLASIFICACISN DE RETENCISN PARA COMISIONES'
/
COMMENT ON COLUMN  CM_CATEGRESULTADOSRET_TD.FEC_ULTMOD IS 'FECHA DE ZLTIMA MODIFICACISN'
/
COMMENT ON COLUMN  CM_CATEGRESULTADOSRET_TD.NOM_USUARIO IS 'USUARIO QUE REALIZS ZLTIMA MODIFICACISN'
/
