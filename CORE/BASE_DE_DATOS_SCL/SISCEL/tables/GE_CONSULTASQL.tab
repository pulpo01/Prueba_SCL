CREATE TABLE GE_CONSULTASQL(
    COD_CONSULTASQL NUMBER(5,0) NOT NULL 
 ,  NOM_CONSULTASQL VARCHAR2(80) NOT NULL 
 ,  DES_CONSULTASQL VARCHAR2(2000) NOT NULL 
 ,  COD_MODULO VARCHAR2(2) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_CONSULTASQL IS 'Consultas SQL de validacion'
/
COMMENT ON COLUMN  GE_CONSULTASQL.COD_CONSULTASQL IS 'codigo de consulta, se genera en una secuencia'
/
COMMENT ON COLUMN  GE_CONSULTASQL.NOM_CONSULTASQL IS 'nombre de la consulta'
/
COMMENT ON COLUMN  GE_CONSULTASQL.DES_CONSULTASQL IS 'consulta SQL'
/
COMMENT ON COLUMN  GE_CONSULTASQL.COD_MODULO IS 'codigo de modulo a que pertenece la consulta'
/
