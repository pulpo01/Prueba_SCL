CREATE TABLE FA_PARCRIMENS(
    COD_CRITMENS VARCHAR2(3) NOT NULL 
 ,  DESC_CRITMENS VARCHAR2(40) NULL 
 ,  COD_TIPPARAM CHAR(1) NOT NULL 
 ,  IND_PREPROC CHAR(1) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  COD_ORIGEN VARCHAR2(2) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_PARCRIMENS IS 'Parametrizacion criterios de Asignacion de mensajes'
/
COMMENT ON COLUMN  FA_PARCRIMENS.COD_CRITMENS IS 'Codigo de criterio asignacion'
/
COMMENT ON COLUMN  FA_PARCRIMENS.DESC_CRITMENS IS 'Descripcion de criterio asignacion'
/
COMMENT ON COLUMN  FA_PARCRIMENS.COD_TIPPARAM IS 'Tipo de parametro (N/D/V)'
/
COMMENT ON COLUMN  FA_PARCRIMENS.IND_PREPROC IS 'Indicador de analisis pre_procesamiento (1/0)'
/
COMMENT ON COLUMN  FA_PARCRIMENS.NOM_USUARIO IS 'Nombre usuario responsable modificacion'
/
COMMENT ON COLUMN  FA_PARCRIMENS.FEC_ULTMOD IS 'Fecha ultima modificacion'
/
COMMENT ON COLUMN  FA_PARCRIMENS.COD_ORIGEN IS 'Codigo de origen (Modulo)'
/
