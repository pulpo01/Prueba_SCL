CREATE TABLE GA_SECDESPCLIE(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_DESPACHO VARCHAR2(5) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_SECDESPCLIE IS 'Secuencias de Despacho por cliente'
/
COMMENT ON COLUMN  GA_SECDESPCLIE.COD_CLIENTE IS 'Codigo de cliente'
/
COMMENT ON COLUMN  GA_SECDESPCLIE.COD_DESPACHO IS 'Codigo de despacho'
/
COMMENT ON COLUMN  GA_SECDESPCLIE.FEC_DESDE IS 'Fecha inicio periodo'
/
COMMENT ON COLUMN  GA_SECDESPCLIE.FEC_HASTA IS 'Fecha termino periodo'
/
COMMENT ON COLUMN  GA_SECDESPCLIE.NOM_USUARIO IS 'Nombre usuario responsable modificacion'
/
COMMENT ON COLUMN  GA_SECDESPCLIE.FEC_ULTMOD IS 'Fecha ultima modificacion'
/
