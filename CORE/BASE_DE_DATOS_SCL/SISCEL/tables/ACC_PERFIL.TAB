CREATE TABLE ACC_PERFIL(
    COD_PERFIL VARCHAR2(5) NOT NULL 
 ,  GLS_PERFIL VARCHAR2(30) NULL 
 ,  USER_LOG VARCHAR2(10) NOT NULL 
 ,  FEC_LOG DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  ACC_PERFIL.COD_PERFIL IS 'Codigo del perfil'
/
COMMENT ON COLUMN  ACC_PERFIL.GLS_PERFIL IS 'Descripcion del perfil'
/
COMMENT ON COLUMN  ACC_PERFIL.USER_LOG IS 'Codigo del usuario logeado en el Sistema'
/
COMMENT ON COLUMN  ACC_PERFIL.FEC_LOG IS 'Fecha en que el Usuario realizo la transaccion'
/
