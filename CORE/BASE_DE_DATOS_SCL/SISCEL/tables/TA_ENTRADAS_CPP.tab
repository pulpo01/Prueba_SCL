CREATE TABLE TA_ENTRADAS_CPP(
    FECHA_LLAMADA DATE NULL 
 ,  HORA_LLAMADA VARCHAR2(6) NULL 
 ,  NUMERO_ORIGEN VARCHAR2(15) NULL 
 ,  MIN_DESTINO VARCHAR2(15) NULL 
 ,  TIEMPO_SEND_ANS VARCHAR2(6) NULL 
 ,  TIME_ANS_END VARCHAR2(6) NULL 
 ,  DURACION_TOTAL_SEG NUMBER(6,0) NULL 
 ,  DURACION_TOTAL_MINI NUMBER(6,0) NULL 
 ,  RUTA_SALIDA VARCHAR2(6) NULL 
 ,  RUTA_ENTRADA VARCHAR2(6) NULL 
 ,  ANOMALIA VARCHAR2(3) NULL 
 ,  COD_SERV VARCHAR2(2) NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_ENTRADAS_CPP IS 'Llamadas de entrada con alguna anomalia.'
/
COMMENT ON COLUMN  TA_ENTRADAS_CPP.FECHA_LLAMADA IS 'Fecha de la llamada.'
/
COMMENT ON COLUMN  TA_ENTRADAS_CPP.HORA_LLAMADA IS 'Hora de la llamada.'
/
COMMENT ON COLUMN  TA_ENTRADAS_CPP.NUMERO_ORIGEN IS 'Numero que origina la llamada: MIN origen.'
/
COMMENT ON COLUMN  TA_ENTRADAS_CPP.MIN_DESTINO IS 'Numero que recibe la llamada: MIN destino.'
/
COMMENT ON COLUMN  TA_ENTRADAS_CPP.TIEMPO_SEND_ANS IS 'HH:MM:SS en que se inicia el send.'
/
COMMENT ON COLUMN  TA_ENTRADAS_CPP.TIME_ANS_END IS 'HH:MM:SS en que se inicia la respuesta.'
/
COMMENT ON COLUMN  TA_ENTRADAS_CPP.DURACION_TOTAL_SEG IS 'Duracion de la llamada en segundos.'
/
COMMENT ON COLUMN  TA_ENTRADAS_CPP.DURACION_TOTAL_MINI IS 'Duracion de la llamada en minutos, ajustada al minuto superior.'
/
COMMENT ON COLUMN  TA_ENTRADAS_CPP.RUTA_SALIDA IS 'Ruta de salida de la llamada.'
/
COMMENT ON COLUMN  TA_ENTRADAS_CPP.RUTA_ENTRADA IS 'Ruta de entrada de la llamada.'
/
COMMENT ON COLUMN  TA_ENTRADAS_CPP.ANOMALIA IS 'Codigo de la anomalia.'
/
