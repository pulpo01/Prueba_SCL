CREATE TABLE CM_CONCEPTOSTIPORED_TD(
    COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) NOT NULL 
 ,  COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  NIV_SELECCION NUMBER(2,0) NOT NULL 
 ,  NIV_APLICACION NUMBER(2,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_CONCEPTOSTIPORED_TD IS 'ASOCIACION DE CONCEPTOS A LOS TIPOS DE REDES'
/
COMMENT ON COLUMN  CM_CONCEPTOSTIPORED_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
COMMENT ON COLUMN  CM_CONCEPTOSTIPORED_TD.COD_PLANCOMIS IS 'CODIGO DEL PLAN DE COMISIONES'
/
COMMENT ON COLUMN  CM_CONCEPTOSTIPORED_TD.COD_CONCEPTO IS 'CODIGO DEL CONCEPTO'
/
COMMENT ON COLUMN  CM_CONCEPTOSTIPORED_TD.NIV_SELECCION IS 'NIVEL AL QUE SE APLICA LA SELECCION '
/
COMMENT ON COLUMN  CM_CONCEPTOSTIPORED_TD.NIV_APLICACION IS 'NIVEL AL QUE SE APLICA LA LIQUIDACION'
/
COMMENT ON COLUMN  CM_CONCEPTOSTIPORED_TD.FEC_ULTMOD IS 'FECHA ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  CM_CONCEPTOSTIPORED_TD.NOM_USUARIO IS 'USUARIO ULTIMA MODIFICACION'
/
