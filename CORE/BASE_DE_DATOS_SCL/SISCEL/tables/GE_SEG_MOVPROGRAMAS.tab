CREATE TABLE GE_SEG_MOVPROGRAMAS(
    COD_PROGRAMA VARCHAR2(3) NOT NULL 
 ,  NUM_VERSION NUMBER(2,0) NOT NULL 
 ,  NUM_SUBVERSION NUMBER(4,0) NOT NULL 
 ,  NOM_EXE VARCHAR2(20) NOT NULL 
 ,  FEC_SOLICITUD DATE NOT NULL 
 ,  NOM_USUARIO_S VARCHAR2(30) NOT NULL 
 ,  FEC_TRANSMISION DATE NOT NULL 
 ,  NOM_USUARIO_T VARCHAR2(30) NULL 
 ,  FEC_PROCESO DATE NULL 
 ,  IND_PROCESO VARCHAR2(1) NOT NULL 
 ,  OBSERVACIONES VARCHAR2(255) NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_SEG_MOVPROGRAMAS IS 'BITACORA DE ACTUALIZACISN DE VERSIONES'
/
COMMENT ON COLUMN  GE_SEG_MOVPROGRAMAS.COD_PROGRAMA IS 'CODIGO DE PROGRA EJECUTABLE (SEGURDAD)'
/
COMMENT ON COLUMN  GE_SEG_MOVPROGRAMAS.NUM_VERSION IS 'NUMERO DE VERSION DEL EJECUTABLE'
/
COMMENT ON COLUMN  GE_SEG_MOVPROGRAMAS.NUM_SUBVERSION IS 'NUMERO DE SUBVERSION DEL EJECUTABLE'
/
COMMENT ON COLUMN  GE_SEG_MOVPROGRAMAS.NOM_EXE IS 'NOMBRE FISICO DEL EJECUTABLE'
/
COMMENT ON COLUMN  GE_SEG_MOVPROGRAMAS.FEC_SOLICITUD IS 'FECHA INGRESO SOLICITIUD'
/
COMMENT ON COLUMN  GE_SEG_MOVPROGRAMAS.NOM_USUARIO_S IS 'NOMBRE DE USUARIO ORACLE SOLICITANTE'
/
COMMENT ON COLUMN  GE_SEG_MOVPROGRAMAS.FEC_TRANSMISION IS 'FECHA SOLICITA TRANSMISION'
/
COMMENT ON COLUMN  GE_SEG_MOVPROGRAMAS.NOM_USUARIO_T IS 'NOMBRE DE USUARIO ORACLE QUE TRANSMITE'
/
COMMENT ON COLUMN  GE_SEG_MOVPROGRAMAS.FEC_PROCESO IS 'FECHA EN QUE SE REALIA LA TRANSMISION'
/
COMMENT ON COLUMN  GE_SEG_MOVPROGRAMAS.IND_PROCESO IS 'TIPO DE TRANSMINISON 1:NORMAL 2:INMEDIATA 3:FECHA'
/
COMMENT ON COLUMN  GE_SEG_MOVPROGRAMAS.OBSERVACIONES IS 'OBSERVACIONES'
/
