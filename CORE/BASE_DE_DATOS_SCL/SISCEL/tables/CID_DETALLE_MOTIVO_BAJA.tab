 ,  DES_DETALLE_BAJA VARCHAR2(60) NOT NULL 
 ,  COD_VIGENCIA CHAR(1) NOT NULL 
 ,  TIP_INCIDENCIA NUMBER(3,0) NULL 
 ,  COD_CAUSABAJA VARCHAR2(2) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  CID_DETALLE_MOTIVO_BAJA.TIP_INCIDENCIA IS 'Indica el codigo de reclamo analogo.'
/
COMMENT ON COLUMN  CID_DETALLE_MOTIVO_BAJA.COD_CAUSABAJA IS 'Codigo de Causas de Baja'
/
