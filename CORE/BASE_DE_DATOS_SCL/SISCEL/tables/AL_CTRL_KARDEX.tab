CREATE TABLE AL_CTRL_KARDEX(
    NUM_MOVIMIENTO NUMBER(12,0) NOT NULL 
 ,  FEC_TRASPASO DATE NOT NULL 
 ,  CANT_REG NUMBER(25,0) NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  DES_RESPUESTA VARCHAR2(80) NULL 
 ,  FEC_INICIO DATE NULL 
 ) TABLESPACE    TS_LOGISTICA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_CTRL_KARDEX IS 'Tabla De Resumen y control de traspaso de moviminentos a Kardex'
/
COMMENT ON COLUMN  AL_CTRL_KARDEX.NUM_MOVIMIENTO IS 'Ultimo movimiento procesado'
/
COMMENT ON COLUMN  AL_CTRL_KARDEX.FEC_TRASPASO IS 'Fecha de proceso'
/
COMMENT ON COLUMN  AL_CTRL_KARDEX.CANT_REG IS 'Cantidad de registros procesados'
/
COMMENT ON COLUMN  AL_CTRL_KARDEX.COD_ESTADO IS 'Estado de el proceso'
/
COMMENT ON COLUMN  AL_CTRL_KARDEX.NOM_USUARIO IS 'Usuario'
/
COMMENT ON COLUMN  AL_CTRL_KARDEX.DES_RESPUESTA IS 'glosa de resultado del proceso'
/
