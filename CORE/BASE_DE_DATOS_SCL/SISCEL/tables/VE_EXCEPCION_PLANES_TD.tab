CREATE TABLE VE_EXCEPCION_PLANES_TD (  
   COD_PLANTARIF                 VARCHAR2(3)        NOT NULL   
 , COD_PRODUCTO                  NUMBER(1)          NOT NULL   
 , FEC_DESDE                     DATE               NOT NULL   
 , FEC_HASTA                     DATE               NOT NULL   
 , FEC_MODIFICACION              DATE               NOT NULL   
 , NOM_USUARORA                  VARCHAR2(30)       NOT NULL 
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  VE_EXCEPCION_PLANES_TD                      IS '[ VE_EXPL ] Planes excepcionados de procesos de actualizacionPlanes excepcionados de procesos de actualizacion'
/

COMMENT ON COLUMN VE_EXCEPCION_PLANES_TD.COD_PLANTARIF                  IS 'codigo plan tarifario'
/
COMMENT ON COLUMN VE_EXCEPCION_PLANES_TD.COD_PRODUCTO                   IS 'codigo producto'
/
COMMENT ON COLUMN VE_EXCEPCION_PLANES_TD.FEC_DESDE                      IS 'fecha inicio vigencia'
/
COMMENT ON COLUMN VE_EXCEPCION_PLANES_TD.FEC_HASTA                      IS 'fecha fin vigencia'
/
COMMENT ON COLUMN VE_EXCEPCION_PLANES_TD.FEC_MODIFICACION               IS 'fecha modificación'
/
COMMENT ON COLUMN VE_EXCEPCION_PLANES_TD.NOM_USUARORA                   IS 'usuario modificación'
/

