CREATE TABLE VE_CALIFICACION_TARIFARIO_TD
(
  COD_CALIFICACION  VARCHAR2(5 BYTE),
  COD_PLANTARIF     VARCHAR2(3 BYTE),
  NOM_USUARIO       VARCHAR2(50 BYTE),
  FEC_CREACION      DATE,
  IND_VIGENCIA      NUMBER(1)
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          1M
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING
/
COMMENT ON COLUMN VE_CALIFICACION_TARIFARIO_TD.COD_CALIFICACION IS 'Codigo Calificacion';
/
COMMENT ON COLUMN VE_CALIFICACION_TARIFARIO_TD.COD_PLANTARIF IS 'Codigo Plan Tarifario';
/
COMMENT ON COLUMN VE_CALIFICACION_TARIFARIO_TD.NOM_USUARIO IS 'Nombre Usuario';
/
COMMENT ON COLUMN VE_CALIFICACION_TARIFARIO_TD.FEC_CREACION IS 'Fecha Creacion';
/
COMMENT ON COLUMN VE_CALIFICACION_TARIFARIO_TD.IND_VIGENCIA IS 'Indicador Vigencia';
/
