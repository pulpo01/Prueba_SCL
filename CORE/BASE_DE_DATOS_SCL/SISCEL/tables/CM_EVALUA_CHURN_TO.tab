 ,  COD_CICLCOMIS NUMBER(8,0) NOT NULL 
 ,  ID_CICLCOMIS VARCHAR2(10) NOT NULL 
 ,  NUM_ALTAS NUMBER(6,0) NOT NULL 
 ,  NUM_BAJAS NUMBER(6,0) NOT NULL 
 ,  NUM_CHURN NUMBER(14,4) NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  COD_TIPVENDEDOR VARCHAR2(2) DEFAULT 'C' NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
