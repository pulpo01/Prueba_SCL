 ,  FEC_OOSS DATE NOT NULL 
 ,  OBS_OOSS VARCHAR2(250) NULL 
 ,  NUM_MOVIMIENTO NUMBER(9,0) NULL 
 ,  VAL_OOSS NUMBER(5,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
