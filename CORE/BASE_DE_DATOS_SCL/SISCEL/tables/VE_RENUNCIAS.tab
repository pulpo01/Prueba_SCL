CREATE TABLE VE_RENUNCIAS(
    COD_RENUNCIA NUMBER(8,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ,  FEC_RENUNCIA DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_RENUNCIAS IS 'Renuncias de los abonados del sistema'
/
COMMENT ON COLUMN  VE_RENUNCIAS.COD_RENUNCIA IS 'Codigo de la renuncia'
/
COMMENT ON COLUMN  VE_RENUNCIAS.COD_TIPCOMIS IS 'Codigo de tipo de comisionista'
/
COMMENT ON COLUMN  VE_RENUNCIAS.COD_VENDEDOR IS 'Codigo de vendedor'
/
COMMENT ON COLUMN  VE_RENUNCIAS.COD_CLIENTE IS 'Codigo de cliente al que esta asociado el abonado'
/
COMMENT ON COLUMN  VE_RENUNCIAS.NUM_ABONADO IS 'Numero del abonado que renuncia al sistema'
/
COMMENT ON COLUMN  VE_RENUNCIAS.COD_PRODUCTO IS 'Codigo del Producto'
/
COMMENT ON COLUMN  VE_RENUNCIAS.COD_OFICINA IS 'Codigo de la Oficina'
/
COMMENT ON COLUMN  VE_RENUNCIAS.FEC_ALTA IS 'Fecha de alta de una venta en el sistema.'
/
COMMENT ON COLUMN  VE_RENUNCIAS.FEC_RENUNCIA IS 'Fecha de la renuncia'
/
