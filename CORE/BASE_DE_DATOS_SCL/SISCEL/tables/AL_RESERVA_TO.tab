CREATE TABLE AL_RESERVA_TO(
    NUM_SOLICITUD NUMBER(8,0) NOT NULL 
 ,  CANTIDAD NUMBER(6,0) NOT NULL 
 ,  COD_ACCION NUMBER(1,0) NOT NULL 
 ,  TIPO_PROCESO NUMBER(1,0) NOT NULL 
 ,  COD_SUBALM VARCHAR2(5) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  COD_CAT NUMBER(2,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  FEC_SOLICITUD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_RESERVA_TO IS 'Cabecera de Auditoria Reserva de Numeros Almacen.'
/
COMMENT ON COLUMN  AL_RESERVA_TO.NUM_SOLICITUD IS 'Numero de Solicitud asociado a una reserva o desreserva de numeros.'
/
COMMENT ON COLUMN  AL_RESERVA_TO.CANTIDAD IS 'Cantidad de numeros reservados o resreservados para la solicitud.'
/
COMMENT ON COLUMN  AL_RESERVA_TO.COD_ACCION IS 'Indicador de Reserva(0) o Desreserva(1).'
/
COMMENT ON COLUMN  AL_RESERVA_TO.TIPO_PROCESO IS 'Indica si la reserva o desreserva se hizo en forma aleatoria(0), puntual(1), masiva(2) o por rangos(3).'
/
COMMENT ON COLUMN  AL_RESERVA_TO.COD_SUBALM IS 'Codigo de subalm.'
/
COMMENT ON COLUMN  AL_RESERVA_TO.COD_CENTRAL IS 'Codigo de Central.'
/
COMMENT ON COLUMN  AL_RESERVA_TO.COD_CAT IS 'Codigo de Categoria.'
/
COMMENT ON COLUMN  AL_RESERVA_TO.COD_USO IS 'Codigo de Uso.'
/
COMMENT ON COLUMN  AL_RESERVA_TO.COD_PRODUCTO IS 'Codigo de Producto.'
/
COMMENT ON COLUMN  AL_RESERVA_TO.FEC_SOLICITUD IS 'Fecha en que se genero la solicitud.'
/
COMMENT ON COLUMN  AL_RESERVA_TO.NOM_USUARIO IS 'Nombre de usuario que genero la solicitud.'
/
