CREATE TABLE GE_TIPABONADOS_TD(
    COD_MODULO VARCHAR2(2) NOT NULL 
 ,  COD_TIPO_ABON NUMBER(3,0) NOT NULL 
 ,  DES_TIPO_ABON VARCHAR2(30) NOT NULL 
 ,  IND_RETENCION CHAR(1) NOT NULL 
 ,  IND_VIGENCIA CHAR(1) NOT NULL 
 ,  OBSERVACION VARCHAR2(400) NULL 
 ,  MIN_TIPO NUMBER(4,0) NULL 
 ,  MAX_TIPO NUMBER(4,0) NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_TIPABONADOS_TD IS 'TIPOS DE ABONADO'
/
COMMENT ON COLUMN  GE_TIPABONADOS_TD.COD_MODULO IS 'CODIGO DE MODULO (APLICACION)'
/
COMMENT ON COLUMN  GE_TIPABONADOS_TD.COD_TIPO_ABON IS 'CODIGO DE TIPO DE ABONADO'
/
COMMENT ON COLUMN  GE_TIPABONADOS_TD.DES_TIPO_ABON IS 'DESCRIPCION DEL TIPO DE ABONADO'
/
COMMENT ON COLUMN  GE_TIPABONADOS_TD.IND_RETENCION IS 'INDICADOR DE RETENCION'
/
COMMENT ON COLUMN  GE_TIPABONADOS_TD.IND_VIGENCIA IS 'INDICADOR DE VIGENCIA'
/
COMMENT ON COLUMN  GE_TIPABONADOS_TD.OBSERVACION IS 'DETALLE DEL TIPO DE ABONADO'
/
COMMENT ON COLUMN  GE_TIPABONADOS_TD.MIN_TIPO IS 'VALOR MINIMO DEL RANGO PARA EL TIPO'
/
COMMENT ON COLUMN  GE_TIPABONADOS_TD.MAX_TIPO IS 'VALOR MAXIMO DEL RANGO PARA EL TIPO'
/
