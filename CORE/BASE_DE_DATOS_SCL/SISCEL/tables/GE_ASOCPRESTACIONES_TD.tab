CREATE TABLE GE_ASOCPRESTACIONES_TD
(
  ID_ASOC      VARCHAR2(5 BYTE)                 NOT NULL,
  ASOCORIGEN   VARCHAR2(5 BYTE)                 NOT NULL,
  ASOCDESTINO  VARCHAR2(5 BYTE)                 NOT NULL,
  USO          VARCHAR2(10 BYTE)				,
  FEC_MOD      DATE                             DEFAULT SYSDATE,
  USUARIO      VARCHAR2(35 BYTE)                DEFAULT USER
)
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON COLUMN GE_ASOCPRESTACIONES_TD.ID_ASOC IS 'Id de la asociación';

COMMENT ON COLUMN GE_ASOCPRESTACIONES_TD.ASOCORIGEN IS 'Asociación Origen';

COMMENT ON COLUMN GE_ASOCPRESTACIONES_TD.ASOCDESTINO IS 'Asociación destino';

COMMENT ON COLUMN GE_ASOCPRESTACIONES_TD.FEC_MOD IS 'Fecha ultima modificacón';

COMMENT ON COLUMN GE_ASOCPRESTACIONES_TD.USUARIO IS 'Usuario que creo o modificó el registro';

COMMENT ON COLUMN GE_ASOCPRESTACIONES_TD.USO IS 'Código de Uso';

SHOW ERRORS