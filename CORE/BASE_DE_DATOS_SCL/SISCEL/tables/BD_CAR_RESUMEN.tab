 ,  CAR_FINAL NUMBER(7,0) NOT NULL 
 ,  CAR_INICIAL NUMBER(7,0) NOT NULL 
 ,  TOT_HABILITACIONES NUMBER(7,0) DEFAULT 0 NOT NULL 
 ,  TOT_MIGRACIONES NUMBER(7,0) DEFAULT 0 NOT NULL 
 ,  ACUM_VENACEP NUMBER(7,0) DEFAULT 0 NOT NULL 
 ,  ACUM_VENPENACEP NUMBER(7,0) DEFAULT 0 NOT NULL 
 ,  ACUM_VENPENRECEP NUMBER(7,0) DEFAULT 0 NOT NULL 
 ,  ACUM_VENPENCUMPL NUMBER(7,0) DEFAULT 0 NOT NULL 
 ,  ACUM_ANULBAJA NUMBER(7,0) DEFAULT 0 NOT NULL 
 ,  ACUM_DEBLOQUEO NUMBER(7,0) DEFAULT 0 NOT NULL 
 ,  ACUM_BAJAVOLUN NUMBER(7,0) DEFAULT 0 NOT NULL 
 ,  ACUM_BAJAMORA NUMBER(7,0) DEFAULT 0 NOT NULL 
 ,  ACUM_BLOQUEO NUMBER(7,0) DEFAULT 0 NOT NULL 
 ,  ACUM_RECHAZO NUMBER(7,0) DEFAULT 0 NOT NULL 
 ,  ACUM_ANULAVTA NUMBER(7,0) DEFAULT 0 NOT NULL 
