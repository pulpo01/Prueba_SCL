CREATE TABLE SP_MONTO_ASEGURADORA(
    COD_ASEG NUMBER(3,0) NOT NULL 
 ,  COD_NIVEL VARCHAR2(5) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  IMP_NIVEL NUMBER(11,4) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SP_MONTO_ASEGURADORA IS 'Montos Entregados Por Aseguradora Por Nivel De Reparacion.'
/
COMMENT ON COLUMN  SP_MONTO_ASEGURADORA.COD_ASEG IS 'Codigo de la aseguradora.'
/
COMMENT ON COLUMN  SP_MONTO_ASEGURADORA.COD_NIVEL IS 'Codigo del Nivel de la reparacion a la que se asocia el importe.'
/
COMMENT ON COLUMN  SP_MONTO_ASEGURADORA.COD_MONEDA IS 'Moneda en que se representa el importe.'
/
COMMENT ON COLUMN  SP_MONTO_ASEGURADORA.IMP_NIVEL IS 'Importe del nivel.'
/
COMMENT ON COLUMN  SP_MONTO_ASEGURADORA.NOM_USUARIO IS 'Nombre de usuario que genera ocurrencia.'
/
COMMENT ON COLUMN  SP_MONTO_ASEGURADORA.FEC_ULTMOD IS 'Fecha en que se genera la ocurrencia.'
/
