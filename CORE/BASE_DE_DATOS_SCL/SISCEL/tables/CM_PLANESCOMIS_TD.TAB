CREATE TABLE CM_PLANESCOMIS_TD(
    COD_PLANCOMIS VARCHAR2(6) NOT NULL 
 ,  DES_PLANCOMIS VARCHAR2(60) NOT NULL 
 ,  TIP_CICLCOMIS CHAR(1) NOT NULL 
 ,  ID_CICLCOMIS VARCHAR2(10) NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_PLANESCOMIS_TD IS 'PLANES DE COMISIONES'
/
COMMENT ON COLUMN  CM_PLANESCOMIS_TD.COD_PLANCOMIS IS 'CODIGO DEL PLAN DE COMISIONES.'
/
COMMENT ON COLUMN  CM_PLANESCOMIS_TD.DES_PLANCOMIS IS 'NOMBRE DEL PLAN DE COMISIONES.'
/
COMMENT ON COLUMN  CM_PLANESCOMIS_TD.TIP_CICLCOMIS IS 'TIPO DE CICLO AL QUE ESTA ORIENTADO (PERIODICO / ESPORADICO).'
/
COMMENT ON COLUMN  CM_PLANESCOMIS_TD.ID_CICLCOMIS IS 'EN CASO DE ESPORADICO, EL IDENTIFICADOR DEL CICLO.'
/
COMMENT ON COLUMN  CM_PLANESCOMIS_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  CM_PLANESCOMIS_TD.NOM_USUARIO IS 'USUARIO QUE HIZO LA ULTIMA MODIFICACION.'
/
