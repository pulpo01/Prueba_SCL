CREATE TABLE CA_PANTALLAS(
    COD_PROGRAMA VARCHAR2(3) NOT NULL 
 ,  NUM_VERSION NUMBER(2,0) NOT NULL 
 ,  COD_PROCESO VARCHAR2(7) NOT NULL 
 ,  FEC_DESDE_DH DATE NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_PANTALLAS IS 'Pantallas a las que se puede acceder desde el SAC.'
/
COMMENT ON COLUMN  CA_PANTALLAS.COD_PROGRAMA IS 'Programa'
/
COMMENT ON COLUMN  CA_PANTALLAS.NUM_VERSION IS 'Numero de version'
/
COMMENT ON COLUMN  CA_PANTALLAS.COD_PROCESO IS 'Codigo de proceso'
/
