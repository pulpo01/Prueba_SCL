CREATE TABLE GA_AUDREST(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  COD_SITUACION VARCHAR2(3) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ,  FEC_RESTAURA DATE NOT NULL 
 ,  NOM_PROC VARCHAR2(30) NOT NULL 
 ,  NUM_VENTA NUMBER(8,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_AUDREST IS 'Tabla auditora de  restauracion de Ventas y Cambios de Series'
/
COMMENT ON COLUMN  GA_AUDREST.NUM_ABONADO IS 'Numero de abonado Celular'
/
COMMENT ON COLUMN  GA_AUDREST.NUM_CELULAR IS 'Numero celular'
/
COMMENT ON COLUMN  GA_AUDREST.NUM_SERIE IS 'Numero de Serie decimal'
/
COMMENT ON COLUMN  GA_AUDREST.COD_SITUACION IS 'Situacion en que se encuentra Abonado'
/
COMMENT ON COLUMN  GA_AUDREST.NOM_USUARORA IS ' '
/
COMMENT ON COLUMN  GA_AUDREST.FEC_ALTA IS 'Fecha de Ingreso de Venta en SISCEL'
/
COMMENT ON COLUMN  GA_AUDREST.FEC_RESTAURA IS 'Fecha del proceso de restauracion'
/
COMMENT ON COLUMN  GA_AUDREST.NOM_PROC IS 'Proceso de Restauracion'
/
COMMENT ON COLUMN  GA_AUDREST.NUM_VENTA IS 'Numero de Venta restaurada.'
/
