CREATE TABLE FA_RANGOSHOST_TO(
    COD_CICLFACT NUMBER(6,0) NOT NULL 
 ,  HOST_ID VARCHAR2(20) NOT NULL 
 ,  COD_CLIENTEINI NUMBER(10,0) NOT NULL 
 ,  COD_CLIENTEFIN NUMBER(10,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_RANGOSHOST_TO IS '[ RNGH ] Rangos de Clientes por MáquinaRangos de Clientes por Máquina'
/
COMMENT ON COLUMN  FA_RANGOSHOST_TO.COD_CICLFACT IS 'Código de Ciclo de Facturación'
/
COMMENT ON COLUMN  FA_RANGOSHOST_TO.HOST_ID IS 'Indentificador Máquina'
/
COMMENT ON COLUMN  FA_RANGOSHOST_TO.COD_CLIENTEINI IS 'Cliente Incial para Rango de ejecución en la Máquina indicada'
/
COMMENT ON COLUMN  FA_RANGOSHOST_TO.COD_CLIENTEFIN IS 'Cliente Final para Rango de ejecución en la Máquina indicada'
/
COMMENT ON COLUMN  FA_RANGOSHOST_TO.FEC_ULTMOD IS 'Última Modificación del Registro'
/
COMMENT ON COLUMN  FA_RANGOSHOST_TO.NOM_USUARIO IS 'Nombre Usuario que Modifica Registro'
/
