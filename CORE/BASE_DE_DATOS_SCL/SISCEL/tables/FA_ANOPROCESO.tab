CREATE TABLE FA_ANOPROCESO(
    NUM_PROCESO NUMBER(8,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NULL 
 ,  COD_CICLFACT NUMBER(6,0) NULL 
 ,  NUM_ABONADO NUMBER(8,0) NULL 
 ,  DES_PROCESO VARCHAR2(40) NULL 
 ,  COD_ANOMALIA NUMBER(4,0) NULL 
 ,  OBS_ANOMALIA VARCHAR2(100) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_ANOPROCESO IS 'Anomalias de procesos de Factuiracion'
/
COMMENT ON COLUMN  FA_ANOPROCESO.NUM_PROCESO IS 'SECUENCIA DEL PROCESO EN EJECUCION EN FACTURACION'
/
COMMENT ON COLUMN  FA_ANOPROCESO.COD_CLIENTE IS 'CODIGO DEL CLIENTE'
/
COMMENT ON COLUMN  FA_ANOPROCESO.COD_CONCEPTO IS 'CODIGO DEL CONCEPTO'
/
COMMENT ON COLUMN  FA_ANOPROCESO.COD_PRODUCTO IS 'Producto al que pertenece el concepto - Retrofitted'
/
COMMENT ON COLUMN  FA_ANOPROCESO.COD_CICLFACT IS 'CODIGO DEL CICLO DE FACTURACION'
/
COMMENT ON COLUMN  FA_ANOPROCESO.NUM_ABONADO IS 'NUMERO DE ABONADO'
/
COMMENT ON COLUMN  FA_ANOPROCESO.DES_PROCESO IS 'DESCRIPCION DE LA ANOMALIA'
/
COMMENT ON COLUMN  FA_ANOPROCESO.COD_ANOMALIA IS 'Codigo de Anomalia'
/
COMMENT ON COLUMN  FA_ANOPROCESO.OBS_ANOMALIA IS 'OBSERVACION SOBRE LA ANOMALIA DEL PROCESO'
/
