CREATE TABLE CM_VALRETENCIONES_TD(
    COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) NOT NULL 
 ,  COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_VALRETENCIONES_TD IS 'SE CONFIGURA LA MATRIZ PARA LAS METAS DE RETENCIONES'
/
COMMENT ON COLUMN  CM_VALRETENCIONES_TD.COD_TIPORED IS 'CSDIGO DEL TIPO DE RED'
/
COMMENT ON COLUMN  CM_VALRETENCIONES_TD.COD_PLANCOMIS IS 'CSDIGO DEL PLAN DE COMISIONES'
/
COMMENT ON COLUMN  CM_VALRETENCIONES_TD.COD_CONCEPTO IS 'CSDIGO DEL CONCEPTO'
/
COMMENT ON COLUMN  CM_VALRETENCIONES_TD.FEC_ULTMOD IS 'FECHA DE ZLTIMA MODIFICACISN'
/
COMMENT ON COLUMN  CM_VALRETENCIONES_TD.NOM_USUARIO IS 'USUARIO QUE REALIZS ZLTIMA MODIFICACISN'
/
