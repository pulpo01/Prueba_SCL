CREATE TABLE SISCEL.AL_VENTAS
(
  NUM_VENTA            NUMBER(8)                NOT NULL,
  COD_ESTADO           VARCHAR2(2 BYTE)         NOT NULL,
  COD_PRODUCTO         NUMBER(1)                NOT NULL,
  COD_OFICINA          VARCHAR2(2 BYTE)         NOT NULL,
  COD_TIPCOMIS         VARCHAR2(2 BYTE)         NOT NULL,
  COD_VENDEDOR         NUMBER(6)                NOT NULL,
  COD_VENDEDOR_AGENTE  NUMBER(6)                NOT NULL,
  NUM_UNIDADES         NUMBER(6)                NOT NULL,
  FEC_VENTA            DATE                     NOT NULL,
  COD_REGION           VARCHAR2(3 BYTE)         NOT NULL,
  COD_PROVINCIA        VARCHAR2(5 BYTE)         NOT NULL,
  COD_CIUDAD           VARCHAR2(5 BYTE)         NOT NULL,
  IND_ESTVENTA         VARCHAR2(2 BYTE)         NOT NULL,
  NUM_TRANSACCION      NUMBER(9)                DEFAULT 0                     NOT NULL,
  NOM_USUAR_VTA        VARCHAR2(30 BYTE)        DEFAULT 'SISCEL'              NOT NULL,
  IND_VENTA            CHAR(1 BYTE)             DEFAULT 'V'                   NOT NULL,
  IMP_VENTA            NUMBER(14,4),
  IND_TIPVENTA         NUMBER(1),
  COD_CLIENTE          NUMBER(8),
  COD_MODVENTA         NUMBER(2),
  TIP_VALOR            NUMBER(2),
  COD_TIPTARJETA       VARCHAR2(3 BYTE),
  NUM_TARJETA          VARCHAR2(18 BYTE),
  COD_AUTTARJ          VARCHAR2(20 BYTE),
  FEC_VENCITARJ        DATE,
  COD_BANCOTARJ        VARCHAR2(15 BYTE),
  NUM_CTACORR          VARCHAR2(18 BYTE),
  NUM_CHEQUE           VARCHAR2(20 BYTE),
  COD_BANCO            VARCHAR2(15 BYTE),
  COD_SUCURSAL         VARCHAR2(4 BYTE),
  COD_TIPDOCUM         NUMBER(2),
  NUM_FOLIO            NUMBER(9),
  NOM_USUAPROC         VARCHAR2(30 BYTE),
  FEC_PROCESO          DATE,
  FEC_ACEPREC          DATE,
  SQLCODE              VARCHAR2(10 BYTE),
  SQLERRM              VARCHAR2(1024 BYTE),
  FEC_VENCIMIENTO      DATE,
  IND_ACTIVACION       NUMBER(1),
  COD_CUENTA           NUMBER(8),
  COD_PEDIDO           NUMBER(12),
  TIP_VENTA            VARCHAR2(1 BYTE),
  COD_OPERADORA        VARCHAR2(5 BYTE),
  PREF_PLAZA           VARCHAR2(25 BYTE),
  TIP_FOLIACION        CHAR(1 BYTE)
)  TABLESPACE TS_LOGISTICA_SDAT
/

COMMENT ON TABLE SISCEL.AL_VENTAS IS 'Ventas Diferidas A Distribuidores.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.NUM_VENTA IS 'Numero de Venta.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_ESTADO IS 'Estado de la Venta, PD-Pendiente, CE-Finalizada.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_PRODUCTO IS 'Codigo de Producto.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_OFICINA IS 'Codigo de Oficina.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_TIPCOMIS IS 'Codigo de Tipo de Comisionista.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_VENDEDOR IS 'Codigo del Vendedor.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_VENDEDOR_AGENTE IS 'Codigo de Agente.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.NUM_UNIDADES IS 'Numero de Unidades Vendidas.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.FEC_VENTA IS 'Fecha de Venta.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_REGION IS 'Codigo de Region.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_PROVINCIA IS 'Codigo de Provincia.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_CIUDAD IS 'Codigo de Ciudad.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.IND_ESTVENTA IS 'Indicativo de Estado de la Venta (PA) / Pendiente de aceptar / (PC) Pendiente de cumplimentar / (PD).';

COMMENT ON COLUMN SISCEL.AL_VENTAS.NUM_TRANSACCION IS 'Numero de Transaccion.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.NOM_USUAR_VTA IS 'Usuario que introduce la venta.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.IND_VENTA IS 'Indicativo de Venta ; (V)enta, (E)quipo cambio,(R)ent a Phone.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.IMP_VENTA IS 'Importe del Presupuesto de Venta.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.IND_TIPVENTA IS 'Indicativo de Tipo de Venta.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_CLIENTE IS 'Codigo de Cliente.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_MODVENTA IS 'Codigo de Modalidad de Venta.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.TIP_VALOR IS 'Tipo de Valor de Pago de Venta.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_TIPTARJETA IS 'Codigo de Tipo de Tarjeta.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.NUM_TARJETA IS 'Numero de Tarjeta.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_AUTTARJ IS 'Codigo de Autorizacion de Tarjeta.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.FEC_VENCITARJ IS 'Fecha de Vencimiento de Tarjeta.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_BANCOTARJ IS 'Codigo de Banco de la Tarjeta.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.NUM_CTACORR IS 'Numero de Cuenta Corriente.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.NUM_CHEQUE IS 'Numero de Cheque.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_BANCO IS 'Codigo de Banco del Cheque.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_SUCURSAL IS 'Codigo de Sucursal del Cheque.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_TIPDOCUM IS 'Tipo de documento.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.NUM_FOLIO IS 'Numero de folio.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.NOM_USUAPROC IS 'Usuario que lanza el proceso diferido.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.FEC_PROCESO IS 'Fecha de ejecucion del proceso diferido.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.FEC_ACEPREC IS 'Fecha de Aceptacion de venta.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.SQLCODE IS 'Codigo de error, si lo hubo,del proceso diferido.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.SQLERRM IS 'Mensaje de error, si lo hubo,del proceso diferido.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.FEC_VENCIMIENTO IS 'Fecha de vencimiento del pago.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.IND_ACTIVACION IS '1 - activacion de las series restringidas';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_PEDIDO IS 'Numero de Pedido de WEB.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.TIP_VENTA IS 'clasifica el tipo de venta, si es realizada por la WEB W = Web.';

COMMENT ON COLUMN SISCEL.AL_VENTAS.COD_OPERADORA IS 'Codigo de Operadora';

COMMENT ON COLUMN SISCEL.AL_VENTAS.PREF_PLAZA IS 'Prefijo de folio de la plaza';

COMMENT ON COLUMN SISCEL.AL_VENTAS.TIP_FOLIACION IS 'Tipo de Folialcion A: autof; P: Prefol';
