CREATE TABLE GA_TREKNUM_CENTRAL(
    NUM_DESDE NUMBER(15,0) NOT NULL 
 ,  NUM_HASTA NUMBER(15,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  NUM_SIGUIENTE NUMBER(15,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_TREKNUM_CENTRAL IS 'Numeracion trek abierta por central'
/
COMMENT ON COLUMN  GA_TREKNUM_CENTRAL.NUM_DESDE IS 'Numero desde del rango'
/
COMMENT ON COLUMN  GA_TREKNUM_CENTRAL.NUM_HASTA IS 'Numero hasta del rango'
/
COMMENT ON COLUMN  GA_TREKNUM_CENTRAL.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_TREKNUM_CENTRAL.COD_CENTRAL IS 'Codigo de Central'
/
COMMENT ON COLUMN  GA_TREKNUM_CENTRAL.NUM_SIGUIENTE IS 'Siguiente numero libre - Retrofitted'
/
