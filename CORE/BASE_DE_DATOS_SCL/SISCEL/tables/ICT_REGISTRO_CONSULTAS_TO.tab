/
COMMENT ON COLUMN  ICT_REGISTRO_CONSULTAS_TO.COD_TECNOLOGIA IS 'CSDIGO DE LA TECNOLOGMA'
/
COMMENT ON COLUMN  ICT_REGISTRO_CONSULTAS_TO.NUM_ABONADO IS 'SECUENCIA NUMERO ABONADO'
/
COMMENT ON COLUMN  ICT_REGISTRO_CONSULTAS_TO.NUM_OS IS 'NZMERO DE ORDEN DE SERVICIO'
