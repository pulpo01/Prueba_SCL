CREATE TABLE ICT_REGISTRO_CONSULTAS_TO(
    NUM_SEQ_CONSULTA NUMBER(18,0) NOT NULL 
 ,  COD_MOTIVO NUMBER(18,0) NOT NULL 
 ,  COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  NUM_ESN VARCHAR2(25) NOT NULL 
 ,  COD_TECNOLOGIA VARCHAR2(7) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  NUM_OS NUMBER(10,0) NULL 
 ,  NUM_RECLAMO NUMBER(10,0) NULL 
 ,  FEC_ALTA DATE NULL 
 ,  FEC_BAJA DATE NULL 
 ,  FEC_CAMBIOSER DATE NULL 
 ,  VAL_MTO_PAGADO NUMBER(18,0) NULL 
 ,  COD_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ,  FEC_CONSULTA DATE NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
