CREATE TABLE AL_TIPOS_MOVIMIENTOS(
    TIP_MOVIMIENTO NUMBER(2,0) NOT NULL 
 ,  DES_MOVIMIENTO VARCHAR2(20) NOT NULL 
 ,  IND_ENTSAL VARCHAR2(1) NOT NULL 
 ,  TIP_STOCK NUMBER(2,0) NULL 
 ,  CAT_MOVIMIENTO VARCHAR2(1) DEFAULT 'N' NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_TIPOS_MOVIMIENTOS IS 'Tipos de movimientos posibles determinando si son: ENTRADA, SALIDA,TRASPASO'
/
COMMENT ON COLUMN  AL_TIPOS_MOVIMIENTOS.TIP_MOVIMIENTO IS 'Tipo de movimiento'
/
COMMENT ON COLUMN  AL_TIPOS_MOVIMIENTOS.DES_MOVIMIENTO IS 'Descripcion del movimiento'
/
COMMENT ON COLUMN  AL_TIPOS_MOVIMIENTOS.IND_ENTSAL IS 'Entrada / Salida'
/
COMMENT ON COLUMN  AL_TIPOS_MOVIMIENTOS.TIP_STOCK IS 'Tipo de Stock Resultante'
/
COMMENT ON COLUMN  AL_TIPOS_MOVIMIENTOS.CAT_MOVIMIENTO IS 'Indicador de categorma del movimiento.'
/
