CREATE TABLE CO_HISTDETCONVENIO
(
  NUM_CONVENIO        NUMBER(8)                 NOT NULL,
  NUM_SECUENCI        NUMBER(8)                 NOT NULL,
  FEC_HISTORICO       DATE                      NOT NULL,
  TIPO_CONVENIO       VARCHAR2(5 BYTE)          NOT NULL,
  NUM_PAGARE          NUMBER(8),
  NUM_SECUENCI_CA     NUMBER(10)                NOT NULL,
  COD_TIPDOCUM_CA     NUMBER(2)                 NOT NULL,
  COD_VEND_AGENTE_CA  NUMBER(6),
  LETRA_CA            VARCHAR2(1 BYTE),
  COD_CLIENTE         NUMBER(8)                 NOT NULL,
  NUM_FOLIO_CA        NUMBER(9)                 NOT NULL,
  NUM_CUOTA_CA        NUMBER(8),
  SEC_CUOTA_CA        NUMBER(3),
  IMPORTE_CA          NUMBER(14,4)              NOT NULL,
  PREF_PLAZA          VARCHAR2(25 BYTE)
)
TABLESPACE TS_RECYCOB_SDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          40K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE CO_HISTDETCONVENIO IS 'Historico Detalle Convenio';

COMMENT ON COLUMN CO_HISTDETCONVENIO.NUM_CONVENIO IS 'Numero Convenio';

COMMENT ON COLUMN CO_HISTDETCONVENIO.NUM_SECUENCI IS 'Numero de Secuencia';

COMMENT ON COLUMN CO_HISTDETCONVENIO.FEC_HISTORICO IS 'Fecha paso a Historico';

COMMENT ON COLUMN CO_HISTDETCONVENIO.TIPO_CONVENIO IS 'Tipo Convenio';

COMMENT ON COLUMN CO_HISTDETCONVENIO.NUM_PAGARE IS 'Numero Pagare';

COMMENT ON COLUMN CO_HISTDETCONVENIO.NUM_SECUENCI_CA IS 'Numero de Secuencia co_carte.';

COMMENT ON COLUMN CO_HISTDETCONVENIO.COD_TIPDOCUM_CA IS 'Tipo Documento co_cartera';

COMMENT ON COLUMN CO_HISTDETCONVENIO.COD_VEND_AGENTE_CA IS 'Csdigo Vendedor co_cartera';

COMMENT ON COLUMN CO_HISTDETCONVENIO.LETRA_CA IS 'Letra co_cartera';

COMMENT ON COLUMN CO_HISTDETCONVENIO.COD_CLIENTE IS 'Csdigo Cliente';

COMMENT ON COLUMN CO_HISTDETCONVENIO.NUM_FOLIO_CA IS 'Nzmero de Folio co_cartera';

COMMENT ON COLUMN CO_HISTDETCONVENIO.NUM_CUOTA_CA IS 'Nzmero de Cuota co_cartera';

COMMENT ON COLUMN CO_HISTDETCONVENIO.SEC_CUOTA_CA IS 'Importe co_cartera';

COMMENT ON COLUMN CO_HISTDETCONVENIO.PREF_PLAZA IS 'Prefijo Plaza.';

