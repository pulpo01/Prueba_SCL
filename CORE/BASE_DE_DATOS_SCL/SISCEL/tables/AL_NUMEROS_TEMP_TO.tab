CREATE TABLE AL_NUMEROS_TEMP_TO(
    NUM_RESERVA NUMBER(9,0) NOT NULL 
 ,  NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  COD_ERROR NUMBER(2,0) NOT NULL 
 ,  PROC_INVOCADOR VARCHAR2(50) NOT NULL 
 ,  FEC_CREACION DATE DEFAULT SYSDATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(50) DEFAULT USER NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_NUMEROS_TEMP_TO IS 'TABLA TEMPORAL PARA CARGA MASIVA DE NUMEROS'
/
COMMENT ON COLUMN  AL_NUMEROS_TEMP_TO.NUM_RESERVA IS 'NUMERO DE RESERVA-DESRESERVA'
/
COMMENT ON COLUMN  AL_NUMEROS_TEMP_TO.NUM_CELULAR IS 'NUMERO CELULAR'
/
COMMENT ON COLUMN  AL_NUMEROS_TEMP_TO.COD_ERROR IS 'CODIGO ERROR 0 SIN ERROR'
/
COMMENT ON COLUMN  AL_NUMEROS_TEMP_TO.PROC_INVOCADOR IS 'PROCESO INVOCADOR 1)"RSV" RESERVA'
/
COMMENT ON COLUMN  AL_NUMEROS_TEMP_TO.FEC_CREACION IS 'FECHA DE CREACION REGISTRO'
/
COMMENT ON COLUMN  AL_NUMEROS_TEMP_TO.NOM_USUARIO IS 'USUAIO '
/
