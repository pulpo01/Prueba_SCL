CREATE TABLE EV_SALIDAWS_SCORING_TO
(
  NUM_SOLSCORING  NUMBER(10)                    NOT NULL,
  MENSAJE         VARCHAR2(200 BYTE)            NOT NULL,
  REFERENCIA      VARCHAR2(5 BYTE),
  CLASIFICACION   CHAR(1 BYTE),
  PUNTEO          VARCHAR2(20 BYTE)
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          1M
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE EV_SALIDAWS_SCORING_TO IS '[ ESSC ] Registro  de salida de evaluación de SCORINGRegistro  de salida de evaluación de SCORING';

COMMENT ON COLUMN EV_SALIDAWS_SCORING_TO.NUM_SOLSCORING IS 'Numero de Solicitud Scoring Asociado';

COMMENT ON COLUMN EV_SALIDAWS_SCORING_TO.MENSAJE IS 'Resultado analisis evaluación crediticia WS';

COMMENT ON COLUMN EV_SALIDAWS_SCORING_TO.REFERENCIA IS 'Resultado referencia WS';

COMMENT ON COLUMN EV_SALIDAWS_SCORING_TO.CLASIFICACION IS 'Resultado clasificación WS';

COMMENT ON COLUMN EV_SALIDAWS_SCORING_TO.PUNTEO IS 'Resultado Punteo WS';
