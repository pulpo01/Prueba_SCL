CREATE TABLE CRH_ESTAEVENTOS(
    NUM_EVENTO NUMBER(5,0) NOT NULL 
 ,  COD_ESTADOEVE NUMBER(2,0) NOT NULL 
 ,  FEC_INGRESO_DH DATE NOT NULL 
 ,  FEC_ESTADO_DH DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  GLS_ESTAEVENTO VARCHAR2(500) NULL 
 ,  NUM_CORREO NUMBER(3,0) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CRH_ESTAEVENTOS IS 'Tabla de seguimientos de estados de evento.'
/
COMMENT ON COLUMN  CRH_ESTAEVENTOS.NUM_EVENTO IS 'Numero de evento.'
/
COMMENT ON COLUMN  CRH_ESTAEVENTOS.FEC_INGRESO_DH IS 'Fecha de registro del cambio de estado.'
/
COMMENT ON COLUMN  CRH_ESTAEVENTOS.FEC_ESTADO_DH IS 'Fecha real del cambio de estado.'
/
COMMENT ON COLUMN  CRH_ESTAEVENTOS.NOM_USUARIO IS 'Usuario que registra el cambio de estado del evento.'
/
COMMENT ON COLUMN  CRH_ESTAEVENTOS.GLS_ESTAEVENTO IS 'Glosa o descripcion asociada al cambio de estado del evento.'
/
COMMENT ON COLUMN  CRH_ESTAEVENTOS.NUM_CORREO IS 'Codigo de correo asociado al estado registrado para el evento.'
/
