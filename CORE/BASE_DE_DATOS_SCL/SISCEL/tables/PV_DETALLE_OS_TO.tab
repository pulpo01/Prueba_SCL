CREATE TABLE PV_DETALLE_OS_TO(
    NUM_OS NUMBER(38,0) NOT NULL 
 ,  COD_ERROR NUMBER(6,0) NULL 
 ,  DES_ERROR VARCHAR2(3000) NULL 
 ,  ESTADO VARCHAR2(15) NOT NULL 
 ,  FEC_ESTADO DATE NOT NULL 
 ,  NUM_EVENTO NUMBER(9,0) NULL 
 ,  NUM_PROCESO NUMBER(38,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(38,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(38,0) NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_DETALLE_OS_TO IS '[ DETO ] Detalle del Proceso de Ordenes de Servicio'
/
COMMENT ON COLUMN  PV_DETALLE_OS_TO.NUM_OS IS 'Número de la Orden de Servicio'
/
COMMENT ON COLUMN  PV_DETALLE_OS_TO.COD_ERROR IS 'Código de Error del proceso de la Orden de Servicio'
/
COMMENT ON COLUMN  PV_DETALLE_OS_TO.DES_ERROR IS 'Descripción de Error del proceso de la Orden de Servicio'
/
COMMENT ON COLUMN  PV_DETALLE_OS_TO.ESTADO IS 'Estado del Proceso de la Orden de Servicio'
/
COMMENT ON COLUMN  PV_DETALLE_OS_TO.FEC_ESTADO IS 'Fecha del Estado del proceso de la Orden de Servicio'
/
COMMENT ON COLUMN  PV_DETALLE_OS_TO.NUM_EVENTO IS 'Número de Evento del proceso de la Orden de Servicio'
/
COMMENT ON COLUMN  PV_DETALLE_OS_TO.NUM_PROCESO IS 'Número de Proceso de la Orden de Servicio'
/
COMMENT ON COLUMN  PV_DETALLE_OS_TO.NUM_ABONADO IS 'Número de Abonado al que se le ejecuta la OS'
/
COMMENT ON COLUMN  PV_DETALLE_OS_TO.COD_CLIENTE IS 'Código del Cliente al que pertenece el Abonado'
/
