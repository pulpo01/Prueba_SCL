CREATE TABLE VE_RESLIQUIDAC(
    NUM_LIQUIDACION NUMBER(8,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CTOLIQ NUMBER(6,0) NOT NULL 
 ,  IMP_COMISION NUMBER(14,4) NOT NULL 
 ,  IND_ACEPTADO NUMBER(1,0) NOT NULL 
 ,  COD_CUADRANTE NUMBER(8,0) NOT NULL 
 ,  NUM_META NUMBER(12,4) NULL 
 ,  IMP_BASE NUMBER(12,4) NULL 
 ,  NUM_LIMITE NUMBER(4,0) NULL 
 ,  NUM_ANOMALIA NUMBER(8,0) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_RESLIQUIDAC IS 'Resultado de cada proceso de liquidacion de comisiones'
/
COMMENT ON COLUMN  VE_RESLIQUIDAC.NUM_LIQUIDACION IS 'Numero del proceso de liquidacion'
/
COMMENT ON COLUMN  VE_RESLIQUIDAC.COD_TIPCOMIS IS 'Codigo de Tipo de Comisionista'
/
COMMENT ON COLUMN  VE_RESLIQUIDAC.COD_VENDEDOR IS 'Codigo del vendedor comisionista'
/
COMMENT ON COLUMN  VE_RESLIQUIDAC.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  VE_RESLIQUIDAC.COD_CTOLIQ IS 'Codigo del concepto de liquidacion'
/
COMMENT ON COLUMN  VE_RESLIQUIDAC.IMP_COMISION IS 'Importe del concepto de liquidacion'
/
COMMENT ON COLUMN  VE_RESLIQUIDAC.IND_ACEPTADO IS 'Indicativo de Resultado aceptado (0) - No / (1) - Si'
/
COMMENT ON COLUMN  VE_RESLIQUIDAC.COD_CUADRANTE IS 'Codigo de cuadrante para el concepto'
/
COMMENT ON COLUMN  VE_RESLIQUIDAC.NUM_META IS 'Meta (si existe)'
/
COMMENT ON COLUMN  VE_RESLIQUIDAC.IMP_BASE IS 'Importe para la Base (si existe)'
/
COMMENT ON COLUMN  VE_RESLIQUIDAC.NUM_LIMITE IS 'Limite del importe Base (si existe)'
/
COMMENT ON COLUMN  VE_RESLIQUIDAC.NUM_ANOMALIA IS 'Codigo de Anomalia si el proceso de liquidacion falla'
/
