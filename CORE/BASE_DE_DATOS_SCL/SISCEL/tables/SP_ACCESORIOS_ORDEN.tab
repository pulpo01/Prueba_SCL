CREATE TABLE SP_ACCESORIOS_ORDEN(
    NUM_ORDEN NUMBER(9,0) NOT NULL 
 ,  NUM_LINEA NUMBER(2,0) NOT NULL 
 ,  TIP_STOCK NUMBER(2,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_ESTADO NUMBER(2,0) NOT NULL 
 ,  CANTIDAD NUMBER(2,0) NOT NULL 
 ,  CAN_ORIGINAL NUMBER(2,0) NULL 
 ,  CAN_FALLA NUMBER(2,0) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SP_ACCESORIOS_ORDEN IS 'Accesorios que recibimos del cliente con el equipo a reparar'
/
COMMENT ON COLUMN  SP_ACCESORIOS_ORDEN.NUM_ORDEN IS 'Numero de Orden de Reparacion a la que pertenecen los accesorios'
/
COMMENT ON COLUMN  SP_ACCESORIOS_ORDEN.NUM_LINEA IS 'Numero de linea de accesorios para la orden de reparacion'
/
COMMENT ON COLUMN  SP_ACCESORIOS_ORDEN.TIP_STOCK IS 'Tipo de Stock de los accesorios'
/
COMMENT ON COLUMN  SP_ACCESORIOS_ORDEN.COD_ARTICULO IS 'Codigo del articulo que tiene el accesorio'
/
COMMENT ON COLUMN  SP_ACCESORIOS_ORDEN.COD_USO IS 'Codigo del uso que tiene el accesorio'
/
COMMENT ON COLUMN  SP_ACCESORIOS_ORDEN.COD_ESTADO IS 'Codigo del estado que tiene el accesorio'
/
COMMENT ON COLUMN  SP_ACCESORIOS_ORDEN.CANTIDAD IS 'Cantidad de accesorios.'
/
COMMENT ON COLUMN  SP_ACCESORIOS_ORDEN.CAN_ORIGINAL IS 'Cantidad de accesorios originales'
/
COMMENT ON COLUMN  SP_ACCESORIOS_ORDEN.CAN_FALLA IS 'Cantidad de accesorios con falla.'
/
