CREATE TABLE PV_COMBINATORIA_OS_TD(
    COD_OS VARCHAR2(5) NOT NULL 
 ,  NOM_COMBINATORIA VARCHAR2(50) NOT NULL 
 ,  FLG_ESTADO NUMBER(1,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_COMBINATORIA_OS_TD IS '[ COMBINOS ] Configuración combinatoria para ordenes de servicio.'
/
COMMENT ON COLUMN  PV_COMBINATORIA_OS_TD.COD_OS IS 'Código de orden de servicio.'
/
COMMENT ON COLUMN  PV_COMBINATORIA_OS_TD.NOM_COMBINATORIA IS 'Nombre combinatoria.'
/
COMMENT ON COLUMN  PV_COMBINATORIA_OS_TD.FLG_ESTADO IS 'Estado (1:activada, 0:desactivada)'
/
COMMENT ON COLUMN  PV_COMBINATORIA_OS_TD.NOM_USUARIO IS 'Nombre usuario'
/
COMMENT ON COLUMN  PV_COMBINATORIA_OS_TD.FEC_ULTMOD IS 'Fecha modificación'
/
