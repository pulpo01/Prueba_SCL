CREATE TABLE GSM_DEFINICION_PREFIJO_TO(
    COD_PREFIJO VARCHAR2(10) NOT NULL 
 ,  COD_CAMPO VARCHAR2(10) NOT NULL 
 ,  COD_PARAMETRO VARCHAR2(10) NOT NULL 
 ,  NUM_SEQ_ORDEN NUMBER NOT NULL 
 ,  NUM_VALOR_CAMPO VARCHAR2(18) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_DEFINICION_PREFIJO_TO IS 'Tabla de definicion de prefijos y sus campos.'
/
COMMENT ON COLUMN  GSM_DEFINICION_PREFIJO_TO.COD_PREFIJO IS 'Codigo del prefijo.'
/
COMMENT ON COLUMN  GSM_DEFINICION_PREFIJO_TO.COD_CAMPO IS 'Codigo de Campo.'
/
COMMENT ON COLUMN  GSM_DEFINICION_PREFIJO_TO.COD_PARAMETRO IS 'Codigo del parametro.'
/
COMMENT ON COLUMN  GSM_DEFINICION_PREFIJO_TO.NUM_SEQ_ORDEN IS 'Secuencial de Orden.'
/
COMMENT ON COLUMN  GSM_DEFINICION_PREFIJO_TO.NUM_VALOR_CAMPO IS 'Valor del Campo.'
/
