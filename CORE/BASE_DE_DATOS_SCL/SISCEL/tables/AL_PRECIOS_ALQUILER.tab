CREATE TABLE AL_PRECIOS_ALQUILER(
    COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  PRC_DIA NUMBER(14,4) NULL 
 ,  PRC_SEMANA NUMBER(14,4) NULL 
 ,  PRC_MES NUMBER(14,4) NULL 
 ,  PRC_ARTIC NUMBER(14,4) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_PRECIOS_ALQUILER IS 'Tarifas de Alquiler de Articulos'
/
COMMENT ON COLUMN  AL_PRECIOS_ALQUILER.COD_ARTICULO IS 'Csdigo de Artmculo'
/
COMMENT ON COLUMN  AL_PRECIOS_ALQUILER.FEC_DESDE IS 'Fecha en la que entra en vigor la tarifa de alquiler'
/
COMMENT ON COLUMN  AL_PRECIOS_ALQUILER.FEC_HASTA IS 'Fecha en la que pierde vigor la tarifa de alquiler'
/
COMMENT ON COLUMN  AL_PRECIOS_ALQUILER.COD_MONEDA IS 'Moneda en la que se encuentran definidos los precios'
/
COMMENT ON COLUMN  AL_PRECIOS_ALQUILER.PRC_DIA IS 'Precio de alquiler por dma'
/
COMMENT ON COLUMN  AL_PRECIOS_ALQUILER.PRC_SEMANA IS 'Precio de alquiler por semana'
/
COMMENT ON COLUMN  AL_PRECIOS_ALQUILER.PRC_MES IS 'Precio de alquiler por mes'
/
COMMENT ON COLUMN  AL_PRECIOS_ALQUILER.PRC_ARTIC IS 'Precio que se cobrara al cliente en caso de no devolucion'
/
