CREATE TABLE GE_TIPTARJETAS(
    COD_TIPTARJETA VARCHAR2(3) NOT NULL 
 ,  DES_TIPTARJETA VARCHAR2(40) NOT NULL 
 ,  IND_VALBIN NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  COD_TARJETA_ASOC VARCHAR2(3) NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_TIPTARJETAS IS 'Tipos de Tarjeta'
/
COMMENT ON COLUMN  GE_TIPTARJETAS.COD_TIPTARJETA IS 'Codigo de Tipo de Tarjeta'
/
COMMENT ON COLUMN  GE_TIPTARJETAS.DES_TIPTARJETA IS 'Descripcion de Tipo de Tarjeta'
/
COMMENT ON COLUMN  GE_TIPTARJETAS.IND_VALBIN IS 'Indicador de validacion de BIN para el tipo de tarjeta (1=valida, 0=no valida)'
/
COMMENT ON COLUMN  GE_TIPTARJETAS.COD_TARJETA_ASOC IS 'Tipo de tarjeta asociada por defecto para validacion de BIN'
/
