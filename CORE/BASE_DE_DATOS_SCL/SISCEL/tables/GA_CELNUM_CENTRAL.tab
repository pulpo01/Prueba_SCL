CREATE TABLE GA_CELNUM_CENTRAL(
    NUM_DESDE NUMBER(15,0) NOT NULL 
 ,  NUM_HASTA NUMBER(15,0) NOT NULL 
 ,  COD_SUBALM VARCHAR2(5) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  IND_EQUIPADO NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_CELNUM_CENTRAL IS 'Numeracion celular abierta por central'
/
COMMENT ON COLUMN  GA_CELNUM_CENTRAL.NUM_DESDE IS 'Numero desde del rango'
/
COMMENT ON COLUMN  GA_CELNUM_CENTRAL.NUM_HASTA IS 'Numero hasta del rango'
/
COMMENT ON COLUMN  GA_CELNUM_CENTRAL.COD_SUBALM IS 'Codigo de Subalm'
/
COMMENT ON COLUMN  GA_CELNUM_CENTRAL.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_CELNUM_CENTRAL.COD_CENTRAL IS 'Codigo de Central'
/
COMMENT ON COLUMN  GA_CELNUM_CENTRAL.IND_EQUIPADO IS 'Indicativo de equipado ; (0) desequipado (1) equipado'
/
