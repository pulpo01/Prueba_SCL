CREATE TABLE VE_CUADARTICULOS(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CATEGORIA VARCHAR2(3) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  IMP_VTAMNT NUMBER(12,4) NOT NULL 
 ,  IMP_VTAPCT NUMBER(5,2) NOT NULL 
 ,  LIQ_MONPCT NUMBER(1,0) NOT NULL 
 ,  IMP_ALQPCT NUMBER(5,2) NOT NULL 
 ,  FEC_EFECTIVIDAD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_FINEFECTIVIDAD DATE NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CUADARTICULOS IS 'Cuadrante de Articulos/Accesorios'
/
COMMENT ON COLUMN  VE_CUADARTICULOS.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  VE_CUADARTICULOS.COD_CATEGORIA IS 'Codigo de categoria del Articulo/Accesorio'
/
COMMENT ON COLUMN  VE_CUADARTICULOS.COD_TIPCOMIS IS 'Codigo de Tipo de Comisionista'
/
COMMENT ON COLUMN  VE_CUADARTICULOS.IMP_VTAMNT IS 'Importe a pagar en monto por venta'
/
COMMENT ON COLUMN  VE_CUADARTICULOS.IMP_VTAPCT IS 'Importe a pagar en porcentaje por venta'
/
COMMENT ON COLUMN  VE_CUADARTICULOS.LIQ_MONPCT IS 'Indica si comisiona por monto (0) o porcentaje (1)'
/
COMMENT ON COLUMN  VE_CUADARTICULOS.IMP_ALQPCT IS 'Importe a pagar en porcentaje por alquiler'
/
COMMENT ON COLUMN  VE_CUADARTICULOS.FEC_EFECTIVIDAD IS 'Fecha de efectividad'
/
COMMENT ON COLUMN  VE_CUADARTICULOS.NOM_USUARIO IS 'Usuario que comisiona/descomisiona'
/
COMMENT ON COLUMN  VE_CUADARTICULOS.FEC_FINEFECTIVIDAD IS 'Fecha de fin de efectividad'
/
