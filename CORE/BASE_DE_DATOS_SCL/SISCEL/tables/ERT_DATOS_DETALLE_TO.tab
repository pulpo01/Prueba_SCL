CREATE TABLE ERT_DATOS_DETALLE_TO(
    NUM_IDENT VARCHAR2(20) NOT NULL 
 ,  COD_TIPIDENT VARCHAR2(2) NOT NULL 
 ,  FECHA_ACTUALIZACION DATE NOT NULL 
 ,  IND_TIPO_SOLICITUD NUMBER(2,0) NOT NULL 
 ,  APELLIDO VARCHAR2(16) NULL 
 ,  INGRESOS NUMBER(10,0) NULL 
 ,  COD_RESPUESTA_REG99 VARCHAR2(2) NULL 
 ,  CALIFICACION_REG20 VARCHAR2(7) NULL 
 ,  CLASIFICACION_REG20 VARCHAR2(1) NULL 
 ,  GCC_REG20 VARCHAR2(5) NULL 
 ,  GAMA_REG20 VARCHAR2(5) NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERT_DATOS_DETALLE_TO IS '[ DADE ] Información de Score de trama de datacréditoInformación de Score de trama de datacrédito'
/
COMMENT ON COLUMN  ERT_DATOS_DETALLE_TO.NUM_IDENT IS 'Num_ident consultado a la fuente externa'
/
COMMENT ON COLUMN  ERT_DATOS_DETALLE_TO.COD_TIPIDENT IS 'Tipo de identificacion'
/
COMMENT ON COLUMN  ERT_DATOS_DETALLE_TO.FECHA_ACTUALIZACION IS 'Fecha de actualización de datos de la fuente externa'
/
COMMENT ON COLUMN  ERT_DATOS_DETALLE_TO.IND_TIPO_SOLICITUD IS 'Indicador de Tipo de Solicitud (0= Prechequeo, 1=Evaluacion)'
/
COMMENT ON COLUMN  ERT_DATOS_DETALLE_TO.APELLIDO IS 'Apellido del cliente consultado'
/
COMMENT ON COLUMN  ERT_DATOS_DETALLE_TO.INGRESOS IS 'Ingreso de Persona Jurídica'
/
COMMENT ON COLUMN  ERT_DATOS_DETALLE_TO.COD_RESPUESTA_REG99 IS 'respuesta registro 99'
/
COMMENT ON COLUMN  ERT_DATOS_DETALLE_TO.CALIFICACION_REG20 IS 'calificación registro 20'
/
COMMENT ON COLUMN  ERT_DATOS_DETALLE_TO.CLASIFICACION_REG20 IS 'clasificación registro 20'
/
COMMENT ON COLUMN  ERT_DATOS_DETALLE_TO.GCC_REG20 IS 'gasto consumo celular registro 20'
/
COMMENT ON COLUMN  ERT_DATOS_DETALLE_TO.GAMA_REG20 IS 'gama registro 20'
/
