CREATE TABLE VE_VENDPERFIL(
    COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_PERFIL VARCHAR2(3) NOT NULL 
 ,  FEC_ASIGNACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_DESASIGNAC DATE NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_VENDPERFIL IS 'Perfiles personalizados asociados a Vendedores comisionistas'
/
COMMENT ON COLUMN  VE_VENDPERFIL.COD_VENDEDOR IS 'Codigo del vendedor comisionista'
/
COMMENT ON COLUMN  VE_VENDPERFIL.COD_PERFIL IS 'Codigo del perfil personalizado'
/
COMMENT ON COLUMN  VE_VENDPERFIL.FEC_ASIGNACION IS 'Fecha de asignacion del perfil'
/
COMMENT ON COLUMN  VE_VENDPERFIL.NOM_USUARIO IS 'Usuario que realiza la asignacion'
/
COMMENT ON COLUMN  VE_VENDPERFIL.FEC_DESASIGNAC IS 'Fecha de desasignacion'
/
