CREATE TABLE GSM_PROVEEDORES_ARCHIVOS_TO(
    COD_PROVEEDOR NUMBER NOT NULL 
 ,  COD_ARCHIVO VARCHAR2(6) NOT NULL 
 ,  NUM_VERSION NUMBER(10,0) NOT NULL 
 ,  FEC_ACTUALIZACION DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_PROVEEDORES_ARCHIVOS_TO IS 'Tabla de relacion de proveedores y sus archivos.'
/
COMMENT ON COLUMN  GSM_PROVEEDORES_ARCHIVOS_TO.COD_PROVEEDOR IS 'Codigo del proveedor.'
/
COMMENT ON COLUMN  GSM_PROVEEDORES_ARCHIVOS_TO.COD_ARCHIVO IS 'Codigo de Grupo de archivos.'
/
COMMENT ON COLUMN  GSM_PROVEEDORES_ARCHIVOS_TO.NUM_VERSION IS 'Numero de Version'
/
COMMENT ON COLUMN  GSM_PROVEEDORES_ARCHIVOS_TO.FEC_ACTUALIZACION IS 'Fecha de la ultima actualizacion.'
/
COMMENT ON COLUMN  GSM_PROVEEDORES_ARCHIVOS_TO.COD_USUARIO IS 'Codigo del usuario que actualizo.'
/
