CREATE TABLE CM_REQUISITOS_TD(
    NUM_CRITERIO_REL NUMBER(6,0) NOT NULL 
 ,  NUM_PERFIL_REL NUMBER(6,0) NOT NULL 
 ,  NUM_PERIODO_REL NUMBER(3,0) NOT NULL 
 ,  NUM_CRITERIO_REQ NUMBER(6,0) NOT NULL 
 ,  NUM_PERFIL_REQ NUMBER(6,0) NOT NULL 
 ,  NUM_PERIODO_REQ NUMBER(3,0) NOT NULL 
 ,  IND_CUMPLIMIENTO CHAR(1) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_REQUISITOS_TD IS 'Requisitos De Evaluacion De Un Perfil..'
/
COMMENT ON COLUMN  CM_REQUISITOS_TD.NUM_CRITERIO_REL IS 'CRITERIO SOBRE EL QUE SE APLICA EL REQUISITO.'
/
COMMENT ON COLUMN  CM_REQUISITOS_TD.NUM_PERFIL_REL IS 'PERFIL SOBRE EL QUE SE APLICA EL REQUISITO.'
/
COMMENT ON COLUMN  CM_REQUISITOS_TD.NUM_PERIODO_REL IS 'PERIODO SOBRE EL QUE SE APLICA EL REQUISITO.(0,1,2,3,...).'
/
COMMENT ON COLUMN  CM_REQUISITOS_TD.NUM_CRITERIO_REQ IS 'CRITERIO A EVALUAR COMO REQUISITO.'
/
COMMENT ON COLUMN  CM_REQUISITOS_TD.NUM_PERFIL_REQ IS 'PERFIL A EVALUAR COMO REQUISITO.'
/
COMMENT ON COLUMN  CM_REQUISITOS_TD.NUM_PERIODO_REQ IS 'PERIODO A EVALUAR COMO REQUISITO.(0,1,2,3,...).'
/
COMMENT ON COLUMN  CM_REQUISITOS_TD.IND_CUMPLIMIENTO IS 'INDICADOR SI EL PERFIL ANTERIOR SE DEBIO CUMPLIR O NO..'
/
COMMENT ON COLUMN  CM_REQUISITOS_TD.NOM_USUARIO IS 'USUARIO DE ULTIMA MODIFICACION.'
/
COMMENT ON COLUMN  CM_REQUISITOS_TD.FEC_ULTMOD IS 'FECHA DE ULTMA MODIFICACION.'
/
