 ,  COD_TIPO_ABONESP NUMBER(3,0) NOT NULL 
 ,  DES_TIPO_ABONESP VARCHAR2(30) NOT NULL 
 ,  IND_RETENCION CHAR(1) NOT NULL 
 ,  IND_VIGENCIA CHAR(1) NOT NULL 
 ,  OBSERVACION VARCHAR2(400) NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_TIPABONADOS_ESP_TD IS 'TIPOS DE ABONADO ESPECIALES'
/
COMMENT ON COLUMN  GE_TIPABONADOS_ESP_TD.COD_MODULO IS 'CODIGO DE MODULO (APLICACION)'
/
COMMENT ON COLUMN  GE_TIPABONADOS_ESP_TD.COD_TIPO_ABONESP IS 'CODIGO DE TIPO DE ABONADO ESPECIAL'
/
COMMENT ON COLUMN  GE_TIPABONADOS_ESP_TD.DES_TIPO_ABONESP IS 'DESCRIPCION DEL TIPO DE ABONADO ESPECIAL'
/
COMMENT ON COLUMN  GE_TIPABONADOS_ESP_TD.IND_RETENCION IS 'INDICADOR DE RETENCION'
/
COMMENT ON COLUMN  GE_TIPABONADOS_ESP_TD.IND_VIGENCIA IS 'INDICADOR DE VIGENCIA'
/
COMMENT ON COLUMN  GE_TIPABONADOS_ESP_TD.OBSERVACION IS 'DETALLE DEL TIPO DE ABONADO ESPECIAL'
/
