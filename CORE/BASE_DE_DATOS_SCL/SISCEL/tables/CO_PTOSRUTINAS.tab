CREATE TABLE CO_PTOSRUTINAS(
    COD_PTOGEST VARCHAR2(5) NOT NULL 
 ,  COD_RUTINA VARCHAR2(5) NOT NULL 
 ,  COD_CATEGORIA VARCHAR2(5) NOT NULL 
 ,  NUM_DIAS NUMBER(3,0) NULL 
 ,  NUM_PROCESO NUMBER(4,0) NOT NULL 
 ,  COD_PARAM VARCHAR2(15) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  CO_PTOSRUTINAS.COD_CATEGORIA IS 'Categoria asociada.'
/
COMMENT ON COLUMN  CO_PTOSRUTINAS.NUM_DIAS IS 'Dias de Vencida la Deuda.'
/
COMMENT ON COLUMN  CO_PTOSRUTINAS.NUM_PROCESO IS 'Proceso asociado al Pto. de Gestisn'
/
COMMENT ON COLUMN  CO_PTOSRUTINAS.COD_PARAM IS 'la accion requiere un parametro adicional'
/
