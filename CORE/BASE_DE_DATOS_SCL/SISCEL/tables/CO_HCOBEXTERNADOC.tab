CREATE TABLE CO_HCOBEXTERNADOC
(
  NUM_IDENT      VARCHAR2(20 BYTE)              NOT NULL,
  COD_TIPIDENT   VARCHAR2(2 BYTE)               NOT NULL,
  COD_CLIENTE    NUMBER(8)                      NOT NULL,
  NUM_ABONADO    NUMBER(8)                      NOT NULL,
  NUM_FOLIO      NUMBER(9)                      NOT NULL,
  COD_TIPDOCUM   NUMBER(2)                      NOT NULL,
  NUM_CUOTAS     NUMBER(2)                      NOT NULL,
  SEC_CUOTA      NUMBER(2)                      NOT NULL,
  NUM_PROCESO    NUMBER(8)                      NOT NULL,
  COD_ENTIDAD    VARCHAR2(5 BYTE),
  FEC_VENCIMIE   DATE,
  IND_INFORMADO  CHAR(1 BYTE),
  IND_CANCELADO  NUMBER(1)                      DEFAULT 0                     NOT NULL,
  FEC_HISTORICO  DATE,
  IMPORTE_DEBE   NUMBER(14,4)                   DEFAULT 0                     NOT NULL,
  COD_CONCEPTO   NUMBER(4),
  PREF_PLAZA     VARCHAR2(25 BYTE)
)
TABLESPACE TS_RECYCOB_SDAT
PCTUSED    0
PCTFREE    5
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          40M
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON COLUMN CO_HCOBEXTERNADOC.COD_ENTIDAD IS 'Codigo Entidad';

COMMENT ON COLUMN CO_HCOBEXTERNADOC.FEC_VENCIMIE IS 'Fecha Vencimiento Factura';

COMMENT ON COLUMN CO_HCOBEXTERNADOC.IND_INFORMADO IS 'Indicador de documento informado a cobranza externa';

COMMENT ON COLUMN CO_HCOBEXTERNADOC.IND_CANCELADO IS 'Indicador de Documento cancelado';

COMMENT ON COLUMN CO_HCOBEXTERNADOC.FEC_HISTORICO IS 'Fecha de traspaso a Historico';

COMMENT ON COLUMN CO_HCOBEXTERNADOC.IMPORTE_DEBE IS 'Importe debe del Documento';

COMMENT ON COLUMN CO_HCOBEXTERNADOC.COD_CONCEPTO IS 'Concepto del Documento';

COMMENT ON COLUMN CO_HCOBEXTERNADOC.PREF_PLAZA IS 'Prefijo Plaza';

