CREATE TABLE CO_CATCUENTAS(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_CUENTA NUMBER(8,0) NULL 
 ,  COD_CATCUENTA VARCHAR2(5) NOT NULL 
 ,  FEC_DESDE DATE NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  NOM_EJECUTIVO VARCHAR2(30) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_CATCUENTAS IS 'Categorias De Clientes.'
/
COMMENT ON COLUMN  CO_CATCUENTAS.COD_CLIENTE IS 'Codigo del Cliente.'
/
COMMENT ON COLUMN  CO_CATCUENTAS.COD_CUENTA IS 'Cuenta asociada al cliente.'
/
COMMENT ON COLUMN  CO_CATCUENTAS.COD_CATCUENTA IS 'Categoria del Cliente.'
/
COMMENT ON COLUMN  CO_CATCUENTAS.FEC_DESDE IS 'Fecha desde.'
/
COMMENT ON COLUMN  CO_CATCUENTAS.FEC_HASTA IS 'Fecha hasta.'
/
COMMENT ON COLUMN  CO_CATCUENTAS.NOM_EJECUTIVO IS 'Ejecutivo de la Cuenta.'
/
COMMENT ON COLUMN  CO_CATCUENTAS.NOM_USUARIO IS 'Usuario Oracle.'
/
COMMENT ON COLUMN  CO_CATCUENTAS.FEC_ULTMOD IS 'Fecha de ultima modificacion.'
/
