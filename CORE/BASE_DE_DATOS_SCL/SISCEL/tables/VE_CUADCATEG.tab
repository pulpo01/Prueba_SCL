CREATE TABLE VE_CUADCATEG(
    COD_CUADRANTE NUMBER(8,0) NOT NULL 
 ,  COD_CATEGORIA VARCHAR2(3) NOT NULL 
 ,  IND_ARTICPLAN NUMBER(1,0) NOT NULL 
 ,  IMP_CATEGORIA NUMBER(12,4) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CUADCATEG IS 'Contenido de Cuadrante por Categorias (Articulos o Planes Tarifarios)'
/
COMMENT ON COLUMN  VE_CUADCATEG.COD_CUADRANTE IS 'Codigo de Cuadrante Liquidacion'
/
COMMENT ON COLUMN  VE_CUADCATEG.COD_CATEGORIA IS 'Categoria asociada (Articulo o Plan Tarifario)'
/
COMMENT ON COLUMN  VE_CUADCATEG.IND_ARTICPLAN IS 'Indicativo de Articulo o Plan Tarifario'
/
COMMENT ON COLUMN  VE_CUADCATEG.IMP_CATEGORIA IS 'Importe para la Categoria asociada'
/
