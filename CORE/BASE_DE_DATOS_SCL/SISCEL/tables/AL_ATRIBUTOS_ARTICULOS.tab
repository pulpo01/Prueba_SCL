CREATE TABLE SISCEL.AL_ATRIBUTOS_ARTICULOS
(
  TIP_ARTICULO    NUMBER(3)                     NOT NULL,
  COD_ARTICULO    NUMBER(6)                     NOT NULL,
  COD_ATRIBUTO    NUMBER(3)                     NOT NULL,
  VAL_ATRIBUTO    VARCHAR2(200 BYTE)            NOT NULL,
  CLASE_SERVICIO  VARCHAR2(255 BYTE),
  PERFIL_ABONADO  VARCHAR2(255 BYTE)
)
TABLESPACE TS_LOGISTICA_SDAT
/

COMMENT ON TABLE SISCEL.AL_ATRIBUTOS_ARTICULOS IS 'Atributos de Articulos';

COMMENT ON COLUMN SISCEL.AL_ATRIBUTOS_ARTICULOS.CLASE_SERVICIO IS 'Clase de servicio creacion abonado prepago';

COMMENT ON COLUMN SISCEL.AL_ATRIBUTOS_ARTICULOS.PERFIL_ABONADO IS 'Perfil abonado creacion abonado prepago';

COMMENT ON COLUMN SISCEL.AL_ATRIBUTOS_ARTICULOS.TIP_ARTICULO IS 'Tipo de Articulo';

COMMENT ON COLUMN SISCEL.AL_ATRIBUTOS_ARTICULOS.COD_ARTICULO IS 'Codigo identificativo del articulo';

COMMENT ON COLUMN SISCEL.AL_ATRIBUTOS_ARTICULOS.COD_ATRIBUTO IS 'Codigo de atributo asociado';

COMMENT ON COLUMN SISCEL.AL_ATRIBUTOS_ARTICULOS.VAL_ATRIBUTO IS 'Valor del atributo';
