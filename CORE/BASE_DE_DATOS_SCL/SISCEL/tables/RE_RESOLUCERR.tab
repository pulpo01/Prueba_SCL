CREATE TABLE RE_RESOLUCERR(
    NUM_RECLAMO NUMBER(10,0) NOT NULL 
 ,  TIP_CATEGORIA VARCHAR2(1) NOT NULL 
 ,  SOL_USERNAME VARCHAR2(30) NOT NULL 
 ,  COD_SOLUCION NUMBER(2,0) NOT NULL 
 ,  FEC_RESOLUCION DATE NOT NULL 
 ,  GLS_RESOLUCION VARCHAR2(300) NOT NULL 
 ,  CER_USERNAME VARCHAR2(30) NULL 
 ,  FEC_RECIERRE DATE NULL 
 ,  FEC_COMUNIC DATE NULL 
 ,  VIA_COMUNIC NUMBER(2,0) NULL 
 ,  NOM_NOTIFICADOR VARCHAR2(50) NULL 
 ,  NOM_NOTIFICADO VARCHAR2(50) NULL 
 ,  GLS_RECIERRE VARCHAR2(300) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  RE_RESOLUCERR IS 'Tabla que guarda la informacion sobre las resoluciones y recierres
de los reclamos que han sido reabiertos y regestionados'
/
COMMENT ON COLUMN  RE_RESOLUCERR.NUM_RECLAMO IS 'Numero de reclamo'
/
COMMENT ON COLUMN  RE_RESOLUCERR.TIP_CATEGORIA IS 'Tipo de categoria del reclamo'
/
COMMENT ON COLUMN  RE_RESOLUCERR.SOL_USERNAME IS 'Usuario siscel que resoluciona el reclamo'
/
COMMENT ON COLUMN  RE_RESOLUCERR.COD_SOLUCION IS 'Codigo de resolucion'
/
COMMENT ON COLUMN  RE_RESOLUCERR.FEC_RESOLUCION IS 'Fecha de la resolucion'
/
COMMENT ON COLUMN  RE_RESOLUCERR.GLS_RESOLUCION IS 'Descripcion y/o detalle de la resolucion'
/
COMMENT ON COLUMN  RE_RESOLUCERR.CER_USERNAME IS 'Usuario siscel que recierra el reclamo'
/
COMMENT ON COLUMN  RE_RESOLUCERR.FEC_RECIERRE IS 'Fecha de recierre'
/
COMMENT ON COLUMN  RE_RESOLUCERR.FEC_COMUNIC IS 'Fecha de comunicacion al cliente de su solucion'
/
COMMENT ON COLUMN  RE_RESOLUCERR.VIA_COMUNIC IS 'Via de comunicacion de la solucion al cliente'
/
COMMENT ON COLUMN  RE_RESOLUCERR.NOM_NOTIFICADOR IS 'Persona que notifico al cliente su solucion'
/
COMMENT ON COLUMN  RE_RESOLUCERR.NOM_NOTIFICADO IS 'Persona a la cual se le notifico'
/
COMMENT ON COLUMN  RE_RESOLUCERR.GLS_RECIERRE IS 'Descripcion y/o detalle del recierre'
/
