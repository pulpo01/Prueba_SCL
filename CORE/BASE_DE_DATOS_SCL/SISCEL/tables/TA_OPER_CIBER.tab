CREATE TABLE TA_OPER_CIBER(
    COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  DES_OPERADOR VARCHAR2(30) NULL 
 ,  NUM_LOT_RECIBIDO NUMBER(6,0) NULL 
 ,  NUM_LOT_GENERADO NUMBER(6,0) NULL 
 ,  DIA_CORTE NUMBER(2,0) NULL 
 ,  FEC_ULT_PROCESO DATE NULL 
 ,  COD_MONEDA NUMBER(2,0) NULL 
 ,  DIA_ANTIGUEDAD NUMBER(3,0) NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_OPER_CIBER IS 'Pendiente Depto. facturacion'
/
COMMENT ON COLUMN  TA_OPER_CIBER.COD_OPERADOR IS 'Codigo de operador'
/
COMMENT ON COLUMN  TA_OPER_CIBER.DES_OPERADOR IS 'Descripcion de operador'
/
COMMENT ON COLUMN  TA_OPER_CIBER.NUM_LOT_RECIBIDO IS 'Ultimo lote recibido'
/
COMMENT ON COLUMN  TA_OPER_CIBER.NUM_LOT_GENERADO IS 'Ultimo lote generado'
/
COMMENT ON COLUMN  TA_OPER_CIBER.DIA_CORTE IS 'Dia de corte de trafico'
/
COMMENT ON COLUMN  TA_OPER_CIBER.FEC_ULT_PROCESO IS 'Fecha de ultimo proceso'
/
COMMENT ON COLUMN  TA_OPER_CIBER.COD_MONEDA IS 'Codigo de moneda (1=Dolar, 8=Peso chileno)'
/
COMMENT ON COLUMN  TA_OPER_CIBER.DIA_ANTIGUEDAD IS 'Maxima antiguedad para aceptar llamadas'
/
