CREATE TABLE GSM_SIMCARD_ESTADOS_TH(
    COD_MODULO VARCHAR2(2) NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) NOT NULL 
 ,  NUM_SIMCARD VARCHAR2(20) NOT NULL 
 ,  FEC_ESTADO DATE NOT NULL 
 ,  FEC_TERMINO_ESTADO DATE NULL 
 ,  FEC_ACTUALIZACION DATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_SIMCARD_ESTADOS_TH IS 'Tabla Historica de Estados historicos de la sim card.'
/
COMMENT ON COLUMN  GSM_SIMCARD_ESTADOS_TH.COD_MODULO IS 'Codigo de modulo.'
/
COMMENT ON COLUMN  GSM_SIMCARD_ESTADOS_TH.COD_ESTADO IS 'Codigo de estado.'
/
COMMENT ON COLUMN  GSM_SIMCARD_ESTADOS_TH.NUM_SIMCARD IS 'Numero de SIMcard.'
/
COMMENT ON COLUMN  GSM_SIMCARD_ESTADOS_TH.FEC_ESTADO IS 'Fecha de creacion del estado.'
/
COMMENT ON COLUMN  GSM_SIMCARD_ESTADOS_TH.FEC_TERMINO_ESTADO IS 'Fecha de termino de estado.'
/
COMMENT ON COLUMN  GSM_SIMCARD_ESTADOS_TH.FEC_ACTUALIZACION IS 'Fecha de la ultima actualizacion.'
/
COMMENT ON COLUMN  GSM_SIMCARD_ESTADOS_TH.COD_USUARIO IS 'Codigo del usuario que actualizo.'
/
