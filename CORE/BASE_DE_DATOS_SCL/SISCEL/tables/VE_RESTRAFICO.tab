CREATE TABLE VE_RESTRAFICO(
    NUM_LIQUIDACION NUMBER(8,0) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_CTOLIQ NUMBER(6,0) NOT NULL 
 ,  TIP_PLANTARIF CHAR(1) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  FEC_INILIQ DATE NOT NULL 
 ,  FEC_FINLIQ DATE NOT NULL 
 ,  FEC_LIQUIDACION DATE NOT NULL 
 ,  IMPORTE_TRAFICO NUMBER(16,4) NOT NULL 
 ,  TOT_CLIENTES NUMBER(8,0) NULL 
 ,  TOT_ABONADOS NUMBER(8,0) NULL 
 ,  IMPORTE_PAGOS NUMBER(16,0) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_RESTRAFICO IS 'Resultados calculo bono por trafico'
/
COMMENT ON COLUMN  VE_RESTRAFICO.NUM_LIQUIDACION IS 'Numero de liquidacion'
/
COMMENT ON COLUMN  VE_RESTRAFICO.COD_VENDEDOR IS 'Codigo de vendedor'
/
COMMENT ON COLUMN  VE_RESTRAFICO.COD_CTOLIQ IS 'Codigo de concepto de liquidacion'
/
COMMENT ON COLUMN  VE_RESTRAFICO.TIP_PLANTARIF IS 'Tipo de plan tarifario'
/
COMMENT ON COLUMN  VE_RESTRAFICO.COD_TIPCOMIS IS 'Codigo de tipo de comisionista'
/
COMMENT ON COLUMN  VE_RESTRAFICO.FEC_INILIQ IS 'Fecha inicio periodo de liquidacion'
/
COMMENT ON COLUMN  VE_RESTRAFICO.FEC_FINLIQ IS 'Fecha fin periodo de liquidacion'
/
COMMENT ON COLUMN  VE_RESTRAFICO.FEC_LIQUIDACION IS 'Fecha calculo de liquidacion'
/
COMMENT ON COLUMN  VE_RESTRAFICO.IMPORTE_TRAFICO IS 'Monto del bono por trafico'
/
COMMENT ON COLUMN  VE_RESTRAFICO.TOT_CLIENTES IS 'Total de clientes con pago por trafico'
/
COMMENT ON COLUMN  VE_RESTRAFICO.TOT_ABONADOS IS 'Total de abonados con pago por trafico'
/
COMMENT ON COLUMN  VE_RESTRAFICO.IMPORTE_PAGOS IS 'Importe de pagos por trafico'
/
