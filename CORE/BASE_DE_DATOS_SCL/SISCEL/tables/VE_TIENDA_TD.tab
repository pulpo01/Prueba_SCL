CREATE TABLE VE_TIENDA_TD
(
  COD_TIENDA            VARCHAR2(10 BYTE)       NOT NULL,
  DES_TIENDA            VARCHAR2(50 BYTE)       NOT NULL,
  NOM_USUARIO_VENDEDOR  VARCHAR2(30 BYTE)       NOT NULL,
  NOM_USUARIO_CAJERO    VARCHAR2(30 BYTE)       NOT NULL,
  NOM_USUARIO           VARCHAR2(30 BYTE)       NOT NULL,
  COD_CLIENTE           NUMBER(8)                   NULL,
  COD_CAJA              VARCHAR2(4 BYTE)            NULL,
  FEC_INI_VIGENCIA      DATE                        NULL,
  FEC_FIN_VIGENCIA      DATE                        NULL,
  IND_APLI_PAGO         CHAR(1 BYTE)                NULL
)TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/

COMMENT ON TABLE VE_TIENDA_TD IS '[ TIEND ] Tipificacion y Clientizacion de ArticulosTipificacion y Clientizacion de Articulos';

COMMENT ON COLUMN VE_TIENDA_TD.COD_TIENDA IS 'Codigo de Tienda';

COMMENT ON COLUMN VE_TIENDA_TD.DES_TIENDA IS 'Descripcion de Tienda';

COMMENT ON COLUMN VE_TIENDA_TD.NOM_USUARIO_VENDEDOR IS 'Nombre Usuario Vendedot';

COMMENT ON COLUMN VE_TIENDA_TD.NOM_USUARIO_CAJERO IS 'Nombre Usuario Cajero';

COMMENT ON COLUMN VE_TIENDA_TD.NOM_USUARIO IS 'Mombre de Ususario Registro';

COMMENT ON COLUMN VE_TIENDA_TD.COD_CLIENTE IS 'Codigo de Cliente';

COMMENT ON COLUMN VE_TIENDA_TD.COD_CAJA IS 'Codigo de Caja';

COMMENT ON COLUMN VE_TIENDA_TD.FEC_INI_VIGENCIA IS 'Fecha Inicio de Vigencia';

COMMENT ON COLUMN VE_TIENDA_TD.FEC_FIN_VIGENCIA IS 'Fecha Fin de Vigencia';

COMMENT ON COLUMN VE_TIENDA_TD.IND_APLI_PAGO IS 'INDICA SI APLICA PAGO O NO ( 1 = SI , 0 = NO )';
