CREATE TABLE VE_HISVENDEALER(
    COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_VENDEALER NUMBER(6,0) NOT NULL 
 ,  COD_VENDE_RAIZ NUMBER(6,0) NOT NULL 
 ,  NOM_VENDEALER VARCHAR2(40) NOT NULL 
 ,  COD_TIPIDENT VARCHAR2(2) NOT NULL 
 ,  NUM_IDENT VARCHAR2(20) NOT NULL 
 ,  FEC_CONTRATO DATE NOT NULL 
 ,  COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  FEC_FINCONTRATO DATE NOT NULL 
 ,  NUM_PIN VARCHAR2(10) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_HISVENDEALER IS 'Historico de vendedores de los distribuidores'
/
COMMENT ON COLUMN  VE_HISVENDEALER.COD_TIPCOMIS IS 'Codigo de tipo de comisionista'
/
COMMENT ON COLUMN  VE_HISVENDEALER.COD_VENDEDOR IS 'Codigo de vendedor del que depende'
/
COMMENT ON COLUMN  VE_HISVENDEALER.COD_VENDEALER IS 'Codigo del vendedor del distribuidor'
/
COMMENT ON COLUMN  VE_HISVENDEALER.COD_VENDE_RAIZ IS 'Codigo del vendedor superior del arbol'
/
COMMENT ON COLUMN  VE_HISVENDEALER.NOM_VENDEALER IS 'Nombre del vendedor'
/
COMMENT ON COLUMN  VE_HISVENDEALER.COD_TIPIDENT IS 'Codigo de tipo de identificacion'
/
COMMENT ON COLUMN  VE_HISVENDEALER.NUM_IDENT IS 'Numero de identificacion'
/
COMMENT ON COLUMN  VE_HISVENDEALER.FEC_CONTRATO IS 'Fecha de contratacion'
/
COMMENT ON COLUMN  VE_HISVENDEALER.COD_OFICINA IS 'Codigo de Oficina (COD_VENDEDOR)'
/
COMMENT ON COLUMN  VE_HISVENDEALER.FEC_FINCONTRATO IS 'Fecha de expiracion de contrato'
/
COMMENT ON COLUMN  VE_HISVENDEALER.NUM_PIN IS 'PIN'
/
