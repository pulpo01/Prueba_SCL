CREATE TABLE GAD_CATEGORIAS_ACTUACION(
    COD_CATEGORIA VARCHAR2(3) NOT NULL 
 ,  NUM_ORDEN NUMBER(2,0) NOT NULL 
 ,  COD_ACTABO VARCHAR2(2) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAD_CATEGORIAS_ACTUACION IS 'Actuaciones Por Categoria De Planes Tarifarios.'
/
COMMENT ON COLUMN  GAD_CATEGORIAS_ACTUACION.COD_CATEGORIA IS 'Codigo de categoria del plan tarifario.'
/
COMMENT ON COLUMN  GAD_CATEGORIAS_ACTUACION.NUM_ORDEN IS 'Numero de sercuencia.'
/
COMMENT ON COLUMN  GAD_CATEGORIAS_ACTUACION.COD_ACTABO IS 'Codigo de actuacion.'
/
COMMENT ON COLUMN  GAD_CATEGORIAS_ACTUACION.FEC_DESDE IS 'fecha inicio vigencia.'
/
COMMENT ON COLUMN  GAD_CATEGORIAS_ACTUACION.FEC_HASTA IS 'fecha final vigencia.'
/
COMMENT ON COLUMN  GAD_CATEGORIAS_ACTUACION.NOM_USUARIO IS 'usuario responsable .'
/
