CREATE TABLE CO_CAJEROS(
    COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  COD_CAJA NUMBER(4,0) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ,  NOM_USUALTA VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_CAJEROS IS 'CAJEROS'
/
COMMENT ON COLUMN  CO_CAJEROS.COD_OFICINA IS 'CODIGO DE OFICINA'
/
COMMENT ON COLUMN  CO_CAJEROS.COD_CAJA IS 'CODIGO DE CAJA'
/
COMMENT ON COLUMN  CO_CAJEROS.NOM_USUARORA IS 'NOMBRE DE USUARIO'
/
COMMENT ON COLUMN  CO_CAJEROS.FEC_ALTA IS 'FECHA DE ALTA DEL CAJERO'
/
COMMENT ON COLUMN  CO_CAJEROS.NOM_USUALTA IS 'NOMBRE DE USUARIO DE ALTA'
/
