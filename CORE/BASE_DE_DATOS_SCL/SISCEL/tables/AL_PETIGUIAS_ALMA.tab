CREATE TABLE AL_PETIGUIAS_ALMA(
    NUM_PETICION NUMBER(9,0) NOT NULL 
 ,  TIP_PETIDOR NUMBER(1,0) NOT NULL 
 ,  NUM_PETIDOR NUMBER(9,0) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_PETIGUIAS_ALMA IS 'Peticiones de Guias de Despacho del modulo de almacen'
/
COMMENT ON COLUMN  AL_PETIGUIAS_ALMA.TIP_PETIDOR IS 'Tipo de Petidor 1= Devolucion 2=Traspaso'
/
COMMENT ON COLUMN  AL_PETIGUIAS_ALMA.NUM_PETIDOR IS 'Numero de Petidor'
/
COMMENT ON COLUMN  AL_PETIGUIAS_ALMA.NUM_PETICION IS 'Numero de Peticion'
/
