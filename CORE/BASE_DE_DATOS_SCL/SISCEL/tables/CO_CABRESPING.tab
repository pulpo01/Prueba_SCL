CREATE TABLE CO_CABRESPING(
    NUM_RESPALDO NUMBER(8,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  IND_APLICADO NUMBER(1,0) NOT NULL 
 ,  FECHA DATE NOT NULL 
 ,  USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_PAGO NUMBER(1,0) NOT NULL 
 ,  FORMA_PAGO NUMBER(1,0) NULL 
 ,  AJUS_FECHAS NUMBER(1,0) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_CABRESPING IS 'Tabla cabecera de respaldo de ingreso- Retrofitted'
/
COMMENT ON COLUMN  CO_CABRESPING.NUM_RESPALDO IS 'Numero de respaldo de ingreso - Retrofitted'
/
COMMENT ON COLUMN  CO_CABRESPING.COD_CLIENTE IS 'Codigo de Cliente - Retrofitted'
/
COMMENT ON COLUMN  CO_CABRESPING.IND_APLICADO IS 'Indicador de aplicado - Retrofitted'
/
COMMENT ON COLUMN  CO_CABRESPING.FECHA IS 'Fecha de insercion del registro - Retrofitted'
/
COMMENT ON COLUMN  CO_CABRESPING.USUARIO IS 'Usuario que dio de alta el reg. - Retrofitted'
/
COMMENT ON COLUMN  CO_CABRESPING.COD_PAGO IS 'Indica origen de datos(cons,cuot,cheques - Retrofitted'
/
COMMENT ON COLUMN  CO_CABRESPING.FORMA_PAGO IS 'Forma de pago seleccionada en Formulario - Retrofitted'
/
COMMENT ON COLUMN  CO_CABRESPING.AJUS_FECHAS IS 'Ind. de ajustar fechas - Retrofitted'
/
