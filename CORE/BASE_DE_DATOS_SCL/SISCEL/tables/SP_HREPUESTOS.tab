CREATE TABLE SP_HREPUESTOS(
    NUM_ORDEN NUMBER(9,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  COD_BODEGA NUMBER(6,0) NOT NULL 
 ,  TIP_STOCK NUMBER(2,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_ESTADO NUMBER(2,0) NOT NULL 
 ,  CAN_STOCK NUMBER(6,0) NOT NULL 
 ,  TIP_MOVIMIENTO NUMBER(2,0) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SP_HREPUESTOS IS 'Historico De Repuestos De Orden De Reparacion.'
/
COMMENT ON COLUMN  SP_HREPUESTOS.NUM_ORDEN IS 'Numero de orden.'
/
COMMENT ON COLUMN  SP_HREPUESTOS.COD_ARTICULO IS 'Codigo de articulo repuesto.'
/
COMMENT ON COLUMN  SP_HREPUESTOS.COD_BODEGA IS 'Codigo de la bodega .'
/
COMMENT ON COLUMN  SP_HREPUESTOS.TIP_STOCK IS 'Tipo de Stock.'
/
COMMENT ON COLUMN  SP_HREPUESTOS.COD_USO IS 'Codigo de Uso.'
/
COMMENT ON COLUMN  SP_HREPUESTOS.COD_ESTADO IS 'Codigo de estado.'
/
COMMENT ON COLUMN  SP_HREPUESTOS.CAN_STOCK IS 'Cantidad de repuestos .'
/
COMMENT ON COLUMN  SP_HREPUESTOS.TIP_MOVIMIENTO IS 'Tipo de movimiento a ser gatillado en invetario.'
/
