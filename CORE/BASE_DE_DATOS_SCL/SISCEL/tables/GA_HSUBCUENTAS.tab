CREATE TABLE GA_HSUBCUENTAS(
    COD_SUBCUENTA VARCHAR2(15) NOT NULL 
 ,  COD_CUENTA NUMBER(8,0) NOT NULL 
 ,  DES_SUBCUENTA VARCHAR2(30) NOT NULL 
 ,  COD_DIRECCION NUMBER(20,0) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_HSUBCUENTAS IS 'Historico de Subcuentas de Clientes'
/
COMMENT ON COLUMN  GA_HSUBCUENTAS.COD_SUBCUENTA IS 'Codigo de Subcuenta'
/
COMMENT ON COLUMN  GA_HSUBCUENTAS.COD_CUENTA IS 'Codigo de cuenta'
/
COMMENT ON COLUMN  GA_HSUBCUENTAS.DES_SUBCUENTA IS 'Descripcion de Subuenta'
/
COMMENT ON COLUMN  GA_HSUBCUENTAS.COD_DIRECCION IS 'Codigo de Direccion'
/
COMMENT ON COLUMN  GA_HSUBCUENTAS.FEC_ALTA IS 'Fecha de Alta'
/
COMMENT ON COLUMN  GA_HSUBCUENTAS.FEC_HISTORICO IS 'Fecha de Historico'
/
