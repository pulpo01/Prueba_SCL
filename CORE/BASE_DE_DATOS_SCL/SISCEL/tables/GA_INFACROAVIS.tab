CREATE TABLE GA_INFACROAVIS(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  NUM_ESTADIA NUMBER(8,0) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ,  FEC_BAJA DATE NOT NULL 
 ,  NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  NUM_CELULARORIG NUMBER(15,0) NOT NULL 
 ,  IND_ACTUAC NUMBER(1,0) NOT NULL 
 ,  IND_FACTUR NUMBER(1,0) NOT NULL 
 ,  IND_CARGOS NUMBER(1,0) NOT NULL 
 ,  IND_PENALIZA NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_INFACROAVIS IS 'Interfase Abonados/Facturacion de Abonados Roaming Manual'
/
COMMENT ON COLUMN  GA_INFACROAVIS.COD_CLIENTE IS 'Codigo de Cliente'
/
COMMENT ON COLUMN  GA_INFACROAVIS.NUM_ABONADO IS 'Numero de Abonado/Producto'
/
COMMENT ON COLUMN  GA_INFACROAVIS.NUM_ESTADIA IS 'Numero de Estadia'
/
COMMENT ON COLUMN  GA_INFACROAVIS.FEC_ALTA IS 'Fecha de Alta Abonado'
/
COMMENT ON COLUMN  GA_INFACROAVIS.FEC_BAJA IS 'Fecha de Baja Abonado'
/
COMMENT ON COLUMN  GA_INFACROAVIS.NUM_CELULAR IS 'Numero de Celular'
/
COMMENT ON COLUMN  GA_INFACROAVIS.NUM_CELULARORIG IS 'Numero de Celular en Operadora Origen'
/
COMMENT ON COLUMN  GA_INFACROAVIS.IND_ACTUAC IS 'Indicativo de Actuacion o Movimiento; (1) Normal, (2) Baja, (4) Liquidacion, (7) Baja en Proceso, (8)'
/
COMMENT ON COLUMN  GA_INFACROAVIS.IND_FACTUR IS 'Indicativo de Abonado Facturable'
/
COMMENT ON COLUMN  GA_INFACROAVIS.IND_CARGOS IS 'Indicativo de Cargos Generados para el Ciclo'
/
COMMENT ON COLUMN  GA_INFACROAVIS.IND_PENALIZA IS 'Indicativo de Penalizaciones al Abonado'
/
