CREATE TABLE TA_NUMNACIONAL(
    COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  COD_AREA VARCHAR2(7) NOT NULL 
 ,  IND_NUMERO NUMBER(1,0) NOT NULL 
 ,  IND_ORDEN NUMBER(7,0) NOT NULL 
 ,  NUM_TDESDE VARCHAR2(15) NOT NULL 
 ,  NUM_THASTA VARCHAR2(15) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(40) DEFAULT 'TARIFICA' NOT NULL 
 ,  FEC_ULTIMAMOD DATE DEFAULT to_date('01-01-99','dd-mm-yy') NOT NULL 
 ,  COD_DOPE VARCHAR2(10) NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_NUMNACIONAL IS 'Numeracion nacional'
/
COMMENT ON COLUMN  TA_NUMNACIONAL.COD_OPERADOR IS 'Codigo de Operador'
/
COMMENT ON COLUMN  TA_NUMNACIONAL.COD_AREA IS 'Codigo de Area (Alms o Areas)'
/
COMMENT ON COLUMN  TA_NUMNACIONAL.IND_NUMERO IS 'Indicador de Numero 1Telecom, 2Prepago, 3Alquiler, 4Normal'
/
COMMENT ON COLUMN  TA_NUMNACIONAL.IND_ORDEN IS 'Indice de Orden'
/
COMMENT ON COLUMN  TA_NUMNACIONAL.NUM_TDESDE IS 'Numeracisn Desde'
/
COMMENT ON COLUMN  TA_NUMNACIONAL.NUM_THASTA IS 'Numeracisn Hasta'
/
COMMENT ON COLUMN  TA_NUMNACIONAL.NOM_USUARIO IS 'Almacena Nombre Usuario'
/
COMMENT ON COLUMN  TA_NUMNACIONAL.FEC_ULTIMAMOD IS 'Fecha Ultima Modificacion'
/
COMMENT ON COLUMN  TA_NUMNACIONAL.COD_DOPE IS 'Código DOPE, dirección de la Operadora'
/
