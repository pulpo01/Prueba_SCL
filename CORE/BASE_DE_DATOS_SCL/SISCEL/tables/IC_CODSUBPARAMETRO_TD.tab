CREATE TABLE IC_CODSUBPARAMETRO_TD(
    COD_SUBPARAMETRO NUMBER(3,0) NOT NULL 
 ,  NOM_SUBPARAMETRO VARCHAR2(15) NOT NULL 
 ,  DES_SUBPARAMETRO VARCHAR2(50) NOT NULL 
 ,  TIP_SUBPARAMETRO VARCHAR2(20) NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  IC_CODSUBPARAMETRO_TD IS '[ CODSUBPAR ] [ IC_CODSUBPAR ] Códigos de Subparametros para movimientos'
/
COMMENT ON COLUMN  IC_CODSUBPARAMETRO_TD.COD_SUBPARAMETRO IS 'Código de subparametro'
/
COMMENT ON COLUMN  IC_CODSUBPARAMETRO_TD.NOM_SUBPARAMETRO IS 'Nombre del Subparámetro'
/
COMMENT ON COLUMN  IC_CODSUBPARAMETRO_TD.DES_SUBPARAMETRO IS 'Descripción del Subparametro'
/
COMMENT ON COLUMN  IC_CODSUBPARAMETRO_TD.TIP_SUBPARAMETRO IS 'Tipo de Subparametro'
/
