CREATE TABLE FA_PARCRISEP(
    COD_CRITDESP VARCHAR2(3) NOT NULL 
 ,  DESC_CRITDESP VARCHAR2(40) NULL 
 ,  COD_TIPPARAM CHAR(1) NULL 
 ,  IND_PREPROC CHAR(1) NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  FEC_ULTMOD DATE NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_PARCRISEP IS 'Parametrizacion criterios de separacion archivos'
/
COMMENT ON COLUMN  FA_PARCRISEP.COD_CRITDESP IS 'Codigo de criterio de despacho. [PK 1/1]'
/
COMMENT ON COLUMN  FA_PARCRISEP.DESC_CRITDESP IS 'Descripcion Criterio de despacho.'
/
COMMENT ON COLUMN  FA_PARCRISEP.COD_TIPPARAM IS 'Tipo de parametro.'
/
COMMENT ON COLUMN  FA_PARCRISEP.IND_PREPROC IS 'Indicador de analisis pre-procesamiento.'
/
COMMENT ON COLUMN  FA_PARCRISEP.NOM_USUARIO IS 'Nombre usuario responsable modificacion.'
/
COMMENT ON COLUMN  FA_PARCRISEP.FEC_ULTMOD IS 'Fecha ultima modificacion.'
/
