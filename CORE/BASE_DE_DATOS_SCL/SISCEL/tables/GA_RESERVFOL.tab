CREATE TABLE GA_RESERVFOL(
    NUM_FOLIO NUMBER(9,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  FEC_RESERVA DATE NOT NULL 
 ,  COD_TIPDOCUM NUMBER(2,0) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_RESERVFOL IS 'Folios de guias y boletas reservados en la venta.  Es una tabla de data temporal.'
/
COMMENT ON COLUMN  GA_RESERVFOL.NUM_FOLIO IS 'Numero de folio'
/
COMMENT ON COLUMN  GA_RESERVFOL.COD_ARTICULO IS 'Codigo de Articulo'
/
COMMENT ON COLUMN  GA_RESERVFOL.FEC_RESERVA IS 'Fecha de Reserva'
/
COMMENT ON COLUMN  GA_RESERVFOL.COD_TIPDOCUM IS 'Codigo del tipo de documento (guia o boleta)'
/
COMMENT ON COLUMN  GA_RESERVFOL.COD_VENDEDOR IS 'Codigo del Vendedor'
/
COMMENT ON COLUMN  GA_RESERVFOL.NUM_SERIE IS 'Numero de Serie '
/
