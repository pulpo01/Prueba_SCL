CREATE TABLE CO_CONCELIM(
    COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  DES_CONCEPTO VARCHAR2(60) NOT NULL 
 ,  IND_VIGENTE NUMBER(1,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(15) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_CONCELIM IS 'Permiso de eliminacion de conceptos por codigo'
/
COMMENT ON COLUMN  CO_CONCELIM.COD_CONCEPTO IS 'Concepto de facturacion'
/
COMMENT ON COLUMN  CO_CONCELIM.DES_CONCEPTO IS 'Descripcion del concepto'
/
COMMENT ON COLUMN  CO_CONCELIM.IND_VIGENTE IS 'indicador de vigencia'
/
COMMENT ON COLUMN  CO_CONCELIM.FEC_ULTMOD IS 'fecha ultima modificacion'
/
COMMENT ON COLUMN  CO_CONCELIM.NOM_USUARIO IS 'Usuario responsable modificacion'
/
