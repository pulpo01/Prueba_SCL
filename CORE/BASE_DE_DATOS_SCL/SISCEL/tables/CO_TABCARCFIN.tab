 ,  NRT_CLIE VARCHAR2(20) NOT NULL 
 ,  DGV_CLIE VARCHAR2(1) NOT NULL 
 ,  NUM_CONT VARCHAR2(20) NOT NULL 
 ,  NUM_FACT VARCHAR2(20) NOT NULL 
 ,  FEC_FACT VARCHAR2(20) NULL 
 ,  MTO_FACT VARCHAR2(20) NOT NULL 
 ,  MTO_APLI VARCHAR2(20) NOT NULL 
 ,  SEC_CARG VARCHAR2(20) NOT NULL 
 ,  NUM_LINE VARCHAR2(15) NOT NULL 
 ,  FEC_VENC VARCHAR2(20) NULL 
 ,  MTO_CAST VARCHAR2(20) NULL 
 ,  FEC_MIGRACION DATE NULL 
 ,  NOM_USUA_MIGRACION VARCHAR2(30) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
