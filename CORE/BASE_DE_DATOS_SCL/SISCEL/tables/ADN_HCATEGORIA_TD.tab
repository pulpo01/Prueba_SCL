CREATE TABLE ADN_HCATEGORIA_TD(
    COD_PLANTARIF VARCHAR2(3) NOT NULL 
 ,  FEC_BAJA DATE NOT NULL 
 ,  TIP_ADMINISTRATIVO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ADN_HCATEGORIA_TD IS 'Hostorico de Categorias Administrativas'
/
COMMENT ON COLUMN  ADN_HCATEGORIA_TD.COD_PLANTARIF IS 'Codigo Plan Tarifario'
/
COMMENT ON COLUMN  ADN_HCATEGORIA_TD.FEC_BAJA IS 'Fecha en que se da de baja la categoria'
/
COMMENT ON COLUMN  ADN_HCATEGORIA_TD.TIP_ADMINISTRATIVO IS 'Indicador de Administrativo'
/
COMMENT ON COLUMN  ADN_HCATEGORIA_TD.NOM_USUARIO IS 'Nombre de Usuario'
/
