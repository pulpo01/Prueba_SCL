CREATE TABLE CA_DATOSGENER(
    NIV_BUZON NUMBER(1,0) NOT NULL 
 ,  TIP_INCIMOROS NUMBER(3,0) NULL 
 ,  TIP_INCILIMCRE NUMBER(3,0) NULL 
 ,  COD_MODULO VARCHAR2(2) NULL 
 ,  COD_MODULO_NOTAS VARCHAR2(2) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_DATOSGENER IS 'Datos Generales del SAC.'
/
COMMENT ON COLUMN  CA_DATOSGENER.NIV_BUZON IS 'Nivel de usuarios que pueden recibir llamadas'
/
COMMENT ON COLUMN  CA_DATOSGENER.TIP_INCIMOROS IS 'Tipo de incidencia no suspendible por morosidad'
/
COMMENT ON COLUMN  CA_DATOSGENER.TIP_INCILIMCRE IS 'Tipo de incidencia no suspendible por limite de credito'
/
COMMENT ON COLUMN  CA_DATOSGENER.COD_MODULO IS 'Codigo de modulo de Gestion de Abonados'
/
