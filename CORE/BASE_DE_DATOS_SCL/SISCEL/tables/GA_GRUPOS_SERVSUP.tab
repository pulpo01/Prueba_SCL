CREATE TABLE GA_GRUPOS_SERVSUP(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_GRUPO VARCHAR2(10) NOT NULL 
 ,  DES_GRUPO VARCHAR2(50) NOT NULL 
 ,  COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  NUM_LINEAS NUMBER(4,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_GRUPOS_SERVSUP IS 'Grupos de Servicios Suplementarios'
/
COMMENT ON COLUMN  GA_GRUPOS_SERVSUP.COD_PRODUCTO IS 'Codigo del Producto.'
/
COMMENT ON COLUMN  GA_GRUPOS_SERVSUP.COD_GRUPO IS 'Codigo del Grupo de Servicios.'
/
COMMENT ON COLUMN  GA_GRUPOS_SERVSUP.DES_GRUPO IS 'Descripcisn del Grupo de Servicios'
/
COMMENT ON COLUMN  GA_GRUPOS_SERVSUP.COD_SERVICIO IS 'Codigo de Servicio Suplementario.'
/
COMMENT ON COLUMN  GA_GRUPOS_SERVSUP.FEC_DESDE IS 'Fecha de ingreso del servicio al grupo.'
/
COMMENT ON COLUMN  GA_GRUPOS_SERVSUP.NOM_USUARIO IS 'Usuario que ingresa servicio.'
/
COMMENT ON COLUMN  GA_GRUPOS_SERVSUP.FEC_HASTA IS 'Fecha final de relacisn Servicio con Grupo.'
/
COMMENT ON COLUMN  GA_GRUPOS_SERVSUP.NUM_LINEAS IS 'Numero de lineas permitidas para el servicio Friends and Family'
/
