CREATE TABLE CO_TABDCASTCFIN(
    COD_NEGO VARCHAR2(5) NOT NULL 
 ,  NUM_CONT VARCHAR2(20) NOT NULL 
 ,  COD_CAST VARCHAR2(1) NOT NULL 
 ,  FEC_CAST VARCHAR2(20) NOT NULL 
 ,  NUM_FACT VARCHAR2(20) NOT NULL 
 ,  NRT_CLIE VARCHAR2(20) NOT NULL 
 ,  SEC_CARG VARCHAR2(20) NOT NULL 
 ,  MTO_FACT VARCHAR2(20) NULL 
 ,  MTO_APLI VARCHAR2(20) NULL 
 ,  MTO_CAST VARCHAR2(20) NULL 
 ,  LOG_USUA VARCHAR2(20) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_TABDCASTCFIN IS 'Tabdcastcfin .'
/
COMMENT ON COLUMN  CO_TABDCASTCFIN.COD_NEGO IS 'CODIGO DEL NEGOCIO.'
