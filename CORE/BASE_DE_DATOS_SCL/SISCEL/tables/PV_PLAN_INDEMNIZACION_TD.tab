CREATE TABLE PV_PLAN_INDEMNIZACION_TD(
    COD_PLAN_INDEMNIZACION NUMBER(3,0) NOT NULL 
 ,  DES_PLAN_INDEMNIZACION VARCHAR2(50) NOT NULL 
 ,  NOM_PROCEDURE VARCHAR2(50) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_PLAN_INDEMNIZACION_TD IS 'Planes de Indemnizacion por termino de Contrato.'
/
COMMENT ON COLUMN  PV_PLAN_INDEMNIZACION_TD.COD_PLAN_INDEMNIZACION IS 'Codigo del Plan de Indemnizacion.'
/
COMMENT ON COLUMN  PV_PLAN_INDEMNIZACION_TD.DES_PLAN_INDEMNIZACION IS 'Nombre del Plan de Indemnizacion.'
/
COMMENT ON COLUMN  PV_PLAN_INDEMNIZACION_TD.NOM_PROCEDURE IS 'Nombre del Procedimiento a Ejecutar.'
/
COMMENT ON COLUMN  PV_PLAN_INDEMNIZACION_TD.FEC_DESDE IS 'Fecha desde para la vigencia'
/
COMMENT ON COLUMN  PV_PLAN_INDEMNIZACION_TD.FEC_HASTA IS 'Fecha hasta para la vigencia'
/
COMMENT ON COLUMN  PV_PLAN_INDEMNIZACION_TD.COD_CONCEPTO IS 'Codigo de Concepto.'
/
