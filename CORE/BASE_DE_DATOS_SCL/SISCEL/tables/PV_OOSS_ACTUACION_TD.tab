CREATE TABLE PV_OOSS_ACTUACION_TD(
    COD_OS VARCHAR2(5) NOT NULL 
 ,  COD_ACTUACION VARCHAR2(2) NOT NULL 
 ,  FEC_MOD DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FLG_ESTADO VARCHAR2(5) DEFAULT 'TRUE' NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_OOSS_ACTUACION_TD IS '[ OSAC ] Tabla de OOSS por Actuacion'
/
COMMENT ON COLUMN  PV_OOSS_ACTUACION_TD.COD_OS IS 'Codigo de Orden de Servicio'
/
COMMENT ON COLUMN  PV_OOSS_ACTUACION_TD.COD_ACTUACION IS 'Codigo de Actuacion'
/
COMMENT ON COLUMN  PV_OOSS_ACTUACION_TD.FEC_MOD IS 'Fecha de Modificacion'
/
COMMENT ON COLUMN  PV_OOSS_ACTUACION_TD.NOM_USUARORA IS 'Nombre de Usuario'
/
COMMENT ON COLUMN  PV_OOSS_ACTUACION_TD.FLG_ESTADO IS 'Flag de Estado (TRUE= Rutina Activada FALSE= Desactivada )'
/
