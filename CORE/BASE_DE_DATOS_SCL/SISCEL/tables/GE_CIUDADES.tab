CREATE TABLE GE_CIUDADES(
    COD_REGION VARCHAR2(3) NOT NULL 
 ,  COD_PROVINCIA VARCHAR2(5) NOT NULL 
 ,  COD_CIUDAD VARCHAR2(5) NOT NULL 
 ,  DES_CIUDAD VARCHAR2(30) NOT NULL 
 ,  COD_CELDA VARCHAR2(7) NOT NULL 
 ,  IND_CIUDAD NUMBER(1,0) NULL 
 ,  COD_PLAZA VARCHAR2(5) NULL 
 ,  COD_OFICINA_PRINCIPAL VARCHAR2(2) NULL 
 ,  MDO_CONTABLE NUMBER(5,0) NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_CIUDADES IS 'Ciudades y Pueblos'
/
COMMENT ON COLUMN  GE_CIUDADES.COD_REGION IS 'Codigo de Region'
/
COMMENT ON COLUMN  GE_CIUDADES.COD_PROVINCIA IS 'Codigo de Provincia'
/
COMMENT ON COLUMN  GE_CIUDADES.COD_CIUDAD IS 'Codigo de Ciudad'
/
COMMENT ON COLUMN  GE_CIUDADES.DES_CIUDAD IS 'Descripcion de Ciudad'
/
COMMENT ON COLUMN  GE_CIUDADES.COD_CELDA IS 'Celda Asociada'
/
COMMENT ON COLUMN  GE_CIUDADES.IND_CIUDAD IS 'Indicativo de Ciudad(0)/Pueblo(1)'
/
COMMENT ON COLUMN  GE_CIUDADES.COD_PLAZA IS 'Plaza a la que pertenece una ciudad.'
/
COMMENT ON COLUMN  GE_CIUDADES.COD_OFICINA_PRINCIPAL IS 'Codigo Oficina Principal'
/
COMMENT ON COLUMN  GE_CIUDADES.MDO_CONTABLE IS 'MERCADO CONTABLE'
/
