CREATE TABLE CM_FORMASCOMISION_TD(
    COD_FORMA VARCHAR2(10) NOT NULL 
 ,  DES_FORMA VARCHAR2(60) NOT NULL 
 ,  IND_TECNOLOGIA CHAR(1) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  IND_PARCIAL CHAR(1) DEFAULT NULL NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_FORMASCOMISION_TD IS 'ESPECIFICACION DE FORMAS COMISIONALES PARA CONCEPTOS.'
/
COMMENT ON COLUMN  CM_FORMASCOMISION_TD.COD_FORMA IS 'CODIGO DE LA FORMA COMISIONAL.'
/
COMMENT ON COLUMN  CM_FORMASCOMISION_TD.DES_FORMA IS 'DESCRIPCION DE LA FORMA COMISIONAL.'
/
COMMENT ON COLUMN  CM_FORMASCOMISION_TD.IND_TECNOLOGIA IS 'LA FORMA CONSIDERA EL FACTOR TECNOLOGIA DEL EQUIPO.'
/
COMMENT ON COLUMN  CM_FORMASCOMISION_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  CM_FORMASCOMISION_TD.NOM_USUARIO IS 'USUARIO QUE HIZO LA ULTIMA MODIFICACION'
/
