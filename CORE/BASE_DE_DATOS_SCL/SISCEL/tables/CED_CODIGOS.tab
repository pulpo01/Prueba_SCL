CREATE TABLE CED_CODIGOS(
    NOM_TABLA VARCHAR2(30) NOT NULL 
 ,  NOM_COLUMNA VARCHAR2(50) NOT NULL 
 ,  COD_VALOR VARCHAR2(5) NOT NULL 
 ,  DES_VALOR VARCHAR2(30) NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CED_CODIGOS IS 'Codigos del sistema '
/
COMMENT ON COLUMN  CED_CODIGOS.NOM_TABLA IS 'Nombre tabla codificada'
/
COMMENT ON COLUMN  CED_CODIGOS.NOM_COLUMNA IS 'Nombre columna codificada'
/
COMMENT ON COLUMN  CED_CODIGOS.COD_VALOR IS 'Valor codificacion utilizada'
/
COMMENT ON COLUMN  CED_CODIGOS.DES_VALOR IS 'Descripcion de despliegue'
/
COMMENT ON COLUMN  CED_CODIGOS.FEC_ULTMOD IS 'Fecha ultima actualizacion'
/
COMMENT ON COLUMN  CED_CODIGOS.NOM_USUARIO IS 'Nombre usuario responsable'
/
