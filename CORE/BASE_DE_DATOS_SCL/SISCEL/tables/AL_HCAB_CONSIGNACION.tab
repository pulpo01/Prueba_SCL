CREATE TABLE AL_HCAB_CONSIGNACION(
    NUM_LISTA NUMBER(6,0) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  COD_PROVEEDOR NUMBER NOT NULL 
 ,  FEC_VALIDEZ DATE NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_HCAB_CONSIGNACION IS 'Historico de Cabeceras de Listas de Precios de Articulos en Consignacion'
/
COMMENT ON COLUMN  AL_HCAB_CONSIGNACION.NUM_LISTA IS 'Numero de lista'
/
COMMENT ON COLUMN  AL_HCAB_CONSIGNACION.FEC_HISTORICO IS 'Fecha de Histsrico'
/
COMMENT ON COLUMN  AL_HCAB_CONSIGNACION.COD_PROVEEDOR IS 'Proveedor'
/
COMMENT ON COLUMN  AL_HCAB_CONSIGNACION.FEC_VALIDEZ IS 'Fecha de validez'
/
COMMENT ON COLUMN  AL_HCAB_CONSIGNACION.COD_MONEDA IS 'Moneda en la que esta valorada la lista'
/
