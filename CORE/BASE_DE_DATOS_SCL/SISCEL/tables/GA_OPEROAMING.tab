CREATE TABLE GA_OPEROAMING(
    COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  COD_TIPIDENT VARCHAR2(2) NOT NULL 
 ,  NUM_IDENT VARCHAR2(20) NOT NULL 
 ,  COD_PAIS VARCHAR2(3) NOT NULL 
 ,  IND_TIPACUERDO VARCHAR2(1) NOT NULL 
 ,  NOM_RESPONSABLE VARCHAR2(40) NOT NULL 
 ,  COD_TIPIDENT_RES VARCHAR2(2) NOT NULL 
 ,  NUM_IDENT_RES VARCHAR2(20) NOT NULL 
 ,  COD_DIRECCION NUMBER(20,0) NULL 
 ,  DES_DIRECCION VARCHAR2(100) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_OPEROAMING IS 'Operadoras con Acuerdos de Roaming'
/
COMMENT ON COLUMN  GA_OPEROAMING.COD_OPERADOR IS 'Codigo de Operador Roaming'
/
COMMENT ON COLUMN  GA_OPEROAMING.COD_TIPIDENT IS 'Tipo de Identificacion'
/
COMMENT ON COLUMN  GA_OPEROAMING.NUM_IDENT IS 'Numero de Identificacion'
/
COMMENT ON COLUMN  GA_OPEROAMING.COD_PAIS IS 'Codigo de Pais'
/
COMMENT ON COLUMN  GA_OPEROAMING.IND_TIPACUERDO IS 'Indicativo de Tipo de Acuerdo'
/
COMMENT ON COLUMN  GA_OPEROAMING.NOM_RESPONSABLE IS 'Nombre de Responsable'
/
COMMENT ON COLUMN  GA_OPEROAMING.COD_TIPIDENT_RES IS 'Codigo de Tipo de Identificacion del Responsable'
/
COMMENT ON COLUMN  GA_OPEROAMING.NUM_IDENT_RES IS 'Numero de Identificacion del Responsable'
/
COMMENT ON COLUMN  GA_OPEROAMING.COD_DIRECCION IS 'Codigo de Pais'
/
COMMENT ON COLUMN  GA_OPEROAMING.DES_DIRECCION IS 'Descripcion de la direccion'
/
