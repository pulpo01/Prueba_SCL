CREATE TABLE ICG_NIVELSERVICIO(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_SERVICIO NUMBER(2,0) NOT NULL 
 ,  COD_NIVEL NUMBER(4,0) NOT NULL 
 ,  DES_NIVEL VARCHAR2(50) NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICG_NIVELSERVICIO IS 'Niveles de Servicio Abonado'
/
COMMENT ON COLUMN  ICG_NIVELSERVICIO.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  ICG_NIVELSERVICIO.COD_SERVICIO IS 'Clase de Servicio'
/
COMMENT ON COLUMN  ICG_NIVELSERVICIO.COD_NIVEL IS 'Codigo de Nivel'
/
COMMENT ON COLUMN  ICG_NIVELSERVICIO.DES_NIVEL IS 'Descripcion del Nivel'
/
