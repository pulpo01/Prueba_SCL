CREATE TABLE CED_TIPPERCONT(
    COD_TIPPERIODO VARCHAR2(5) NOT NULL 
 ,  COD_TIPPERDIA VARCHAR2(1) NOT NULL 
 ,  COD_HORARIO VARCHAR2(1) NOT NULL 
 ,  IND_CIERREFIJ VARCHAR2(1) NOT NULL 
 ,  IND_CIERREAUT VARCHAR2(1) NULL 
 ,  NUM_SETDIAS NUMBER(5,0) NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CED_TIPPERCONT IS 'Tipos de periodos estandar'
/
COMMENT ON COLUMN  CED_TIPPERCONT.COD_TIPPERIODO IS 'Codigo tipo de periodo'
/
COMMENT ON COLUMN  CED_TIPPERCONT.COD_TIPPERDIA IS 'Codigo tipo de periodo en dias D:Diario, M:Mensual,S: Semanal,B: bi-dia , T:Transaccion'
/
COMMENT ON COLUMN  CED_TIPPERCONT.COD_HORARIO IS 'Codigo tipo de horario asociado N: Normal, F: Financiero'
/
COMMENT ON COLUMN  CED_TIPPERCONT.IND_CIERREFIJ IS 'Indicador de cierres en dias fijos mensualmente (1,31)'
/
COMMENT ON COLUMN  CED_TIPPERCONT.IND_CIERREAUT IS 'Indicador de cierre periodo en forma automatica (1:automatico, 0:manual)'
/
COMMENT ON COLUMN  CED_TIPPERCONT.NUM_SETDIAS IS 'Numero de set de detalle de dias para cierre.'
/
COMMENT ON COLUMN  CED_TIPPERCONT.FEC_ULTMOD IS 'Fecha ultima modificacion'
/
COMMENT ON COLUMN  CED_TIPPERCONT.NOM_USUARIO IS 'Responsable ultima modificacion'
/
