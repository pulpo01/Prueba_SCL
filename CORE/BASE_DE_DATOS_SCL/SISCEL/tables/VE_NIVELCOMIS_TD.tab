CREATE TABLE VE_NIVELCOMIS_TD(
    COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  COD_TECNOLOGIA VARCHAR2(7) NOT NULL 
 ,  NUM_NIVEL NUMBER(2,0) NOT NULL 
 ,  FEC_DESDE DATE DEFAULT SYSDATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_NIVELCOMIS_TD IS 'NIVEL DE COMISION POR TECNOLOGIA'
/
COMMENT ON COLUMN  VE_NIVELCOMIS_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED'
/
COMMENT ON COLUMN  VE_NIVELCOMIS_TD.COD_TECNOLOGIA IS 'CODIGO DE TECNOLOGIA'
/
COMMENT ON COLUMN  VE_NIVELCOMIS_TD.NUM_NIVEL IS 'NIVEL DE COMISION'
/
COMMENT ON COLUMN  VE_NIVELCOMIS_TD.FEC_DESDE IS 'FECHA INICIAL DE VIGENCIA'
/
COMMENT ON COLUMN  VE_NIVELCOMIS_TD.FEC_HASTA IS 'FECHA FINAL DE VIGENCIA'
/
COMMENT ON COLUMN  VE_NIVELCOMIS_TD.NOM_USUARIO IS 'USUARIO ULTIMA MODIFICACION'
/
