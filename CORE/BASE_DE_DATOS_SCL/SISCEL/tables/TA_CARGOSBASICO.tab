CREATE TABLE TA_CARGOSBASICO(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CARGOBASICO VARCHAR2(3) NOT NULL 
 ,  DES_CARGOBASICO VARCHAR2(30) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  IMP_CARGOBASICO NUMBER(12,4) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) DEFAULT '001' NOT NULL 
 ,  IMP_CARGOBASHIB NUMBER(12,4) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_CARGOSBASICO IS 'Cargos basicos'
/
COMMENT ON COLUMN  TA_CARGOSBASICO.IMP_CARGOBASHIB IS 'Importe adicional cargo básico híbrido '
/
COMMENT ON COLUMN  TA_CARGOSBASICO.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  TA_CARGOSBASICO.COD_CARGOBASICO IS 'Codigo de Cargo Basico'
/
COMMENT ON COLUMN  TA_CARGOSBASICO.DES_CARGOBASICO IS 'Descripcion Cargo Basico'
/
COMMENT ON COLUMN  TA_CARGOSBASICO.FEC_DESDE IS 'Fecha Desde'
/
COMMENT ON COLUMN  TA_CARGOSBASICO.FEC_HASTA IS 'Fecha Hasta'
/
COMMENT ON COLUMN  TA_CARGOSBASICO.IMP_CARGOBASICO IS 'Importe Cargo Basico'
/
