CREATE TABLE FA_REGDETLLAM_TD (  
   POSICION                      NUMBER(6)         NOT NULL
 , COD_REGISTRO                  VARCHAR2(5)       NOT NULL
 , TIP_REGISTRO                  VARCHAR2(3)       NOT NULL
 , VALOR                         VARCHAR2(5)       NOT NULL
 , USUARIO                       VARCHAR2(30)
 , FEC_ULTMOD                    DATE                                          
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  FA_REGDETLLAM_TD                      IS '[ FA_REDELLA ] Configuración Registro Tipo DConfiguración Registro Tipo D'
/

COMMENT ON COLUMN FA_REGDETLLAM_TD.POSICION                       IS 'Posición'
/
COMMENT ON COLUMN FA_REGDETLLAM_TD.COD_REGISTRO                   IS 'Código Registro'
/
COMMENT ON COLUMN FA_REGDETLLAM_TD.TIP_REGISTRO                   IS 'Tipo Registro'
/
COMMENT ON COLUMN FA_REGDETLLAM_TD.VALOR                          IS 'Valor'
/
COMMENT ON COLUMN FA_REGDETLLAM_TD.USUARIO                        IS 'Usuario'
/
COMMENT ON COLUMN FA_REGDETLLAM_TD.FEC_ULTMOD                     IS 'Fecha Ultima Modificación'
/

