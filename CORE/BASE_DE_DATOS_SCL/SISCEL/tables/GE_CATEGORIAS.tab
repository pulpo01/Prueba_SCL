CREATE TABLE GE_CATEGORIAS(
    COD_CATEGORIA NUMBER(4,0) NOT NULL 
 ,  DES_CATEGORIA VARCHAR2(50) NOT NULL 
 ,  IND_ASESOR CHAR(1) DEFAULT 'S' NULL 
 ,  IND_VIGENCIA CHAR(1) DEFAULT 'S' NULL 
 ,  FEC_IND_VIGENCIA DATE DEFAULT sysdate NULL 
 ,  NOM_USU_INDVIGENCIA VARCHAR2(30) DEFAULT user NULL 
 ,  FEC_ULTMOD DATE DEFAULT sysdate NULL 
 ,  NOM_USUARIO VARCHAR2(30) DEFAULT user NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_CATEGORIAS IS 'Tabla de Categorias de Clientes'
/
COMMENT ON COLUMN  GE_CATEGORIAS.COD_CATEGORIA IS 'Codigo de Categoria de Cliente'
/
COMMENT ON COLUMN  GE_CATEGORIAS.DES_CATEGORIA IS 'Descripcion del tipo de Categoria'
/
COMMENT ON COLUMN  GE_CATEGORIAS.IND_ASESOR IS 'INDICADOR DE ASESOR S/N'
/
COMMENT ON COLUMN  GE_CATEGORIAS.IND_VIGENCIA IS 'INDICADOR DE VIGENCIA S/N'
/
COMMENT ON COLUMN  GE_CATEGORIAS.FEC_IND_VIGENCIA IS 'FECHA DE ACTUALIZACION DE LA VIGENCIA'
/
COMMENT ON COLUMN  GE_CATEGORIAS.NOM_USU_INDVIGENCIA IS 'NOMBRE USUARIO MODIFICADOR DE VIGENCIA'
/
COMMENT ON COLUMN  GE_CATEGORIAS.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION DEL REGISTRO'
/
COMMENT ON COLUMN  GE_CATEGORIAS.NOM_USUARIO IS 'NOMBRE USUARIO MODIFICADOR DE REGISTRO'
/
