CREATE TABLE AL_PROCESOS_MASIVOS_TO(
    IND_PROCESO NUMBER(3,0) NOT NULL 
 ,  NUM_PROCESO NUMBER(9,0) NOT NULL 
 ,  SEC_REG NUMBER(9,0) NOT NULL 
 ,  NRO_GRUPO NUMBER(6,0) NULL 
 ,  COD_ARTICULO_PPAL NUMBER(6,0) NULL 
 ,  CAN_SOLICITADA NUMBER(9,0) NULL 
 ,  COD_ARTICULO NUMBER(6,0) NULL 
 ,  NUM_SERIE VARCHAR2(25) NULL 
 ,  CAN_ARTICULOS NUMBER(9,0) NULL 
 ,  IND_TELEFONO NUMBER(1,0) NULL 
 ,  IND_ERROR NUMBER(2,0) DEFAULT 0 NULL 
 ,  OBSERVACIONES VARCHAR2(40) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_PROCESOS_MASIVOS_TO IS 'TABLA TEMPORAL PARA PROCESOS DE CARGA MASIVA'
/
COMMENT ON COLUMN  AL_PROCESOS_MASIVOS_TO.IND_PROCESO IS 'Indicador de Proceso      '
/
COMMENT ON COLUMN  AL_PROCESOS_MASIVOS_TO.NUM_PROCESO IS 'Numero de Proceso         '
/
COMMENT ON COLUMN  AL_PROCESOS_MASIVOS_TO.SEC_REG IS 'Secuencia del Registro    '
/
COMMENT ON COLUMN  AL_PROCESOS_MASIVOS_TO.NRO_GRUPO IS 'Numero del Grupo          '
/
COMMENT ON COLUMN  AL_PROCESOS_MASIVOS_TO.COD_ARTICULO_PPAL IS 'Codigo Articulo Principal '
/
COMMENT ON COLUMN  AL_PROCESOS_MASIVOS_TO.CAN_SOLICITADA IS 'Cantidad Solicitada       '
/
COMMENT ON COLUMN  AL_PROCESOS_MASIVOS_TO.COD_ARTICULO IS 'Codigo del Articulo       '
/
COMMENT ON COLUMN  AL_PROCESOS_MASIVOS_TO.NUM_SERIE IS 'Numero de Serie           '
/
COMMENT ON COLUMN  AL_PROCESOS_MASIVOS_TO.CAN_ARTICULOS IS 'Cantidad de Articulos     '
/
COMMENT ON COLUMN  AL_PROCESOS_MASIVOS_TO.IND_TELEFONO IS 'Indicador de Telefono     '
/
COMMENT ON COLUMN  AL_PROCESOS_MASIVOS_TO.IND_ERROR IS 'Indicador del Error       '
/
COMMENT ON COLUMN  AL_PROCESOS_MASIVOS_TO.OBSERVACIONES IS 'Observaciones del Articulo'
/
