CREATE TABLE CM_VALRECARGAS_TD(
    COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  COD_ORIGEN NUMBER(6,0) NOT NULL 
 ,  MTO_MIN_RECARGA NUMBER(14,4) NOT NULL 
 ,  COD_CONCEPTO_ASOC NUMBER(3,0) NOT NULL 
 ,  NUM_PERIODOS NUMBER(3,0) NOT NULL 
 ,  IMP_COMISION NUMBER(14,4) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) DEFAULT 'C' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_VALRECARGAS_TD IS 'Matriz De Evaluacion De Churn.'
/
COMMENT ON COLUMN  CM_VALRECARGAS_TD.COD_CONCEPTO IS 'CONCEPTO DE PENALIZACION.'
/
COMMENT ON COLUMN  CM_VALRECARGAS_TD.COD_ORIGEN IS 'NUMERO DEL VENDEDOR.'
/
COMMENT ON COLUMN  CM_VALRECARGAS_TD.MTO_MIN_RECARGA IS 'PERIODO.'
/
COMMENT ON COLUMN  CM_VALRECARGAS_TD.COD_CONCEPTO_ASOC IS 'CONCEPTO ASOCIADO.'
/
COMMENT ON COLUMN  CM_VALRECARGAS_TD.NUM_PERIODOS IS 'NUEMERO DE PERIODOS.'
/
COMMENT ON COLUMN  CM_VALRECARGAS_TD.IMP_COMISION IS 'IMPORTE DE COMISION.'
/
COMMENT ON COLUMN  CM_VALRECARGAS_TD.FEC_ULTMOD IS 'FECHA ULT. MIDUFICACION.'
/
COMMENT ON COLUMN  CM_VALRECARGAS_TD.NOM_USUARIO IS 'NOMBRE USUARIO.'
/
COMMENT ON COLUMN  CM_VALRECARGAS_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
COMMENT ON COLUMN  CM_VALRECARGAS_TD.COD_PLANCOMIS IS 'PLAN DE COMISIONES QUE CONTIENEN AL CONCEPTO.'
/
