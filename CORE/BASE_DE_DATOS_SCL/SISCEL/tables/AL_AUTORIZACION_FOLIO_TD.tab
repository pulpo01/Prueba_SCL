 ,  DES_AUTORIZACION VARCHAR2(100) NOT NULL 
 ,  COD_SISTEMA VARCHAR2(30) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) DEFAULT 'USER' NOT NULL 
 ,  FEC_DESDE DATE DEFAULT SYSDATE NOT NULL 
 ,  FEC_TERMINO DATE NOT NULL 
 ,  FEC_ACTUALIZA DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_AUTORIZACION_FOLIO_TD IS '[ AL_AUTO_FOLIO_TD ] TABLA DE CODIGO DE AUTORIZACION LEGAL DE DOCUMENTOSTABLA DE CODIGO DE AUTORIZACION LEGAL DE DOCUMENTOS'
/
