CREATE TABLE CO_CARGOS_CAJA_TD(
    NUM_SECUENCIA NUMBER(8,0) NOT NULL 
 ,  DES_CARGO VARCHAR2(40) NOT NULL 
 ,  MTO_CARGO NUMBER(14,4) NOT NULL 
 ,  COD_CONCEPTO_FACT NUMBER(4,0) NOT NULL 
 ,  FEC_VIGENCIA_DD DATE NOT NULL 
 ,  FEC_VIGENCIA_HH DATE NOT NULL 
 ,  FEC_MODIFICACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_CARGOS_CAJA_TD IS 'CARGOS APLICADOS POR PAGO EN CAJA'
/
COMMENT ON COLUMN  CO_CARGOS_CAJA_TD.NUM_SECUENCIA IS 'SECUENCIA DEL CARGO'
/
COMMENT ON COLUMN  CO_CARGOS_CAJA_TD.DES_CARGO IS 'DESCRIPCION DEL CARGO'
/
COMMENT ON COLUMN  CO_CARGOS_CAJA_TD.MTO_CARGO IS 'MONTO BRUTO DEL CARGO'
/
COMMENT ON COLUMN  CO_CARGOS_CAJA_TD.COD_CONCEPTO_FACT IS 'CONCEPTO FACTURABLE ASOCIADO AL CARGO'
/
COMMENT ON COLUMN  CO_CARGOS_CAJA_TD.FEC_VIGENCIA_DD IS 'FECHA DE INICIO DE VIGENCIA'
/
COMMENT ON COLUMN  CO_CARGOS_CAJA_TD.FEC_VIGENCIA_HH IS 'FECHA DE TERMINO DE VIGENCIA'
/
COMMENT ON COLUMN  CO_CARGOS_CAJA_TD.FEC_MODIFICACION IS 'FECHA DE MODIFICACION DEL REGISTRO'
/
COMMENT ON COLUMN  CO_CARGOS_CAJA_TD.NOM_USUARIO IS 'IDENTIFICACION DEL USUARIO'
/
