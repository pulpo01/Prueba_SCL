CREATE TABLE CA_RESPCAMPOS(
    COD_CUESTIONARIO VARCHAR2(6) NOT NULL 
 ,  NUM_PREGUNTA NUMBER(2,0) NOT NULL 
 ,  NUM_RESPUESTA NUMBER(2,0) NOT NULL 
 ,  COD_CAMPO NUMBER(3,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(3,0) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_RESPCAMPOS IS 'Campos Asociados a Respuesta'
/
COMMENT ON COLUMN  CA_RESPCAMPOS.COD_CUESTIONARIO IS 'Codigo Cuestionario'
/
COMMENT ON COLUMN  CA_RESPCAMPOS.NUM_PREGUNTA IS 'Numero Pregunta'
/
COMMENT ON COLUMN  CA_RESPCAMPOS.NUM_RESPUESTA IS 'Numero Respuesta'
/
COMMENT ON COLUMN  CA_RESPCAMPOS.COD_CAMPO IS 'Codigo Campo'
/
COMMENT ON COLUMN  CA_RESPCAMPOS.NUM_ORDEN IS 'Numero de orden'
/
