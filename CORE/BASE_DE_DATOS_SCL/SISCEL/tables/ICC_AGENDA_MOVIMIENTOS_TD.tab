CREATE TABLE ICC_AGENDA_MOVIMIENTOS_TD(
    COD_MASIVO VARCHAR2(10) NOT NULL 
 ,  FEC_EJEC_INI DATE NULL 
 ,  COD_INTERVALO VARCHAR2(4) NULL 
 ,  NUM_INTERVALO NUMBER(4,0) NULL 
 ,  DIA_SEMANA VARCHAR2(7) NULL 
 ,  HOR_INI_FRANJA NUMBER(4,0) DEFAULT -1 NULL 
 ,  HOR_FIN_FRANJA NUMBER(4,0) DEFAULT -1 NULL 
 ,  DES_USER VARCHAR2(30) NULL 
 ,  FEC_ENTRADA DATE DEFAULT SYSDATE NULL 
 ,  NUM_RANGO_PENDIENTES NUMBER(6,0) NULL 
 ,  NUM_RANGO_REINTENTABLES NUMBER(6,0) NULL 
 ,  IND_DESBLOQUEO NUMBER(1,0) NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICC_AGENDA_MOVIMIENTOS_TD IS 'Tabla de Inscripcion de ejecuciones de procesamientos masivos'
/
COMMENT ON COLUMN  ICC_AGENDA_MOVIMIENTOS_TD.IND_DESBLOQUEO IS 'Indicador de Desbloqueo'
/
COMMENT ON COLUMN  ICC_AGENDA_MOVIMIENTOS_TD.COD_MASIVO IS 'Codigo de agrupamiento para procesos masivos'
/
COMMENT ON COLUMN  ICC_AGENDA_MOVIMIENTOS_TD.FEC_EJEC_INI IS 'Fecha de Inicio de la proxima ejecucion'
/
COMMENT ON COLUMN  ICC_AGENDA_MOVIMIENTOS_TD.COD_INTERVALO IS 'Identificador del intervalo de eje. (Minutos, Horas, Dias,Semanas)'
/
COMMENT ON COLUMN  ICC_AGENDA_MOVIMIENTOS_TD.NUM_INTERVALO IS 'Cantidad que se repite el intervalo'
/
COMMENT ON COLUMN  ICC_AGENDA_MOVIMIENTOS_TD.DIA_SEMANA IS 'Marca de los dias de la semana en que se ejecuta el servicio'
/
COMMENT ON COLUMN  ICC_AGENDA_MOVIMIENTOS_TD.HOR_INI_FRANJA IS 'Hora de Inicio de franja horaria para ejecucion'
/
COMMENT ON COLUMN  ICC_AGENDA_MOVIMIENTOS_TD.HOR_FIN_FRANJA IS 'Hora de termino de franja horaria para ejecucion'
/
COMMENT ON COLUMN  ICC_AGENDA_MOVIMIENTOS_TD.DES_USER IS 'Usuario que realiza la transaccion'
/
COMMENT ON COLUMN  ICC_AGENDA_MOVIMIENTOS_TD.FEC_ENTRADA IS 'Fecha en que se realiza la transaccion'
/
COMMENT ON COLUMN  ICC_AGENDA_MOVIMIENTOS_TD.NUM_RANGO_PENDIENTES IS 'Cantidad de Movimientos para procesar en estado pendiente'
/
COMMENT ON COLUMN  ICC_AGENDA_MOVIMIENTOS_TD.NUM_RANGO_REINTENTABLES IS 'Cantidad de Movimientos para procesar en estado reintentable'
/
