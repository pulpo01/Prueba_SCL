CREATE TABLE VE_FRANDTOLLAM(
    COD_FRANJA NUMBER(6,0) NOT NULL 
 ,  HOR_INICIO VARCHAR2(6) NOT NULL 
 ,  HOR_FIN VARCHAR2(6) NOT NULL 
 ,  PCT_DESCUENTO NUMBER(3,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_TIPODIA VARCHAR2(2) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_OPERADOR NUMBER(5,0) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_FRANDTOLLAM IS 'Franjas para Planes de Descuento de Llamadas'
/
COMMENT ON COLUMN  VE_FRANDTOLLAM.COD_FRANJA IS 'Codigo de franja'
/
COMMENT ON COLUMN  VE_FRANDTOLLAM.HOR_INICIO IS 'Hora de Inicio'
/
COMMENT ON COLUMN  VE_FRANDTOLLAM.HOR_FIN IS 'Hora de fin'
/
COMMENT ON COLUMN  VE_FRANDTOLLAM.PCT_DESCUENTO IS 'Porcentaje de descuento a aplicar'
/
COMMENT ON COLUMN  VE_FRANDTOLLAM.NOM_USUARIO IS 'Usuario que define la franja'
/
COMMENT ON COLUMN  VE_FRANDTOLLAM.COD_TIPODIA IS 'Codigo de Tipo de Dia'
/
COMMENT ON COLUMN  VE_FRANDTOLLAM.COD_PRODUCTO IS 'Codigo de Producto para el Tipo de Dia'
/
COMMENT ON COLUMN  VE_FRANDTOLLAM.COD_OPERADOR IS 'Codigo de Operador para el Tipo de Dia'
/
