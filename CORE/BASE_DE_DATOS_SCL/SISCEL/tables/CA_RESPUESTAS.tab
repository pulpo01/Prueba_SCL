CREATE TABLE CA_RESPUESTAS(
    COD_CUESTIONARIO VARCHAR2(6) NOT NULL 
 ,  NUM_PREGUNTA NUMBER(2,0) NOT NULL 
 ,  NUM_RESPUESTA NUMBER(2,0) NOT NULL 
 ,  DES_RESPUESTA VARCHAR2(80) NOT NULL 
 ,  COD_CUESTIONARIO_NEW VARCHAR2(6) NOT NULL 
 ,  NUM_PREGUNTA_NEW NUMBER(2,0) NOT NULL 
 ,  IND_FINAL NUMBER(1,0) NOT NULL 
 ,  TIP_INCIDENCIA NUMBER(3,0) NULL 
 ,  NUM_EXPLICACION NUMBER(4,0) NULL 
 ,  COD_PROGRAMA VARCHAR2(3) NULL 
 ,  COD_PROCESO VARCHAR2(7) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_RESPUESTAS IS 'Respuesta por Pregunta'
/
COMMENT ON COLUMN  CA_RESPUESTAS.COD_CUESTIONARIO IS 'Codigo Cuestionario'
/
COMMENT ON COLUMN  CA_RESPUESTAS.NUM_PREGUNTA IS 'Numero Pregunta'
/
COMMENT ON COLUMN  CA_RESPUESTAS.NUM_RESPUESTA IS 'Numero Respuesta'
/
COMMENT ON COLUMN  CA_RESPUESTAS.DES_RESPUESTA IS 'Texto Respuesta'
/
COMMENT ON COLUMN  CA_RESPUESTAS.COD_CUESTIONARIO_NEW IS 'Codigo cuestionario enlace'
/
COMMENT ON COLUMN  CA_RESPUESTAS.NUM_PREGUNTA_NEW IS 'Numero pregunta enlace'
/
COMMENT ON COLUMN  CA_RESPUESTAS.IND_FINAL IS 'Indicativo final incidencia'
/
COMMENT ON COLUMN  CA_RESPUESTAS.TIP_INCIDENCIA IS 'Tipo Incidencia'
/
COMMENT ON COLUMN  CA_RESPUESTAS.NUM_EXPLICACION IS 'Numero Explicacion'
/
COMMENT ON COLUMN  CA_RESPUESTAS.COD_PROGRAMA IS 'Programa asociado al proceso'
/
COMMENT ON COLUMN  CA_RESPUESTAS.COD_PROCESO IS 'Proceso asociado a la respuesta'
/
