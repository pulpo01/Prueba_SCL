CREATE TABLE GED_CODIGOS(
    COD_MODULO VARCHAR2(2) NOT NULL 
 ,  NOM_TABLA VARCHAR2(30) NOT NULL 
 ,  NOM_COLUMNA VARCHAR2(50) NOT NULL 
 ,  COD_VALOR VARCHAR2(5) NOT NULL 
 ,  DES_VALOR VARCHAR2(512) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GED_CODIGOS IS 'General De Dominios De Codigos.'
/
COMMENT ON COLUMN  GED_CODIGOS.COD_MODULO IS 'Modulo dueo del dominio.'
/
COMMENT ON COLUMN  GED_CODIGOS.NOM_TABLA IS 'Tabla asociada al nuevo codigo.'
/
COMMENT ON COLUMN  GED_CODIGOS.NOM_COLUMNA IS 'Columna Asociada al nuevo codigo.'
/
COMMENT ON COLUMN  GED_CODIGOS.COD_VALOR IS 'Codigo Asignado.'
/
COMMENT ON COLUMN  GED_CODIGOS.DES_VALOR IS 'Descripcion de codigo asignado.'
/
COMMENT ON COLUMN  GED_CODIGOS.FEC_ULTMOD IS 'Fecha de ultima modificacion.'
/
COMMENT ON COLUMN  GED_CODIGOS.NOM_USUARIO IS 'Usuario responsable.'
/
