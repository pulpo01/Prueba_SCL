CREATE TABLE CM_CATEGVENTAS_TD(
    COD_CATEGVENTA VARCHAR2(10) NOT NULL 
 ,  DES_CATEGVENTA VARCHAR2(60) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_CATEGVENTAS_TD IS 'CATEGORIZACION DE VENTAS CELULAR CONTRATO.'
/
COMMENT ON COLUMN  CM_CATEGVENTAS_TD.COD_CATEGVENTA IS 'CODIGO DE CATEGORIA DE VENTAS'
/
COMMENT ON COLUMN  CM_CATEGVENTAS_TD.DES_CATEGVENTA IS 'DESCRIPCION DE CATEGORIA DE VENTAS'
/
COMMENT ON COLUMN  CM_CATEGVENTAS_TD.NOM_USUARIO IS 'USUARIO QUE HIZO LA ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  CM_CATEGVENTAS_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION'
/
