CREATE TABLE CA_ACUMALARMAS(
    TIP_INCIDENCIA NUMBER(3,0) NOT NULL 
 ,  FEC_PRIMERA DATE NOT NULL 
 ,  FEC_ULTIMA DATE NOT NULL 
 ,  NUM_ACUMULADO NUMBER(3,0) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_ACUMALARMAS IS 'Acumulados incidencias con alarma'
/
COMMENT ON COLUMN  CA_ACUMALARMAS.TIP_INCIDENCIA IS 'Tipo Incidencia'
/
COMMENT ON COLUMN  CA_ACUMALARMAS.FEC_PRIMERA IS 'Desde Fecha'
/
COMMENT ON COLUMN  CA_ACUMALARMAS.FEC_ULTIMA IS 'Hasta Fecha'
/
COMMENT ON COLUMN  CA_ACUMALARMAS.NUM_ACUMULADO IS 'Acumulado'
/
