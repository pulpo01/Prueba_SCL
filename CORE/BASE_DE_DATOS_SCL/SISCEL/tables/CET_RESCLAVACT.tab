CREATE TABLE CET_RESCLAVACT(
    NUM_PROCESO NUMBER(16,0) NOT NULL 
 ,  FEC_PROCESO DATE NOT NULL 
 ,  NUM_CLAVES NUMBER(6,0) NULL 
 ,  NUM_CLAVERR NUMBER(6,0) NULL 
 ,  COD_TIPCLAVE VARCHAR2(5) NOT NULL 
 ,  FEC_ULTMOD DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  COD_ESTADO CHAR(1) NULL 
 ,  FEC_INICIOPROC DATE NULL 
 ,  FEC_TERMINOPROC DATE NULL 
 ,  TOTAL_CLAVES NUMBER(6,0) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CET_RESCLAVACT IS 'Resumen Activacion de Claves'
/
COMMENT ON COLUMN  CET_RESCLAVACT.NUM_PROCESO IS 'Numero de proceso Activacion'
/
COMMENT ON COLUMN  CET_RESCLAVACT.FEC_PROCESO IS 'Fecha de Proceso'
/
COMMENT ON COLUMN  CET_RESCLAVACT.NUM_CLAVES IS 'Total de claves Activadas'
/
COMMENT ON COLUMN  CET_RESCLAVACT.NUM_CLAVERR IS 'Total de claves con error'
/
COMMENT ON COLUMN  CET_RESCLAVACT.COD_TIPCLAVE IS 'Tipo de clave ( tipos de tarjeta )'
/
COMMENT ON COLUMN  CET_RESCLAVACT.FEC_ULTMOD IS 'Fecha ultima modificacion'
/
COMMENT ON COLUMN  CET_RESCLAVACT.NOM_USUARIO IS 'Responsable ultima modificacion'
/
COMMENT ON COLUMN  CET_RESCLAVACT.COD_ESTADO IS 'Codigo del estado del proceso.'
/
COMMENT ON COLUMN  CET_RESCLAVACT.FEC_INICIOPROC IS 'Fecha de inicio del Proceso.'
/
COMMENT ON COLUMN  CET_RESCLAVACT.FEC_TERMINOPROC IS 'Fecha de Termino del Proceso.'
/
COMMENT ON COLUMN  CET_RESCLAVACT.TOTAL_CLAVES IS 'Numero de Claves solicitadas a activar.'
/
