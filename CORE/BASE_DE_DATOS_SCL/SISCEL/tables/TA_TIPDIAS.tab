CREATE TABLE TA_TIPDIAS(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  TIP_DIA VARCHAR2(2) NOT NULL 
 ,  DES_TIPDIA VARCHAR2(40) NOT NULL 
 ,  IND_ESTADO NUMBER(1,0) DEFAULT 1 NOT NULL 
 ,  NOM_USUARIO VARCHAR2(40) DEFAULT 'TARIFICA' NOT NULL 
 ,  FEC_ULTIMAMOD DATE DEFAULT '01-jan-99' NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_TIPDIAS IS 'Tipos de Dias'
/
COMMENT ON COLUMN  TA_TIPDIAS.COD_PRODUCTO IS 'Csdigo de Producto'
/
COMMENT ON COLUMN  TA_TIPDIAS.TIP_DIA IS 'Tipo de Dmas'
/
COMMENT ON COLUMN  TA_TIPDIAS.DES_TIPDIA IS 'Descripcisn Tipo de Dmas'
/
COMMENT ON COLUMN  TA_TIPDIAS.IND_ESTADO IS 'Indicador Estado Del Registro'
/
COMMENT ON COLUMN  TA_TIPDIAS.NOM_USUARIO IS 'Almacena Nombre Usuario'
/
COMMENT ON COLUMN  TA_TIPDIAS.FEC_ULTIMAMOD IS 'Fecha Ultima Modificacion'
/
