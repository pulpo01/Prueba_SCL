CREATE TABLE CO_ACCECLI(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  DIR_IP VARCHAR2(15) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_INICIO DATE NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_ACCECLI IS 'CONTROL DE ACCESO A LOS CLIENTES'
/
COMMENT ON COLUMN  CO_ACCECLI.COD_CLIENTE IS 'CODIGO DE CLIENTE'
/
COMMENT ON COLUMN  CO_ACCECLI.DIR_IP IS 'DIRECCION IP DE LA MAQUINA'
/
COMMENT ON COLUMN  CO_ACCECLI.NOM_USUARORA IS 'NOMBRE DE USUARIO'
/
COMMENT ON COLUMN  CO_ACCECLI.FEC_INICIO IS 'FECHA DE INICIO'
/
