CREATE TABLE TA_TFESPECIAL(
    COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  COD_EFECTIVIDAD NUMBER(2,0) NOT NULL 
 ,  SEG_FRACCION NUMBER(4,0) NULL 
 ,  CLS_TARIFA VARCHAR2(3) NOT NULL 
 ,  IMP_LLAMADA NUMBER(16,6) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_TFESPECIAL IS 'Tarifa componente especial de numeros especiales cuando se tasa por llamada y no por duracion.'
/
COMMENT ON COLUMN  TA_TFESPECIAL.SEG_FRACCION IS 'Fraccion en segundos'
/
COMMENT ON COLUMN  TA_TFESPECIAL.CLS_TARIFA IS 'Clase de tarifa de componente especial, se toma de TA_SERVESPECIALES.TAR_REDES cuando se tasa por llamada.'
/
COMMENT ON COLUMN  TA_TFESPECIAL.IMP_LLAMADA IS 'Importe fijo de la componente especial.'
/
COMMENT ON COLUMN  TA_TFESPECIAL.COD_OPERADOR IS 'Operador del numero especial.'
/
COMMENT ON COLUMN  TA_TFESPECIAL.COD_EFECTIVIDAD IS 'Efectividad de la tarifa'
/
