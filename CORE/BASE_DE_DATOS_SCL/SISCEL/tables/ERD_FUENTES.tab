CREATE TABLE ERD_FUENTES(
    COD_FUENTE NUMBER(6,0) NOT NULL 
 ,  NOM_FUENTE VARCHAR2(30) NULL 
 ,  NUM_DIRECCION_IP VARCHAR2(20) NULL 
 ,  NUM_PUERTO VARCHAR2(6) NULL 
 ,  NOM_USUARIO VARCHAR2(20) NULL 
 ,  NUM_PASSWORD VARCHAR2(20) NULL 
 ,  NOM_USUARIO_MODI VARCHAR2(30) NOT NULL 
 ,  FEC_MODI_H DATE NOT NULL 
 ,  NUM_TERMINAL NUMBER(8,0) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERD_FUENTES IS 'Fuentes De Informacion Para El Sistema.'
/
COMMENT ON COLUMN  ERD_FUENTES.COD_FUENTE IS 'Identificador unico para fuentes de informacion.'
/
COMMENT ON COLUMN  ERD_FUENTES.NOM_FUENTE IS 'Nombre fisico de la fuente.'
/
COMMENT ON COLUMN  ERD_FUENTES.NUM_DIRECCION_IP IS 'Direccion Ip de la fuente.'
/
COMMENT ON COLUMN  ERD_FUENTES.NUM_PUERTO IS 'Numero de puerto de Acceso.'
/
COMMENT ON COLUMN  ERD_FUENTES.NOM_USUARIO IS 'Usuario de conexion con la fuente.'
/
COMMENT ON COLUMN  ERD_FUENTES.NUM_PASSWORD IS 'Password de acceso a la fuente.'
/
COMMENT ON COLUMN  ERD_FUENTES.NOM_USUARIO_MODI IS 'Nombre del usuario que creo/actualizo este registro.'
/
COMMENT ON COLUMN  ERD_FUENTES.FEC_MODI_H IS 'fecha de creacion/modificacion de este registro.'
/
COMMENT ON COLUMN  ERD_FUENTES.NUM_TERMINAL IS 'Numero de Terminal'
/
