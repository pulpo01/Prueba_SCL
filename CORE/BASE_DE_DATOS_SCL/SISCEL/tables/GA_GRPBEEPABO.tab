CREATE TABLE GA_GRPBEEPABO(
    COD_GRUPO NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  FEC_ASIGNA DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_DESASIGNA DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_GRPBEEPABO IS 'Tabla de Abonados x Grupos de Beeper'
/
COMMENT ON COLUMN  GA_GRPBEEPABO.COD_GRUPO IS 'Codigo de Grupo'
/
COMMENT ON COLUMN  GA_GRPBEEPABO.NUM_ABONADO IS 'Numero de Abonado Beeper'
/
COMMENT ON COLUMN  GA_GRPBEEPABO.FEC_ASIGNA IS 'Fecha de Asociacion'
/
COMMENT ON COLUMN  GA_GRPBEEPABO.NOM_USUARORA IS 'Usuario Creador del Grupo'
/
COMMENT ON COLUMN  GA_GRPBEEPABO.FEC_DESASIGNA IS 'Fecha de Desasociado'
/
