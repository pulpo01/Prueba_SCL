CREATE TABLE FAD_PLANDESC(
    COD_PLANDESC VARCHAR2(5) NOT NULL 
 ,  DES_PLANDESC VARCHAR2(30) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  IND_RESTRICCION VARCHAR2(1) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAD_PLANDESC IS 'Planes de Descuentos'
/
COMMENT ON COLUMN  FAD_PLANDESC.COD_PLANDESC IS 'Codigo de plan de descuento'
/
COMMENT ON COLUMN  FAD_PLANDESC.DES_PLANDESC IS 'Codigo de plan de descuento'
/
COMMENT ON COLUMN  FAD_PLANDESC.FEC_DESDE IS 'Fecha desde inicio vigencia de asignacion en ventas'
/
COMMENT ON COLUMN  FAD_PLANDESC.FEC_HASTA IS 'Fecha hasta fin de vigencia de asignacion en ventas'
/
COMMENT ON COLUMN  FAD_PLANDESC.IND_RESTRICCION IS 'Indicador de restriccion por categoria de planes tarifarios 1:Con Restriccion 0:Sin Restriccion'
/
COMMENT ON COLUMN  FAD_PLANDESC.FEC_ULTMOD IS 'Fecha ultima actualizacion'
/
COMMENT ON COLUMN  FAD_PLANDESC.NOM_USUARIO IS 'Nombre usuario responsable'
/
