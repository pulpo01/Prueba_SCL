CREATE TABLE SC_ERROR_ASIENTO(
    NUM_LOTE NUMBER(10,0) NOT NULL 
 ,  COD_ERROR NUMBER(2,0) NOT NULL 
 ,  FEC_ERROR DATE NOT NULL 
 ,  DES_ERROR VARCHAR2(100) NOT NULL 
 ,  COD_OPERADORA_SCL VARCHAR2(5) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SC_ERROR_ASIENTO IS 'ERRORES EN GENERACION DE ASIENTOS CONTABLES'
/
COMMENT ON COLUMN  SC_ERROR_ASIENTO.NUM_LOTE IS 'NUMERO  DE LOTE'
/
COMMENT ON COLUMN  SC_ERROR_ASIENTO.COD_ERROR IS 'CODIGO DE ERROR EN GENERACION DE ASIENTO CONTABLE'
/
COMMENT ON COLUMN  SC_ERROR_ASIENTO.FEC_ERROR IS 'FECHA DEL ERROR'
/
COMMENT ON COLUMN  SC_ERROR_ASIENTO.DES_ERROR IS 'DESCRIPCION DEL ERROR'
/
COMMENT ON COLUMN  SC_ERROR_ASIENTO.COD_OPERADORA_SCL IS 'CODIGO DE OPERADORA'
/
