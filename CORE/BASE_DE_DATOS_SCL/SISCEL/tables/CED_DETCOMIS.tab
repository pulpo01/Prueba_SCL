CREATE TABLE CED_DETCOMIS(
    NUM_CUADRANTE NUMBER(5,0) NOT NULL 
 ,  FEC_INICIO DATE NOT NULL 
 ,  FEC_TERMINO DATE NOT NULL 
 ,  COD_CONCSIMPLE VARCHAR2(5) NOT NULL 
 ,  VAL_INICIO NUMBER(8,0) DEFAULT 0 NOT NULL 
 ,  VAL_TERMINO NUMBER(8,0) DEFAULT 0 NULL 
 ,  COD_CONCDOBLE VARCHAR2(5) NULL 
 ,  VAL_INICIO2 NUMBER(8,0) DEFAULT 0 NOT NULL 
 ,  VAL_TERMINO2 NUMBER(8,0) DEFAULT 0 NULL 
 ,  VAL_COMISION NUMBER(14,4) NOT NULL 
 ,  TIP_UNIDAD VARCHAR2(2) NULL 
 ,  TIP_VALOR VARCHAR2(2) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NULL 
 ,  FEC_ULTMOD DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  MTO_ACUMULADO NUMBER(14,4) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CED_DETCOMIS IS 'Definicion detalle comisiones pactadas '
/
COMMENT ON COLUMN  CED_DETCOMIS.NUM_CUADRANTE IS 'Numero de cuadrante '
/
COMMENT ON COLUMN  CED_DETCOMIS.FEC_INICIO IS 'Fecha Inicio Vigencia '
/
COMMENT ON COLUMN  CED_DETCOMIS.FEC_TERMINO IS 'Fecha termino Vigencia '
/
COMMENT ON COLUMN  CED_DETCOMIS.COD_CONCSIMPLE IS 'Codigo concepto entrada simple '
/
COMMENT ON COLUMN  CED_DETCOMIS.VAL_INICIO IS 'Valor inicial cuadrante fila '
/
COMMENT ON COLUMN  CED_DETCOMIS.VAL_TERMINO IS 'Valor termino cuadrante  fila '
/
COMMENT ON COLUMN  CED_DETCOMIS.COD_CONCDOBLE IS 'Codigo concepto entrada doble '
/
COMMENT ON COLUMN  CED_DETCOMIS.VAL_INICIO2 IS 'Valor inicial cuadrante columna   DEFAULT 0 '
/
COMMENT ON COLUMN  CED_DETCOMIS.VAL_TERMINO2 IS 'Valor termino cuadrante columna '
/
COMMENT ON COLUMN  CED_DETCOMIS.VAL_COMISION IS 'Valor comision '
/
COMMENT ON COLUMN  CED_DETCOMIS.TIP_UNIDAD IS 'Tipo de unidad de cobro (T: transaccion C: Cuadrante) '
/
COMMENT ON COLUMN  CED_DETCOMIS.TIP_VALOR IS 'Tipo de valor comision M: monto, P: Porcentaje '
/
COMMENT ON COLUMN  CED_DETCOMIS.COD_MONEDA IS 'Tipo de moneda comision '
/
COMMENT ON COLUMN  CED_DETCOMIS.FEC_ULTMOD IS 'Fecha ultima modificacion '
/
COMMENT ON COLUMN  CED_DETCOMIS.NOM_USUARIO IS 'Responsable ultima modificacion '
/
COMMENT ON COLUMN  CED_DETCOMIS.MTO_ACUMULADO IS 'Monto acumulado cuadrante menores'
/
