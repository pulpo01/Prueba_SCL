CREATE TABLE GSM_PARAMETROS_TD(
    COD_IDENTIFICADOR VARCHAR2(10) NOT NULL 
 ,  COD_PARAMETRO VARCHAR2(10) NOT NULL 
 ,  DES_PARAMETRO VARCHAR2(50) NOT NULL 
 ,  DES_CORTA_PARAMETOR VARCHAR2(20) NULL 
 ,  COD_VIGENCIA VARCHAR2(1) DEFAULT 1 NOT NULL 
 ,  FEC_ACTUALIZACION DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_PARAMETROS_TD IS 'Tabla de parametros generales.'
/
COMMENT ON COLUMN  GSM_PARAMETROS_TD.COD_IDENTIFICADOR IS 'Codigo de Indentificador.'
/
COMMENT ON COLUMN  GSM_PARAMETROS_TD.COD_PARAMETRO IS 'Codigo de parametros.'
/
COMMENT ON COLUMN  GSM_PARAMETROS_TD.DES_PARAMETRO IS 'Descripcion del parametro.'
/
COMMENT ON COLUMN  GSM_PARAMETROS_TD.DES_CORTA_PARAMETOR IS 'Descripcion corta del parametro.'
/
COMMENT ON COLUMN  GSM_PARAMETROS_TD.COD_VIGENCIA IS 'Codigo de vigencia de campo. 1 vigente 0 no vigente.'
/
COMMENT ON COLUMN  GSM_PARAMETROS_TD.FEC_ACTUALIZACION IS 'Fecha de actualizacion del parametro.'
/
COMMENT ON COLUMN  GSM_PARAMETROS_TD.COD_USUARIO IS 'Usuario con que se actualizo la ultima vez el parametro.'
/
