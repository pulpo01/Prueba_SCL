CREATE TABLE CO_CONCEPTOS(
    COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  DES_CONCEPTO VARCHAR2(40) NOT NULL 
 ,  COD_TIPCONCE NUMBER(2,0) NOT NULL 
 ,  ORDEN_CAN NUMBER(3,0) NOT NULL 
 ,  COD_CONCFACT NUMBER(4,0) DEFAULT 0 NOT NULL 
 ,  IND_PORCENTAJE NUMBER(1,0) DEFAULT 0 NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_CONCEPTOS IS 'Conceptos Cobros'
/
COMMENT ON COLUMN  CO_CONCEPTOS.COD_CONCEPTO IS 'Codigo de Concepto'
/
COMMENT ON COLUMN  CO_CONCEPTOS.DES_CONCEPTO IS 'Descripcion Concepto'
/
COMMENT ON COLUMN  CO_CONCEPTOS.COD_TIPCONCE IS 'Tipo de conceptos'
/
COMMENT ON COLUMN  CO_CONCEPTOS.ORDEN_CAN IS 'Orden de Cancelacion'
/
COMMENT ON COLUMN  CO_CONCEPTOS.COD_CONCFACT IS 'Contiene el concepto de facturacion de mora para generar el cargo'
/
COMMENT ON COLUMN  CO_CONCEPTOS.IND_PORCENTAJE IS 'Indicador de pago parcial 0=NO 1=SI'
/
