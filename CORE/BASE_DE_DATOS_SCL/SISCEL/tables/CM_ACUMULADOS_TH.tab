CREATE TABLE CM_ACUMULADOS_TH(
    COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  ID_PERIODO VARCHAR2(10) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_COMISIONISTA NUMBER(6,0) NOT NULL 
 ,  NUM_LOGRO NUMBER(6,0) NULL 
 ,  IMP_CONCEPTO NUMBER(14,4) NOT NULL 
 ,  NUM_REGISTROS NUMBER(6,0) NOT NULL 
 ,  IND_SUBORDINADA CHAR(1) DEFAULT 'N' NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_ACUMULADOS_TH IS 'Tabla Historica De Acumulados.'
/
COMMENT ON COLUMN  CM_ACUMULADOS_TH.COD_CONCEPTO IS 'CONCEPTO DE FACTURACION RECUPERADO..'
/
COMMENT ON COLUMN  CM_ACUMULADOS_TH.COD_PERIODO IS 'CODIGO DE PERIODO. (YYYMM)..'
/
COMMENT ON COLUMN  CM_ACUMULADOS_TH.ID_PERIODO IS 'IDENTIFICADOR DE PRIODO (YYYYMM<N|D><M|Q|S>.'
/
COMMENT ON COLUMN  CM_ACUMULADOS_TH.COD_TIPCOMIS IS 'CODIGO DE CANAL DE VENTAS       [MD]..'
/
COMMENT ON COLUMN  CM_ACUMULADOS_TH.COD_COMISIONISTA IS 'CODIGO DEL COMISIONISTA          [MD]..'
/
COMMENT ON COLUMN  CM_ACUMULADOS_TH.NUM_LOGRO IS 'VALOR DE ENTRADA A LA MATRIZ DE COMISIONES...'
/
COMMENT ON COLUMN  CM_ACUMULADOS_TH.IMP_CONCEPTO IS 'COMISION LOGRADA...'
/
COMMENT ON COLUMN  CM_ACUMULADOS_TH.NUM_REGISTROS IS 'CANTIDAD DE REGISTROS SOBRE LOS QUE SE APLICA LA COMISION LOGRADA...'
/
COMMENT ON COLUMN  CM_ACUMULADOS_TH.IND_SUBORDINADA IS 'INDICA SI LA COMISION SE APLICA AL NIVEL SUBORDINADO DEL NIVEL DE APLICACION DEFINIDO'
/
COMMENT ON COLUMN  CM_ACUMULADOS_TH.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
