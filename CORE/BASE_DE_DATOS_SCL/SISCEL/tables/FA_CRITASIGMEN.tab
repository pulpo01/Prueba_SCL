CREATE TABLE FA_CRITASIGMEN(
    COD_CRITMENS VARCHAR2(3) NOT NULL 
 ,  CORR_MENSAJE NUMBER(8,0) NOT NULL 
 ,  COD_FORMULARIO NUMBER(2,0) NOT NULL 
 ,  COD_BLOQUE NUMBER(1,0) NOT NULL 
 ,  NUM_LINEAS NUMBER(2,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  VAL_PARAM VARCHAR2(20) NULL 
 ,  VAL_OPERADOR VARCHAR2(2) NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  COD_ORIGEN VARCHAR2(2) NOT NULL 
 ,  IND_CICLO CHAR(1) NOT NULL 
 ,  DESC_MENSAJE VARCHAR2(40) NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  COD_CICLFACT NUMBER(8,0) NULL 
 ,  FEC_PROCESO DATE NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  FA_CRITASIGMEN.COD_CRITMENS IS 'Codigo de criterio asignacion.    [PK 1/6]'
/
COMMENT ON COLUMN  FA_CRITASIGMEN.CORR_MENSAJE IS 'Correlativo de mensaje asignado.  [PK 2/6]'
/
COMMENT ON COLUMN  FA_CRITASIGMEN.COD_FORMULARIO IS 'Codigo de documento de destino.   [PK 3/6]'
/
COMMENT ON COLUMN  FA_CRITASIGMEN.COD_BLOQUE IS 'Codigo de bloque destino mensaje. [PK 4/6]'
/
COMMENT ON COLUMN  FA_CRITASIGMEN.NUM_LINEAS IS 'Numero de lineas del mensaje.     [PK 5/6]'
/
COMMENT ON COLUMN  FA_CRITASIGMEN.FEC_DESDE IS 'Fecha inicio periodo vigencia.    [PK 6/6]'
/
COMMENT ON COLUMN  FA_CRITASIGMEN.VAL_PARAM IS 'Valor parametro a considerar.'
/
COMMENT ON COLUMN  FA_CRITASIGMEN.VAL_OPERADOR IS 'Operando para busqueda de clientes.'
/
COMMENT ON COLUMN  FA_CRITASIGMEN.FEC_HASTA IS 'Fecha termino periodo vigencia.'
/
COMMENT ON COLUMN  FA_CRITASIGMEN.COD_ORIGEN IS 'Codigo origen del mensaje.'
/
COMMENT ON COLUMN  FA_CRITASIGMEN.IND_CICLO IS 'Indicador de Incorporacion facturas: C=de ciclo | N=no ciclicas | T=Todas.'
/
COMMENT ON COLUMN  FA_CRITASIGMEN.DESC_MENSAJE IS 'Descripcion corta del mensaje.'
/
COMMENT ON COLUMN  FA_CRITASIGMEN.NOM_USUARIO IS 'Nombre usuario responsable modificacion.'
/
COMMENT ON COLUMN  FA_CRITASIGMEN.FEC_INGRESO IS 'Fecha ingreso mensaje.'
/
