CREATE TABLE PV_INTERFAZARCHIVOS_TO(
    NOM_ARCHIVO VARCHAR2(40) NOT NULL 
 ,  NOM_PROCESO VARCHAR2(20) NOT NULL 
 ,  CUENTA VARCHAR2(5) NOT NULL 
 ,  IND_PROCESO VARCHAR2(2) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  FEC_PROCESADO DATE NULL 
 ,  DIR_RESCATE VARCHAR2(200) NULL 
 ,  DIR_TRASPASO VARCHAR2(200) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_INTERFAZARCHIVOS_TO IS 'Tabla de interfaz de Archivos .'
/
COMMENT ON COLUMN  PV_INTERFAZARCHIVOS_TO.NOM_ARCHIVO IS 'Nombre del Archivo.'
/
COMMENT ON COLUMN  PV_INTERFAZARCHIVOS_TO.NOM_PROCESO IS 'Nombre del Proceso.'
/
COMMENT ON COLUMN  PV_INTERFAZARCHIVOS_TO.CUENTA IS 'Nombre del Usuario.'
/
COMMENT ON COLUMN  PV_INTERFAZARCHIVOS_TO.IND_PROCESO IS 'Indicador de Proceso,0=Para Traspaso,1=Por Procesar,2=En Proceso,3=Procesado'
/
COMMENT ON COLUMN  PV_INTERFAZARCHIVOS_TO.FEC_INGRESO IS 'Fecha de Ingreso.'
/
COMMENT ON COLUMN  PV_INTERFAZARCHIVOS_TO.FEC_PROCESADO IS 'Fecha de Procesamiento.'
/
COMMENT ON COLUMN  PV_INTERFAZARCHIVOS_TO.DIR_RESCATE IS 'Direccion donde se rescata el archivo.'
/
COMMENT ON COLUMN  PV_INTERFAZARCHIVOS_TO.DIR_TRASPASO IS 'Direccion donde se traspasA el archivo.'
/
