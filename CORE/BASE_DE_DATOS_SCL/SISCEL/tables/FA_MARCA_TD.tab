 ,  COD_MARCA VARCHAR2(6) NOT NULL 
 ,  DES_MARCA VARCHAR2(50) NOT NULL 
 ,  IND_IMPRESION VARCHAR2(2) DEFAULT 'S' NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_MARCA_TD IS '[ MARCA ] Dominio de MarcasDominio de Marcas'
/
COMMENT ON COLUMN  FA_MARCA_TD.TIP_MARCA IS 'Tipo de Marca'
/
COMMENT ON COLUMN  FA_MARCA_TD.COD_MARCA IS 'Codigo de Marca'
/
COMMENT ON COLUMN  FA_MARCA_TD.DES_MARCA IS 'Descripción de Marcas'
/
COMMENT ON COLUMN  FA_MARCA_TD.IND_IMPRESION IS 'Indicador S/N'
/
COMMENT ON COLUMN  FA_MARCA_TD.FEC_ULTMOD IS 'Fechas Ultima Actualziacion'
/
COMMENT ON COLUMN  FA_MARCA_TD.NOM_USUARIO IS 'Codigo del usuario que actualizo.'
/
