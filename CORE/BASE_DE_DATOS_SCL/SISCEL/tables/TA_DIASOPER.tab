CREATE TABLE TA_DIASOPER(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  FEC_DIAESPE DATE NOT NULL 
 ,  DES_DIAESPE VARCHAR2(30) NOT NULL 
 ,  TIP_DIAESPE VARCHAR2(2) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_DIASOPER IS 'Dias especiales por operador'
/
COMMENT ON COLUMN  TA_DIASOPER.COD_PRODUCTO IS 'Codigo de producto'
/
COMMENT ON COLUMN  TA_DIASOPER.COD_OPERADOR IS 'Codigo de operador'
/
COMMENT ON COLUMN  TA_DIASOPER.FEC_DIAESPE IS 'Fecha dia especial'
/
COMMENT ON COLUMN  TA_DIASOPER.DES_DIAESPE IS 'Descripcion dia especial'
/
COMMENT ON COLUMN  TA_DIASOPER.TIP_DIAESPE IS 'Tipo dia especial'
/
