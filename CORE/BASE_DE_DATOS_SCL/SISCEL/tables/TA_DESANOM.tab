 ,  DES_RESPONSABLE VARCHAR2(50) NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_DESANOM IS 'Descripcion de codigos de anomalias'
/
COMMENT ON COLUMN  TA_DESANOM.COD_PRODUCTO IS 'Csdigo de Producto'
/
COMMENT ON COLUMN  TA_DESANOM.COD_ANOMALIA IS 'Csdigo de Anomalia'
/
COMMENT ON COLUMN  TA_DESANOM.DES_ANOMALIA IS 'Descripcisn de Anomalia'
