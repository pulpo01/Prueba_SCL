CREATE TABLE CM_DETALLE_DOCUM_TD(
    COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  TIP_PLAN VARCHAR2(5) NOT NULL 
 ,  COD_CATEGCLIENTE VARCHAR2(10) NOT NULL 
 ,  IMP_PENALIZACION NUMBER(14,4) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) DEFAULT 'C' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_DETALLE_DOCUM_TD IS 'Detalle De La Documentacion.'
/
COMMENT ON COLUMN  CM_DETALLE_DOCUM_TD.COD_PLANCOMIS IS 'PLAN DE COMISIONES QUE CONTIENEN AL CONCEPTO.'
/
COMMENT ON COLUMN  CM_DETALLE_DOCUM_TD.COD_CONCEPTO IS 'CONCEPTO DE PENALIZACION.'
/
COMMENT ON COLUMN  CM_DETALLE_DOCUM_TD.TIP_PLAN IS 'TIPO DE PLAN A COMISIONAR.'
/
COMMENT ON COLUMN  CM_DETALLE_DOCUM_TD.COD_CATEGCLIENTE IS 'CATEGORIA DE CLIENTE A COMISIONAR.'
/
COMMENT ON COLUMN  CM_DETALLE_DOCUM_TD.IMP_PENALIZACION IS 'IMPORTE DE PENALIZACION.'
/
COMMENT ON COLUMN  CM_DETALLE_DOCUM_TD.NOM_USUARIO IS 'NOMBRE USUARIO.'
/
COMMENT ON COLUMN  CM_DETALLE_DOCUM_TD.FEC_ULTMOD IS 'FECHA ULT. MODIFICACION.'
/
COMMENT ON COLUMN  CM_DETALLE_DOCUM_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
