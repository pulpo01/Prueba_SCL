CREATE TABLE AL_LIN_COR_MONE(
    NUM_CORRE NUMBER(9,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  TIP_ARTICULO NUMBER(3,0) NOT NULL 
 ,  CAN_STOCK NUMBER(9,0) NOT NULL 
 ,  PMP_HIST NUMBER(21,5) NOT NULL 
 ,  MTO_CORRE NUMBER(20,4) NOT NULL 
 ,  FEC_IMPUTACION DATE NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_LIN_COR_MONE IS 'Tabla De Detalle De Correccion Monetaria.'
/
COMMENT ON COLUMN  AL_LIN_COR_MONE.NUM_CORRE IS 'Secuencial de la correccion monetaria.'
/
COMMENT ON COLUMN  AL_LIN_COR_MONE.COD_ARTICULO IS 'Codigo de Articulo.'
/
COMMENT ON COLUMN  AL_LIN_COR_MONE.TIP_ARTICULO IS 'Tipo de articulo'
/
COMMENT ON COLUMN  AL_LIN_COR_MONE.CAN_STOCK IS 'Cantidad de Stock.'
/
COMMENT ON COLUMN  AL_LIN_COR_MONE.PMP_HIST IS 'Precio Medio Ponderado Historico.'
/
COMMENT ON COLUMN  AL_LIN_COR_MONE.MTO_CORRE IS 'Correccion Monetaria Historica por Articulo.'
/
COMMENT ON COLUMN  AL_LIN_COR_MONE.FEC_IMPUTACION IS 'Fecha de imputacion Contable.'
/
