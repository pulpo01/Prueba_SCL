CREATE TABLE AL_TECNOARTICULO_TD(
    COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  COD_TECNOLOGIA VARCHAR2(7) NOT NULL 
 ,  IND_DEFECTO NUMBER(1,0) DEFAULT 0 NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_TECNOARTICULO_TD IS 'TABLA DE RELACION TECNOLOGIA ARTICULO'
/
COMMENT ON COLUMN  AL_TECNOARTICULO_TD.COD_ARTICULO IS 'CODIGO ARTICULO'
/
COMMENT ON COLUMN  AL_TECNOARTICULO_TD.COD_TECNOLOGIA IS 'CODIGO DE TECNOLOGIA'
/
COMMENT ON COLUMN  AL_TECNOARTICULO_TD.IND_DEFECTO IS 'INDICADOR TECNOLOGIA POR DEFECTO'
/
