CREATE TABLE SP_AVERIAS_CARGOCTA(
    NUM_SOLICITUD NUMBER(6,0) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_AVERIA NUMBER(3,0) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  IMP_CONCEPTO NUMBER(14,4) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SP_AVERIAS_CARGOCTA IS 'Averias para un equipo en una solicitud de cargo a cuenta'
/
COMMENT ON COLUMN  SP_AVERIAS_CARGOCTA.NUM_SOLICITUD IS 'Numero de Solicitud para el que estan reflejadas las averias'
/
COMMENT ON COLUMN  SP_AVERIAS_CARGOCTA.COD_CONCEPTO IS 'Codigo del concepto asociado al importe de la averia'
/
COMMENT ON COLUMN  SP_AVERIAS_CARGOCTA.COD_PRODUCTO IS 'Codigo del producto al que pertenece la averia'
/
COMMENT ON COLUMN  SP_AVERIAS_CARGOCTA.COD_AVERIA IS 'Codigo de la Averia'
/
COMMENT ON COLUMN  SP_AVERIAS_CARGOCTA.COD_MONEDA IS 'codigo de moneda en que esta reflejado el importe'
/
COMMENT ON COLUMN  SP_AVERIAS_CARGOCTA.IMP_CONCEPTO IS 'Importe del concepto que se cobra por la averia'
/
