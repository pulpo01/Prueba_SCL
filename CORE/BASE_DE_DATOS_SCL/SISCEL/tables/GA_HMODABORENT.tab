 ,  NUM_SERIEHEX VARCHAR2(8) NULL 
 ,  NUM_SERIEMEC VARCHAR2(20) NULL 
 ,  COD_CREDMOR NUMBER(3,0) NULL 
 ,  COD_CREDCON NUMBER(3,0) NULL 
 ,  IND_FACTUR NUMBER(1,0) NULL 
 ,  IND_SUSPEN NUMBER(1,0) NULL 
 ,  IND_REHABI NUMBER(1,0) NULL 
 ,  NUM_PERSONAL VARCHAR2(4) NULL 
 ,  COD_CAUSA VARCHAR2(2) NULL 
