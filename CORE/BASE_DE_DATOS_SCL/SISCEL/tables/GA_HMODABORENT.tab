 ,  COD_CIUDAD VARCHAR2(5) NULL 
 ,  COD_CELDA VARCHAR2(7) NULL 
 ,  COD_USO NUMBER(2,0) NULL 
 ,  TIP_TERMINAL VARCHAR2(1) NULL 
