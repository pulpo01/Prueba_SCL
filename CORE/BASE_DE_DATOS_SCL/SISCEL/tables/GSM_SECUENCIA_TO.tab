CREATE TABLE GSM_SECUENCIA_TO(
    COD_SECUENCIA VARCHAR2(18) NOT NULL 
 ,  NOM_SEQUENCIA VARCHAR2(50) NOT NULL 
 ,  NUM_VALOR_MINIMO NUMBER NOT NULL 
 ,  NUM_VALOR_MAXIMO NUMBER(18,0) NOT NULL 
 ,  NUM_SECUENCIA NUMBER(18,0) NOT NULL 
 ,  NUM_LARGO_SECUENCIA NUMBER NOT NULL 
 ,  FEC_ACTUALIZACION DATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_SECUENCIA_TO IS 'Tabla de secuencias para prefijos'
/
COMMENT ON COLUMN  GSM_SECUENCIA_TO.COD_SECUENCIA IS 'Codigo de la secuencia.'
/
COMMENT ON COLUMN  GSM_SECUENCIA_TO.NOM_SEQUENCIA IS 'Nombre de la secuencia.'
/
COMMENT ON COLUMN  GSM_SECUENCIA_TO.NUM_VALOR_MINIMO IS 'Valor minimo de la secuencia.'
/
COMMENT ON COLUMN  GSM_SECUENCIA_TO.NUM_VALOR_MAXIMO IS 'Valor maximo de la secuencia.'
/
COMMENT ON COLUMN  GSM_SECUENCIA_TO.NUM_SECUENCIA IS 'Valor actual de la secuencia.'
/
COMMENT ON COLUMN  GSM_SECUENCIA_TO.NUM_LARGO_SECUENCIA IS 'Largo en caracteres de la secuencia.'
/
COMMENT ON COLUMN  GSM_SECUENCIA_TO.FEC_ACTUALIZACION IS 'Fecha de la ultima actualizacion.'
/
COMMENT ON COLUMN  GSM_SECUENCIA_TO.COD_USUARIO IS 'Codigo del usuario que actualizo.'
/
