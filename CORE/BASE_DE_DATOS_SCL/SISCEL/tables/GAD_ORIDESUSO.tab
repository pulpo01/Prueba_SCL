CREATE TABLE GAD_ORIDESUSO(
    USO_ORIGEN NUMBER(3,0) NOT NULL 
 ,  TEC_ORIGEN VARCHAR2(7) NOT NULL 
 ,  USO_DESTINO NUMBER(3,0) NOT NULL 
 ,  TEC_DESTINO VARCHAR2(7) NOT NULL 
 ,  COD_ACTABO VARCHAR2(2) NOT NULL 
 ,  COD_MODULO VARCHAR2(2) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  IND_PORTAB NUMBER(1,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAD_ORIDESUSO IS 'Configuracion de migracion permitida entre usos'
/
COMMENT ON COLUMN  GAD_ORIDESUSO.USO_ORIGEN IS 'Uso origen de la migracion permitida'
/
COMMENT ON COLUMN  GAD_ORIDESUSO.TEC_ORIGEN IS 'Tecnologia origen del uso origen'
/
COMMENT ON COLUMN  GAD_ORIDESUSO.USO_DESTINO IS 'Uso destino de la migracion permitida'
/
COMMENT ON COLUMN  GAD_ORIDESUSO.TEC_DESTINO IS 'Tecnologia origen del uso destino'
/
COMMENT ON COLUMN  GAD_ORIDESUSO.COD_ACTABO IS 'Codigo de actuacion comercial'
/
COMMENT ON COLUMN  GAD_ORIDESUSO.COD_MODULO IS 'Modulo de SCL'
/
COMMENT ON COLUMN  GAD_ORIDESUSO.FEC_DESDE IS 'Fecha desde vigencia'
/
COMMENT ON COLUMN  GAD_ORIDESUSO.FEC_HASTA IS 'Fecha hasta vigencia'
/
COMMENT ON COLUMN  GAD_ORIDESUSO.IND_PORTAB IS 'Indicador de portabilidad para la migración de uso'
/
