CREATE TABLE SC_ERROR_INGRESO(
    COD_EVENTO NUMBER(2,0) NOT NULL 
 ,  ID_LOTE VARCHAR2(50) NOT NULL 
 ,  COD_ERROR NUMBER(2,0) NOT NULL 
 ,  FEC_ERROR DATE NOT NULL 
 ,  DES_ERROR VARCHAR2(100) NOT NULL 
 ,  COD_OPERADORA_SCL VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SC_ERROR_INGRESO IS 'ERRORES EN INGRESO DE LOTES'
/
COMMENT ON COLUMN  SC_ERROR_INGRESO.COD_EVENTO IS 'CODIGO DE EVENTO'
/
COMMENT ON COLUMN  SC_ERROR_INGRESO.ID_LOTE IS 'IDENTIFICADOR DE LOTE'
/
COMMENT ON COLUMN  SC_ERROR_INGRESO.COD_ERROR IS 'CODIGO DE ERROR EN INGRESO DE LOTE'
/
COMMENT ON COLUMN  SC_ERROR_INGRESO.FEC_ERROR IS 'FECHA DEL ERROR'
/
COMMENT ON COLUMN  SC_ERROR_INGRESO.DES_ERROR IS 'DESCRIPCION DEL ERROR'
/
COMMENT ON COLUMN  SC_ERROR_INGRESO.COD_OPERADORA_SCL IS 'CODIGO DE OPERADORA'
/
