CREATE TABLE VE_PLAN_CALCLI(
    COD_PLANCOM NUMBER(6,0) NOT NULL 
 ,  COD_CALCLIEN VARCHAR2(2) NOT NULL 
 ,  FEC_ASIGNACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_DESASIGNAC DATE NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_PLAN_CALCLI IS 'Plan Comercial por Calidad de Cliente'
/
COMMENT ON COLUMN  VE_PLAN_CALCLI.COD_PLANCOM IS 'Csdigo de Plan Comercial'
/
COMMENT ON COLUMN  VE_PLAN_CALCLI.COD_CALCLIEN IS 'Csdigo de Calidad de Cliente'
/
COMMENT ON COLUMN  VE_PLAN_CALCLI.FEC_ASIGNACION IS 'Fecha de Asignacisn'
/
COMMENT ON COLUMN  VE_PLAN_CALCLI.NOM_USUARIO IS 'Usuario que realiza la asignacisn/desasignacisn'
/
COMMENT ON COLUMN  VE_PLAN_CALCLI.FEC_DESASIGNAC IS 'Fecha de desasignacisn'
/
