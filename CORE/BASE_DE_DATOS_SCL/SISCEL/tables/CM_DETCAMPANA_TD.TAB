CREATE TABLE CM_DETCAMPANA_TD(
    COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_CICLCOMIS NUMBER(8,0) NOT NULL 
 ,  COD_COMISIONISTA NUMBER(6,0) NOT NULL 
 ,  VAL_UMBRAL NUMBER(6,0) NOT NULL 
 ,  IMP_COMISION NUMBER(14,4) NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  FEC_CORTE DATE NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_DETCAMPANA_TD IS 'ESPECIFICACION DE VALORACION BONO CAMPA?A (TMC)..'
/
COMMENT ON COLUMN  CM_DETCAMPANA_TD.COD_TIPORED IS 'TIPO DE RED SOBRE EL QUE SE APLICA EL CONCEPTOS.'
/
COMMENT ON COLUMN  CM_DETCAMPANA_TD.COD_PLANCOMIS IS 'PLAN DE COMISIONES QUE CONTIENE EL CONCEPTO.'
/
COMMENT ON COLUMN  CM_DETCAMPANA_TD.COD_CONCEPTO IS 'CONCEPTO DE COMISION'
/
COMMENT ON COLUMN  CM_DETCAMPANA_TD.COD_CICLCOMIS IS 'PERIODO DE APLICACION.'
/
COMMENT ON COLUMN  CM_DETCAMPANA_TD.COD_COMISIONISTA IS 'COMISIONISTA A EVALUAR POR BONO CAMPANA.'
/
COMMENT ON COLUMN  CM_DETCAMPANA_TD.VAL_UMBRAL IS 'VAOR DE UMBRAL DE VALORACION (CANTIDAD DE VENTAS).'
/
COMMENT ON COLUMN  CM_DETCAMPANA_TD.IMP_COMISION IS 'IMPORTE DE LA COMISION ASIGNADA.'
/
COMMENT ON COLUMN  CM_DETCAMPANA_TD.NOM_USUARIO IS 'USUARIO ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  CM_DETCAMPANA_TD.FEC_ULTMOD IS 'FECHA ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  CM_DETCAMPANA_TD.FEC_CORTE IS 'FECHA DE CORTE.'
/
