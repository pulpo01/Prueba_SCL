CREATE TABLE FAD_PLANVENCIMIE(
    COD_PLANVENCIMIE VARCHAR2(5) NOT NULL 
 ,  DES_PLANVENCIMIE VARCHAR2(40) NOT NULL 
 ,  COD_TIPPLANVENC VARCHAR2(5) NOT NULL 
 ,  NUM_DIASVENCIMIE NUMBER(3,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAD_PLANVENCIMIE IS 'Planes De Vencimiento.'
/
COMMENT ON COLUMN  FAD_PLANVENCIMIE.COD_PLANVENCIMIE IS 'Codigo de Plan de Vencimiento.'
/
COMMENT ON COLUMN  FAD_PLANVENCIMIE.DES_PLANVENCIMIE IS 'Descripcion del Plan de vencimiento.'
/
COMMENT ON COLUMN  FAD_PLANVENCIMIE.COD_TIPPLANVENC IS 'Codigo de Tipo de Plan de Vencimiento.'
/
COMMENT ON COLUMN  FAD_PLANVENCIMIE.NUM_DIASVENCIMIE IS 'Cantidad de dias para vencimiento.'
/
COMMENT ON COLUMN  FAD_PLANVENCIMIE.FEC_ULTMOD IS 'Fecha de modificacion.'
/
COMMENT ON COLUMN  FAD_PLANVENCIMIE.NOM_USUARIO IS 'Nombre de Usuario.'
/
