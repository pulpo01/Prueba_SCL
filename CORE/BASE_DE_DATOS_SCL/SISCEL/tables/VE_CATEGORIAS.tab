CREATE TABLE VE_CATEGORIAS(
    COD_CATEGORIA VARCHAR2(3) NOT NULL 
 ,  DES_CATEGORIA VARCHAR2(30) NOT NULL 
 ,  IND_ARTICPLAN NUMBER(1,0) NOT NULL 
 ,  IND_NIVELCAT NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CATEGORIAS IS 'Categorias (Articulos, Planes Tarifarios y Sucursales)'
/
COMMENT ON COLUMN  VE_CATEGORIAS.COD_CATEGORIA IS 'Codigo de Categoria'
/
COMMENT ON COLUMN  VE_CATEGORIAS.DES_CATEGORIA IS 'Descripcion de Categoria'
/
COMMENT ON COLUMN  VE_CATEGORIAS.IND_ARTICPLAN IS 'Indicativo de Articulo (0)/Plan Tarifario (1)/Sucursal (2)'
/
COMMENT ON COLUMN  VE_CATEGORIAS.IND_NIVELCAT IS 'Indicativo de Nivel de Categoria (Cambio de Equipo)'
/
