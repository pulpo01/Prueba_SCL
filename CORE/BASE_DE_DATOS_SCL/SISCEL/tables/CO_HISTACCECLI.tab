CREATE TABLE CO_HISTACCECLI(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  DIR_IP VARCHAR2(15) NOT NULL 
 ,  FEC_INICIO DATE NOT NULL 
 ,  FEC_FIN DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_HISTACCECLI IS 'HISTORICO DE LOS ACCESOS A LOS CLIENTES'
/
COMMENT ON COLUMN  CO_HISTACCECLI.COD_CLIENTE IS 'CODIGO DE CLIENTE'
/
COMMENT ON COLUMN  CO_HISTACCECLI.DIR_IP IS 'DIRECCION IP DE LA MAQUINA'
/
COMMENT ON COLUMN  CO_HISTACCECLI.FEC_INICIO IS 'FECHA INICIO'
/
COMMENT ON COLUMN  CO_HISTACCECLI.FEC_FIN IS 'FECHA FIN'
/
COMMENT ON COLUMN  CO_HISTACCECLI.NOM_USUARORA IS 'NOMBRE DEL USUARIO'
/
