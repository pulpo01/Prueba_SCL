CREATE TABLE CM_VALCAMPANA_TD(
    COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_CATEGVENTA VARCHAR2(10) NOT NULL 
 ,  IND_FECCORTE CHAR(1) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_VALCAMPANA_TD IS 'ESPECIFICACION DE VALORACION BONO CAMPA?A (TMC)..'
/
COMMENT ON COLUMN  CM_VALCAMPANA_TD.COD_TIPORED IS 'TIPO DE RED SOBRE EL QUE SE APLICA EL CONCEPTOS.'
/
COMMENT ON COLUMN  CM_VALCAMPANA_TD.COD_PLANCOMIS IS 'PLAN DE COMISIONES QUE CONTIENE EL CONCEPTO.'
/
COMMENT ON COLUMN  CM_VALCAMPANA_TD.COD_CONCEPTO IS 'CONCEPTO DE COMISION.'
/
COMMENT ON COLUMN  CM_VALCAMPANA_TD.COD_CATEGVENTA IS 'CATEGORIA DE LA VENTA, EN FUNCION DE LAS CATEGORIAS DE PLAN Y DE CLIENTE..'
/
COMMENT ON COLUMN  CM_VALCAMPANA_TD.IND_FECCORTE IS 'INDICADOR DE CONTEO HASTA FECHA DE CORTE.'
/
COMMENT ON COLUMN  CM_VALCAMPANA_TD.NOM_USUARIO IS 'USUARIO QUE HIZO LA ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  CM_VALCAMPANA_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION'
/
