CREATE TABLE SISCEL.ICC_HISTMOVIMIENTO
(
  COD_ACTABO           VARCHAR2(2 BYTE)         NOT NULL,
  NUM_MOVIMIENTO       NUMBER(9)                NOT NULL,
  FEC_HISTORICO        DATE                     DEFAULT SYSDATE               NOT NULL,
  NUM_ABONADO          NUMBER(8)                NOT NULL,
  COD_ACTUACION        NUMBER(5)                NOT NULL,
  NOM_USUARORA         VARCHAR2(30 BYTE)        NOT NULL,
  COD_ESTADO           NUMBER(3)                NOT NULL,
  COD_MODULO           VARCHAR2(2 BYTE)         NOT NULL,
  COD_CENTRAL_NUE      NUMBER(3),
  NUM_INTENTOS         NUMBER(3)                NOT NULL,
  FEC_INGRESO          DATE                     NOT NULL,
  COD_CENTRAL          NUMBER(3)                NOT NULL,
  DES_RESPUESTA        VARCHAR2(80 BYTE),
  FEC_LECTURA          DATE,
  COD_ACT              VARCHAR2(2 BYTE),
  NUM_MOVPOS           NUMBER(9),
  TIP_TERMINAL         VARCHAR2(1 BYTE)         NOT NULL,
  FEC_EJECUCION        DATE,
  TIP_TERMINAL_NUE     VARCHAR2(1 BYTE),
  NUM_CELULAR          NUMBER(15)               NOT NULL,
  NUM_SERIE            VARCHAR2(25 BYTE)        NOT NULL,
  NUM_PERSONAL         VARCHAR2(4 BYTE),
  NUM_CELULAR_NUE      NUMBER(15),
  NUM_SERIE_NUE        VARCHAR2(25 BYTE),
  NUM_PERSONAL_NUE     VARCHAR2(4 BYTE),
  NUM_MSNB             VARCHAR2(12 BYTE),
  NUM_MSNB_NUE         VARCHAR2(12 BYTE),
  COD_SUSPREHA         VARCHAR2(3 BYTE),
  COD_SERVICIOS        VARCHAR2(255 BYTE),
  NUM_MIN              VARCHAR2(3 BYTE)         DEFAULT '',
  NUM_MIN_NUE          VARCHAR2(3 BYTE),
  STA                  CHAR(1 BYTE),
  COD_MENSAJE          NUMBER(3),
  PARAM1_MENS          VARCHAR2(30 BYTE),
  PARAM2_MENS          VARCHAR2(30 BYTE),
  PARAM3_MENS          VARCHAR2(30 BYTE),
  PLAN                 VARCHAR2(3 BYTE),
  CARGA                NUMBER(14,4),
  VALOR_PLAN           NUMBER(14,4),
  PIN                  VARCHAR2(13 BYTE),
  FEC_EXPIRA           DATE,
  COD_PIN              VARCHAR2(16 BYTE),
  DES_MENSAJE          VARCHAR2(512 BYTE),
  DES_ORIGEN_PIN       VARCHAR2(8 BYTE),
  NUM_LOTE_PIN         NUMBER(9),
  NUM_SERIE_PIN        VARCHAR2(16 BYTE),
  COD_IDIOMA           VARCHAR2(5 BYTE),
  COD_ENRUTAMIENTO     NUMBER(4),
  TIP_ENRUTAMIENTO     NUMBER(2),
  TIP_TECNOLOGIA       VARCHAR2(7 BYTE),
  IMSI                 VARCHAR2(15 BYTE),
  IMSI_NUE             VARCHAR2(15 BYTE),
  IMEI                 VARCHAR2(20 BYTE),
  IMEI_NUE             VARCHAR2(20 BYTE),
  ICC                  VARCHAR2(25 BYTE),
  ICC_NUE              VARCHAR2(25 BYTE),
  FEC_ACTIVACION       DATE,
  COD_ESPEC_PROV       NUMBER(38),
  COD_PROD_CONTRATADO  NUMBER(38),
  IND_BAJATRANS        NUMBER(2)
)   TABLESPACE TS_PROVISIONING_SDAT
/


COMMENT ON TABLE SISCEL.ICC_HISTMOVIMIENTO IS 'Historico movimientos celular';

COMMENT ON COLUMN SISCEL.ICC_HISTMOVIMIENTO.FEC_ACTIVACION IS 'Fecha activación';

COMMENT ON COLUMN SISCEL.ICC_HISTMOVIMIENTO.COD_ESPEC_PROV IS 'Código de especificacion de provision';

COMMENT ON COLUMN SISCEL.ICC_HISTMOVIMIENTO.COD_PROD_CONTRATADO IS 'Código producto contratado';

COMMENT ON COLUMN SISCEL.ICC_HISTMOVIMIENTO.IND_BAJATRANS IS 'Indicador de control de transaccion';

COMMENT ON COLUMN SISCEL.ICC_HISTMOVIMIENTO.COD_ACTABO IS 'Codigo de Actuacion de Abonados';

COMMENT ON COLUMN SISCEL.ICC_HISTMOVIMIENTO.NUM_MOVIMIENTO IS 'Numero de Movimiento';

COMMENT ON COLUMN SISCEL.ICC_HISTMOVIMIENTO.FEC_HISTORICO IS 'Fecha de insercion en la tabla';

COMMENT ON COLUMN SISCEL.ICC_HISTMOVIMIENTO.NUM_ABONADO IS 'Numero de Abonado';

