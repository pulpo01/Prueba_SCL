CREATE TABLE VE_CLASIF_CREDCLI_TD(
    COD_CLASIFICACION VARCHAR2(5) NOT NULL 
 ,  DES_CLASIFICACION VARCHAR2(50) NOT NULL 
 ,  IND_ACCION VARCHAR2(1) NOT NULL 
 ,  MENS_USUARIO VARCHAR2(100) NOT NULL 
 ,  IND_VIGENCIA VARCHAR2(1) NOT NULL 
 ,  FEC_MODIFICACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CLASIF_CREDCLI_TD IS '[ VE_CLAS ] Clasificación Datacredit de Clientes'
/
COMMENT ON COLUMN  VE_CLASIF_CREDCLI_TD.COD_CLASIFICACION IS 'Clasificación Datacredit del cliente'
/
COMMENT ON COLUMN  VE_CLASIF_CREDCLI_TD.DES_CLASIFICACION IS 'Descripción de clasificación'
/
COMMENT ON COLUMN  VE_CLASIF_CREDCLI_TD.IND_ACCION IS 'Acción a seguir  S=Si/N=No/O=Opcional'
/
COMMENT ON COLUMN  VE_CLASIF_CREDCLI_TD.MENS_USUARIO IS 'Mensaje a desplegar al usuario en caso de Accion N/O'
/
COMMENT ON COLUMN  VE_CLASIF_CREDCLI_TD.IND_VIGENCIA IS 'Indicador de vigencia de clasificación (V:Vigente/N:No vigente)'
/
COMMENT ON COLUMN  VE_CLASIF_CREDCLI_TD.FEC_MODIFICACION IS 'Fecha de última modificación'
/
COMMENT ON COLUMN  VE_CLASIF_CREDCLI_TD.NOM_USUARIO IS 'Usuario que modifica el registro'
/
