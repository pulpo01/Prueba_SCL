CREATE TABLE CEH_RESERVCONS(
    NUM_PROCESO NUMBER(6,0) NOT NULL 
 ,  COD_EMPRESA VARCHAR2(5) NOT NULL 
 ,  CAN_MOVIMIENTOS NUMBER(10,0) NOT NULL 
 ,  MTO_TOTAL NUMBER(14,4) NOT NULL 
 ,  FEC_CONTABLE DATE NOT NULL 
 ,  FEC_INFORME DATE NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CEH_RESERVCONS IS 'Resumen de servicios consumidos por empresa'
/
COMMENT ON COLUMN  CEH_RESERVCONS.NUM_PROCESO IS 'Numero proceso de cuadratura'
/
COMMENT ON COLUMN  CEH_RESERVCONS.COD_EMPRESA IS 'Codigo empresa financiera (tarjetahabiente)'
/
COMMENT ON COLUMN  CEH_RESERVCONS.CAN_MOVIMIENTOS IS 'Numero de transacciones informadas'
/
COMMENT ON COLUMN  CEH_RESERVCONS.MTO_TOTAL IS 'Monto total informado'
/
COMMENT ON COLUMN  CEH_RESERVCONS.FEC_CONTABLE IS 'Fecha contable de la transaccion'
/
COMMENT ON COLUMN  CEH_RESERVCONS.FEC_INFORME IS 'Fecha Informe archivo'
/
