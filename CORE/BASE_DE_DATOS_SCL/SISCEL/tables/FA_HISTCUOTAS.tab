 ,  ORD_CUOTA NUMBER(3,0) NOT NULL 
 ,  FEC_EMISION DATE NOT NULL 
 ,  IMP_CUOTA NUMBER(18,4) NOT NULL 
 ,  IND_FACTURADO NUMBER(1,0) NOT NULL 
 ,  IND_PAGADO NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_HISTCUOTAS IS 'Tabla de Historicos de Cuotas'
/
COMMENT ON COLUMN  FA_HISTCUOTAS.SEQ_CUOTAS IS 'Secuencia de Cuotas'
/
COMMENT ON COLUMN  FA_HISTCUOTAS.ORD_CUOTA IS 'Orden Cuota'
/
COMMENT ON COLUMN  FA_HISTCUOTAS.FEC_EMISION IS 'Fecha de Emision de la Cuota'
/
COMMENT ON COLUMN  FA_HISTCUOTAS.IMP_CUOTA IS 'Importe Cuota'
/
COMMENT ON COLUMN  FA_HISTCUOTAS.IND_FACTURADO IS 'Indicativo de Facturado'
/
COMMENT ON COLUMN  FA_HISTCUOTAS.IND_PAGADO IS 'Indicativo de Pagado'
/
