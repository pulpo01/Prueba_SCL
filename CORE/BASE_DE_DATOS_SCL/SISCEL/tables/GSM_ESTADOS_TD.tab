CREATE TABLE GSM_ESTADOS_TD(
    COD_MODULO VARCHAR2(2) NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) NOT NULL 
 ,  DES_ESTADO VARCHAR2(30) NOT NULL 
 ,  TIP_ESTADO NUMBER(1,0) NOT NULL 
 ,  FEC_ACTUALIZACION DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_ESTADOS_TD IS 'Tabla contiene los estados existente  para la simcard en un modulo o sistema.'
/
COMMENT ON COLUMN  GSM_ESTADOS_TD.COD_MODULO IS 'Codigo del modulo.'
/
COMMENT ON COLUMN  GSM_ESTADOS_TD.COD_ESTADO IS 'Estado de la SIMcard.'
/
COMMENT ON COLUMN  GSM_ESTADOS_TD.DES_ESTADO IS 'Descripcion del estado.'
/
COMMENT ON COLUMN  GSM_ESTADOS_TD.TIP_ESTADO IS 'Tipo de estado, Modificable o estatico'
/
COMMENT ON COLUMN  GSM_ESTADOS_TD.FEC_ACTUALIZACION IS 'Fecha de actualizacion.'
/
COMMENT ON COLUMN  GSM_ESTADOS_TD.COD_USUARIO IS 'Usuario que actualizo.'
/
