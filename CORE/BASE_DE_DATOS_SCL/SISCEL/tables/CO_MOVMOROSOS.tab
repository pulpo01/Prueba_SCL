CREATE TABLE CO_MOVMOROSOS(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_SUSPREHA VARCHAR2(3) NOT NULL 
 ,  COD_ACTABO VARCHAR2(2) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  FEC_EFECTIVIDAD DATE NOT NULL 
 ,  IMP_DEUDA NUMBER(14,4) NOT NULL 
 ,  NUM_DIASDEUDA NUMBER(3,0) NOT NULL 
 ,  NUM_PROCESO NUMBER(8,0) NOT NULL 
 ,  RESP_CENTRAL VARCHAR2(40) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_MOVMOROSOS IS 'Movimientos Contra Morosos- Retrofitted'
/
COMMENT ON COLUMN  CO_MOVMOROSOS.COD_CLIENTE IS 'Codigo de Cliente - Retrofitted'
/
COMMENT ON COLUMN  CO_MOVMOROSOS.NUM_ABONADO IS 'Numero de abonado - Retrofitted'
/
COMMENT ON COLUMN  CO_MOVMOROSOS.COD_SUSPREHA IS 'Codigo de Movimiento para Morosos - Retrofitted'
/
COMMENT ON COLUMN  CO_MOVMOROSOS.COD_ACTABO IS 'Codigo de Actuacion para Morosos - Retrofitted'
/
COMMENT ON COLUMN  CO_MOVMOROSOS.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  CO_MOVMOROSOS.FEC_EFECTIVIDAD IS 'Fecha de Efectividad - Retrofitted'
/
COMMENT ON COLUMN  CO_MOVMOROSOS.IMP_DEUDA IS 'Importe de la Deuda - Retrofitted'
/
COMMENT ON COLUMN  CO_MOVMOROSOS.NUM_DIASDEUDA IS 'Antiguedad de la Deuda - Retrofitted'
/
COMMENT ON COLUMN  CO_MOVMOROSOS.NUM_PROCESO IS 'Numero de Proceso - Retrofitted'
/
COMMENT ON COLUMN  CO_MOVMOROSOS.RESP_CENTRAL IS 'Respuesta de IC - Retrofitted'
/
