CREATE TABLE GA_LIMITE_CLIABO_TH
(
  COD_CLIENTE     NUMBER(8)                     NOT NULL,
  NUM_ABONADO     NUMBER(8)                     NOT NULL,
  COD_LIMCONS     VARCHAR2(5)              NOT NULL,
  COD_PLANTARIF   VARCHAR2(5)              NOT NULL,
  FEC_DESDE       DATE                          NOT NULL,
  FEC_HISTORICO   DATE                          NOT NULL,
  FEC_HASTA       DATE,
  NOM_USUARORA    VARCHAR2(30),
  FEC_ASIGNACION  DATE                          NOT NULL,
  COD_CAUSA_HIST  VARCHAR2(5)              NOT NULL,
  EST_APLICA_TOL  NUMBER(2),
  FEC_APLICA_TOL  DATE
)
TABLESPACE TS_CRM_POSVENTA_SDAT
PCTUSED    0
PCTFREE    5
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          6160K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING
/

COMMENT ON TABLE GA_LIMITE_CLIABO_TH IS 'Tabla Con Detalle De Series De Una Venta.'
/

COMMENT ON COLUMN GA_LIMITE_CLIABO_TH.COD_CLIENTE IS 'Codigo de Cliente'
/

COMMENT ON COLUMN GA_LIMITE_CLIABO_TH.NUM_ABONADO IS 'Numero de Abonado'
/

COMMENT ON COLUMN GA_LIMITE_CLIABO_TH.COD_LIMCONS IS 'Codigo de Limite de Consumo'
/

COMMENT ON COLUMN GA_LIMITE_CLIABO_TH.COD_PLANTARIF IS 'Codigo Plan Tarifario del Abonado'
/

COMMENT ON COLUMN GA_LIMITE_CLIABO_TH.FEC_DESDE IS 'Fecha Inicio Vigencia'
/

COMMENT ON COLUMN GA_LIMITE_CLIABO_TH.FEC_HISTORICO IS 'Fecha ingreso registro'
/

COMMENT ON COLUMN GA_LIMITE_CLIABO_TH.FEC_HASTA IS 'Fecha Termino Vigencia (Null=Vigencia Infinita)'
/

COMMENT ON COLUMN GA_LIMITE_CLIABO_TH.NOM_USUARORA IS 'Usuaurio Oracle que realiza elingreso o cambio'
/

COMMENT ON COLUMN GA_LIMITE_CLIABO_TH.FEC_ASIGNACION IS 'Fecha Ingreso Registro'
/

COMMENT ON COLUMN GA_LIMITE_CLIABO_TH.COD_CAUSA_HIST IS 'Codigo Causa Paso a Historico'
/

COMMENT ON COLUMN GA_LIMITE_CLIABO_TH.EST_APLICA_TOL IS 'Estado de Aplicacion en TOL'
/

COMMENT ON COLUMN GA_LIMITE_CLIABO_TH.FEC_APLICA_TOL IS 'Fecha Estadoalcanzadoen Tol'
/