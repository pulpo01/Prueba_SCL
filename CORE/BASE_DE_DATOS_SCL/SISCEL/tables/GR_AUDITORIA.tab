CREATE TABLE GR_AUDITORIA(
    NUM_EJECUCION NUMBER(10,0) NOT NULL 
 ,  COD_REPORTE VARCHAR2(8) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_EMISION_DH DATE NOT NULL 
 ,  FEC_EMISION_D DATE NOT NULL 
 ,  IND_MODO CHAR(1) NOT NULL 
 ,  FEC_TERMINO_DH DATE NULL 
 ,  FEC_TERMINO_D DATE NULL 
 ,  COD_ESTADO CHAR(1) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GR_AUDITORIA IS 'Tabla de Auditoria para la generacion de Reportes'
/
COMMENT ON COLUMN  GR_AUDITORIA.NUM_EJECUCION IS 'Genera una secuencia'
/
COMMENT ON COLUMN  GR_AUDITORIA.COD_REPORTE IS 'Codigo del reporte ejecutado. Tabla GR_REPORTES'
/
COMMENT ON COLUMN  GR_AUDITORIA.NOM_USUARIO IS 'Nombre del usuario que ejecuta el reporte. Tabla GE_SEG_USUARIO'
/
COMMENT ON COLUMN  GR_AUDITORIA.FEC_EMISION_DH IS 'Fecha emision del reporte NO truncada'
/
COMMENT ON COLUMN  GR_AUDITORIA.FEC_EMISION_D IS 'Fecha emision del reporte truncada'
/
COMMENT ON COLUMN  GR_AUDITORIA.IND_MODO IS 'Indicador modo de ejecucion. T=Trasfondo; I=Interactivo'
/
COMMENT ON COLUMN  GR_AUDITORIA.FEC_TERMINO_DH IS 'Fecha termino de ejecucion del reporte NO truncada'
/
COMMENT ON COLUMN  GR_AUDITORIA.FEC_TERMINO_D IS 'Fecha termino de ejecucion del reporte NO truncada'
/
COMMENT ON COLUMN  GR_AUDITORIA.COD_ESTADO IS 'Codigo de estado de la ejecucion del reporte. E=En Ejecucion; F=Finalizado'
/
