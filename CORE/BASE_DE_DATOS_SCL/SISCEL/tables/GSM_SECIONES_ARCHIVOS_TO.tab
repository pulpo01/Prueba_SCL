CREATE TABLE GSM_SECIONES_ARCHIVOS_TO(
    SEC_ARCHIVO_SECCION NUMBER(10,0) NOT NULL 
 ,  COD_ARCHIVO VARCHAR2(6) NOT NULL 
 ,  NUM_VERSION NUMBER(10,0) NOT NULL 
 ,  NUM_SEQ_ARCHIVO NUMBER(3,0) NULL 
 ,  COD_SECCION VARCHAR2(10) NOT NULL 
 ,  COD_TIPO_SECCION VARCHAR2(2) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_SECIONES_ARCHIVOS_TO IS 'Tabla de Relacion Secciones para un archivo'
/
COMMENT ON COLUMN  GSM_SECIONES_ARCHIVOS_TO.SEC_ARCHIVO_SECCION IS 'Sequential.'
/
COMMENT ON COLUMN  GSM_SECIONES_ARCHIVOS_TO.COD_ARCHIVO IS 'Codigo de archivo.'
/
COMMENT ON COLUMN  GSM_SECIONES_ARCHIVOS_TO.NUM_VERSION IS 'Version del archivo.'
/
COMMENT ON COLUMN  GSM_SECIONES_ARCHIVOS_TO.NUM_SEQ_ARCHIVO IS 'Codigo de seccion del archivo.'
/
COMMENT ON COLUMN  GSM_SECIONES_ARCHIVOS_TO.COD_SECCION IS 'Numero secuencial de la seccion en el archivo.'
/
COMMENT ON COLUMN  GSM_SECIONES_ARCHIVOS_TO.COD_TIPO_SECCION IS 'Tipo de seccion.'
/
