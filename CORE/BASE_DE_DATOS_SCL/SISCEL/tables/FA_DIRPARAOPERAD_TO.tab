CREATE TABLE FA_DIRPARAOPERAD_TO (  
   COD_OPERAD                    VARCHAR2(5)    NOT NULL
 , TIP_SUJETO                    NUMBER(3)      NOT NULL
 , COD_DISPLAY                   NUMBER(3)      NOT NULL
 , DIR_PARA                      VARCHAR2(1000) NOT NULL 
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  FA_DIRPARAOPERAD_TO                      IS '[ FADR ] Direccion Parametrica de la OperadoraDireccion Parametrica de la Operadora'
/

COMMENT ON COLUMN FA_DIRPARAOPERAD_TO.COD_OPERAD                     IS 'Código de la Operadora'
/
COMMENT ON COLUMN FA_DIRPARAOPERAD_TO.TIP_SUJETO                     IS 'Tipo Sujeto (1:Cliente,2:Usuario)'
/
COMMENT ON COLUMN FA_DIRPARAOPERAD_TO.COD_DISPLAY                    IS 'Despliegue de Dirección'
/
COMMENT ON COLUMN FA_DIRPARAOPERAD_TO.DIR_PARA                       IS 'Query para la obtención de la Dirección'
/

