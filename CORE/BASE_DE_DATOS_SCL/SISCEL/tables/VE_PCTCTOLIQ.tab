CREATE TABLE VE_PCTCTOLIQ(
    COD_CTOLIQ NUMBER(6,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  IMP_PORCENTAJE NUMBER(5,2) NOT NULL 
 ,  FEC_EFECTIVIDAD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_FINEFECTIVIDAD DATE NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_PCTCTOLIQ IS 'Porcentaje de participacion de los Conceptos de Liquidacion'
/
COMMENT ON COLUMN  VE_PCTCTOLIQ.COD_CTOLIQ IS 'Codigo de Concepto Liquidacion'
/
COMMENT ON COLUMN  VE_PCTCTOLIQ.COD_TIPCOMIS IS 'Codigo de Tipo de Comisionista'
/
COMMENT ON COLUMN  VE_PCTCTOLIQ.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  VE_PCTCTOLIQ.IMP_PORCENTAJE IS 'Porcentaje del concepto liquidacion'
/
COMMENT ON COLUMN  VE_PCTCTOLIQ.FEC_EFECTIVIDAD IS 'Fecha de efectividad'
/
COMMENT ON COLUMN  VE_PCTCTOLIQ.NOM_USUARIO IS 'Usuario que define el porcentaje'
/
COMMENT ON COLUMN  VE_PCTCTOLIQ.FEC_FINEFECTIVIDAD IS 'Fecha de fin de efectividad'
/
