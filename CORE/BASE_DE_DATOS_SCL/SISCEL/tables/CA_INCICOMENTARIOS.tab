CREATE TABLE CA_INCICOMENTARIOS(
    NUM_INCIDENCIA NUMBER(9,0) NOT NULL 
 ,  FEC_COMENTARIO DATE NOT NULL 
 ,  COMENTARIO VARCHAR2(500) NOT NULL 
 ,  USU_COMENTARIO VARCHAR2(30) NOT NULL 
 ,  COD_SECTOR NUMBER(2,0) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_INCICOMENTARIOS IS 'Incidencias - Comentarios'
/
COMMENT ON COLUMN  CA_INCICOMENTARIOS.NUM_INCIDENCIA IS 'Numero Inciencia'
/
COMMENT ON COLUMN  CA_INCICOMENTARIOS.FEC_COMENTARIO IS 'Fecha Comentario'
/
COMMENT ON COLUMN  CA_INCICOMENTARIOS.COMENTARIO IS 'Comentario'
/
COMMENT ON COLUMN  CA_INCICOMENTARIOS.USU_COMENTARIO IS 'Usuario Comentario'
/
COMMENT ON COLUMN  CA_INCICOMENTARIOS.COD_SECTOR IS 'Codigo Sector'
/
