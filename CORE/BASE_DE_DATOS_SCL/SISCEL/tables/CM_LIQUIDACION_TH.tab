 ,  TOT_PAGAR NUMBER(14,4) DEFAULT 0 NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  ID_PERIODO VARCHAR2(10) DEFAULT 'NN' NOT NULL 
 ,  IND_APLIC_SUBORDINADA CHAR(1) DEFAULT 'N' NOT NULL 
 ,  IND_CUMPLIMIENTO CHAR(1) DEFAULT 'S' NOT NULL 
