CREATE TABLE PV_INTERFAZ_MIGRA_TO
(
  NUM_CELULAR    NUMBER(15)                     NOT NULL,
  NUM_ABONADO    NUMBER(8)                      NOT NULL,
  COD_CLIENTE    NUMBER(8)                      NOT NULL,
  COD_PLANTARIF  VARCHAR2(3 BYTE)               NOT NULL,
  IND_PROCEQUI   VARCHAR2(2 BYTE)               NOT NULL,
  NOM_USUARIO    VARCHAR2(30 BYTE)              NOT NULL,
  NUM_SERIE      VARCHAR2(25 BYTE)              NOT NULL,
  FEC_EJECUCION  DATE                           NOT NULL,
  COD_ESTADO     VARCHAR2(1 BYTE),
  COD_ACTABO     VARCHAR2(2 BYTE)               NOT NULL,
  FEC_PROCESO    DATE,
  NUM_OS         NUMBER(10),
  COD_OS         VARCHAR2(5 BYTE),
  NUM_EVENTO     NUMBER(9),
  DESCRIPCION    VARCHAR2(3000 BYTE),
  IMP_SALDO      VARCHAR2(255 BYTE)
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          1M
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;


COMMENT ON TABLE PV_INTERFAZ_MIGRA_TO IS 'Tabla proceso migracion masiva de Prepagos';

COMMENT ON COLUMN PV_INTERFAZ_MIGRA_TO.NUM_CELULAR IS 'Numero Celular';

COMMENT ON COLUMN PV_INTERFAZ_MIGRA_TO.NUM_ABONADO IS 'numero de abonado';

COMMENT ON COLUMN PV_INTERFAZ_MIGRA_TO.COD_CLIENTE IS 'Código cliente';

COMMENT ON COLUMN PV_INTERFAZ_MIGRA_TO.COD_PLANTARIF IS 'Código de Plan Tarifario';

COMMENT ON COLUMN PV_INTERFAZ_MIGRA_TO.IND_PROCEQUI IS 'Indicador de Procedencia';

COMMENT ON COLUMN PV_INTERFAZ_MIGRA_TO.NOM_USUARIO IS 'Nombre de usuario';

COMMENT ON COLUMN PV_INTERFAZ_MIGRA_TO.FEC_EJECUCION IS 'Fecha de ejecución';

COMMENT ON COLUMN PV_INTERFAZ_MIGRA_TO.COD_ESTADO IS 'Estado de Proceso (1-Inscrita,2-Procesada,3-Cancelada)';

COMMENT ON COLUMN PV_INTERFAZ_MIGRA_TO.COD_ACTABO IS 'Codigo Actuacion Comercial';

COMMENT ON COLUMN PV_INTERFAZ_MIGRA_TO.FEC_PROCESO IS 'Fecha de proceso';

COMMENT ON COLUMN PV_INTERFAZ_MIGRA_TO.NUM_OS IS 'numero de la OOSS';

COMMENT ON COLUMN PV_INTERFAZ_MIGRA_TO.COD_OS IS 'codigo de la OOSS';

COMMENT ON COLUMN PV_INTERFAZ_MIGRA_TO.NUM_EVENTO IS 'numero de evento del error';

COMMENT ON COLUMN PV_INTERFAZ_MIGRA_TO.DESCRIPCION IS 'Descripción del error';

COMMENT ON COLUMN PV_INTERFAZ_MIGRA_TO.IMP_SALDO IS 'Saldo del prepago';