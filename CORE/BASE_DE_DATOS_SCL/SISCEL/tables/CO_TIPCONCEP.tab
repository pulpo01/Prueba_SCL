CREATE TABLE CO_TIPCONCEP(
    COD_TIPCONCE NUMBER(2,0) NOT NULL 
 ,  DES_TIPCONCE VARCHAR2(40) NOT NULL 
 ,  IND_ABONO NUMBER(1,0) NOT NULL 
 ,  IND_RECARGO NUMBER(1,0) NOT NULL 
 ,  IND_IMPUESTO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_TIPCONCEP IS 'Tipos de Conceptos Cobros'
/
COMMENT ON COLUMN  CO_TIPCONCEP.COD_TIPCONCE IS 'Tipo de conceptos'
/
COMMENT ON COLUMN  CO_TIPCONCEP.DES_TIPCONCE IS 'Descripcion tipo concepto'
/
COMMENT ON COLUMN  CO_TIPCONCEP.IND_ABONO IS 'Indicador de Abono'
/
COMMENT ON COLUMN  CO_TIPCONCEP.IND_RECARGO IS 'Indicador Recargo'
/
COMMENT ON COLUMN  CO_TIPCONCEP.IND_IMPUESTO IS 'Indicador que identifica a un tipo de concepto que genera Impuesto'
/
