CREATE TABLE GE_MTOPREAUTOCLI_TH (  
   COD_CLIENTE                   NUMBER(8)          NOT NULL
 , MTO_PREAUTO                   NUMBER(14,4)       NOT NULL
 , COD_CREDITICIA                VARCHAR2(5)        NOT NULL
 , FEC_MODIFICACION              DATE               NOT NULL
 , NOM_USUARORA                  VARCHAR2(30)       NOT NULL
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  GE_MTOPREAUTOCLI_TH                      IS '[ GA_MPAH ] Monto Preautorizado historico ClienteMonto Preautorizado historico Cliente'
/

COMMENT ON COLUMN GE_MTOPREAUTOCLI_TH.COD_CLIENTE                    IS 'codigo cliente SCL'
/
COMMENT ON COLUMN GE_MTOPREAUTOCLI_TH.MTO_PREAUTO                    IS 'monto preautorizado'
/
COMMENT ON COLUMN GE_MTOPREAUTOCLI_TH.COD_CREDITICIA                 IS 'clasificacion crediticia'
/
COMMENT ON COLUMN GE_MTOPREAUTOCLI_TH.FEC_MODIFICACION               IS 'fecha modificacion'
/
COMMENT ON COLUMN GE_MTOPREAUTOCLI_TH.NOM_USUARORA                   IS 'usuario de modificacion'
/

