CREATE TABLE GA_REASIGNA_CLI_TO(
    COD_PRODUCTO NUMBER(1,0) DEFAULT 1 NOT NULL 
 ,  FEC_REASIGNA DATE NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_CLIENTE_ORIG NUMBER(8,0) NOT NULL 
 ,  COD_CLIENTE_DEST NUMBER(8,0) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  COD_ACTABO VARCHAR2(2) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_REASIGNA_CLI_TO IS 'REASIGNACION DEL CLIENTE DE UN ABONADO'
/
COMMENT ON COLUMN  GA_REASIGNA_CLI_TO.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_REASIGNA_CLI_TO.FEC_REASIGNA IS 'Fecha en que se realiza la Reasignacion'
/
COMMENT ON COLUMN  GA_REASIGNA_CLI_TO.NUM_ABONADO IS 'Identificador del Numero de Abonado que es reasignado'
/
COMMENT ON COLUMN  GA_REASIGNA_CLI_TO.COD_CLIENTE_ORIG IS 'Codigo del cliente al que originalmente pertenece el abonado'
/
COMMENT ON COLUMN  GA_REASIGNA_CLI_TO.COD_CLIENTE_DEST IS 'Codigo del cliente al que es reasignado el abonado'
/
COMMENT ON COLUMN  GA_REASIGNA_CLI_TO.NOM_USUARORA IS 'Usuario que realiza la reasignacion'
/
COMMENT ON COLUMN  GA_REASIGNA_CLI_TO.COD_ACTABO IS 'Actuacion que causa la reasignacion'
/
