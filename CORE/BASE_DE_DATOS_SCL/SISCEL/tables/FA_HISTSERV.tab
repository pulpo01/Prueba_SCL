CREATE TABLE FA_HISTSERV(
    COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_CICLFACT NUMBER(6,0) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  IMP_PERIODO NUMBER(12,4) NOT NULL 
 ,  NUM_DIAS NUMBER(2,0) NOT NULL 
 ,  NUM_PERIODOS NUMBER(2,0) NOT NULL 
 ,  FEC_EFECTIVIDAD DATE NOT NULL 
 ,  NUM_PROCESO NUMBER(8,0) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_HISTSERV IS 'Historico de servicios.'
/
COMMENT ON COLUMN  FA_HISTSERV.COD_CLIENTE IS 'Codigo de Cliente'
/
COMMENT ON COLUMN  FA_HISTSERV.NUM_ABONADO IS 'Numero de abonado'
/
COMMENT ON COLUMN  FA_HISTSERV.COD_CICLFACT IS 'Codigo de ciclo periodico de Facturacion'
/
COMMENT ON COLUMN  FA_HISTSERV.COD_CONCEPTO IS 'Codigo de concepto de facturacion'
/
COMMENT ON COLUMN  FA_HISTSERV.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  FA_HISTSERV.IMP_PERIODO IS 'Importe Peridodo'
/
COMMENT ON COLUMN  FA_HISTSERV.NUM_DIAS IS 'Dias que se aplica la tarifa prorrateada'
/
COMMENT ON COLUMN  FA_HISTSERV.NUM_PERIODOS IS 'Numero de periodos que se cobran'
/
COMMENT ON COLUMN  FA_HISTSERV.FEC_EFECTIVIDAD IS 'Fecha de Efectividad'
/
COMMENT ON COLUMN  FA_HISTSERV.NUM_PROCESO IS 'Numero de Proceso'
/
