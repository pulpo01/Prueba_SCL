CREATE TABLE AL_CAB_NUMERACION
(
  NUM_NUMERACION     NUMBER(9)                  NOT NULL,
  COD_ESTADO         VARCHAR2(2 BYTE)           NOT NULL,
  COD_BODEGA         NUMBER(6)                  NOT NULL,
  TIP_NUMERACION     NUMBER(1)                  NOT NULL,
  FEC_CIERRE         DATE                       NOT NULL,
  USU_CIERRE         VARCHAR2(30 BYTE)          NOT NULL,
  DES_OBSERVACIONES  VARCHAR2(100 BYTE),
  TIP_LLAMADA        NUMBER(1)
)
TABLESPACE TS_LOGISTICA_SDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          400K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE AL_CAB_NUMERACION IS 'Cabeceras de Numeracion';

COMMENT ON COLUMN AL_CAB_NUMERACION.TIP_LLAMADA IS 'Tipo de llamada inicial. 1=Call Center,2=primera llamada';

COMMENT ON COLUMN AL_CAB_NUMERACION.NUM_NUMERACION IS 'Numero de Numeracion';

COMMENT ON COLUMN AL_CAB_NUMERACION.COD_ESTADO IS 'Estado en el que se encuentra la entrada/salida extra';

COMMENT ON COLUMN AL_CAB_NUMERACION.COD_BODEGA IS 'Bodega en la que se efectua la numeracion';

COMMENT ON COLUMN AL_CAB_NUMERACION.TIP_NUMERACION IS 'Tipo de numeracion';

COMMENT ON COLUMN AL_CAB_NUMERACION.FEC_CIERRE IS 'Fecha de Cierre.';

COMMENT ON COLUMN AL_CAB_NUMERACION.USU_CIERRE IS 'Usuario que realiza el cierre.';

COMMENT ON COLUMN AL_CAB_NUMERACION.DES_OBSERVACIONES IS 'Obsevaciones';

