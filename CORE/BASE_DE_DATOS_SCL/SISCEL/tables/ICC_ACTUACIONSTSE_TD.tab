CREATE TABLE ICC_ACTUACIONSTSE_TD(
    COD_ACTUACION_STSE NUMBER NOT NULL 
 ,  COD_ACTUACION_EXT VARCHAR2(15) NOT NULL 
 ,  IND_VOZ VARCHAR2(1) NOT NULL 
 ,  IND_DAT VARCHAR2(1) NOT NULL 
 ,  IND_SMS VARCHAR2(1) NOT NULL 
 ,  IND_WAP VARCHAR2(1) NOT NULL 
 ,  COD_SERVICIOS VARCHAR2(255) NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICC_ACTUACIONSTSE_TD IS 'Actuaciones que se gatillan desde sixbell'
/
COMMENT ON COLUMN  ICC_ACTUACIONSTSE_TD.COD_ACTUACION_STSE IS 'Codigo de Actuacion de Siscel'
/
COMMENT ON COLUMN  ICC_ACTUACIONSTSE_TD.COD_ACTUACION_EXT IS 'Codigo de Actuacion enviada por sixbell'
/
COMMENT ON COLUMN  ICC_ACTUACIONSTSE_TD.IND_VOZ IS 'Indica la Accion sobre el servicio de voz'
/
COMMENT ON COLUMN  ICC_ACTUACIONSTSE_TD.IND_DAT IS 'Indica la Accion sobre el servicio de datos'
/
COMMENT ON COLUMN  ICC_ACTUACIONSTSE_TD.IND_SMS IS 'Indica la Accion sobre el servicio de sms'
/
COMMENT ON COLUMN  ICC_ACTUACIONSTSE_TD.IND_WAP IS 'Indica la Accion sobre el servicio de wap'
/
COMMENT ON COLUMN  ICC_ACTUACIONSTSE_TD.COD_SERVICIOS IS 'Indica los servicios asociados a la llave'
/
