CREATE TABLE VE_MODDTOLLAM(
    COD_DTOLLAM NUMBER(6,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  DES_DTOLLAM VARCHAR2(30) NOT NULL 
 ,  FEC_MODIFIC DATE NOT NULL 
 ,  COD_MATRICFRANJA VARCHAR2(2000) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_MINVIGENCIA DATE NOT NULL 
 ,  FEC_MAXVIGENCIA DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_MODDTOLLAM IS 'Modificaciones de Planes de Descuento de Llamadas'
/
COMMENT ON COLUMN  VE_MODDTOLLAM.COD_DTOLLAM IS 'Codigo Plan Descuento Llamadas'
/
COMMENT ON COLUMN  VE_MODDTOLLAM.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  VE_MODDTOLLAM.DES_DTOLLAM IS 'Descripcion Plan Descuento Llamadas'
/
COMMENT ON COLUMN  VE_MODDTOLLAM.FEC_MODIFIC IS 'Fecha modificacion'
/
COMMENT ON COLUMN  VE_MODDTOLLAM.COD_MATRICFRANJA IS 'Matricula de franjas asociadas'
/
COMMENT ON COLUMN  VE_MODDTOLLAM.NOM_USUARIO IS 'Usuario que realiza la modificacion'
/
COMMENT ON COLUMN  VE_MODDTOLLAM.FEC_MINVIGENCIA IS 'Fecha Inicio Vigencia'
/
COMMENT ON COLUMN  VE_MODDTOLLAM.FEC_MAXVIGENCIA IS 'Fecha Fin Vigencia'
/
