CREATE TABLE GET_ERR_DETPARAMETROS(
    NUM_EVENTO NUMBER(10,0) NOT NULL 
 ,  COD_PARAMETRO NUMBER(6,0) NOT NULL 
 ,  VAL_PARAMETRO VARCHAR2(100) NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GET_ERR_DETPARAMETROS IS 'Registra, En Detalle, El Valor De Los Parametros De La Transaccion.'
/
COMMENT ON COLUMN  GET_ERR_DETPARAMETROS.NUM_EVENTO IS 'Numero de evento al que pertenece el detalle.'
/
COMMENT ON COLUMN  GET_ERR_DETPARAMETROS.COD_PARAMETRO IS 'Parametro en particular que es detallado.'
/
COMMENT ON COLUMN  GET_ERR_DETPARAMETROS.VAL_PARAMETRO IS 'Valor del parametro al momento de la incidencia de error..'
/
