CREATE TABLE GAD_CAUSAS_OOSS(
    COD_PRODUCTO NUMBER(1,0) DEFAULT 1 NOT NULL 
 ,  COD_OS VARCHAR2(5) NOT NULL 
 ,  TIP_SELECCION NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  NOM_TABLA VARCHAR2(30) NOT NULL 
 ,  COD_CAUSA VARCHAR2(5) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAD_CAUSAS_OOSS IS 'Tabla De Datos De Causas Por Orden De Servicio..'
/
COMMENT ON COLUMN  GAD_CAUSAS_OOSS.COD_PRODUCTO IS 'código de producto.'
/
COMMENT ON COLUMN  GAD_CAUSAS_OOSS.COD_OS IS 'código de Orden de Servicio.'
/
COMMENT ON COLUMN  GAD_CAUSAS_OOSS.TIP_SELECCION IS 'Tipo de selección 0 xxxxxxxxxxx.'
/
COMMENT ON COLUMN  GAD_CAUSAS_OOSS.NOM_TABLA IS 'Nombre Tabla que contitne las causas.'
/
COMMENT ON COLUMN  GAD_CAUSAS_OOSS.COD_CAUSA IS 'Código de la causa en la tabla especidicada.'
/
COMMENT ON COLUMN  GAD_CAUSAS_OOSS.FEC_DESDE IS 'Fecha inicio validez de la realción.'
/
COMMENT ON COLUMN  GAD_CAUSAS_OOSS.NOM_USUARIO IS 'último Usuario Oracle que ingresa el registro o lo modifica.'
/
COMMENT ON COLUMN  GAD_CAUSAS_OOSS.FEC_HASTA IS 'Fecha limite validez de la relación.'
/
