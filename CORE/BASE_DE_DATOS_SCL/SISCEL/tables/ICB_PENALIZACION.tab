CREATE TABLE ICB_PENALIZACION(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_MODULO VARCHAR2(2) NOT NULL 
 ,  COD_SUSPREHA VARCHAR2(3) NOT NULL 
 ,  FEC_INGRESO DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_SERVICIO NUMBER(2,0) NOT NULL 
 ,  COD_NIVEL NUMBER(4,0) NOT NULL 
 ,  TIP_SUSPENSION VARCHAR2(2) NOT NULL 
 ,  COD_NIVEL_REA NUMBER(4,0) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICB_PENALIZACION IS 'Penalizaciones del abonado beeper y control'
/
COMMENT ON COLUMN  ICB_PENALIZACION.NUM_ABONADO IS 'Numero de Abonado'
/
COMMENT ON COLUMN  ICB_PENALIZACION.COD_MODULO IS 'Codigo de Modulo'
/
COMMENT ON COLUMN  ICB_PENALIZACION.COD_SUSPREHA IS 'Codigo de Suspension/Rehabilitacion'
/
COMMENT ON COLUMN  ICB_PENALIZACION.FEC_INGRESO IS 'Fecha de Ingreso de la Suspension'
/
COMMENT ON COLUMN  ICB_PENALIZACION.COD_SERVICIO IS 'Codigo de Servicio Suspendido'
/
COMMENT ON COLUMN  ICB_PENALIZACION.COD_NIVEL IS 'Nivel al que se supende el servicio'
/
COMMENT ON COLUMN  ICB_PENALIZACION.TIP_SUSPENSION IS 'Tipo de Suspension (Multiservicio o de un unico servicio)'
/
COMMENT ON COLUMN  ICB_PENALIZACION.COD_NIVEL_REA IS 'Nivel al que se rehabilitara el servicio'
/
