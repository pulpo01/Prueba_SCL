CREATE TABLE GA_TARIFAS(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_ACTABO VARCHAR2(2) NOT NULL 
 ,  COD_TIPSERV VARCHAR2(1) NOT NULL 
 ,  COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  COD_PLANSERV VARCHAR2(3) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  IMP_TARIFA NUMBER(14,4) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  IND_PERIODICO VARCHAR2(1) NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_TARIFAS IS 'Tarifas x Servicios-'
/
COMMENT ON COLUMN  GA_TARIFAS.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_TARIFAS.COD_ACTABO IS 'Codigo de Actuacion de Abonados'
/
COMMENT ON COLUMN  GA_TARIFAS.COD_TIPSERV IS 'Codigo de Tipo de Servicio'
/
COMMENT ON COLUMN  GA_TARIFAS.COD_SERVICIO IS 'Codigo de Servicio'
/
COMMENT ON COLUMN  GA_TARIFAS.COD_PLANSERV IS 'Codigo de Plan de Servicios'
/
COMMENT ON COLUMN  GA_TARIFAS.FEC_DESDE IS 'Fecha Desde Vigencia Tarifa'
/
COMMENT ON COLUMN  GA_TARIFAS.IMP_TARIFA IS 'Importe de Tarifa'
/
COMMENT ON COLUMN  GA_TARIFAS.COD_MONEDA IS 'Tipo de Moneda en que se Aplica la Tarifa'
/
COMMENT ON COLUMN  GA_TARIFAS.IND_PERIODICO IS 'Indicativo de (O)casional,(P)eriodico'
/
COMMENT ON COLUMN  GA_TARIFAS.FEC_HASTA IS 'Fecha Hasta Vigencia Tarifa'
/
