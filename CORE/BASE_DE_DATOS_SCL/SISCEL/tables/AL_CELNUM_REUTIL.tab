CREATE TABLE AL_CELNUM_REUTIL(
    NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  COD_SUBALM VARCHAR2(5) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  COD_CAT NUMBER(2,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  FEC_BAJA DATE NOT NULL 
 ,  IND_EQUIPADO NUMBER(1,0) NOT NULL 
 ,  NUM_PROCESO NUMBER(8,0) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_CELNUM_REUTIL IS 'Numeracion celular abierta por uso'
/
COMMENT ON COLUMN  AL_CELNUM_REUTIL.NUM_CELULAR IS 'Numero de Celular'
/
COMMENT ON COLUMN  AL_CELNUM_REUTIL.COD_SUBALM IS 'Codigo de Subalm'
/
COMMENT ON COLUMN  AL_CELNUM_REUTIL.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  AL_CELNUM_REUTIL.COD_CENTRAL IS 'Codigo de Central'
/
COMMENT ON COLUMN  AL_CELNUM_REUTIL.COD_CAT IS 'Codigo de Categoria'
/
COMMENT ON COLUMN  AL_CELNUM_REUTIL.COD_USO IS 'Codigo de Uso'
/
COMMENT ON COLUMN  AL_CELNUM_REUTIL.FEC_BAJA IS 'Indicativo de Equipado ; (0) Desequipado, (1) Equipado'
/
COMMENT ON COLUMN  AL_CELNUM_REUTIL.IND_EQUIPADO IS 'Indicador de equipo en central; no(0), si(1)'
/
COMMENT ON COLUMN  AL_CELNUM_REUTIL.NUM_PROCESO IS 'Numero del proceso que lo esta llamando'
/
