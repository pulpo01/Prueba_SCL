CREATE TABLE ERT_SOLICITUD_DETCAMPO(
    NUM_SOLICITUD NUMBER(10,0) NOT NULL 
 ,  IND_TIPO_DEUDA CHAR(1) NULL 
 ,  IND_TIPO_DOCUMENTO VARCHAR2(2) NULL 
 ,  COD_MONEDA VARCHAR2(2) NULL 
 ,  MTO_DEUDA NUMBER(14,4) NULL 
 ,  FEC_VENCIMIENTO DATE NULL 
 ,  DES_LIBRADOR VARCHAR2(75) NULL 
 ,  DES_CAUSA_PUBLICACION VARCHAR2(50) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERT_SOLICITUD_DETCAMPO IS 'Detalle De La Informacion Obtenida De Las Fuentes Para Una Solicitud. Ver Tabla Ert_Solicitud.'
/
COMMENT ON COLUMN  ERT_SOLICITUD_DETCAMPO.NUM_SOLICITUD IS 'Identificador unico de la solicitud. Ver tabla ERT_SOLICITUD.'
/
COMMENT ON COLUMN  ERT_SOLICITUD_DETCAMPO.IND_TIPO_DEUDA IS 'Indicador del tipo de deuda.'
/
COMMENT ON COLUMN  ERT_SOLICITUD_DETCAMPO.IND_TIPO_DOCUMENTO IS 'Indicador del tipo de documento.'
/
COMMENT ON COLUMN  ERT_SOLICITUD_DETCAMPO.COD_MONEDA IS 'Codigo de moneda.'
/
COMMENT ON COLUMN  ERT_SOLICITUD_DETCAMPO.MTO_DEUDA IS 'Monto de la deuda.'
/
COMMENT ON COLUMN  ERT_SOLICITUD_DETCAMPO.FEC_VENCIMIENTO IS 'Fecha de vencimiento.'
/
COMMENT ON COLUMN  ERT_SOLICITUD_DETCAMPO.DES_LIBRADOR IS 'Descripcion del librador.'
/
COMMENT ON COLUMN  ERT_SOLICITUD_DETCAMPO.DES_CAUSA_PUBLICACION IS 'Descripcion de la causa de publicacion.'
/
