CREATE TABLE RET_RESULTADOS_FASES_TO(
    COD_RESULTADO NUMBER(3,0) NOT NULL 
 ,  COD_FASE NUMBER(2,0) NOT NULL 
 ,  NOM_ESTADO_CONTACTO VARCHAR2(50) NOT NULL 
 ,  COD_ESTADO_FICHA NUMBER(1,0) NOT NULL 
 ,  IND_VIGENCIA CHAR(1) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  RET_RESULTADOS_FASES_TO IS 'RELACIÓN FASE - RESULTADOS DE RETENCIÓN'
/
COMMENT ON COLUMN  RET_RESULTADOS_FASES_TO.COD_RESULTADO IS 'RESULTADO DE CONTACTO'
/
COMMENT ON COLUMN  RET_RESULTADOS_FASES_TO.COD_FASE IS 'FASE DE ATENCIÓN AL ABONADO'
/
COMMENT ON COLUMN  RET_RESULTADOS_FASES_TO.NOM_ESTADO_CONTACTO IS 'CRITERIO DE SEGMENTACIÓN'
/
COMMENT ON COLUMN  RET_RESULTADOS_FASES_TO.COD_ESTADO_FICHA IS 'ESTADO DE FICHA DE RETENCIÓN'
/
COMMENT ON COLUMN  RET_RESULTADOS_FASES_TO.IND_VIGENCIA IS 'INDICADOR DE VIGENCIA'
/
