CREATE TABLE FA_INTPROCESOS(
    COD_APLIC VARCHAR2(3) DEFAULT 'FAC' NOT NULL 
 ,  COD_PROCESO NUMBER(3,0) NOT NULL 
 ,  DES_PROCESO VARCHAR2(30) NOT NULL 
 ,  COD_PRIORIDAD NUMBER(2,0) NOT NULL 
 ,  EXE_COMANDO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_INTPROCESOS IS 'Procesos De Interfaz.'
/
COMMENT ON COLUMN  FA_INTPROCESOS.COD_APLIC IS 'Código de Interfaz.'
/
COMMENT ON COLUMN  FA_INTPROCESOS.COD_PROCESO IS 'Código de proceso de interfaz..'
/
COMMENT ON COLUMN  FA_INTPROCESOS.DES_PROCESO IS 'Descripción de proceso de interfaz.'
/
COMMENT ON COLUMN  FA_INTPROCESOS.COD_PRIORIDAD IS 'Prioridad de proceso..'
/
COMMENT ON COLUMN  FA_INTPROCESOS.EXE_COMANDO IS 'Ejecutable UNIX..'
/
