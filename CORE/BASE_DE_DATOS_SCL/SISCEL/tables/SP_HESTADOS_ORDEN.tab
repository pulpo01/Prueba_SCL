CREATE TABLE SP_HESTADOS_ORDEN(
    NUM_ORDEN NUMBER(9,0) NOT NULL 
 ,  COD_ESTADO_ORDEN NUMBER(2,0) NOT NULL 
 ,  FEC_ESTADO DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SP_HESTADOS_ORDEN IS 'Historico De Cambio De Estados.'
/
COMMENT ON COLUMN  SP_HESTADOS_ORDEN.NUM_ORDEN IS 'Numero de Orden de Reparacion.'
/
COMMENT ON COLUMN  SP_HESTADOS_ORDEN.COD_ESTADO_ORDEN IS 'Estado de la orden.'
/
COMMENT ON COLUMN  SP_HESTADOS_ORDEN.FEC_ESTADO IS 'Fecha de estado.'
/
COMMENT ON COLUMN  SP_HESTADOS_ORDEN.NOM_USUARORA IS 'Usuario que registra movimiento.'
/
