CREATE TABLE SISCEL.GAD_SERVSUP_ART
(
  COD_PRODUCTO  NUMBER(1)                       NOT NULL,
  COD_SERVICIO  VARCHAR2(4 BYTE)                NOT NULL,
  COD_ARTICULO  NUMBER(6)                       NOT NULL,
  FEC_DESDE     DATE                            NOT NULL,
  FEC_HASTA     DATE,
  NOM_USUARIO   VARCHAR2(30 BYTE)               NOT NULL
)  TABLESPACE TS_CRM_POSVENTA_SDAT
/

COMMENT ON TABLE SISCEL.GAD_SERVSUP_ART IS 'Servicios Asociados A Articulos.';

COMMENT ON COLUMN SISCEL.GAD_SERVSUP_ART.COD_PRODUCTO IS 'Codigo del producto.';

COMMENT ON COLUMN SISCEL.GAD_SERVSUP_ART.COD_SERVICIO IS 'Codigo del servicio.';

COMMENT ON COLUMN SISCEL.GAD_SERVSUP_ART.COD_ARTICULO IS 'Codigo del articulo.';

COMMENT ON COLUMN SISCEL.GAD_SERVSUP_ART.FEC_DESDE IS 'Fecha Inicio Vigencia.';

COMMENT ON COLUMN SISCEL.GAD_SERVSUP_ART.FEC_HASTA IS 'Fecha termino vigencia.';

COMMENT ON COLUMN SISCEL.GAD_SERVSUP_ART.NOM_USUARIO IS 'Usuario Responsable de creacion de relacion.';

