CREATE TABLE SISCEL.GA_REFERCLI_TO
(
  COD_CLIENTE     NUMBER(8)                     NOT NULL,
  NUM_REFERENCIA  NUMBER(3)                     NOT NULL,
  NOMBRE          VARCHAR2(50 BYTE)                 NULL,
  APELLIDO1       VARCHAR2(20 BYTE)                 NULL,
  APELLIDO2       VARCHAR2(20 BYTE)                 NULL,
  FEC_DESDE       DATE                          NOT NULL,
  FEC_HASTA       DATE                              NULL,
  FEC_ULTMOD      DATE                              NULL,
  NOM_USUARORA    VARCHAR2(30 BYTE)             NOT NULL,
  NUM_TELFIJO     VARCHAR2(15 BYTE)                 NULL,
  NUM_TELMOVIL    VARCHAR2(15 BYTE)                 NULL
)
TABLESPACE INSTALL_DATA
/

COMMENT ON TABLE SISCEL.GA_REFERCLI_TO IS '[ GA_REFCL ] Registro de Referencias de Clientes'
/

COMMENT ON COLUMN SISCEL.GA_REFERCLI_TO.COD_CLIENTE IS 'Codigo de Cliente'
/

COMMENT ON COLUMN SISCEL.GA_REFERCLI_TO.NUM_REFERENCIA IS 'Número de referencia'
/

COMMENT ON COLUMN SISCEL.GA_REFERCLI_TO.NOMBRE IS 'Nombre de Referencia'
/

COMMENT ON COLUMN SISCEL.GA_REFERCLI_TO.APELLIDO1 IS 'Primer Apellido referencia'
/

COMMENT ON COLUMN SISCEL.GA_REFERCLI_TO.APELLIDO2 IS 'Segundo apellido referencia'
/

COMMENT ON COLUMN SISCEL.GA_REFERCLI_TO.NUM_TELFIJO IS 'Telefono Fijo asociado a la referencia'
/

COMMENT ON COLUMN SISCEL.GA_REFERCLI_TO.FEC_DESDE IS 'Fecha inicio vigencia'
/

COMMENT ON COLUMN SISCEL.GA_REFERCLI_TO.FEC_HASTA IS 'Fecha fin vigencia'
/

COMMENT ON COLUMN SISCEL.GA_REFERCLI_TO.FEC_ULTMOD IS 'Fecha Ultima Modificacion'
/

COMMENT ON COLUMN SISCEL.GA_REFERCLI_TO.NOM_USUARORA IS 'Nombre de Usuario'
/

COMMENT ON COLUMN SISCEL.GA_REFERCLI_TO.NUM_TELMOVIL IS 'Telefono Movil asociado a la Referencia'
/

