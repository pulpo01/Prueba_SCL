CREATE TABLE GA_PLANTECPLSERV(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_TECNOLOGIA VARCHAR2(7) NOT NULL 
 ,  COD_PLANTARIF VARCHAR2(3) NOT NULL 
 ,  COD_PLANSERV VARCHAR2(3) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_PLANTECPLSERV IS 'Plan de Servicio por Plan Tarifario y Tecnologia'
/
COMMENT ON COLUMN  GA_PLANTECPLSERV.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_PLANTECPLSERV.COD_TECNOLOGIA IS 'Codigo Tecnologia'
/
COMMENT ON COLUMN  GA_PLANTECPLSERV.COD_PLANTARIF IS 'Codigo Plan Tarifario'
/
COMMENT ON COLUMN  GA_PLANTECPLSERV.COD_PLANSERV IS 'Codigo de Plan de Servicios'
/
COMMENT ON COLUMN  GA_PLANTECPLSERV.FEC_DESDE IS 'Fecha Inicio Vigencia'
/
COMMENT ON COLUMN  GA_PLANTECPLSERV.FEC_HASTA IS 'Fecha Fin Vigencia'
/
COMMENT ON COLUMN  GA_PLANTECPLSERV.FEC_ULTMOD IS 'Fecha Ultima Modificacion'
/
COMMENT ON COLUMN  GA_PLANTECPLSERV.NOM_USUARIO IS 'Usuario quien realizo la modificacion'
/
