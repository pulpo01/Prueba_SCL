CREATE TABLE FA_PARMENSAJE(
    COD_FORMULARIO NUMBER(3,0) NOT NULL 
 ,  COD_BLOQUE NUMBER(1,0) NOT NULL 
 ,  DES_BLOQUE VARCHAR2(40) NULL 
 ,  CANT_LINEASMEN NUMBER(2,0) NOT NULL 
 ,  CANT_CARACTLIN NUMBER(3,0) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_PARMENSAJE IS 'Parametrizacion estructura de mensajes'
/
COMMENT ON COLUMN  FA_PARMENSAJE.COD_FORMULARIO IS 'Tipo de documento destino.    [PK 1/2]'
/
COMMENT ON COLUMN  FA_PARMENSAJE.COD_BLOQUE IS 'Codigo de bloque de mensajes. [PK 2/2]'
/
COMMENT ON COLUMN  FA_PARMENSAJE.DES_BLOQUE IS 'Descripcion del bloque.'
/
COMMENT ON COLUMN  FA_PARMENSAJE.CANT_LINEASMEN IS 'Cantidad maxima de lineas de mensaje en el bloque.'
/
COMMENT ON COLUMN  FA_PARMENSAJE.CANT_CARACTLIN IS 'Cantidad maxima de caracteres por linea.'
/
