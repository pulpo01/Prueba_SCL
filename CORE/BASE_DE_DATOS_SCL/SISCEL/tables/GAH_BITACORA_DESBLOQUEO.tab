CREATE TABLE GAH_BITACORA_DESBLOQUEO(
    NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  COD_BODEGA NUMBER(6,0) NOT NULL 
 ,  FEC_DESBLOQUEO_DH DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ENTRADA DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAH_BITACORA_DESBLOQUEO IS 'Tabla Que Registra Desbloqueos De Celulares En Bodega.'
/
COMMENT ON COLUMN  GAH_BITACORA_DESBLOQUEO.NUM_CELULAR IS 'Numero celular Amistar.'
/
COMMENT ON COLUMN  GAH_BITACORA_DESBLOQUEO.NUM_SERIE IS 'Serie equipo asociada al numero.'
/
COMMENT ON COLUMN  GAH_BITACORA_DESBLOQUEO.COD_BODEGA IS 'Bodega donde encuentra.'
/
COMMENT ON COLUMN  GAH_BITACORA_DESBLOQUEO.FEC_DESBLOQUEO_DH IS 'Fecha de desbloqueo del telefono.'
/
COMMENT ON COLUMN  GAH_BITACORA_DESBLOQUEO.NOM_USUARIO IS 'Usuario que desbloquea el telefono.'
/
COMMENT ON COLUMN  GAH_BITACORA_DESBLOQUEO.FEC_ENTRADA IS 'Fecha de entrada a la bodega.'
/
