CREATE TABLE CO_HISTDETEVENTO(
    COD_EVENTO NUMBER(8,0) NOT NULL 
 ,  IMP_DESDE NUMBER(14,4) NOT NULL 
 ,  IMP_HASTA NUMBER(14,4) NOT NULL 
 ,  DIAS_DESDE NUMBER(3,0) NOT NULL 
 ,  DIAS_HASTA NUMBER(3,0) NOT NULL 
 ,  IND_PAC NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  IND_SAC NUMBER(1,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_HISTDETEVENTO IS 'Tabla historica del detalle del evento'
/
COMMENT ON COLUMN  CO_HISTDETEVENTO.COD_EVENTO IS 'Csdigo de evento'
/
COMMENT ON COLUMN  CO_HISTDETEVENTO.IMP_DESDE IS 'Importe inicial'
/
COMMENT ON COLUMN  CO_HISTDETEVENTO.IMP_HASTA IS 'Importe final'
/
COMMENT ON COLUMN  CO_HISTDETEVENTO.DIAS_DESDE IS 'Nzmero de dmas iniciales'
/
COMMENT ON COLUMN  CO_HISTDETEVENTO.DIAS_HASTA IS 'Nzmero de dmas finales'
/
COMMENT ON COLUMN  CO_HISTDETEVENTO.IND_PAC IS 'Indicador de bancos PAC'
/
COMMENT ON COLUMN  CO_HISTDETEVENTO.IND_SAC IS 'Indicador reclamos SAC'
/
