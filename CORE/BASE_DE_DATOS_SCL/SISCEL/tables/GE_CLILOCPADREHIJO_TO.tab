CREATE TABLE GE_CLILOCPADREHIJO_TO (  
   COD_CLIENTE                   NUMBER(8)          NOT NULL
 , COD_CLI_PADRE                 NUMBER(8)          NOT NULL
 , TIPO_CLIENTE                  VARCHAR(1)             NULL
 , FEC_INI                       DATE               NOT NULL
 , FEC_FIN                       DATE               NOT NULL
 , NOM_USUARIO                   VARCHAR(30)        NOT NULL
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  GE_CLILOCPADREHIJO_TO                      IS '[ GA_CLILOC ] Relacion  histórica clientes locutoriosRelacion  histórica clientes locutorios'
/

COMMENT ON COLUMN GE_CLILOCPADREHIJO_TO.COD_CLIENTE                    IS 'Código de identificacion del cliente'
/
COMMENT ON COLUMN GE_CLILOCPADREHIJO_TO.COD_CLI_PADRE                  IS 'Código de identificacion del cliente padre locutorio'
/
COMMENT ON COLUMN GE_CLILOCPADREHIJO_TO.TIPO_CLIENTE                   IS 'Clasificacion de cliente locutorio (P: cliente Pabre; H: Cliente Hijo)'
/
COMMENT ON COLUMN GE_CLILOCPADREHIJO_TO.FEC_INI                        IS 'Fecha inicio vigencia de la relacion'
/
COMMENT ON COLUMN GE_CLILOCPADREHIJO_TO.FEC_FIN                        IS 'Fecha fin vigencia de la relacion'
/
COMMENT ON COLUMN GE_CLILOCPADREHIJO_TO.NOM_USUARIO                    IS 'Nombre del usuario que modifica el registro'
/

