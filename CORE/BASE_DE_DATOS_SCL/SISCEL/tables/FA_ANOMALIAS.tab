CREATE TABLE FA_ANOMALIAS(
    COD_ANOMALIA NUMBER(4,0) NOT NULL 
 ,  DES_ANOMALIA VARCHAR2(60) NOT NULL 
 ,  IND_GRAVEDAD NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_ANOMALIAS IS 'Anomalias en el Proceso de Facturacion, utilizado en la generacion de logs'
/
COMMENT ON COLUMN  FA_ANOMALIAS.COD_ANOMALIA IS 'Codigo de anomalia'
/
COMMENT ON COLUMN  FA_ANOMALIAS.DES_ANOMALIA IS 'Descripcion de anomalia'
/
COMMENT ON COLUMN  FA_ANOMALIAS.IND_GRAVEDAD IS 'Indicativo de la gravedad de anomalia'
/
