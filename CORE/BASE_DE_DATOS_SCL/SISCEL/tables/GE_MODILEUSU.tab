CREATE TABLE GE_MODILEUSU(
    NOM_TABLA VARCHAR2(30) NULL 
 ,  NOM_COLUMNA VARCHAR2(30) NULL 
 ,  TIP_MOVIMIENTO VARCHAR2(1) NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  NOM_USUARIOSO VARCHAR2(30) NULL 
 ,  FEC_MOVIMIENTO DATE NULL 
 ,  NOM_EJECUTABLE VARCHAR2(48) NULL 
 ,  COD_MODULO VARCHAR2(48) NULL 
 ,  VAL_ANTES VARCHAR2(50) NULL 
 ,  VAL_DESPUES VARCHAR2(50) NULL 
 ,  GLS_ADICIONAL VARCHAR2(200) NULL 
 ) TABLESPACE    TS_GENERAL_MDAT
 NOLOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_MODILEUSU IS 'Tabla para registrar auditorias de campos de otras tablas del sistema'
/
COMMENT ON COLUMN  GE_MODILEUSU.NOM_TABLA IS 'Nombre de la tabla a Auditar'
/
COMMENT ON COLUMN  GE_MODILEUSU.NOM_COLUMNA IS 'Columna de la Tabla a Auditar'
/
COMMENT ON COLUMN  GE_MODILEUSU.TIP_MOVIMIENTO IS 'Tipo de movimientos Update Insert Delete'
/
COMMENT ON COLUMN  GE_MODILEUSU.NOM_USUARIO IS 'Nombre Usuario'
/
COMMENT ON COLUMN  GE_MODILEUSU.NOM_USUARIOSO IS 'Nombre Usuario en el Sistema Operativo'
/
COMMENT ON COLUMN  GE_MODILEUSU.FEC_MOVIMIENTO IS 'Fecha en que se realizo el movimiento'
/
COMMENT ON COLUMN  GE_MODILEUSU.NOM_EJECUTABLE IS 'Ejecutable en donde se activa movimiento'
/
COMMENT ON COLUMN  GE_MODILEUSU.COD_MODULO IS 'Codigo del modulo'
/
COMMENT ON COLUMN  GE_MODILEUSU.VAL_ANTES IS 'Valor anterior de la columna'
/
COMMENT ON COLUMN  GE_MODILEUSU.VAL_DESPUES IS 'Valor posterior de la columna'
/
COMMENT ON COLUMN  GE_MODILEUSU.GLS_ADICIONAL IS 'Valores Adicionales o descripciones especificas del mov.'
/
