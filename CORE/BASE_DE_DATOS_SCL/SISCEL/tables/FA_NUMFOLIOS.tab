CREATE TABLE FA_NUMFOLIOS(
    NUM_PROCESO NUMBER(8,0) NOT NULL 
 ,  COD_TIPDOCUM NUMBER(2,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  NOM_FACTURA VARCHAR2(40) NOT NULL 
 ,  NUM_FOLIO NUMBER(9,0) NULL 
 ,  IND_ORDENTOTAL NUMBER(12,0) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_NUMFOLIOS IS 'Asignacion de numeros de folios a documentos distintos de ciclo'
/
COMMENT ON COLUMN  FA_NUMFOLIOS.NUM_PROCESO IS 'Numero de Proceso de Facturacion'
/
COMMENT ON COLUMN  FA_NUMFOLIOS.COD_TIPDOCUM IS 'Tipo de Documento'
/
COMMENT ON COLUMN  FA_NUMFOLIOS.COD_CLIENTE IS 'Codigo de Cliente'
/
COMMENT ON COLUMN  FA_NUMFOLIOS.NUM_ABONADO IS 'Numero de Abonado'
/
COMMENT ON COLUMN  FA_NUMFOLIOS.NOM_FACTURA IS 'Nombre de fichero de factura'
/
COMMENT ON COLUMN  FA_NUMFOLIOS.NUM_FOLIO IS 'Numero de folio'
/
COMMENT ON COLUMN  FA_NUMFOLIOS.IND_ORDENTOTAL IS 'Asociacion con un Documento'
/
