CREATE TABLE RED_ORIGEN_RECLAMO(
    COD_ORIGENRECLAMO NUMBER(3,0) NOT NULL 
 ,  DES_ORIGENRECLAMO VARCHAR2(60) NOT NULL 
 ,  COD_VIGENCIA VARCHAR2(1) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  RED_ORIGEN_RECLAMO IS 'tabla que indica el origen de la categoria de un reclamo.'
/
COMMENT ON COLUMN  RED_ORIGEN_RECLAMO.COD_ORIGENRECLAMO IS 'Codigo de origen del reclamo.'
/
COMMENT ON COLUMN  RED_ORIGEN_RECLAMO.DES_ORIGENRECLAMO IS 'Descripcion del origen del reclamo.'
/
COMMENT ON COLUMN  RED_ORIGEN_RECLAMO.COD_VIGENCIA IS 'Indicador de vigencia del origen del reclamo.'
/
