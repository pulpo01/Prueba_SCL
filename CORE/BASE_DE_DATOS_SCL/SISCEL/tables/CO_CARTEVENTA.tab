CREATE TABLE CO_CARTEVENTA(
    NUM_TRANSACCION NUMBER(9,0) NOT NULL 
 ,  NUM_VENTA NUMBER(8,0) NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  IMP_CONCEPTO NUMBER(14,4) NOT NULL 
 ,  IND_CANCELADO NUMBER(1,0) NOT NULL 
 ,  FEC_EFECTIVIDAD DATE NOT NULL 
 ,  FEC_CANCELACION DATE NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_CARTEVENTA IS 'CARTERA DE PRIMERA VENTA'
/
COMMENT ON COLUMN  CO_CARTEVENTA.NUM_TRANSACCION IS 'Numero de transaccion - Retrofitted'
/
COMMENT ON COLUMN  CO_CARTEVENTA.NUM_VENTA IS 'NUMERO DE VENTA'
/
COMMENT ON COLUMN  CO_CARTEVENTA.COD_CLIENTE IS 'CODIGO DE CLIENTE'
/
COMMENT ON COLUMN  CO_CARTEVENTA.COD_PRODUCTO IS 'CODIGO DE PRODUCTO'
/
COMMENT ON COLUMN  CO_CARTEVENTA.IMP_CONCEPTO IS 'IMPORTE DEL CONCEPTO'
/
COMMENT ON COLUMN  CO_CARTEVENTA.IND_CANCELADO IS 'INDICA SI ESTA (1) O NO (0) CANCELADO'
/
COMMENT ON COLUMN  CO_CARTEVENTA.FEC_EFECTIVIDAD IS 'FECHA DE EFECTIVIDAD'
/
COMMENT ON COLUMN  CO_CARTEVENTA.FEC_CANCELACION IS 'FECHA DE CANCELACION'
/
