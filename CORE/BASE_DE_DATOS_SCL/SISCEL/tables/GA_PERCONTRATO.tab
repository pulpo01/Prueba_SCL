CREATE TABLE GA_PERCONTRATO(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_TIPCONTRATO VARCHAR2(3) NOT NULL 
 ,  NUM_MESES NUMBER(2,0) NOT NULL 
 ,  COD_PENALIZA NUMBER(2,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_PERCONTRATO IS 'Vigencia de Contratos'
/
COMMENT ON COLUMN  GA_PERCONTRATO.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_PERCONTRATO.COD_TIPCONTRATO IS 'Codigo de Tipo de Contrato'
/
COMMENT ON COLUMN  GA_PERCONTRATO.NUM_MESES IS 'Meses de Vigencia de Contrato'
/
COMMENT ON COLUMN  GA_PERCONTRATO.COD_PENALIZA IS 'Codigo de Penalizacion'
/
