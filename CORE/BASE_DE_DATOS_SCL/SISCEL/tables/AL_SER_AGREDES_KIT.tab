CREATE TABLE AL_SER_AGREDES_KIT(
    NUM_GEN_KIT NUMBER(9,0) NOT NULL 
 ,  NUM_LIN NUMBER(9,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  NUM_SEC_LOCA NUMBER(10,0) NULL 
 ,  PLAN VARCHAR2(3) NULL 
 ,  CARGA NUMBER(14,4) NULL 
 ,  NUM_TELEFONO NUMBER(15,0) NULL 
 ,  IND_TELEFONO NUMBER(1,0) DEFAULT 0 NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_SER_AGREDES_KIT IS 'Detalles Series De Agregacion/Desagregacion.'
/
COMMENT ON COLUMN  AL_SER_AGREDES_KIT.NUM_GEN_KIT IS 'Secuencial de Orden de Agregacion o Desagregacion de KIT.'
/
COMMENT ON COLUMN  AL_SER_AGREDES_KIT.NUM_LIN IS 'Numero de Linea.'
/
COMMENT ON COLUMN  AL_SER_AGREDES_KIT.COD_ARTICULO IS 'Codigo de articulo.'
/
COMMENT ON COLUMN  AL_SER_AGREDES_KIT.NUM_SERIE IS 'Numero de  Serie.'
/
COMMENT ON COLUMN  AL_SER_AGREDES_KIT.NUM_SEC_LOCA IS 'Numero de secuencial de Localizacion.'
/
COMMENT ON COLUMN  AL_SER_AGREDES_KIT.PLAN IS 'Plan de Prepago.'
/
COMMENT ON COLUMN  AL_SER_AGREDES_KIT.CARGA IS 'Carga para Prepago.'
/
