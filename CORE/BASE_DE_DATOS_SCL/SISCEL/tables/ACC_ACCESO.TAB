CREATE TABLE ACC_ACCESO(
    COD_APLI NUMBER(4,0) NOT NULL 
 ,  COD_PERFIL VARCHAR2(5) NOT NULL 
 ,  COD_MENU VARCHAR2(30) NOT NULL 
 ,  COD_PERMISO VARCHAR2(5) NOT NULL 
 ,  FLG_EJECUTA VARCHAR2(1) NULL 
 ,  USER_LOG VARCHAR2(10) NOT NULL 
 ,  FEC_LOG DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  ACC_ACCESO.COD_PERFIL IS 'Codigo del perfil'
/
COMMENT ON COLUMN  ACC_ACCESO.COD_MENU IS 'Codigo de la opcion de menu'
/
COMMENT ON COLUMN  ACC_ACCESO.COD_PERMISO IS 'Codigo de los tipos de permisos que tiene el perfil sobre la opcion de menu'
/
COMMENT ON COLUMN  ACC_ACCESO.FLG_EJECUTA IS 'Flag indicador de ejecucion'
/
COMMENT ON COLUMN  ACC_ACCESO.USER_LOG IS 'Codigo del usuario logeado en el Sistema'
/
COMMENT ON COLUMN  ACC_ACCESO.FEC_LOG IS 'Fecha en que el Usuario realizo la transaccion'
/
