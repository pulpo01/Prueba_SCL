CREATE TABLE CA_HINCICOMENTARIOS(
    NUM_INCIDENCIA NUMBER(9,0) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  FEC_COMENTARIO DATE NOT NULL 
 ,  COMENTARIO VARCHAR2(500) NOT NULL 
 ,  USU_COMENTARIO VARCHAR2(30) NOT NULL 
 ,  COD_SECTOR NUMBER(2,0) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_HINCICOMENTARIOS IS 'Comentarios de incidencias en historico'
/
COMMENT ON COLUMN  CA_HINCICOMENTARIOS.NUM_INCIDENCIA IS 'Numero Inciencia'
/
COMMENT ON COLUMN  CA_HINCICOMENTARIOS.FEC_HISTORICO IS 'Fecha Historico'
/
COMMENT ON COLUMN  CA_HINCICOMENTARIOS.FEC_COMENTARIO IS 'Fecha Comentario'
/
COMMENT ON COLUMN  CA_HINCICOMENTARIOS.COMENTARIO IS 'Comentario'
/
COMMENT ON COLUMN  CA_HINCICOMENTARIOS.USU_COMENTARIO IS 'Usuario Comentario'
/
COMMENT ON COLUMN  CA_HINCICOMENTARIOS.COD_SECTOR IS 'Codigo Sector'
/
