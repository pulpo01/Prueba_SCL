CREATE TABLE CA_RESULRESPUESTAS(
    NUM_LLAMADA NUMBER(9,0) NOT NULL 
 ,  NUM_SECUENCIA NUMBER(3,0) NOT NULL 
 ,  COD_CUESTIONARIO VARCHAR2(6) NOT NULL 
 ,  NUM_PREGUNTA NUMBER(2,0) NOT NULL 
 ,  NUM_RESPUESTA NUMBER(2,0) NOT NULL 
 ,  IND_INTERLOCUTOR NUMBER(1,0) NOT NULL 
 ,  NUM_INCIDENCIA NUMBER(9,0) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_RESULRESPUESTAS IS 'Resultado Respuestas'
/
COMMENT ON COLUMN  CA_RESULRESPUESTAS.NUM_LLAMADA IS 'Numero llamada'
/
COMMENT ON COLUMN  CA_RESULRESPUESTAS.NUM_SECUENCIA IS 'Secuencia'
/
COMMENT ON COLUMN  CA_RESULRESPUESTAS.COD_CUESTIONARIO IS 'Codigo cuestionario'
/
COMMENT ON COLUMN  CA_RESULRESPUESTAS.NUM_PREGUNTA IS 'Numero pregunta'
/
COMMENT ON COLUMN  CA_RESULRESPUESTAS.NUM_RESPUESTA IS 'Numero respuesta'
/
COMMENT ON COLUMN  CA_RESULRESPUESTAS.IND_INTERLOCUTOR IS 'Indicativo de interlocutor'
/
COMMENT ON COLUMN  CA_RESULRESPUESTAS.NUM_INCIDENCIA IS 'Nro.Incidencia generada'
/
