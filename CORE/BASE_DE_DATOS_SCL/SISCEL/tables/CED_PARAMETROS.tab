CREATE TABLE CED_PARAMETROS(
    COD_PARAMETRO NUMBER(10,0) NOT NULL 
 ,  DES_PARAMETRO VARCHAR2(1024) NOT NULL 
 ,  TIP_PARAMETRO VARCHAR2(32) NOT NULL 
 ,  VAL_NUMERICO NUMBER(10,0) NULL 
 ,  VAL_CARACTER VARCHAR2(512) NULL 
 ,  VAL_FECHA DATE NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CED_PARAMETROS IS 'Parametros del sistema'
/
COMMENT ON COLUMN  CED_PARAMETROS.COD_PARAMETRO IS 'Codigo parametro'
/
COMMENT ON COLUMN  CED_PARAMETROS.DES_PARAMETRO IS 'Descripcion del parametro'
/
COMMENT ON COLUMN  CED_PARAMETROS.TIP_PARAMETRO IS 'Tipo de parametro'
/
COMMENT ON COLUMN  CED_PARAMETROS.VAL_NUMERICO IS 'Valor Parametro numerico'
/
COMMENT ON COLUMN  CED_PARAMETROS.VAL_CARACTER IS 'Valor Parametro caracter'
/
COMMENT ON COLUMN  CED_PARAMETROS.VAL_FECHA IS 'Valor Parametro fecha'
/
COMMENT ON COLUMN  CED_PARAMETROS.FEC_ULTMOD IS 'Fecha ultima modificacion'
/
COMMENT ON COLUMN  CED_PARAMETROS.NOM_USUARIO IS 'Responsable ultima modificacion'
/
