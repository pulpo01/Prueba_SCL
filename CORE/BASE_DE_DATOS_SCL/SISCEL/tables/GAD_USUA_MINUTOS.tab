CREATE TABLE GAD_USUA_MINUTOS(
    NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  NUM_MINUTOS NUMBER(3,0) NOT NULL 
 ,  NUM_CICLOS NUMBER(2,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_MODIFICACION DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAD_USUA_MINUTOS IS 'Tabla De Datos De Causas Por Orden De Servicio..'
/
COMMENT ON COLUMN  GAD_USUA_MINUTOS.NOM_USUARIO IS 'Usuario al que se le asignan ciclos y minutos.'
/
COMMENT ON COLUMN  GAD_USUA_MINUTOS.NUM_MINUTOS IS 'Minutos asignables.'
/
COMMENT ON COLUMN  GAD_USUA_MINUTOS.NUM_CICLOS IS 'Cantidad de ciclos asignables.'
/
COMMENT ON COLUMN  GAD_USUA_MINUTOS.FEC_DESDE IS 'Inicio vigencia.'
/
COMMENT ON COLUMN  GAD_USUA_MINUTOS.FEC_HASTA IS 'Termino vigencia.'
/
COMMENT ON COLUMN  GAD_USUA_MINUTOS.NOM_USUARORA IS 'Usuario oracle que realiza el cambio.'
/
COMMENT ON COLUMN  GAD_USUA_MINUTOS.FEC_MODIFICACION IS 'Fecha ingreso registro.'
/
