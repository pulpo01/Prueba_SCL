CREATE TABLE ICT_AKEYS(
    NUM_ESN VARCHAR2(25) NOT NULL 
 ,  COD_CLAVE NUMBER(4,0) NULL 
 ,  A_KEY_CRYP VARCHAR2(20) NULL 
 ,  CHK_DIGITS VARCHAR2(6) NULL 
 ,  PRIMER_CODIGO VARCHAR2(16) NULL 
 ,  SEGUNDO_CODIGO VARCHAR2(16) NULL 
 ,  ID_CARGA NUMBER(5,0) DEFAULT -1 NOT NULL 
 ,  FEC_ACTUALIZACION DATE NULL 
 ,  COD_ESTADO NUMBER(2,0) DEFAULT -1 NOT NULL 
 ,  NUM_ESN_HEX VARCHAR2(25) DEFAULT '0' NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICT_AKEYS IS 'Ict_Akeys.'
/
COMMENT ON COLUMN  ICT_AKEYS.NUM_ESN IS 'Numero de serie electronica (decimal).'
/
COMMENT ON COLUMN  ICT_AKEYS.COD_CLAVE IS 'Codigo de clave de encriptacion.'
/
COMMENT ON COLUMN  ICT_AKEYS.A_KEY_CRYP IS 'A-KEY que puede esta encriptado o no encriptado.'
/
COMMENT ON COLUMN  ICT_AKEYS.CHK_DIGITS IS 'Codigos de chequeo (los 6 ultimos de la A-KEY decimal).'
/
COMMENT ON COLUMN  ICT_AKEYS.PRIMER_CODIGO IS 'Codigos de entrada a programacion del equipo.'
/
COMMENT ON COLUMN  ICT_AKEYS.SEGUNDO_CODIGO IS 'Codigos de entrada a programacion del equipo.'
/
COMMENT ON COLUMN  ICT_AKEYS.ID_CARGA IS 'Codigo de identificacion de la carga.'
/
COMMENT ON COLUMN  ICT_AKEYS.FEC_ACTUALIZACION IS 'Fecha ultima actualizacion de registro.'
/
COMMENT ON COLUMN  ICT_AKEYS.COD_ESTADO IS 'Codigo de estado del registro.'
/
COMMENT ON COLUMN  ICT_AKEYS.NUM_ESN_HEX IS 'Numero de serie electronica (hexadecimal).'
/
