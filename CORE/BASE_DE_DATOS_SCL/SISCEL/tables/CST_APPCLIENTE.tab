CREATE TABLE CST_APPCLIENTE(
    NOM_COMPONENTE VARCHAR2(60) NOT NULL 
 ,  NOM_APLICACION VARCHAR2(60) NOT NULL 
 ,  COD_TIPAPLICACION VARCHAR2(5) NOT NULL 
 ,  COD_ESTADO VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CST_APPCLIENTE IS 'Catalogo De Componentes Por Aplicacion Cliente.'
/
COMMENT ON COLUMN  CST_APPCLIENTE.NOM_COMPONENTE IS 'Nombre de la Componente del Sistema.'
/
COMMENT ON COLUMN  CST_APPCLIENTE.NOM_APLICACION IS 'Nombre de la Aplicacion que Requiere de la Componente.'
/
COMMENT ON COLUMN  CST_APPCLIENTE.COD_TIPAPLICACION IS 'Tipo de Aplicacion (V.B. Pro-C, etc).'
/
COMMENT ON COLUMN  CST_APPCLIENTE.COD_ESTADO IS 'Codigo de Estado de la Relacion.'
/
