 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  COD_CATIMPOS NUMBER(2,0) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_CATIMPCLIENTES IS 'Categorias Impositivas de Clientes'
/
COMMENT ON COLUMN  GE_CATIMPCLIENTES.COD_CLIENTE IS 'Codigo del cliente'
/
COMMENT ON COLUMN  GE_CATIMPCLIENTES.FEC_DESDE IS 'Fecha desde de vigencia de la categoria'
/
COMMENT ON COLUMN  GE_CATIMPCLIENTES.FEC_HASTA IS 'Fecha hasta de vigencia de la categoria'
/
COMMENT ON COLUMN  GE_CATIMPCLIENTES.COD_CATIMPOS IS 'Codigo de la categoria (GE_CATIMPOSITIVA)'
/
