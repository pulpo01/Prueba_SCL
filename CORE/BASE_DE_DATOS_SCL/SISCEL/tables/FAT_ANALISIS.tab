CREATE TABLE FAT_ANALISIS(
    COD_CONTROL NUMBER(1,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  IND_FACTURACION VARCHAR2(1) NOT NULL 
 ,  NUM_TERMINAL NUMBER(15,0) NOT NULL 
 ,  MTO_CARGOBASICO VARCHAR2(12) NOT NULL 
 ,  MTO_TRAFICO NUMBER(12,0) NOT NULL 
 ,  MTO_TOTALMES NUMBER(12,0) NOT NULL 
 ,  COD_CLASIFICACION VARCHAR2(5) NOT NULL 
 ,  COD_CAUSABAJA NUMBER(2,0) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAT_ANALISIS IS 'Analisis de facturacion por periodo.'
/
COMMENT ON COLUMN  FAT_ANALISIS.COD_CONTROL IS 'Tipo de Control.'
/
COMMENT ON COLUMN  FAT_ANALISIS.NUM_ABONADO IS 'Numero Abonado.'
/
COMMENT ON COLUMN  FAT_ANALISIS.COD_CLIENTE IS 'Codigo cliente.'
/
COMMENT ON COLUMN  FAT_ANALISIS.COD_PRODUCTO IS 'codigo producto.'
/
COMMENT ON COLUMN  FAT_ANALISIS.IND_FACTURACION IS 'indicador facturacion.'
/
COMMENT ON COLUMN  FAT_ANALISIS.NUM_TERMINAL IS 'Numero terminal (celular/beeper).'
/
COMMENT ON COLUMN  FAT_ANALISIS.MTO_CARGOBASICO IS 'Monto cargo basico facturado.'
/
COMMENT ON COLUMN  FAT_ANALISIS.MTO_TRAFICO IS 'Monto trafico facturado (1,2,3,4).'
/
COMMENT ON COLUMN  FAT_ANALISIS.MTO_TOTALMES IS 'Total facturado mes.'
/
COMMENT ON COLUMN  FAT_ANALISIS.COD_CLASIFICACION IS 'codigo clasificacion analisis.'
/
COMMENT ON COLUMN  FAT_ANALISIS.COD_CAUSABAJA IS 'Codigo causa baja abonado.'
/
