CREATE TABLE GE_PARAMDIR(
    COD_PARAMDIR NUMBER(3,0) NOT NULL 
 ,  DES_PARAMDIR VARCHAR2(50) NOT NULL 
 ,  TIP_DAT VARCHAR2(3) NOT NULL 
 ,  SEC_DAT NUMBER(2,0) NOT NULL 
 ,  CAPTION VARCHAR2(50) NOT NULL 
 ,  NOM_COLUMN VARCHAR2(30) DEFAULT ' ' NOT NULL 
 ,  TIP_FORMAT NUMBER(3,0) DEFAULT 0 NULL 
 ,  LARGO NUMBER(3,0) DEFAULT 1 NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_PARAMDIR IS 'Parametros De Direccion.'
/
COMMENT ON COLUMN  GE_PARAMDIR.COD_PARAMDIR IS 'Codigo del parametro de direccion.'
/
COMMENT ON COLUMN  GE_PARAMDIR.DES_PARAMDIR IS 'Descripción del parametro de direccion.'
/
COMMENT ON COLUMN  GE_PARAMDIR.TIP_DAT IS 'Tipo de dato en Visual (Combo o Texto).'
/
COMMENT ON COLUMN  GE_PARAMDIR.SEC_DAT IS 'Secuencial dentro del arreglo de Combo o de Texto (Visual).'
/
COMMENT ON COLUMN  GE_PARAMDIR.CAPTION IS 'Caption del parametro de dirección en el Form Visual.'
/
COMMENT ON COLUMN  GE_PARAMDIR.NOM_COLUMN IS 'Nombre del campo en tabla de direcciones para el parámetro.'
/
COMMENT ON COLUMN  GE_PARAMDIR.TIP_FORMAT IS 'Indica formato (1: necesita comillas 0: no necesita).'
/
COMMENT ON COLUMN  GE_PARAMDIR.LARGO IS 'Largo del parametro.'
/
