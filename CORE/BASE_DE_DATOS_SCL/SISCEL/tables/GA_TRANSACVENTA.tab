CREATE TABLE GA_TRANSACVENTA(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_INICVENTA DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_TRANSACVENTA IS 'Transacciones abiertas para ingreso de ventas'
/
COMMENT ON COLUMN  GA_TRANSACVENTA.NUM_ABONADO IS 'Numero de Abonado'
/
COMMENT ON COLUMN  GA_TRANSACVENTA.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_TRANSACVENTA.NOM_USUARIO IS 'Usuario que ingresa la venta'
/
COMMENT ON COLUMN  GA_TRANSACVENTA.FEC_INICVENTA IS 'Fecha de inicio de ingreso de venta'
/
