 ,  DES_MODGENER VARCHAR2(30) NOT NULL 
 ,  COD_PRIORIDAD NUMBER(2,0) NOT NULL 
 ,  COD_APLIC VARCHAR2(3) DEFAULT 'FAC' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_INTMODGENER IS 'Modos de Generacion de Documentos.'
/
COMMENT ON COLUMN  FA_INTMODGENER.COD_MODGENER IS 'Codigo Modo de Generacion.'
/
COMMENT ON COLUMN  FA_INTMODGENER.DES_MODGENER IS 'Descripcion de Modo de Generacion.'
/
COMMENT ON COLUMN  FA_INTMODGENER.COD_PRIORIDAD IS 'Prioridad de Generacion.'
/
COMMENT ON COLUMN  FA_INTMODGENER.COD_APLIC IS 'Código de Interfaz.'
/
