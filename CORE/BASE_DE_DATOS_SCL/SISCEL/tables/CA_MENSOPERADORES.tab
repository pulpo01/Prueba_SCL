CREATE TABLE CA_MENSOPERADORES(
    NOM_USUAORIGEN VARCHAR2(30) NOT NULL 
 ,  NOM_USUADESTINO VARCHAR2(30) NOT NULL 
 ,  NUM_LLAMADA NUMBER(9,0) NOT NULL 
 ,  FEC_MENSAJE DATE NOT NULL 
 ,  DES_COMENTARIO VARCHAR2(200) NULL 
 ,  TIP_INTER NUMBER(2,0) NULL 
 ,  COD_INTERLOCUTOR NUMBER(10,0) NULL 
 ,  TIP_INCIDENCIA NUMBER(3,0) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_MENSOPERADORES IS 'Mensajes entre operadores'
/
COMMENT ON COLUMN  CA_MENSOPERADORES.NOM_USUAORIGEN IS 'Usuario de origen'
/
COMMENT ON COLUMN  CA_MENSOPERADORES.NOM_USUADESTINO IS 'Usuarios de destino'
/
COMMENT ON COLUMN  CA_MENSOPERADORES.NUM_LLAMADA IS 'Numero de llamada'
/
COMMENT ON COLUMN  CA_MENSOPERADORES.FEC_MENSAJE IS 'Fecha y hora del mensaje'
/
COMMENT ON COLUMN  CA_MENSOPERADORES.DES_COMENTARIO IS 'Comentarios'
/
COMMENT ON COLUMN  CA_MENSOPERADORES.TIP_INTER IS 'Tipo de interlocutor'
/
COMMENT ON COLUMN  CA_MENSOPERADORES.COD_INTERLOCUTOR IS 'Codigo interlocutor'
/
COMMENT ON COLUMN  CA_MENSOPERADORES.TIP_INCIDENCIA IS 'Tipo de Incidencia'
/
