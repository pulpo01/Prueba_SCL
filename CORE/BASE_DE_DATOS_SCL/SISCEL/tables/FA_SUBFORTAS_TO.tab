CREATE TABLE FA_SUBFORTAS_TO(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  COD_TRAFICO VARCHAR2(2) NOT NULL 
 ,  TOT_IMP_NETO NUMBER(12,4) NOT NULL 
 ,  TOT_SEG_CONSUMIDO NUMBER(9,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_SUBFORTAS_TO IS '[ SUBFORTAS ] Tabla de Acumulado FORTAS por tipo de servicioTabla de Acumulado FORTAS por tipo de servicio'
/
COMMENT ON COLUMN  FA_SUBFORTAS_TO.NUM_ABONADO IS 'Numero de Abonado'
/
COMMENT ON COLUMN  FA_SUBFORTAS_TO.COD_PERIODO IS 'Periodo Alquiler (Si Ind_Alquiler = 1) o Ciclo Facturacion (Si Ind_Alquiler = 0)'
/
COMMENT ON COLUMN  FA_SUBFORTAS_TO.COD_OPERADOR IS 'Código del Portador (Carrier)'
/
COMMENT ON COLUMN  FA_SUBFORTAS_TO.COD_TRAFICO IS 'Código de Tipo de Tráfico, 03=Nacional y 04=Internacional'
/
COMMENT ON COLUMN  FA_SUBFORTAS_TO.TOT_IMP_NETO IS 'Total Importe Neto de registros disgregados'
/
COMMENT ON COLUMN  FA_SUBFORTAS_TO.TOT_SEG_CONSUMIDO IS 'Total Segundo Consumidos de registros disgregados'
/
COMMENT ON COLUMN  FA_SUBFORTAS_TO.COD_CLIENTE IS 'Codigo de Cliente'
/
