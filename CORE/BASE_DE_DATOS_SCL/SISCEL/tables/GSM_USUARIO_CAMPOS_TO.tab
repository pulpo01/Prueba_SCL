CREATE TABLE GSM_USUARIO_CAMPOS_TO(
    NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_CAMPO VARCHAR2(10) NOT NULL 
 ,  FEC_ACTUALIZACION DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_USUARIO_CAMPOS_TO IS 'Tabla de relacion Usuarios Campos, permite restrinjir el acceso a los usuario a campos'
/
COMMENT ON COLUMN  GSM_USUARIO_CAMPOS_TO.NOM_USUARIO IS 'Nombre de Usuario.'
/
COMMENT ON COLUMN  GSM_USUARIO_CAMPOS_TO.COD_CAMPO IS 'Codigo del Campo.'
/
COMMENT ON COLUMN  GSM_USUARIO_CAMPOS_TO.FEC_ACTUALIZACION IS 'Fecha de la ultima actualizacion.'
/
COMMENT ON COLUMN  GSM_USUARIO_CAMPOS_TO.COD_USUARIO IS 'Codigo del usuario que actualizo.'
/
