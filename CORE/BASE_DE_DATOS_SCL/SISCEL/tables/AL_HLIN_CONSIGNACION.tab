CREATE TABLE AL_HLIN_CONSIGNACION(
    NUM_LISTA NUMBER(6,0) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  PRC_ARTICULO NUMBER(11,2) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_HLIN_CONSIGNACION IS 'Historico del Detalle de Precios de Articulos en Consignacion'
/
COMMENT ON COLUMN  AL_HLIN_CONSIGNACION.NUM_LISTA IS 'Numero de lista'
/
COMMENT ON COLUMN  AL_HLIN_CONSIGNACION.FEC_HISTORICO IS 'Fecha Historico'
/
COMMENT ON COLUMN  AL_HLIN_CONSIGNACION.COD_ARTICULO IS 'Codigo de articulo'
/
COMMENT ON COLUMN  AL_HLIN_CONSIGNACION.PRC_ARTICULO IS 'Precio'
/
