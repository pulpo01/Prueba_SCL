CREATE TABLE CM_VALEVALUACIONES_TD(
    COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) NOT NULL 
 ,  COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  VAL_UMBRAL NUMBER(14,4) NOT NULL 
 ,  VAL_EXCEDENTE NUMBER(14,4) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_VALEVALUACIONES_TD IS 'VALORACION DE LOS EXCEDENTES DE EVALUACION'
/
COMMENT ON COLUMN  CM_VALEVALUACIONES_TD.COD_PLANCOMIS IS 'CODIGO DEL PLAN DE COMISIONES.'
/
COMMENT ON COLUMN  CM_VALEVALUACIONES_TD.COD_CONCEPTO IS 'CODIGO DEL CONCEPTO.'
/
COMMENT ON COLUMN  CM_VALEVALUACIONES_TD.VAL_UMBRAL IS 'UMBRAL DE EXCEDENTE (VENTAS/EVALUACIONES).'
/
COMMENT ON COLUMN  CM_VALEVALUACIONES_TD.VAL_EXCEDENTE IS 'VALOR EXCEDENTE.'
/
COMMENT ON COLUMN  CM_VALEVALUACIONES_TD.COD_MONEDA IS 'CODIGO DE LA MODEDA DE CALCULO.'
/
COMMENT ON COLUMN  CM_VALEVALUACIONES_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION.'
/
COMMENT ON COLUMN  CM_VALEVALUACIONES_TD.NOM_USUARIO IS 'USUARIO QUE HIZO LA ULTIMA MODIFICACION.'
/
