CREATE TABLE CMT_ACUMULADOS(
    COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  ID_PERIODO VARCHAR2(10) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_COMISIONISTA NUMBER(6,0) NOT NULL 
 ,  NUM_LOGRO NUMBER(6,0) NULL 
 ,  IMP_CONCEPTO NUMBER(14,4) NOT NULL 
 ,  NUM_REGISTROS NUMBER(6,0) NOT NULL 
 ,  IND_SUBORDINADA CHAR(1) DEFAULT 'N' NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMT_ACUMULADOS IS 'Tabla De Acumulacion De Conceptos.'
/
COMMENT ON COLUMN  CMT_ACUMULADOS.COD_CONCEPTO IS 'Concepto de facturacion recuperado.'
/
COMMENT ON COLUMN  CMT_ACUMULADOS.COD_PERIODO IS 'Codigo de Periodo. (YYYMM).'
/
COMMENT ON COLUMN  CMT_ACUMULADOS.ID_PERIODO IS 'Identificador de priodo (YYYYMM<N|D><M|Q|S><#SEQ>).'
/
COMMENT ON COLUMN  CMT_ACUMULADOS.COD_TIPCOMIS IS 'Codigo de canal de ventas       [MD].'
/
COMMENT ON COLUMN  CMT_ACUMULADOS.COD_COMISIONISTA IS 'Codigo del Comisionista          [MD].'
/
COMMENT ON COLUMN  CMT_ACUMULADOS.NUM_LOGRO IS 'Valor de entrada a la matriz de comisiones..'
/
COMMENT ON COLUMN  CMT_ACUMULADOS.IMP_CONCEPTO IS 'Comision lograda..'
/
COMMENT ON COLUMN  CMT_ACUMULADOS.NUM_REGISTROS IS 'Cantidad de registros sobre los que se aplica la comision lograda..'
/
COMMENT ON COLUMN  CMT_ACUMULADOS.IND_SUBORDINADA IS 'INDICA SI LA COMISION SE APLICA AL NIVEL SUBORDINADO DEL NIVEL DE APLICACION DEFINIDO'
/
COMMENT ON COLUMN  CMT_ACUMULADOS.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
