CREATE TABLE ERT_REPORT_DETALLE_DOC(
    NUM_SOLICITUD NUMBER(10,0) NOT NULL 
 ,  IND_TIPO_DEUDA VARCHAR2(40) NULL 
 ,  IND_TIPO_DOCUMENTO VARCHAR2(40) NULL 
 ,  COD_MONEDA VARCHAR2(30) NULL 
 ,  MTO_DEUDA VARCHAR2(40) NULL 
 ,  FEC_VENCIMIENTO VARCHAR2(40) NULL 
 ,  DES_LIBRADOR VARCHAR2(100) NULL 
 ,  DES_CAUSA_PUBLICACION VARCHAR2(100) NULL 
 ,  MESES VARCHAR2(10) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERT_REPORT_DETALLE_DOC IS 'Tabla de Paso para emitir detalle de Reporte.'
/
COMMENT ON COLUMN  ERT_REPORT_DETALLE_DOC.NUM_SOLICITUD IS 'Numero Solicitud de Evaluacion.'
/
COMMENT ON COLUMN  ERT_REPORT_DETALLE_DOC.IND_TIPO_DEUDA IS 'Tipo de Deuda.'
/
COMMENT ON COLUMN  ERT_REPORT_DETALLE_DOC.IND_TIPO_DOCUMENTO IS 'Tipo Documento.'
/
COMMENT ON COLUMN  ERT_REPORT_DETALLE_DOC.COD_MONEDA IS 'Codigo moneda entregada por Fuente Externa.'
/
COMMENT ON COLUMN  ERT_REPORT_DETALLE_DOC.MTO_DEUDA IS 'Monto Deuda.'
/
COMMENT ON COLUMN  ERT_REPORT_DETALLE_DOC.FEC_VENCIMIENTO IS 'Fecha Vencimiento Deuda.'
/
COMMENT ON COLUMN  ERT_REPORT_DETALLE_DOC.DES_LIBRADOR IS 'Librador de la deuda.'
/
COMMENT ON COLUMN  ERT_REPORT_DETALLE_DOC.DES_CAUSA_PUBLICACION IS 'Causa Publicacion.'
/
COMMENT ON COLUMN  ERT_REPORT_DETALLE_DOC.MESES IS 'Meses de antiguedad.'
/
