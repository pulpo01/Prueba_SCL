CREATE TABLE FA_TIPOREGISTRO_TD(
    COD_REGISTRO VARCHAR2(5) NOT NULL 
 ,  DES_REGISTRO VARCHAR2(100) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_TIPREGISTRO VARCHAR2(3) DEFAULT 'D' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_TIPOREGISTRO_TD IS 'Registros del proceso de impresion.'
/
COMMENT ON COLUMN  FA_TIPOREGISTRO_TD.COD_REGISTRO IS 'Codigo de Registro.'
/
COMMENT ON COLUMN  FA_TIPOREGISTRO_TD.DES_REGISTRO IS 'Descripcion del Registro.'
/
COMMENT ON COLUMN  FA_TIPOREGISTRO_TD.FEC_ULTMOD IS 'Fecha de Ultima Modificacion.'
/
COMMENT ON COLUMN  FA_TIPOREGISTRO_TD.NOM_USUARIO IS 'Nombre de Usuario.'
/
COMMENT ON COLUMN  FA_TIPOREGISTRO_TD.COD_TIPREGISTRO IS 'CÓDIGO DE TIPO DE REGISTRO'
/
