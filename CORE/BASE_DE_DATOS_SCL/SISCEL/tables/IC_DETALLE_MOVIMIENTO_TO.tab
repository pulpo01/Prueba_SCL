CREATE TABLE IC_DETALLE_MOVIMIENTO_TO(
    NUM_MOVIMIENTO NUMBER(9,0) NOT NULL 
 ,  NOM_SUBPARAMETRO VARCHAR2(15) NOT NULL 
 ,  VAL_SUBPARAMETRO VARCHAR2(50) NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  IC_DETALLE_MOVIMIENTO_TO IS '[ IC_DETAMO ] Detalles de Movimientos'
/
COMMENT ON COLUMN  IC_DETALLE_MOVIMIENTO_TO.NUM_MOVIMIENTO IS 'Número Movimiento'
/
COMMENT ON COLUMN  IC_DETALLE_MOVIMIENTO_TO.NOM_SUBPARAMETRO IS 'Nombre del Subparámetro'
/
COMMENT ON COLUMN  IC_DETALLE_MOVIMIENTO_TO.VAL_SUBPARAMETRO IS 'Valor del subaparametro'
/
