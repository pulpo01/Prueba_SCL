CREATE TABLE CM_PLANESTIPORED_TD(
    COD_PLANCOMIS VARCHAR2(6) NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_PLANESTIPORED_TD IS 'PLANES DE COMISIONES ASOCIADOS A LOS TIPOS DE RED'
/
COMMENT ON COLUMN  CM_PLANESTIPORED_TD.COD_PLANCOMIS IS 'CODIGO DEL PLAN DE COMISIONES.'
/
COMMENT ON COLUMN  CM_PLANESTIPORED_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS.'
/
COMMENT ON COLUMN  CM_PLANESTIPORED_TD.FEC_DESDE IS 'FECHA DESDE VIGENCIA DEL PLAN.'
/
COMMENT ON COLUMN  CM_PLANESTIPORED_TD.FEC_HASTA IS 'FECHA HASTA DE VIGENCIA DEL PLAN.'
/
COMMENT ON COLUMN  CM_PLANESTIPORED_TD.FEC_ULTMOD IS 'FECHA DE UNTIMA MOFICACION.'
/
COMMENT ON COLUMN  CM_PLANESTIPORED_TD.NOM_USUARIO IS 'USUARIO QUE HIZO LA ULTIMA MODIFICACION.'
/
