CREATE TABLE GE_COMUNAS(
    COD_REGION VARCHAR2(3) NOT NULL 
 ,  COD_PROVINCIA VARCHAR2(5) NOT NULL 
 ,  COD_COMUNA VARCHAR2(5) NOT NULL 
 ,  DES_COMUNA VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_COMUNAS IS 'Comunas Geograficas'
/
COMMENT ON COLUMN  GE_COMUNAS.COD_REGION IS 'Codigo de Region'
/
COMMENT ON COLUMN  GE_COMUNAS.COD_PROVINCIA IS 'Codigo de Provincia'
/
COMMENT ON COLUMN  GE_COMUNAS.COD_COMUNA IS 'Codigo de Comuna'
/
COMMENT ON COLUMN  GE_COMUNAS.DES_COMUNA IS 'Descripcion de Comuna'
/
