CREATE TABLE TAH_PROM_BUZON(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  FEC_ENVIO DATE NOT NULL 
 ,  FEC_REVISION DATE NULL 
 ,  FEC_ACTIVACION DATE NULL 
 ,  FEC_CARGAPROM DATE NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TAH_PROM_BUZON IS 'Celulares sin buzon de voz'
/
COMMENT ON COLUMN  TAH_PROM_BUZON.NUM_ABONADO IS 'Numero de abonado               '
/
COMMENT ON COLUMN  TAH_PROM_BUZON.NUM_CELULAR IS 'Numero de celular               '
/
COMMENT ON COLUMN  TAH_PROM_BUZON.FEC_ENVIO IS 'Fecha de generacion de registro '
/
COMMENT ON COLUMN  TAH_PROM_BUZON.FEC_REVISION IS 'Fecha de revision si tiene buzon'
/
COMMENT ON COLUMN  TAH_PROM_BUZON.FEC_ACTIVACION IS 'Fecha de creacion de buzon      '
/
COMMENT ON COLUMN  TAH_PROM_BUZON.FEC_CARGAPROM IS 'Fecha de carga de promocion     '
/
