CREATE TABLE VE_CAMPLAN(
    COD_PLANORI VARCHAR2(3) NOT NULL 
 ,  COD_CATPLAORI VARCHAR2(3) NOT NULL 
 ,  COD_PLANDES VARCHAR2(3) NOT NULL 
 ,  COD_CATPLADES VARCHAR2(3) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_CTOLIQ NUMBER(6,0) NOT NULL 
 ,  FEC_CAMBIO DATE NOT NULL 
 ,  IMP_CAMBIO NUMBER(7,2) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CAMPLAN IS 'Almacena los cambios de plan tarifario.'
/
COMMENT ON COLUMN  VE_CAMPLAN.COD_PLANORI IS 'Codigo del Plan Tarifario Original'
/
COMMENT ON COLUMN  VE_CAMPLAN.COD_CATPLAORI IS 'Codigo de la categoria del Plan Tarifario Original'
/
COMMENT ON COLUMN  VE_CAMPLAN.COD_PLANDES IS 'Codigo del Plan Tarifario Nuevo'
/
COMMENT ON COLUMN  VE_CAMPLAN.COD_CATPLADES IS 'Codigo de la categoria del Plan Tarifario Nuevo'
/
COMMENT ON COLUMN  VE_CAMPLAN.COD_PRODUCTO IS 'Codigo del producto'
/
COMMENT ON COLUMN  VE_CAMPLAN.COD_VENDEDOR IS 'Codigo del vendedor'
/
COMMENT ON COLUMN  VE_CAMPLAN.COD_CTOLIQ IS 'Codigo del concepto de liquidacion'
/
COMMENT ON COLUMN  VE_CAMPLAN.FEC_CAMBIO IS 'Fecha en la que se efectuo el cambio de plan tarifario'
/
COMMENT ON COLUMN  VE_CAMPLAN.IMP_CAMBIO IS 'Importe del cambio'
/
