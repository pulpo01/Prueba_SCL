 ,  FEC_PAGO VARCHAR2(20) NOT NULL 
 ,  LOG_USUA VARCHAR2(20) NOT NULL 
 ,  NUM_FACT VARCHAR2(20) NOT NULL 
 ,  MTO_APLI VARCHAR2(20) NOT NULL 
 ,  SEC_CARG VARCHAR2(20) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
