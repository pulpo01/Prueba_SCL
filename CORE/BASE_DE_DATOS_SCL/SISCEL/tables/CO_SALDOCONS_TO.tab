CREATE TABLE CO_SALDOCONS_TO(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  FEC_VENCIMIE DATE NOT NULL 
 ,  SALDO_VENCIDO NUMBER(16,4) NOT NULL 
 ,  SALDO_NOVENCIDO NUMBER(16,4) NOT NULL 
 ,  FEC_SALDO DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  CO_SALDOCONS_TO.COD_CLIENTE IS 'Codigo del cliente'
/
COMMENT ON COLUMN  CO_SALDOCONS_TO.FEC_VENCIMIE IS 'Fecha de vencimiento del documento mas antiguo.'
/
COMMENT ON COLUMN  CO_SALDOCONS_TO.SALDO_VENCIDO IS 'Saldo vencido en cuenta corriente'
/
COMMENT ON COLUMN  CO_SALDOCONS_TO.SALDO_NOVENCIDO IS 'Saldo no vencido en cuenta corriente'
/
COMMENT ON COLUMN  CO_SALDOCONS_TO.FEC_SALDO IS 'Fecha de ingreso del saldo'
/
