CREATE TABLE ER_CLASIF_BURO_TD(
    COD_CLASIFICACION VARCHAR2(1) NOT NULL 
 ,  DES_CLASIFICACION VARCHAR2(50) NOT NULL 
 ,  IND_PJ VARCHAR2(1) NOT NULL 
 ,  IND_PN VARCHAR2(1) NOT NULL 
 ,  MENS_USUARIO VARCHAR2(500) NOT NULL 
 ,  IND_VIGENCIA VARCHAR2(1) DEFAULT 'S' NOT NULL 
 ,  FEC_MODIFICACION DATE NOT NULL 
 ,  NOM_USUARIO_MODI VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ER_CLASIF_BURO_TD IS '[ CLBU ] Clasificación Datacredit de Clientes'
/
COMMENT ON COLUMN  ER_CLASIF_BURO_TD.COD_CLASIFICACION IS 'Clasificación Datacredit del cliente'
/
COMMENT ON COLUMN  ER_CLASIF_BURO_TD.DES_CLASIFICACION IS 'Descripción de clasificación'
/
COMMENT ON COLUMN  ER_CLASIF_BURO_TD.IND_PJ IS 'Acción a seguir para Persona Jurídica. T:true, F:False'
/
COMMENT ON COLUMN  ER_CLASIF_BURO_TD.IND_PN IS 'Acción a seguir para Persona Natural. T:true, F:False'
/
COMMENT ON COLUMN  ER_CLASIF_BURO_TD.MENS_USUARIO IS 'Mensaje a desplegar al usuario en caso de Accion N/O'
/
COMMENT ON COLUMN  ER_CLASIF_BURO_TD.IND_VIGENCIA IS 'Indicador de vigencia de clasificación (V:Vigente/N:No vigente)'
/
COMMENT ON COLUMN  ER_CLASIF_BURO_TD.FEC_MODIFICACION IS 'Fecha de última modificación'
/
COMMENT ON COLUMN  ER_CLASIF_BURO_TD.NOM_USUARIO_MODI IS 'Usuario que modifica el registro'
/
