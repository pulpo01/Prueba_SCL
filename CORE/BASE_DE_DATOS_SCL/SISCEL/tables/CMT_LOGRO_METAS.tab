CREATE TABLE CMT_LOGRO_METAS(
    ID_PERIODO VARCHAR2(10) DEFAULT 'NN' NOT NULL 
 ,  COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) DEFAULT 'C' NOT NULL 
 ,  COD_COMISIONISTA NUMBER(6,0) DEFAULT 0 NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) DEFAULT 0 NOT NULL 
 ,  VAL_LOGRO NUMBER(5,0) NOT NULL 
 ,  IMP_CONCEPTO NUMBER(14,4) NOT NULL 
 ,  NUM_REGISTROS NUMBER(6,0) NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMT_LOGRO_METAS IS 'Tabla De Logro De Metas.'
/
COMMENT ON COLUMN  CMT_LOGRO_METAS.ID_PERIODO IS 'IDENTIFICADOR DEL CICLO DE COMISIONES'
/
COMMENT ON COLUMN  CMT_LOGRO_METAS.COD_PERIODO IS 'Periodo sobre el cual se considera el logro de la meta..'
/
COMMENT ON COLUMN  CMT_LOGRO_METAS.COD_TIPCOMIS IS 'Codigo del tipo de comisionista (Canal de Ventas).'
/
COMMENT ON COLUMN  CMT_LOGRO_METAS.COD_COMISIONISTA IS 'Codigo del Comisionista.'
/
COMMENT ON COLUMN  CMT_LOGRO_METAS.COD_CONCEPTO IS 'Concepto de comisiones evaluado'
/
COMMENT ON COLUMN  CMT_LOGRO_METAS.VAL_LOGRO IS 'Valor, entre 0 y 100 que refleJara el valor de la meta lograda..'
/
COMMENT ON COLUMN  CMT_LOGRO_METAS.IMP_CONCEPTO IS 'Valor Comisional Total para el concepto..'
/
COMMENT ON COLUMN  CMT_LOGRO_METAS.NUM_REGISTROS IS 'Cantidad de registros que participan en el concepto..'
/
COMMENT ON COLUMN  CMT_LOGRO_METAS.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
