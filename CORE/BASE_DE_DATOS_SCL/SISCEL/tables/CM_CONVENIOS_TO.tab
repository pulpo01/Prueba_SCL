CREATE TABLE CM_CONVENIOS_TO(
    NUM_GENERAL NUMBER(10,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_COMISIONISTA NUMBER(6,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  ID_PERIODO VARCHAR2(10) NOT NULL 
 ,  FEC_CONVENIO DATE NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  COD_TIPVENDEDOR VARCHAR2(2) DEFAULT 'C' NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_CONVENIOS_TO IS 'Almacena Los Suscripciones Al Convenio Pac.'
/
COMMENT ON COLUMN  CM_CONVENIOS_TO.NUM_GENERAL IS 'NUMERO IDENTIFICADOR DEL EVENTO COMISIONAL.'
/
COMMENT ON COLUMN  CM_CONVENIOS_TO.COD_TIPCOMIS IS 'TIPO DE COMISIONISTA O CANAL DE VENTAS.'
/
COMMENT ON COLUMN  CM_CONVENIOS_TO.COD_COMISIONISTA IS 'CODIGO DE VENDEDOR O COMISIONISTA.'
/
COMMENT ON COLUMN  CM_CONVENIOS_TO.COD_CLIENTE IS 'CODIGO DEL CLIENTE CON CONVENIO.'
/
COMMENT ON COLUMN  CM_CONVENIOS_TO.COD_PERIODO IS 'PERIODO COMISIONAL AFECTO.'
/
COMMENT ON COLUMN  CM_CONVENIOS_TO.ID_PERIODO IS 'IDENTIFICADOR DE PERIODO.'
/
COMMENT ON COLUMN  CM_CONVENIOS_TO.FEC_CONVENIO IS 'FECHA DE INICIO CONVENIO.'
/
COMMENT ON COLUMN  CM_CONVENIOS_TO.COD_TIPORED IS 'Codigo del tipo de red'
/
COMMENT ON COLUMN  CM_CONVENIOS_TO.COD_TIPVENDEDOR IS 'Codigo de la categoria de venta del vendedor que origina el evento'
/
COMMENT ON COLUMN  CM_CONVENIOS_TO.COD_VENDEDOR IS 'Codigo del vendedor que origina el evento'
/
