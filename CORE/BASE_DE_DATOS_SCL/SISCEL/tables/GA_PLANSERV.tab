CREATE TABLE GA_PLANSERV(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_PLANSERV VARCHAR2(3) NOT NULL 
 ,  DES_PLANSERV VARCHAR2(30) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_PLANSERV IS 'Planes de Servicios por Producto'
/
COMMENT ON COLUMN  GA_PLANSERV.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_PLANSERV.COD_PLANSERV IS 'Codigo de Plan de Servicios'
/
COMMENT ON COLUMN  GA_PLANSERV.DES_PLANSERV IS 'Descripcion de Plan de Servicios'
/
COMMENT ON COLUMN  GA_PLANSERV.FEC_DESDE IS 'Fecha Desde Vigencia Asignacion'
/
COMMENT ON COLUMN  GA_PLANSERV.FEC_HASTA IS 'Fecha Hasta Vigencia'
/
