CREATE TABLE AL_CONSUMO_DOCUMENTOS
(
  COD_OFICINA     VARCHAR2(2 BYTE)              NOT NULL,
  TIP_DOCUM       NUMBER(2)                     NOT NULL,
  NUM_FOLIO       NUMBER(9)                     NOT NULL,
  USU_CONSUMO     VARCHAR2(30 BYTE)             NOT NULL,
  FEC_CONSUMO     DATE                          NOT NULL,
  IND_CONSUMO     VARCHAR2(1 BYTE),
  IND_ANULACION   NUMBER(1)                     NOT NULL,
  COD_VENDEDOR    NUMBER(6),
  COD_ESTADO      CHAR(1 BYTE)                  DEFAULT 'F',
  IND_LIBROIVA    NUMBER(1)                     DEFAULT 0                     NOT NULL,
  PREF_PLAZA      VARCHAR2(25 BYTE)             DEFAULT ' ',
  COD_OPERADORA   VARCHAR2(5 BYTE)              NOT NULL,
  NUM_VENTA       NUMBER(8),
  NUM_PROCESO     NUMBER(8),
  RESOLUCION      VARCHAR2(25 BYTE),
  FEC_RESOLUCION  DATE,
  SERIE           VARCHAR2(10 BYTE),
  ETIQUETA        VARCHAR2(10 BYTE),
  FEC_CREAFOLIO   DATE
)
TABLESPACE TS_LOGISTICA_MDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          4M
            NEXT             4M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE AL_CONSUMO_DOCUMENTOS IS 'Registra los diferentes ducumentos utilizados o consumidos';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.COD_OFICINA IS 'Sucursal';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.TIP_DOCUM IS 'Tipo de Documento';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.NUM_FOLIO IS 'Folio consumido';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.USU_CONSUMO IS 'Usuario';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.FEC_CONSUMO IS 'Fecha del consumo';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.IND_CONSUMO IS 'Manual / Automatico';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.IND_ANULACION IS 'Indicativo de anulacion';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.COD_VENDEDOR IS 'Codigo del vendedor que ha consumido.';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.COD_ESTADO IS 'Estado del documento: F=Finalizado | T=En proceso.';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.IND_LIBROIVA IS 'Indicador Libro de IVA.';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.PREF_PLAZA IS 'Prefijo de folio de la plaza';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.COD_OPERADORA IS 'Codigo de Operadora';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.NUM_VENTA IS 'Numero de Venta';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.NUM_PROCESO IS 'Numero de Proceso';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.RESOLUCION IS 'Resolución';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.FEC_RESOLUCION IS 'Fecha de resolución';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.SERIE IS 'Serie de folios';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.ETIQUETA IS 'Etiqueta de folios';

COMMENT ON COLUMN AL_CONSUMO_DOCUMENTOS.FEC_CREAFOLIO IS 'Fecha de creación de la solicitud de folios';
