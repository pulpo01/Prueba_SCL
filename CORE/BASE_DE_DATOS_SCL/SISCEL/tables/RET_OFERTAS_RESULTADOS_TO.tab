CREATE TABLE RET_OFERTAS_RESULTADOS_TO(
    COD_TIPO_OFERTA NUMBER(2,0) NOT NULL 
 ,  COD_RESULTADO NUMBER(3,0) NOT NULL 
 ,  IND_VIGENCIA CHAR(1) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  RET_OFERTAS_RESULTADOS_TO IS 'RELACIÓN TIPOS OFERTAS - RESULTADOS DE RETENCIÓN'
/
COMMENT ON COLUMN  RET_OFERTAS_RESULTADOS_TO.COD_TIPO_OFERTA IS 'TIPO DE OFERTA'
/
COMMENT ON COLUMN  RET_OFERTAS_RESULTADOS_TO.COD_RESULTADO IS 'RESULTADO DE CONTACTO'
/
COMMENT ON COLUMN  RET_OFERTAS_RESULTADOS_TO.IND_VIGENCIA IS 'INDICADOR DE VIGENCIA'
/
