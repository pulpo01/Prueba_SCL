CREATE TABLE CMD_MATRIZCOM(
    NUM_MATRIZ NUMBER(6,0) NOT NULL 
 ,  NUM_ACTUACION NUMBER(6,0) NOT NULL 
 ,  COD_PERIODO_INI NUMBER(8,0) NOT NULL 
 ,  COD_PERIODO_FIN NUMBER(8,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMD_MATRIZCOM IS 'Definicion De Las Matrices De Comisiones.'
/
COMMENT ON COLUMN  CMD_MATRIZCOM.NUM_MATRIZ IS 'Identificador unico de la matriz de comisiones.'
/
COMMENT ON COLUMN  CMD_MATRIZCOM.NUM_ACTUACION IS 'Identificador de la actuacion de comisiones a la que pertenece.'
/
COMMENT ON COLUMN  CMD_MATRIZCOM.COD_PERIODO_INI IS 'Periodo de inicio de la aplicacion.'
/
COMMENT ON COLUMN  CMD_MATRIZCOM.COD_PERIODO_FIN IS 'Perriodo de termino de la aplicacion.'
/
COMMENT ON COLUMN  CMD_MATRIZCOM.FEC_ULTMOD IS 'Fecha de ultima modificacion del registro..'
/
COMMENT ON COLUMN  CMD_MATRIZCOM.NOM_USUARIO IS 'Usuario que hizo la ultima modificacion del registro en la tabla..'
/
