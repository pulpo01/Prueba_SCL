CREATE TABLE CO_DESCPROC_TD(
    COD_PROCESO VARCHAR2(5) NOT NULL 
 ,  NOM_PROCESO VARCHAR2(40) NOT NULL 
 ,  TIP_PROCESO VARCHAR2(1) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  CO_DESCPROC_TD.COD_PROCESO IS 'Identificador del Proceso'
/
COMMENT ON COLUMN  CO_DESCPROC_TD.NOM_PROCESO IS 'Nombre de Proceso'
/
COMMENT ON COLUMN  CO_DESCPROC_TD.TIP_PROCESO IS 'Tipo de Proceso'
/
