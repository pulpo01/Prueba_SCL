CREATE TABLE SC_ENT_LOTE(
    COD_EVENTO NUMBER(2,0) NOT NULL 
 ,  ID_LOTE VARCHAR2(50) NOT NULL 
 ,  COD_CONCEPTO VARCHAR2(15) NOT NULL 
 ,  IMP_MOVIMIENTO NUMBER(16,4) NOT NULL 
 ,  DES_TRANSACCION VARCHAR2(30) NULL 
 ,  COD_OPERADORA_SCL VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SC_ENT_LOTE IS 'ENTRADA A MISC - DETALLE DE LOTES'
/
COMMENT ON COLUMN  SC_ENT_LOTE.COD_EVENTO IS 'CODIGO DE EVENTO'
/
COMMENT ON COLUMN  SC_ENT_LOTE.ID_LOTE IS 'IDENTIFICACION DEL LOTE PARA UN EVENTO'
/
COMMENT ON COLUMN  SC_ENT_LOTE.COD_CONCEPTO IS 'CODIGO DE CONCEPTO CONTABLE'
/
COMMENT ON COLUMN  SC_ENT_LOTE.IMP_MOVIMIENTO IS 'IMPORTE DEL MOVIMIENTO CONTABLE'
/
COMMENT ON COLUMN  SC_ENT_LOTE.DES_TRANSACCION IS 'Descripcisn Transaccion'
/
COMMENT ON COLUMN  SC_ENT_LOTE.COD_OPERADORA_SCL IS 'CODIGO DE OPERADORA'
/
