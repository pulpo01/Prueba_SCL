CREATE TABLE GA_HMODABOCEL
(
  NUM_ABONADO       NUMBER(8)                   NOT NULL,
  COD_TIPMODI       VARCHAR2(2 BYTE)            NOT NULL,
  FEC_MODIFICA      DATE                        NOT NULL,
  FEC_HISTORICO     DATE                        NOT NULL,
  NOM_USUARORA      VARCHAR2(30 BYTE)           NOT NULL,
  NUM_CELULAR       NUMBER(15),
  COD_REGION        VARCHAR2(3 BYTE),
  COD_PROVINCIA     VARCHAR2(5 BYTE),
  COD_CIUDAD        VARCHAR2(5 BYTE),
  COD_CELDA         VARCHAR2(7 BYTE),
  COD_CENTRAL       NUMBER(3),
  COD_USO           NUMBER(2),
  TIP_PLANTARIF     VARCHAR2(1 BYTE),
  TIP_TERMINAL      VARCHAR2(1 BYTE),
  COD_PLANTARIF     VARCHAR2(3 BYTE),
  COD_CARGOBASICO   VARCHAR2(3 BYTE),
  COD_PLANSERV      VARCHAR2(3 BYTE),
  COD_LIMCONSUMO    VARCHAR2(5 BYTE),
  NUM_SERIE         VARCHAR2(25 BYTE),
  NUM_SERIEHEX      VARCHAR2(8 BYTE),
  NUM_SERIEMEC      VARCHAR2(20 BYTE),
  COD_HOLDING       NUMBER(8),
  COD_EMPRESA       NUMBER(8),
  COD_GRPSERV       VARCHAR2(2 BYTE),
  NUM_TELEFIJA      VARCHAR2(15 BYTE),
  COD_CARRIER       NUMBER(5),
  IND_PLEXSYS       NUMBER(1),
  COD_CENTRAL_PLEX  NUMBER(3),
  NUM_CELULAR_PLEX  NUMBER(15),
  COD_CREDMOR       NUMBER(3),
  COD_CREDCON       NUMBER(3),
  COD_CICLO         NUMBER(2),
  IND_FACTUR        NUMBER(1),
  IND_SUSPEN        NUMBER(1),
  IND_REHABI        NUMBER(1),
  IND_INSGUIAS      NUMBER(1),
  NUM_PERSONAL      VARCHAR2(4 BYTE),
  COD_CAUSA         VARCHAR2(2 BYTE),
  COD_OPREDFIJA     NUMBER(5),
  IND_SUPERTEL      NUMBER(1),
  COD_TECNOLOGIA    VARCHAR2(7 BYTE),
  NUM_IMEI          VARCHAR2(25 BYTE)
)
TABLESPACE TS_CRM_POSVENTA_SDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          40K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
CACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE GA_HMODABOCEL IS 'Historico Abonados Red Celular';

COMMENT ON COLUMN GA_HMODABOCEL.COD_TIPMODI IS 'Tipo de Modificacion';

COMMENT ON COLUMN GA_HMODABOCEL.FEC_MODIFICA IS 'Fecha de Modificacion Abonado';

COMMENT ON COLUMN GA_HMODABOCEL.FEC_HISTORICO IS 'Fecha de Historico';

COMMENT ON COLUMN GA_HMODABOCEL.NOM_USUARORA IS 'Usuario Modificacion';

COMMENT ON COLUMN GA_HMODABOCEL.NUM_CELULAR IS 'Numero de Celular';

COMMENT ON COLUMN GA_HMODABOCEL.COD_REGION IS 'Codigo de Region';

COMMENT ON COLUMN GA_HMODABOCEL.COD_PROVINCIA IS 'Codigo de Provincia';

COMMENT ON COLUMN GA_HMODABOCEL.COD_CIUDAD IS 'Codigo de Ciudad';

COMMENT ON COLUMN GA_HMODABOCEL.COD_CELDA IS 'Codigo de celda';

COMMENT ON COLUMN GA_HMODABOCEL.COD_CENTRAL IS 'Codigo de Central';

COMMENT ON COLUMN GA_HMODABOCEL.COD_USO IS 'Codigo de Uso de Numeracion';

COMMENT ON COLUMN GA_HMODABOCEL.TIP_PLANTARIF IS 'Tipo de Plan Tarifario ; Individual (I), Empresa (E), Holding (H)';

COMMENT ON COLUMN GA_HMODABOCEL.TIP_TERMINAL IS 'Tipo de Terminal ; Analogico (A), Digital (D)';

COMMENT ON COLUMN GA_HMODABOCEL.COD_PLANTARIF IS 'Codigo de Plan Tarifario';

COMMENT ON COLUMN GA_HMODABOCEL.COD_CARGOBASICO IS 'Codigo Cargo Basico';

COMMENT ON COLUMN GA_HMODABOCEL.COD_PLANSERV IS 'Codigo de Plan de Servicios';

COMMENT ON COLUMN GA_HMODABOCEL.COD_LIMCONSUMO IS 'Codigo de Limite de Consumo';

COMMENT ON COLUMN GA_HMODABOCEL.NUM_SERIE IS 'Numero de Serie Decimal';

COMMENT ON COLUMN GA_HMODABOCEL.NUM_SERIEHEX IS 'Numero de Serie Hexadecimal';

COMMENT ON COLUMN GA_HMODABOCEL.NUM_SERIEMEC IS 'Numero de Serie Mecanico';

COMMENT ON COLUMN GA_HMODABOCEL.COD_HOLDING IS 'Holding en el cual se encuentra incluido el abonado';

COMMENT ON COLUMN GA_HMODABOCEL.COD_EMPRESA IS 'Empresa en la cual se encuentra incluido el abonado';

COMMENT ON COLUMN GA_HMODABOCEL.COD_GRPSERV IS 'Grupo de Cobro de Servicios en el cual se encuentra incluido el abonado';

COMMENT ON COLUMN GA_HMODABOCEL.NUM_TELEFIJA IS 'Numero de telefonia fija';

COMMENT ON COLUMN GA_HMODABOCEL.COD_CARRIER IS 'Codigo de operador de Carrier';

COMMENT ON COLUMN GA_HMODABOCEL.IND_PLEXSYS IS 'Indicativo Abonado Zona Plexsys; No (0), Si (1)';

COMMENT ON COLUMN GA_HMODABOCEL.COD_CENTRAL_PLEX IS 'Codigo Segunda Central Abonados Plexsys';

COMMENT ON COLUMN GA_HMODABOCEL.NUM_CELULAR_PLEX IS 'Segundo Numero de Celular Plexsys';

COMMENT ON COLUMN GA_HMODABOCEL.COD_CREDMOR IS 'Codigo de Limite Credito de Morosidad';

COMMENT ON COLUMN GA_HMODABOCEL.COD_CREDCON IS 'Codigo de Limite Credito de Consumo';

COMMENT ON COLUMN GA_HMODABOCEL.COD_CICLO IS 'Codigo de Ciclo Facturacion';

COMMENT ON COLUMN GA_HMODABOCEL.IND_FACTUR IS 'Indicativo de Facturable ; No (0), Si (1)';

COMMENT ON COLUMN GA_HMODABOCEL.IND_SUSPEN IS 'Indicativo de Suspendible ; No (0), Si (1)';

COMMENT ON COLUMN GA_HMODABOCEL.IND_REHABI IS 'Indicativo de Rehabilitable ; No (0), Si (1)';

COMMENT ON COLUMN GA_HMODABOCEL.IND_INSGUIAS IS 'Indicativo de Insercion en Guias ; No (0), Si (1)';

COMMENT ON COLUMN GA_HMODABOCEL.NUM_PERSONAL IS 'Codigo de Bloqueo de Terminal';

COMMENT ON COLUMN GA_HMODABOCEL.COD_CAUSA IS 'Causa de la modificacion';

COMMENT ON COLUMN GA_HMODABOCEL.COD_TECNOLOGIA IS 'Codigo Tecnologia';

COMMENT ON COLUMN GA_HMODABOCEL.NUM_IMEI IS 'Numero Serie Terminal GSM';

COMMENT ON COLUMN GA_HMODABOCEL.NUM_ABONADO IS 'Secuencia Numero Abonado';
