CREATE TABLE VE_CATARTICULOS(
    COD_CATEGORIA VARCHAR2(3) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_EFECTIVIDAD DATE NOT NULL 
 ,  FEC_FINEFECTIVIDAD DATE NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CATARTICULOS IS 'Categorias asociadas a Articulos'
/
COMMENT ON COLUMN  VE_CATARTICULOS.COD_CATEGORIA IS 'Codigo de Categoria de Articulo'
/
COMMENT ON COLUMN  VE_CATARTICULOS.COD_ARTICULO IS 'Codigo de Articulo'
/
COMMENT ON COLUMN  VE_CATARTICULOS.NOM_USUARIO IS 'Usuario que genera/elimina la asociacion'
/
COMMENT ON COLUMN  VE_CATARTICULOS.FEC_EFECTIVIDAD IS 'Fecha de entrada en vigencia de la asociacion Articulo/Categoria'
/
COMMENT ON COLUMN  VE_CATARTICULOS.FEC_FINEFECTIVIDAD IS 'Fecha de expiracion de la asociacion Articulo/Categoria'
/
