CREATE TABLE GE_SECTORES(
    COD_SECTOR NUMBER(2,0) NOT NULL 
 ,  DES_SECTOR VARCHAR2(30) NOT NULL 
 ,  COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  NOM_IMPRESORA VARCHAR2(40) NULL 
 ,  NOM_RESPONSABLE VARCHAR2(40) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_SECTORES IS 'Sectores por Oficina Comercial'
/
COMMENT ON COLUMN  GE_SECTORES.COD_SECTOR IS 'Codigo de Sector'
/
COMMENT ON COLUMN  GE_SECTORES.DES_SECTOR IS 'Descripcion de Sector'
/
COMMENT ON COLUMN  GE_SECTORES.COD_OFICINA IS 'Codigo de Oficina'
/
COMMENT ON COLUMN  GE_SECTORES.NOM_IMPRESORA IS 'Nombre de la impresora'
/
COMMENT ON COLUMN  GE_SECTORES.NOM_RESPONSABLE IS 'Nombre Responsable Sector'
/
