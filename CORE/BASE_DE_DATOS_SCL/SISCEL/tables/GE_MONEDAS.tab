CREATE TABLE GE_MONEDAS(
    COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  DES_MONEDA VARCHAR2(20) NOT NULL 
 ,  NUM_DECIMAL NUMBER(1,0) NOT NULL 
 ,  TIP_PERIODO CHAR(1) DEFAULT 'D' NOT NULL 
 ,  CONVERSION_OBLIGATORIA CHAR(1) NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_MONEDAS IS 'Monedas'
/
COMMENT ON COLUMN  GE_MONEDAS.COD_MONEDA IS 'Codigo de Moneda'
/
COMMENT ON COLUMN  GE_MONEDAS.DES_MONEDA IS 'Descripcion de Moneda'
/
COMMENT ON COLUMN  GE_MONEDAS.TIP_PERIODO IS 'periodo de validez del valor de conversion'
/
COMMENT ON COLUMN  GE_MONEDAS.CONVERSION_OBLIGATORIA IS 'indica si debe existir valor de conversion'
/
