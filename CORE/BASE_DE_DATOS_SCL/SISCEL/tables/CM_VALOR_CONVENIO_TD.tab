CREATE TABLE CM_VALOR_CONVENIO_TD(
    COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  IND_ORGENCONVENIO CHAR(1) NOT NULL 
 ,  IMP_COMISION NUMBER(14,4) NOT NULL 
 ,  TIP_CALCULO CHAR(1) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) DEFAULT 'C' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_VALOR_CONVENIO_TD IS 'Detalle De Los Valores De Los Convenios.'
/
COMMENT ON COLUMN  CM_VALOR_CONVENIO_TD.COD_CONCEPTO IS 'CONCEPTO DE PENALIZACION.'
/
COMMENT ON COLUMN  CM_VALOR_CONVENIO_TD.IND_ORGENCONVENIO IS 'ORIGEN DEL CONVENIO.'
/
COMMENT ON COLUMN  CM_VALOR_CONVENIO_TD.IMP_COMISION IS 'IMPORTE DE COMISION.'
/
COMMENT ON COLUMN  CM_VALOR_CONVENIO_TD.TIP_CALCULO IS 'TIPO DE CALCULO DIRECTO = D, INDIRECTO = I.'
/
COMMENT ON COLUMN  CM_VALOR_CONVENIO_TD.NOM_USUARIO IS 'NOMBRE USUARIO.'
/
COMMENT ON COLUMN  CM_VALOR_CONVENIO_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION.'
/
COMMENT ON COLUMN  CM_VALOR_CONVENIO_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
COMMENT ON COLUMN  CM_VALOR_CONVENIO_TD.COD_PLANCOMIS IS 'PLAN DE COMISIONES QUE CONTIENEN AL CONCEPTO.'
/
