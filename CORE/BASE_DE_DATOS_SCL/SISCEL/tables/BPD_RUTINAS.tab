CREATE TABLE BPD_RUTINAS(
    COD_RUTINA VARCHAR2(5) NOT NULL 
 ,  TIP_RUTINA VARCHAR2(5) NOT NULL 
 ,  DES_RUTINA VARCHAR2(30) NOT NULL 
 ,  FEC_DESDEVIGE DATE NOT NULL 
 ,  FEC_HASTAVIGE DATE NOT NULL 
 ,  ORI_RUTINA CHAR(1) NOT NULL 
 ,  COD_QUERY VARCHAR2(5) NULL 
 ,  IND_APLICACION CHAR(1) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  REV_RUTINA VARCHAR2(5) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  BPD_RUTINAS IS 'Bpd_Rutinas.'
/
COMMENT ON COLUMN  BPD_RUTINAS.COD_RUTINA IS 'Codigo que identifica a la Rutina.'
/
COMMENT ON COLUMN  BPD_RUTINAS.TIP_RUTINA IS 'Tipo de Rutina A : accion C : criterio .'
/
COMMENT ON COLUMN  BPD_RUTINAS.DES_RUTINA IS 'Descripcion del Criterio.'
/
COMMENT ON COLUMN  BPD_RUTINAS.FEC_DESDEVIGE IS 'Fecha Inicio de Vigencia.'
/
COMMENT ON COLUMN  BPD_RUTINAS.FEC_HASTAVIGE IS 'Fecha termino de Vigencia.'
/
COMMENT ON COLUMN  BPD_RUTINAS.ORI_RUTINA IS 'Origen de la rutina; Q: Query P: Procedimiento.'
/
COMMENT ON COLUMN  BPD_RUTINAS.COD_QUERY IS 'Codigo del Query Asociado.'
/
COMMENT ON COLUMN  BPD_RUTINAS.IND_APLICACION IS 'indica si el criterio se utiliza en el proceso de Evaluacion de Criterios(E) o en Descuentos de Facturacion(F).'
/
COMMENT ON COLUMN  BPD_RUTINAS.FEC_ULTMOD IS 'Fecha ultima actualizacion.'
/
COMMENT ON COLUMN  BPD_RUTINAS.NOM_USUARIO IS 'Nombre usuario responsable.'
/
COMMENT ON COLUMN  BPD_RUTINAS.REV_RUTINA IS 'Rutina inversa.'
/
