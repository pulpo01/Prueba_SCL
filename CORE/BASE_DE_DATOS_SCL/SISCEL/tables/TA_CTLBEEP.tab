CREATE TABLE TA_CTLBEEP(
    NUM_BLOQUE VARCHAR2(9) NOT NULL 
 ,  NUM_TICKETS NUMBER(9,0) NOT NULL 
 ,  NUM_SALIENTES NUMBER(8,0) NOT NULL 
 ,  NUM_ANOMALIAS NUMBER(8,0) NOT NULL 
 ,  NUM_CONGELADAS NUMBER(8,0) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_CTLBEEP IS 'Control ficheros tasados beeper'
/
COMMENT ON COLUMN  TA_CTLBEEP.NUM_BLOQUE IS 'Numero de Bloque'
/
COMMENT ON COLUMN  TA_CTLBEEP.NUM_TICKETS IS 'Numero de Tickets'
/
COMMENT ON COLUMN  TA_CTLBEEP.NUM_SALIENTES IS 'Numero de llamadas salientes'
/
COMMENT ON COLUMN  TA_CTLBEEP.NUM_ANOMALIAS IS 'Numero de Llamadas Anomalas'
/
COMMENT ON COLUMN  TA_CTLBEEP.NUM_CONGELADAS IS 'Numero de Llamadas Congeladas'
/
