CREATE TABLE GA_LNVEN_TO (  
   VEN_TELEFONICA                VARCHAR2(1)          NOT NULL
 , NUM_TELEFONO                  VARCHAR2(15)             NULL
 , COD_VENDEDOR                  NUMBER(6)                NULL
 , COD_VENDEALER                 NUMBER(6)                NULL
 , NOM_VENDEDOR                  VARCHAR2(40)             NULL
 , COD_TIPIDENT                  VARCHAR2(2)          NOT NULL
 , NUM_IDENT                     VARCHAR2(20)         NOT NULL
 , COD_ESTADO                    NUMBER(2)            NOT NULL
 , COD_CAUSA                     NUMBER(2)            NOT NULL
 , OBSERVACION                   VARCHAR2(255)            NULL
 , FEC_INGRESO                   DATE                 NOT NULL
 , FEC_MODIFIC                   DATE                 NOT NULL
 , NOM_USUARIO                   VARCHAR2(30)         NOT NULL
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  GA_LNVEN_TO                      IS '[ GA_LNVEN ] Listas Negras vendedoresListas Negras vendedores'
/

COMMENT ON COLUMN GA_LNVEN_TO.VEN_TELEFONICA                 IS 'indicador Vendedor de SCL'
/
COMMENT ON COLUMN GA_LNVEN_TO.NUM_TELEFONO                   IS 'telefono vendedor'
/
COMMENT ON COLUMN GA_LNVEN_TO.COD_VENDEDOR                   IS 'codigo vendedor SCL'
/
COMMENT ON COLUMN GA_LNVEN_TO.COD_VENDEALER                  IS 'codigo vendedor dealer SCL'
/
COMMENT ON COLUMN GA_LNVEN_TO.NOM_VENDEDOR                   IS 'nombre vendedor'
/
COMMENT ON COLUMN GA_LNVEN_TO.COD_TIPIDENT                   IS 'tipo de identificación'
/
COMMENT ON COLUMN GA_LNVEN_TO.NUM_IDENT                      IS 'numero de identificación'
/
COMMENT ON COLUMN GA_LNVEN_TO.COD_ESTADO                     IS 'vigente 1:si, 0:No'
/
COMMENT ON COLUMN GA_LNVEN_TO.COD_CAUSA                      IS 'causa  ingreso a listas negras'
/
COMMENT ON COLUMN GA_LNVEN_TO.OBSERVACION                    IS 'observación'
/
COMMENT ON COLUMN GA_LNVEN_TO.FEC_INGRESO                    IS 'fecha ingreso a listas negras'
/
COMMENT ON COLUMN GA_LNVEN_TO.FEC_MODIFIC                    IS 'Fecha modificación'
/
COMMENT ON COLUMN GA_LNVEN_TO.NOM_USUARIO                    IS 'usuario modificación'
/

