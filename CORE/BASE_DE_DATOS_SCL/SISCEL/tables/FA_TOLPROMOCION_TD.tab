CREATE TABLE FA_TOLPROMOCION_TD(
    UNIDAD_PROMO VARCHAR2(5) NOT NULL 
 ,  COD_CONCEPTO NUMBER(5,0) NOT NULL 
 ,  FEC_DESDE DATE NULL 
 ,  FEC_HASTA DATE NULL 
 ,  FEC_ULTMOD DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_TOLPROMOCION_TD IS 'Tabla de Unidades de Promociones'
/
COMMENT ON COLUMN  FA_TOLPROMOCION_TD.UNIDAD_PROMO IS 'Tipo de Unidad de la Promocion'
/
COMMENT ON COLUMN  FA_TOLPROMOCION_TD.FEC_DESDE IS 'Fecha de Inicio de la vigencia de la unidad'
/
COMMENT ON COLUMN  FA_TOLPROMOCION_TD.FEC_HASTA IS 'Fecha de termino de la vigencia de la unidad'
/
COMMENT ON COLUMN  FA_TOLPROMOCION_TD.FEC_ULTMOD IS 'Fecha de ultima modificacion del registro'
/
COMMENT ON COLUMN  FA_TOLPROMOCION_TD.NOM_USUARIO IS 'Nombre de Usuario'
/
