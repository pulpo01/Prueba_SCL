CREATE TABLE VE_HISTORICO_LABORAL_BURO_TO
(
  COD_REFERENCIA       NUMBER(8),
  COD_TIP_PATRONO      VARCHAR2(10 BYTE),
  NOMBRE_PATRONO       VARCHAR2(200 BYTE),
  FEC_INICIO           VARCHAR2(20 BYTE),
  FEC_COMPLETA_INICIO  VARCHAR2(20 BYTE),
  FEC_FIN              VARCHAR2(20 BYTE),
  FEC_COMPLETA_FIN     VARCHAR2(20 BYTE),
  MESES_LABORADOS      VARCHAR2(10 BYTE),
  CEDULA               VARCHAR2(15 BYTE)
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          1M
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING
/
COMMENT ON COLUMN VE_HISTORICO_LABORAL_BURO_TO.COD_REFERENCIA IS 'Codigo Referencia';
/
COMMENT ON COLUMN VE_HISTORICO_LABORAL_BURO_TO.COD_TIP_PATRONO IS 'Codigo del tipo de patrono';
/
COMMENT ON COLUMN VE_HISTORICO_LABORAL_BURO_TO.NOMBRE_PATRONO IS 'Nombre del patrono';
/
COMMENT ON COLUMN VE_HISTORICO_LABORAL_BURO_TO.FEC_INICIO IS 'Fecha inicio del patrono con formato descriptivos';
/
COMMENT ON COLUMN VE_HISTORICO_LABORAL_BURO_TO.FEC_COMPLETA_INICIO IS 'Fecha inicio del patrono con formato de fechas';
/
COMMENT ON COLUMN VE_HISTORICO_LABORAL_BURO_TO.FEC_FIN IS 'Fecha fin de labores con formato descriptivo';
/
COMMENT ON COLUMN VE_HISTORICO_LABORAL_BURO_TO.FEC_COMPLETA_FIN IS 'Fecha fin de labores con formato fechas';
/
COMMENT ON COLUMN VE_HISTORICO_LABORAL_BURO_TO.MESES_LABORADOS IS 'Meses laborados en ese trabajo';
/
COMMENT ON COLUMN VE_HISTORICO_LABORAL_BURO_TO.CEDULA IS 'Numero de Cedula';
/