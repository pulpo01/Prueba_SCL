CREATE TABLE SC_GRP_DOMINIO(
    COD_GRP_DOMINIO NUMBER(2,0) NOT NULL 
 ,  COD_DOMINIO_CTO NUMBER(2,0) NOT NULL 
 ,  NRO_MIEMBROS_GRP NUMBER(2,0) NULL 
 ,  DES_GRP_DOMINIO VARCHAR2(30) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SC_GRP_DOMINIO IS 'Grupos De Dominios.'
/
COMMENT ON COLUMN  SC_GRP_DOMINIO.COD_GRP_DOMINIO IS 'CODIGO DEL GRUPO DE DOMINIOS.'
/
COMMENT ON COLUMN  SC_GRP_DOMINIO.COD_DOMINIO_CTO IS 'CODIGO DEL DOMINIO DE CONCEPTOS MIEMBRO DEL GRUPO.'
/
COMMENT ON COLUMN  SC_GRP_DOMINIO.NRO_MIEMBROS_GRP IS 'NUMERO DE MIEMBROS DEL GRUPO DE DOMINIOS.'
/
COMMENT ON COLUMN  SC_GRP_DOMINIO.DES_GRP_DOMINIO IS 'IDENTIFICACION VERBAL BREVE DEL GRUPO (DESCRIPCION).'
/
