CREATE TABLE CO_INMUNE_IMPTO(
    COD_TIPIDENT VARCHAR2(2) NOT NULL 
 ,  NUM_IDENT VARCHAR2(20) NOT NULL 
 ,  COD_TIPINMUN VARCHAR2(6) NOT NULL 
 ,  FEC_CREACION DATE NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_INMUNE_IMPTO IS 'Inmunidad a cobros de impuestos.'
/
COMMENT ON COLUMN  CO_INMUNE_IMPTO.COD_TIPIDENT IS 'Tipo de documento de identificacion.'
/
COMMENT ON COLUMN  CO_INMUNE_IMPTO.NUM_IDENT IS 'Numero de documento de identificacion.'
/
COMMENT ON COLUMN  CO_INMUNE_IMPTO.COD_TIPINMUN IS 'fecha de termino de la inmunidad al cobro de impuestos.'
/
COMMENT ON COLUMN  CO_INMUNE_IMPTO.FEC_DESDE IS 'Fecha de creacin de la fila.'
/
COMMENT ON COLUMN  CO_INMUNE_IMPTO.FEC_HASTA IS 'Fecha de inicio de inmunidad al cobro de impuestos.'
/
COMMENT ON COLUMN  CO_INMUNE_IMPTO.NOM_USUARIO IS 'Usuario responsable.'
/
