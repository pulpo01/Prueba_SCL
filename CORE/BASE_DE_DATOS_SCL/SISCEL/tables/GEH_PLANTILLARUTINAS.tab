CREATE TABLE GEH_PLANTILLARUTINAS(
    COD_OPERADORA VARCHAR2(5) NOT NULL 
 ,  COD_MODULO VARCHAR2(2) NOT NULL 
 ,  NUM_CORRELATIVO NUMBER(5,0) NOT NULL 
 ,  NOM_RUTINA VARCHAR2(50) NOT NULL 
 ,  COD_RUTINA VARCHAR2(5) NOT NULL 
 ,  FLG_ESTADO VARCHAR2(5) DEFAULT 'TRUE' NOT NULL 
 ,  DES_RUTINA VARCHAR2(100) NOT NULL 
 ,  FEC_ULTMOD_H DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_HISTORIA_H DATE NOT NULL 
 ,  FLG_ACCION VARCHAR2(1) DEFAULT 'M' NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GEH_PLANTILLARUTINAS IS '.'
/
COMMENT ON COLUMN  GEH_PLANTILLARUTINAS.COD_OPERADORA IS 'Codigo de la operadora actual.'
/
COMMENT ON COLUMN  GEH_PLANTILLARUTINAS.COD_MODULO IS 'Codigo del modulo al que pertenece la rutina (GE_MODULOS.COD_MODULO).'
/
COMMENT ON COLUMN  GEH_PLANTILLARUTINAS.NUM_CORRELATIVO IS 'Numero entero positivo correlativo que parte de 1.'
/
COMMENT ON COLUMN  GEH_PLANTILLARUTINAS.NOM_RUTINA IS 'Nombre de la Rutina que se utilizara para la operadora.'
/
COMMENT ON COLUMN  GEH_PLANTILLARUTINAS.COD_RUTINA IS 'Tipo de Rutina (PL, ProC, etc.).'
/
COMMENT ON COLUMN  GEH_PLANTILLARUTINAS.FLG_ESTADO IS 'Estado de la Rutina A=Activa, I=Inactiva.'
/
COMMENT ON COLUMN  GEH_PLANTILLARUTINAS.DES_RUTINA IS 'Descripcion de las acciones de la Rutina.'
/
COMMENT ON COLUMN  GEH_PLANTILLARUTINAS.FEC_ULTMOD_H IS 'Fecha de modificacion o insercion del registro.'
/
COMMENT ON COLUMN  GEH_PLANTILLARUTINAS.NOM_USUARIO IS 'Identificador del usuario ("username" Oracle)..'
/
COMMENT ON COLUMN  GEH_PLANTILLARUTINAS.FEC_HISTORIA_H IS 'Fecha completa de insercion a la tabla (eliminacion, modificacion o actualizacion en la tabla origen).'
/
COMMENT ON COLUMN  GEH_PLANTILLARUTINAS.FLG_ACCION IS 'Accion de insertar, eliminar o modificar.'
/
