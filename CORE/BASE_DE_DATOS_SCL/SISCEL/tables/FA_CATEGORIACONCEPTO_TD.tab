 ,  COD_IMPUESTO VARCHAR2(5) NOT NULL 
 ,  VIGENTE VARCHAR2(1) DEFAULT 'S' NULL 
 ,  FEC_INI_VIGENCIA DATE NULL 
 ,  FEC_TER_VIGENCIA DATE NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_CATEGORIACONCEPTO_TD IS '[ CACO ] Agrupar el par impuestos conceptos'
/
COMMENT ON COLUMN  FA_CATEGORIACONCEPTO_TD.COD_CONCEPTO IS 'Código del Concepto'
/
COMMENT ON COLUMN  FA_CATEGORIACONCEPTO_TD.COD_IMPUESTO IS 'Código del Impuesto'
/
COMMENT ON COLUMN  FA_CATEGORIACONCEPTO_TD.VIGENTE IS 'Indica si es Vigente o No'
/
