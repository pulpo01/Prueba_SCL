CREATE TABLE ICB_COMPROC(
    NUM_MOVIMIENTO NUMBER(9,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(3,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  COD_SISTEMA NUMBER(2,0) NOT NULL 
 ,  COD_LENGUAJE NUMBER(3,0) NOT NULL 
 ,  TIP_RESPUESTA NUMBER(1,0) DEFAULT -1 NOT NULL 
 ,  IND_BLOQUEO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  COD_COMANDO NUMBER(3,0) NOT NULL 
 ,  DES_COMANDO VARCHAR2(512) NOT NULL 
 ,  FEC_INGRESO DATE DEFAULT SYSDATE NOT NULL 
 ,  FEC_EJECUCION DATE NULL 
 ,  DES_RESPUESTA VARCHAR2(80) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICB_COMPROC IS 'Comandos procesados beepers'
/
COMMENT ON COLUMN  ICB_COMPROC.NUM_MOVIMIENTO IS 'Numero de Movimiento Beeper'
/
COMMENT ON COLUMN  ICB_COMPROC.NUM_ORDEN IS 'Numero de orden del comando'
/
COMMENT ON COLUMN  ICB_COMPROC.COD_CENTRAL IS 'Codigo de Central'
/
COMMENT ON COLUMN  ICB_COMPROC.COD_SISTEMA IS 'Codigo de Sistema'
/
COMMENT ON COLUMN  ICB_COMPROC.COD_LENGUAJE IS 'Codigo de lenguaje del comando'
/
COMMENT ON COLUMN  ICB_COMPROC.TIP_RESPUESTA IS 'Indica si el comando ya se ejecuto correctamente (0) o no (<>0)'
/
COMMENT ON COLUMN  ICB_COMPROC.IND_BLOQUEO IS 'Indica si el comando esta siendo procesado por un operador(1) o no (0)'
/
COMMENT ON COLUMN  ICB_COMPROC.COD_COMANDO IS 'Codigo de comando'
/
COMMENT ON COLUMN  ICB_COMPROC.DES_COMANDO IS 'Comando a enviar'
/
COMMENT ON COLUMN  ICB_COMPROC.FEC_INGRESO IS 'Fecha de ingreso en la tabla'
/
COMMENT ON COLUMN  ICB_COMPROC.FEC_EJECUCION IS 'Fecha de ejecucion del comando'
/
COMMENT ON COLUMN  ICB_COMPROC.DES_RESPUESTA IS 'Respuesta recibida al comando'
/
