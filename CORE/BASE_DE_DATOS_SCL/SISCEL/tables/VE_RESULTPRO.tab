 ,  COD_RECHAZOPRO VARCHAR2(2) NOT NULL 
 ,  COD_RESULTPRO NUMBER(4,0) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_RESULTPRO IS 'Resultados de Entrevistas a Prospectos'
/
COMMENT ON COLUMN  VE_RESULTPRO.COD_ENTREVPRO IS 'Codigo Entrevista a Prospecto'
/
COMMENT ON COLUMN  VE_RESULTPRO.COD_ESTADOPRO IS 'Codigo de Estado de Prospecto'
/
COMMENT ON COLUMN  VE_RESULTPRO.COD_RECHAZOPRO IS 'Codigo de Rechazo de Prospecto'
