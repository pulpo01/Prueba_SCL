CREATE TABLE FAD_IMPSERVCLIE(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  FECHA_DESDE DATE NOT NULL 
 ,  FECHA_HASTA DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  FAD_IMPSERVCLIE.COD_CLIENTE IS 'Codigo del cliente.'
/
COMMENT ON COLUMN  FAD_IMPSERVCLIE.COD_SERVICIO IS 'Codigo del servicio.'
/
COMMENT ON COLUMN  FAD_IMPSERVCLIE.FECHA_DESDE IS 'Fecha de Inicio de Vigecia del Periodo de Asignacion.'
/
COMMENT ON COLUMN  FAD_IMPSERVCLIE.FECHA_HASTA IS 'Fecha de Fin de Vigecia del Periodo de Asignacion.'
/
COMMENT ON COLUMN  FAD_IMPSERVCLIE.NOM_USUARIO IS 'Nombre de Usuario.'
/
COMMENT ON COLUMN  FAD_IMPSERVCLIE.FEC_ULTMOD IS 'Fecha de Ultima Modificacion.'
/
