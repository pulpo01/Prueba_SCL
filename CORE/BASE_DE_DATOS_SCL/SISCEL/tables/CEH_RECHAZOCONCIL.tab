CREATE TABLE CEH_RECHAZOCONCIL(
    NUM_PROCESO NUMBER(10,0) NOT NULL 
 ,  NUM_RECIBO NUMBER(9,0) NOT NULL 
 ,  FEC_CONTABLE DATE NULL 
 ,  NUM_SERIE VARCHAR2(16) NULL 
 ,  COD_EMPRESA_SERV VARCHAR2(5) NULL 
 ,  COD_EMPRESA_ATM VARCHAR2(5) NULL 
 ,  COD_ATMOPER NUMBER(5,0) NOT NULL 
 ,  FEC_TRANSACCION DATE NULL 
 ,  NUM_OPERACION NUMBER(20,0) NOT NULL 
 ,  NUM_TRANSACCION NUMBER(10,0) NULL 
 ,  NUM_TARJETA VARCHAR2(18) NULL 
 ,  COD_EMPRESA_TAR VARCHAR2(5) NULL 
 ,  MTO_TRANSACCION NUMBER(14,4) NULL 
 ,  COD_RESULRED VARCHAR2(4) NULL 
 ,  IND_INCONSISTENCIA CHAR(1) NULL 
 ,  NUM_CELULAR VARCHAR2(15) NULL 
 ,  NUM_CUOTAS NUMBER(2,0) NULL 
 ,  NUM_VOUCHER NUMBER(6,0) NULL 
 ,  NUM_FOLIOCTC VARCHAR2(11) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
