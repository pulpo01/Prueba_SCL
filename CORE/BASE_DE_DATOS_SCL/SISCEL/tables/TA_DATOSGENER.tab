 ,  COD_FRANGRATUITA VARCHAR2(3) NOT NULL 
 ,  TIP_REDONDEO VARCHAR2(5) NULL 
 ,  TIP_REDONDEOENT VARCHAR2(5) NULL 
 ,  NUM_BLOQUEROA VARCHAR2(9) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
