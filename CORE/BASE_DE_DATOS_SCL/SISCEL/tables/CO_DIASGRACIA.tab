CREATE TABLE CO_DIASGRACIA(
    COD_CALCLIEN VARCHAR2(2) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  DIAS_GRACIA NUMBER(2,0) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_DIASGRACIA IS 'DIAS DE GRACIA'
/
COMMENT ON COLUMN  CO_DIASGRACIA.COD_CALCLIEN IS 'CALIDAD DE CLIENTE'
/
COMMENT ON COLUMN  CO_DIASGRACIA.COD_PRODUCTO IS 'CODIGO DE PRODUCTO'
/
COMMENT ON COLUMN  CO_DIASGRACIA.FEC_DESDE IS 'FECHA DESDE'
/
COMMENT ON COLUMN  CO_DIASGRACIA.FEC_HASTA IS 'FECHA HASTA'
/
COMMENT ON COLUMN  CO_DIASGRACIA.DIAS_GRACIA IS 'DIAS DE GRACIA'
/
