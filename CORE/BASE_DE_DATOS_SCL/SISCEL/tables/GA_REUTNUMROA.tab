CREATE TABLE GA_REUTNUMROA(
    NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  COD_PAIS VARCHAR2(3) NOT NULL 
 ,  COD_ZONA VARCHAR2(2) NOT NULL 
 ,  FEC_BAJA DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_REUTNUMROA IS 'Numeracion Reutilizable Operadoras Roaming'
/
COMMENT ON COLUMN  GA_REUTNUMROA.NUM_CELULAR IS 'Numero Celular'
/
COMMENT ON COLUMN  GA_REUTNUMROA.COD_OPERADOR IS 'Codigo de Operador'
/
COMMENT ON COLUMN  GA_REUTNUMROA.COD_PAIS IS 'Codigo de Pais'
/
COMMENT ON COLUMN  GA_REUTNUMROA.COD_ZONA IS 'Codigo de Zona'
/
COMMENT ON COLUMN  GA_REUTNUMROA.FEC_BAJA IS 'Fecha de Baja del Numero'
/
