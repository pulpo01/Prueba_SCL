CREATE TABLE GA_DIRMASIVAS_CABARCHIVO(
    COD_ARCHIVO NUMBER(8,0) NOT NULL 
 ,  FEC_LECTURA_D DATE NOT NULL 
 ,  FEC_PROCESO_D DATE NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  NOM_ARCHIVO VARCHAR2(50) NOT NULL 
 ,  NUM_LINEAS NUMBER(5,0) NOT NULL 
 ,  NUM_PROCESADAS NUMBER(5,0) NOT NULL 
 ,  IND_PROCESADO NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_DIRMASIVAS_CABARCHIVO IS 'Cabecera del archivo leido'
/
COMMENT ON COLUMN  GA_DIRMASIVAS_CABARCHIVO.COD_ARCHIVO IS 'Secuencia'
/
COMMENT ON COLUMN  GA_DIRMASIVAS_CABARCHIVO.FEC_LECTURA_D IS 'Fecha de lectura del archivo'
/
COMMENT ON COLUMN  GA_DIRMASIVAS_CABARCHIVO.FEC_PROCESO_D IS 'Fecha de procesamiento del archivo'
/
COMMENT ON COLUMN  GA_DIRMASIVAS_CABARCHIVO.NOM_USUARORA IS 'Usuario que cargo el archivo'
/
COMMENT ON COLUMN  GA_DIRMASIVAS_CABARCHIVO.NOM_ARCHIVO IS 'Nombre del archivo'
/
COMMENT ON COLUMN  GA_DIRMASIVAS_CABARCHIVO.NUM_LINEAS IS 'Cantidad de lineas del archivo'
/
COMMENT ON COLUMN  GA_DIRMASIVAS_CABARCHIVO.NUM_PROCESADAS IS 'Lineas procesadas'
/
COMMENT ON COLUMN  GA_DIRMASIVAS_CABARCHIVO.IND_PROCESADO IS 'Indicativo de procesado (0/1 No/Si)'
/
