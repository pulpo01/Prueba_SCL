CREATE TABLE GE_CIUCOM(
    COD_REGION VARCHAR2(3) NOT NULL 
 ,  COD_PROVINCIA VARCHAR2(5) NOT NULL 
 ,  COD_CIUDAD VARCHAR2(5) NOT NULL 
 ,  COD_COMUNA VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_CIUCOM IS 'Asociacion de Ciudades/Pueblos a Comunas'
/
COMMENT ON COLUMN  GE_CIUCOM.COD_REGION IS 'Codigo de Region'
/
COMMENT ON COLUMN  GE_CIUCOM.COD_PROVINCIA IS 'Codigo de Provincia'
/
COMMENT ON COLUMN  GE_CIUCOM.COD_CIUDAD IS 'Codigo de Ciudad'
/
COMMENT ON COLUMN  GE_CIUCOM.COD_COMUNA IS 'Codigo de Comuna'
/
