CREATE TABLE VE_PROASIGN(
    COD_PROSPECTO NUMBER(8,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  FEC_ASIGNACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_DESASIGNAC DATE NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_PROASIGN IS 'Asignaciones/Reasignaciones de Prospectos'
/
COMMENT ON COLUMN  VE_PROASIGN.COD_PROSPECTO IS 'Codigo de Prospecto'
/
COMMENT ON COLUMN  VE_PROASIGN.COD_TIPCOMIS IS 'Codigo de Tipo de Comisionista'
/
COMMENT ON COLUMN  VE_PROASIGN.COD_VENDEDOR IS 'Codigo de Vendedor'
/
COMMENT ON COLUMN  VE_PROASIGN.FEC_ASIGNACION IS 'Fecha de Asignacion/Reasignacion'
/
COMMENT ON COLUMN  VE_PROASIGN.NOM_USUARIO IS 'Usuario que efectua la operacion'
/
COMMENT ON COLUMN  VE_PROASIGN.FEC_DESASIGNAC IS 'Fecha de Desasignacion'
/
