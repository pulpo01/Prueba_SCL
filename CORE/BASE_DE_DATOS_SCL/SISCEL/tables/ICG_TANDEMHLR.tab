CREATE TABLE ICG_TANDEMHLR(
    COD_SISTEMA NUMBER(2,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(1,0) NOT NULL 
 ,  NOM_NODO VARCHAR2(15) NULL 
 ,  NOM_USER VARCHAR2(30) NULL 
 ,  NUM_PUERTO NUMBER(6,0) NULL 
 ,  NOM_COMANDO VARCHAR2(100) NOT NULL 
 ,  DES_COMANDO VARCHAR2(50) NOT NULL 
 ,  NOM_CONFIG VARCHAR2(80) NULL 
 ,  TIE_ESPERA NUMBER(3,0) NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICG_TANDEMHLR IS 'Datos de Comunicaciones de Tandem/HLR'
/
COMMENT ON COLUMN  ICG_TANDEMHLR.COD_SISTEMA IS 'codigo de sistema'
/
COMMENT ON COLUMN  ICG_TANDEMHLR.NUM_ORDEN IS 'Orden de intento de acceso al sistema'
/
COMMENT ON COLUMN  ICG_TANDEMHLR.NOM_NODO IS 'nodo de acceso al sistema'
/
COMMENT ON COLUMN  ICG_TANDEMHLR.NOM_USER IS 'usuario/clave de acceso al sistema'
/
COMMENT ON COLUMN  ICG_TANDEMHLR.NUM_PUERTO IS 'puerto de acceso al sistema'
/
COMMENT ON COLUMN  ICG_TANDEMHLR.NOM_COMANDO IS 'Comando auxiliar a utilizar para el acceso al sistema'
/
COMMENT ON COLUMN  ICG_TANDEMHLR.DES_COMANDO IS 'Descripcion del comando de acceso'
/
COMMENT ON COLUMN  ICG_TANDEMHLR.NOM_CONFIG IS 'fichero de configuracion adicional para el comando'
/
COMMENT ON COLUMN  ICG_TANDEMHLR.TIE_ESPERA IS 'tiempo maximo de espera para que termine el comando'
/
