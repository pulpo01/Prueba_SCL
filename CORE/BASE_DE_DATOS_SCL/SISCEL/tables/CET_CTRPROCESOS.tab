CREATE TABLE CET_CTRPROCESOS(
    COD_PROCESO NUMBER(3,0) NOT NULL 
 ,  FEC_ACTIVACION DATE NOT NULL 
 ,  FEC_DESACTIVACION DATE NULL 
 ,  COD_ACTIVACION CHAR(1) NULL 
 ,  COD_ESTADO VARCHAR2(2) NULL 
 ,  PID_PROCESO NUMBER(8,0) NULL 
 ,  OBS_EVENTO VARCHAR2(100) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CET_CTRPROCESOS IS 'Control de procesos de Comercio Electronico.'
/
COMMENT ON COLUMN  CET_CTRPROCESOS.COD_PROCESO IS 'Codigo del proceso.'
/
COMMENT ON COLUMN  CET_CTRPROCESOS.FEC_ACTIVACION IS 'Fecha de activacion.'
/
COMMENT ON COLUMN  CET_CTRPROCESOS.FEC_DESACTIVACION IS 'Fecha de desactivacion.'
/
COMMENT ON COLUMN  CET_CTRPROCESOS.COD_ACTIVACION IS 'Codigo de activacion.'
/
COMMENT ON COLUMN  CET_CTRPROCESOS.COD_ESTADO IS 'Codigo estado.'
/
COMMENT ON COLUMN  CET_CTRPROCESOS.PID_PROCESO IS 'Numero de proceso.'
/
COMMENT ON COLUMN  CET_CTRPROCESOS.OBS_EVENTO IS 'Observacion.'
/
