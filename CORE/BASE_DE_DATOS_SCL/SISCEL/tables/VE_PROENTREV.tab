CREATE TABLE VE_PROENTREV(
    COD_PROSPECTO NUMBER(8,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  FEC_ENTREVISTA DATE NOT NULL 
 ,  FEC_CUMPLIMENT DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_RESULTPRO NUMBER(4,0) NOT NULL 
 ,  OBS_ENTREVISTA VARCHAR2(255) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_PROENTREV IS 'Prospectos entrevistados'
/
COMMENT ON COLUMN  VE_PROENTREV.COD_PROSPECTO IS 'Codigo de Prospecto entrevistado'
/
COMMENT ON COLUMN  VE_PROENTREV.COD_TIPCOMIS IS 'Codigo de Tipo de Comisionista'
/
COMMENT ON COLUMN  VE_PROENTREV.COD_VENDEDOR IS 'Codigo de Vendedor que entrevista'
/
COMMENT ON COLUMN  VE_PROENTREV.FEC_ENTREVISTA IS 'Fecha de la Entrevista'
/
COMMENT ON COLUMN  VE_PROENTREV.FEC_CUMPLIMENT IS 'Fecha de cumplimentacion de la Entrevista'
/
COMMENT ON COLUMN  VE_PROENTREV.NOM_USUARIO IS 'Usuario que introduce los datos'
/
COMMENT ON COLUMN  VE_PROENTREV.COD_RESULTPRO IS 'Resultado de la Entrevista'
/
COMMENT ON COLUMN  VE_PROENTREV.OBS_ENTREVISTA IS 'Observaciones a la Entrevista'
/
