CREATE TABLE GE_PUNTOACCESO_INT_TD (  
   COD_PUNTOACCESO               VARCHAR2(10)                                     NOT NULL 
 , DES_PUNTOACCESO               VARCHAR2(300)                                     NOT NULL 
 ) TABLESPACE     INSTALL_DATA
/


