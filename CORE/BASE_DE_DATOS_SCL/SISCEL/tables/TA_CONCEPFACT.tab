CREATE TABLE TA_CONCEPFACT(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_FACTURACION NUMBER(4,0) NOT NULL 
 ,  IND_ENTSAL NUMBER(1,0) NOT NULL 
 ,  IND_TABLA NUMBER(1,0) NOT NULL 
 ,  COD_TARIFICACION NUMBER(5,0) NOT NULL 
 ,  IND_DESTINO NUMBER(5,0) DEFAULT 1 NOT NULL 
 ,  COD_SERVICIO VARCHAR2(5) DEFAULT ' ' NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_CONCEPFACT IS 'Consumos de tarificacion a conceptos de facturacion'
/
COMMENT ON COLUMN  TA_CONCEPFACT.COD_PRODUCTO IS 'Codigo de Poduccto'
/
COMMENT ON COLUMN  TA_CONCEPFACT.COD_FACTURACION IS 'Codigo de Facturacion'
/
COMMENT ON COLUMN  TA_CONCEPFACT.IND_ENTSAL IS 'Indicador de entrante/Saliente'
/
COMMENT ON COLUMN  TA_CONCEPFACT.IND_TABLA IS 'Indicador de Tabla'
/
COMMENT ON COLUMN  TA_CONCEPFACT.COD_TARIFICACION IS 'Codigo de Tarificacion'
/
COMMENT ON COLUMN  TA_CONCEPFACT.IND_DESTINO IS 'Indicador de destino 1=T.Movil; 2=Red Fija; 3=Otras Moviles.'
/
COMMENT ON COLUMN  TA_CONCEPFACT.COD_SERVICIO IS 'CODIGO DE SERVICIO'
/
