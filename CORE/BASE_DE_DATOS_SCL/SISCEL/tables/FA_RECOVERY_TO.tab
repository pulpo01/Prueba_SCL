CREATE TABLE FA_RECOVERY_TO(
    COD_PROC VARCHAR2(8) NOT NULL 
 ,  NUM_MODULO NUMBER(8,0) NOT NULL 
 ,  VAL_MINIMO NUMBER(14,0) DEFAULT 0 NULL 
 ,  VAL_MAXIMO NUMBER(14,0) DEFAULT 0 NULL 
 ,  COD_ESTADO VARCHAR2(5) DEFAULT 'INI' NULL 
 ,  NUM_ROWS NUMBER(8,0) DEFAULT 0 NULL 
 ,  NUM_BLOQUE NUMBER(8,0) DEFAULT 0 NULL 
 ,  HOST_ID VARCHAR2(20) DEFAULT 'AGRZONA0' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_RECOVERY_TO IS '[ RECY ] RESUMENES DEL  PROCESO DE ACUMULACION'
/
COMMENT ON COLUMN  FA_RECOVERY_TO.COD_PROC IS 'Proceso'
/
COMMENT ON COLUMN  FA_RECOVERY_TO.NUM_MODULO IS 'Modulo'
/
COMMENT ON COLUMN  FA_RECOVERY_TO.VAL_MINIMO IS 'Valor minimo'
/
COMMENT ON COLUMN  FA_RECOVERY_TO.VAL_MAXIMO IS 'Valor maximo'
/
COMMENT ON COLUMN  FA_RECOVERY_TO.COD_ESTADO IS 'Estado'
/
COMMENT ON COLUMN  FA_RECOVERY_TO.NUM_ROWS IS 'Cantiad de registros'
/
COMMENT ON COLUMN  FA_RECOVERY_TO.NUM_BLOQUE IS 'Número de Bloque'
/
COMMENT ON COLUMN  FA_RECOVERY_TO.HOST_ID IS 'Indentificación de Máquina'
/
