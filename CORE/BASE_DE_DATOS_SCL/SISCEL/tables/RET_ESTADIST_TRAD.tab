CREATE TABLE RET_ESTADIST_TRAD(
    COD_ENTIDAD VARCHAR2(5) NOT NULL 
 ,  FEC_CARGA_H DATE NOT NULL 
 ,  GLS_NOMBRE VARCHAR2(60) NOT NULL 
 ,  VAL_REGINFORMADOS NUMBER(14,4) DEFAULT 0 NULL 
 ,  MTO_INFORMADO NUMBER(14,4) DEFAULT 0 NULL 
 ,  VAL_REGLEIDOS NUMBER(6,0) DEFAULT 0 NOT NULL 
 ,  VAL_REGACEPTADOS NUMBER(6,0) DEFAULT 0 NOT NULL 
 ,  MTO_LEIDOS NUMBER(14,4) DEFAULT 0 NOT NULL 
 ,  MTO_ACEPTADO NUMBER(14,4) DEFAULT 0 NOT NULL 
 ,  IND_ESTADO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  RET_ESTADIST_TRAD IS 'Mantiene Las Estadisticas De Los Procesos De Carga.'
/
COMMENT ON COLUMN  RET_ESTADIST_TRAD.COD_ENTIDAD IS '.'
/
COMMENT ON COLUMN  RET_ESTADIST_TRAD.FEC_CARGA_H IS 'Fecha y hora de carga del archivo.'
/
COMMENT ON COLUMN  RET_ESTADIST_TRAD.GLS_NOMBRE IS 'Nombre del archivo procesado.'
/
COMMENT ON COLUMN  RET_ESTADIST_TRAD.VAL_REGINFORMADOS IS 'cantidad de registros informados por la entidad.'
/
COMMENT ON COLUMN  RET_ESTADIST_TRAD.MTO_INFORMADO IS 'Monto total informados por la entidad.'
/
COMMENT ON COLUMN  RET_ESTADIST_TRAD.VAL_REGLEIDOS IS 'cantidad de registros leidos en el archivo.'
/
COMMENT ON COLUMN  RET_ESTADIST_TRAD.VAL_REGACEPTADOS IS 'cantidad de registros aceptados del archivo.'
/
COMMENT ON COLUMN  RET_ESTADIST_TRAD.MTO_LEIDOS IS 'Monto total leido en el archivo.'
/
COMMENT ON COLUMN  RET_ESTADIST_TRAD.MTO_ACEPTADO IS 'Monto total de registros aceptados en el archivo.'
/
COMMENT ON COLUMN  RET_ESTADIST_TRAD.IND_ESTADO IS 'Estado que indica en la etapa del proceso en que esta el archivo.'
/
