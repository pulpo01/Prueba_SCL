CREATE TABLE AL_GAINTERAL(
    TIPO NUMBER(1,0) NOT NULL 
 ,  TIP_STOCK NUMBER(2,0) NOT NULL 
 ,  COD_BODEGA NUMBER(6,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_ESTADO NUMBER(2,0) NOT NULL 
 ,  NUM_VENTA NUMBER(10,0) NOT NULL 
 ,  NUM_CANTIDAD NUMBER(6,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NULL 
 ,  IND_TELEFONO NUMBER(1,0) NULL 
 ,  NUM_TRANSACCION NUMBER(9,0) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_GAINTERAL IS 'Tabla Auxiliar para ejecucion de GA_PAC_INTERAL'
/
COMMENT ON COLUMN  AL_GAINTERAL.TIPO IS 'Tipo de operacion sobre la serie'
/
COMMENT ON COLUMN  AL_GAINTERAL.TIP_STOCK IS 'Tipo de stock'
/
COMMENT ON COLUMN  AL_GAINTERAL.COD_BODEGA IS 'Codigo de bodega'
/
COMMENT ON COLUMN  AL_GAINTERAL.COD_ARTICULO IS 'Codigo de articulo'
/
COMMENT ON COLUMN  AL_GAINTERAL.COD_USO IS 'Codigo de uso'
/
COMMENT ON COLUMN  AL_GAINTERAL.COD_ESTADO IS 'Codigo de estado'
/
COMMENT ON COLUMN  AL_GAINTERAL.NUM_VENTA IS 'Numero de venta'
/
COMMENT ON COLUMN  AL_GAINTERAL.NUM_CANTIDAD IS 'Cantidad'
/
COMMENT ON COLUMN  AL_GAINTERAL.NUM_SERIE IS 'Numero de serie'
/
COMMENT ON COLUMN  AL_GAINTERAL.IND_TELEFONO IS 'Indicativo de estado de telefono'
/
COMMENT ON COLUMN  AL_GAINTERAL.NUM_TRANSACCION IS 'Numero de transaccion'
/
