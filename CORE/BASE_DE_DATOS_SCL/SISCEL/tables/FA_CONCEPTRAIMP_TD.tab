CREATE TABLE FA_CONCEPTRAIMP_TD(
    COD_ZONAORIG VARCHAR2(5) NOT NULL 
 ,  COD_CONCORIG NUMBER(5,0) NOT NULL 
 ,  COD_CONCDEST NUMBER(4,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_CONCEPTRAIMP_TD IS '[ CTRI ] CONCEPTOS DE IMPUESTO ESPECIFICOS PARA AREAS TOL'
/
COMMENT ON COLUMN  FA_CONCEPTRAIMP_TD.COD_ZONAORIG IS 'Zona Origen de trafico'
/
COMMENT ON COLUMN  FA_CONCEPTRAIMP_TD.COD_CONCORIG IS 'Concepto Original de tráfico'
/
COMMENT ON COLUMN  FA_CONCEPTRAIMP_TD.COD_CONCDEST IS 'Concepto de trafico a aplicar impuestos especificos'
/
COMMENT ON COLUMN  FA_CONCEPTRAIMP_TD.FEC_DESDE IS 'Vigencia'
/
COMMENT ON COLUMN  FA_CONCEPTRAIMP_TD.FEC_HASTA IS 'Fin vigencia'
/
