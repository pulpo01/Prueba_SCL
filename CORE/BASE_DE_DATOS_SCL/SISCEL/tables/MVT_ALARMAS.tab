CREATE TABLE MVT_ALARMAS(
    FEC_PROCESO DATE NOT NULL 
 ,  COD_CUENTA NUMBER(8,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_CRITERIO NUMBER(4,0) NOT NULL 
 ,  VAL_VARCRITERIO VARCHAR2(50) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  MVT_ALARMAS IS 'Tabla Alarmas.'
/
COMMENT ON COLUMN  MVT_ALARMAS.FEC_PROCESO IS 'FECHA PROCESO.'
/
COMMENT ON COLUMN  MVT_ALARMAS.COD_CUENTA IS 'NUMERO DE LA CUENTA.'
/
COMMENT ON COLUMN  MVT_ALARMAS.COD_CLIENTE IS 'ENTIDAD FACTURABLE.'
/
COMMENT ON COLUMN  MVT_ALARMAS.NUM_ABONADO IS 'LINEA.'
/
