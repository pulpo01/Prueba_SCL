CREATE TABLE VE_NIVACCUSER(
    NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_NIVELACC VARCHAR2(2) NOT NULL 
 ,  COD_OFICINA VARCHAR2(2) NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_NIVACCUSER IS 'Nivel de Acceso a Datos para Usuario'
/
COMMENT ON COLUMN  VE_NIVACCUSER.NOM_USUARIO IS 'Usuario que es objeto de los permisos de acceso'
/
COMMENT ON COLUMN  VE_NIVACCUSER.COD_NIVELACC IS 'Codigo de Nivel de Acceso ("TO", "OF", "VE", "CM")'
/
COMMENT ON COLUMN  VE_NIVACCUSER.COD_OFICINA IS 'Codigo de Oficina para Acceso "OF"'
/
COMMENT ON COLUMN  VE_NIVACCUSER.COD_VENDEDOR IS 'Codigo de Vendedor para Acceso "VE"'
/
COMMENT ON COLUMN  VE_NIVACCUSER.COD_TIPCOMIS IS 'Codigo de Tipo de Comisionista para Acceso "CM"'
/
