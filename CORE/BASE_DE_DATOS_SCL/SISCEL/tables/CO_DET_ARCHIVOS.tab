CREATE TABLE CO_DET_ARCHIVOS(
    NUM_PROCESO NUMBER(8,0) NULL 
 ,  COD_PROCESO VARCHAR2(5) NULL 
 ,  COD_ENTIDAD VARCHAR2(5) NULL 
 ,  TIP_REGITRO CHAR(1) NULL 
 ,  TXT_REGISTRO VARCHAR2(1024) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_DET_ARCHIVOS IS 'Registros Formateados Para Archivos De Salida.'
/
COMMENT ON COLUMN  CO_DET_ARCHIVOS.NUM_PROCESO IS 'Numero Proceso Pro-C.'
/
COMMENT ON COLUMN  CO_DET_ARCHIVOS.COD_PROCESO IS 'Codigo Proceso (COBEX, DICOM).'
/
COMMENT ON COLUMN  CO_DET_ARCHIVOS.COD_ENTIDAD IS 'Codigo Entidad.'
/
COMMENT ON COLUMN  CO_DET_ARCHIVOS.TIP_REGITRO IS 'Tipo Registro (Encabezado, Detalle, Pie).'
/
COMMENT ON COLUMN  CO_DET_ARCHIVOS.TXT_REGISTRO IS 'Registro formateado para archivo.'
/
