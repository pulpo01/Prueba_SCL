CREATE TABLE TA_CLTFREDES(
    COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  IND_AREA NUMBER(1,0) NOT NULL 
 ,  COD_AREA VARCHAR2(5) NOT NULL 
 ,  COD_ALM VARCHAR2(3) NOT NULL 
 ,  COD_EFECTIVIDAD NUMBER(2,0) NOT NULL 
 ,  TAR_REDES VARCHAR2(3) NOT NULL 
 ,  TIP_REDONDEO VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_CLTFREDES IS 'Clases de tarifas otras redes'
/
COMMENT ON COLUMN  TA_CLTFREDES.COD_OPERADOR IS 'Codigo de Operador'
/
COMMENT ON COLUMN  TA_CLTFREDES.IND_AREA IS 'Indicador area (Mantenimiento 0Pais/1Area)'
/
COMMENT ON COLUMN  TA_CLTFREDES.COD_AREA IS 'Codigo de Area'
/
COMMENT ON COLUMN  TA_CLTFREDES.COD_ALM IS 'Codigo de ALM'
/
COMMENT ON COLUMN  TA_CLTFREDES.COD_EFECTIVIDAD IS 'Codigo de Efectividad'
/
COMMENT ON COLUMN  TA_CLTFREDES.TAR_REDES IS 'Tarifa Redes'
/
COMMENT ON COLUMN  TA_CLTFREDES.TIP_REDONDEO IS 'Tipo de Redondeo'
/
