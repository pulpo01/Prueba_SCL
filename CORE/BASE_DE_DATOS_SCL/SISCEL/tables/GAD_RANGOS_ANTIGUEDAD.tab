CREATE TABLE GAD_RANGOS_ANTIGUEDAD(
    COD_RANGO VARCHAR2(3) NOT NULL 
 ,  COD_TIPRANGO VARCHAR2(5) NOT NULL 
 ,  RAN_DESDE NUMBER(3,0) NOT NULL 
 ,  RAN_HASTA NUMBER(3,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAD_RANGOS_ANTIGUEDAD IS 'Dominios De Rangos De Antiguedad.'
/
COMMENT ON COLUMN  GAD_RANGOS_ANTIGUEDAD.COD_RANGO IS 'Codigo de Rango.'
/
COMMENT ON COLUMN  GAD_RANGOS_ANTIGUEDAD.COD_TIPRANGO IS 'Tipo de Rango (meses,das).'
/
COMMENT ON COLUMN  GAD_RANGOS_ANTIGUEDAD.RAN_DESDE IS 'Inicio del rango.'
/
COMMENT ON COLUMN  GAD_RANGOS_ANTIGUEDAD.RAN_HASTA IS 'Final del rango.'
/
COMMENT ON COLUMN  GAD_RANGOS_ANTIGUEDAD.FEC_DESDE IS 'fecha inicio vigencia.'
/
COMMENT ON COLUMN  GAD_RANGOS_ANTIGUEDAD.FEC_HASTA IS 'fecha final de vigencia.'
/
COMMENT ON COLUMN  GAD_RANGOS_ANTIGUEDAD.NOM_USUARIO IS 'Usuario responsable.'
/
