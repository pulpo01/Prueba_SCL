CREATE TABLE AL_PRECIOS_DEDUCIBLE_TO
(
  COD_ARTICULO   NUMBER(6)                      NOT NULL,
  COD_MONEDA     VARCHAR2(3 BYTE)               NOT NULL,
  PRC_DEDUCIBLE  NUMBER(14,4)                   NOT NULL,
  FEC_DESDE      DATE                           NOT NULL,
  FEC_HASTA      DATE                           NOT NULL,
  NOM_USUARIO    VARCHAR2(30 BYTE)              NOT NULL
)
;

COMMENT ON TABLE AL_PRECIOS_DEDUCIBLE_TO IS '[ PRDE ] Tabla de precios deduciblesTabla de precios deducibles';

COMMENT ON COLUMN AL_PRECIOS_DEDUCIBLE_TO.COD_ARTICULO IS 'codigo de articulo';

COMMENT ON COLUMN AL_PRECIOS_DEDUCIBLE_TO.COD_MONEDA IS 'codigo moneda';

COMMENT ON COLUMN AL_PRECIOS_DEDUCIBLE_TO.PRC_DEDUCIBLE IS 'Precio';

COMMENT ON COLUMN AL_PRECIOS_DEDUCIBLE_TO.FEC_DESDE IS 'fecha desde';

COMMENT ON COLUMN AL_PRECIOS_DEDUCIBLE_TO.FEC_HASTA IS 'fecha hasta';

COMMENT ON COLUMN AL_PRECIOS_DEDUCIBLE_TO.NOM_USUARIO IS 'nombre de usuario';