CREATE TABLE FA_CRITSERV(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  COD_CRITERIO VARCHAR2(5) NOT NULL 
 ,  VAL_CRITERIO VARCHAR2(10) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_CRITSERV IS 'Tabla Que Almacena Los Criterios Para Los Servicios.'
/
COMMENT ON COLUMN  FA_CRITSERV.COD_PRODUCTO IS 'CODIGO DEL SERVICIO A QUE CORRESPONDE EL CRITERIO.'
/
COMMENT ON COLUMN  FA_CRITSERV.COD_SERVICIO IS 'CODIGO DEL SERVICIO A QUE CORRESPONDE EL CRITERIO.'
/
COMMENT ON COLUMN  FA_CRITSERV.COD_CRITERIO IS 'CODIGO DEL CRITERIO ASOCIADO AL SERVICIO.'
/
COMMENT ON COLUMN  FA_CRITSERV.VAL_CRITERIO IS 'VALOR DEL CRITERIO ASOCIADO AL SERVICIO.'
/
