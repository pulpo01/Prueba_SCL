CREATE TABLE SC_CAB_LOTE(
    COD_EVENTO NUMBER(2,0) NOT NULL 
 ,  ID_LOTE VARCHAR2(50) NOT NULL 
 ,  PER_CONTABLE NUMBER(6,0) NOT NULL 
 ,  NOM_USUARIO_LOTE VARCHAR2(30) NOT NULL 
 ,  FEC_LOTE DATE NOT NULL 
 ,  NUM_LOTE NUMBER(10,0) NOT NULL 
 ,  NOM_USUARIO_CONT VARCHAR2(30) NULL 
 ,  FEC_CONT DATE NULL 
 ,  NOM_USUARIO_OK VARCHAR2(30) NULL 
 ,  FEC_OK DATE NULL 
 ,  COD_OPERADORA_SCL VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SC_CAB_LOTE IS 'CABECERA DE LOTES'
/
COMMENT ON COLUMN  SC_CAB_LOTE.COD_EVENTO IS 'CODIGO DE EVENTO'
/
COMMENT ON COLUMN  SC_CAB_LOTE.ID_LOTE IS 'IDENTIFICACION DEL LOTE PARA UN EVENTO'
/
COMMENT ON COLUMN  SC_CAB_LOTE.PER_CONTABLE IS 'PERIODO CONTABLE'
/
COMMENT ON COLUMN  SC_CAB_LOTE.NOM_USUARIO_LOTE IS 'USUARIO QUE GENERA EL LOTE'
/
COMMENT ON COLUMN  SC_CAB_LOTE.FEC_LOTE IS 'FECHA DE EMISION DEL LOTE'
/
COMMENT ON COLUMN  SC_CAB_LOTE.NUM_LOTE IS 'NUMERO (UNICO) DE LOTE'
/
COMMENT ON COLUMN  SC_CAB_LOTE.NOM_USUARIO_CONT IS 'USUARIO QUE GENERA ASIENTO CONTABLE'
/
COMMENT ON COLUMN  SC_CAB_LOTE.FEC_CONT IS 'FECHA E GENERACION DE ASIENTO CONTABLE'
/
COMMENT ON COLUMN  SC_CAB_LOTE.NOM_USUARIO_OK IS 'USUARIO QUE CONFIRMA CONTABILIZACION OK'
/
COMMENT ON COLUMN  SC_CAB_LOTE.FEC_OK IS 'FECHA DE CONFIRMACION DE CONTABILIZACION OK'
/
COMMENT ON COLUMN  SC_CAB_LOTE.COD_OPERADORA_SCL IS 'CODIGO DE OPERADORA'
/
