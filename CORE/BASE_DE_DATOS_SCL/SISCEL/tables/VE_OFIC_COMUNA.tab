CREATE TABLE VE_OFIC_COMUNA(
    COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  COD_REGION VARCHAR2(3) NOT NULL 
 ,  COD_PROVINCIA VARCHAR2(5) NOT NULL 
 ,  COD_COMUNA VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_OFIC_COMUNA IS 'Asignacion de Comunas a Oficinas'
/
COMMENT ON COLUMN  VE_OFIC_COMUNA.COD_OFICINA IS 'Codigo de Oficina'
/
COMMENT ON COLUMN  VE_OFIC_COMUNA.COD_REGION IS 'Codigo de Region'
/
COMMENT ON COLUMN  VE_OFIC_COMUNA.COD_PROVINCIA IS 'Codigo de Provincia'
/
COMMENT ON COLUMN  VE_OFIC_COMUNA.COD_COMUNA IS 'Codigo de Comuna'
/
