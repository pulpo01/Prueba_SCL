CREATE TABLE AL_LOCALIZA(
    NUM_SEC_LOCA NUMBER(10,0) NOT NULL 
 ,  COD_BODEGA NUMBER(6,0) NOT NULL 
 ,  COD_ZONA VARCHAR2(5) NOT NULL 
 ,  COD_RACK VARCHAR2(5) NOT NULL 
 ,  NUM_FILA NUMBER(3,0) NOT NULL 
 ,  NUM_COLUMNA NUMBER(3,0) NOT NULL 
 ,  FEC_CREA DATE NULL 
 ,  COD_USUA VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_LOCALIZA IS 'Tabla De Localizaciones Por Bodega.'
/
COMMENT ON COLUMN  AL_LOCALIZA.NUM_SEC_LOCA IS 'Identificador de la Localizacion..'
/
COMMENT ON COLUMN  AL_LOCALIZA.COD_BODEGA IS 'Codigo de Bodega..'
/
COMMENT ON COLUMN  AL_LOCALIZA.COD_ZONA IS 'Codigo de Zona..'
/
COMMENT ON COLUMN  AL_LOCALIZA.COD_RACK IS 'Codigo de Rack..'
/
COMMENT ON COLUMN  AL_LOCALIZA.NUM_FILA IS 'Numero de Fila.'
/
COMMENT ON COLUMN  AL_LOCALIZA.NUM_COLUMNA IS 'Numero de Columna.'
/
COMMENT ON COLUMN  AL_LOCALIZA.FEC_CREA IS 'Fecha de Creacion.'
/
COMMENT ON COLUMN  AL_LOCALIZA.COD_USUA IS 'Login de Usuario de Creacion.'
/
