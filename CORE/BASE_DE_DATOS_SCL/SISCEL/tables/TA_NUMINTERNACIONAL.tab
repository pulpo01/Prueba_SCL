CREATE TABLE TA_NUMINTERNACIONAL(
    COD_PAIS VARCHAR2(3) NOT NULL 
 ,  IND_ORDEN NUMBER(3,0) NOT NULL 
 ,  NUM_TDESDE VARCHAR2(15) NOT NULL 
 ,  NUM_THASTA VARCHAR2(15) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(40) DEFAULT 'TARIFICA' NOT NULL 
 ,  FEC_ULTIMAMOD DATE DEFAULT '01-jan-99' NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_NUMINTERNACIONAL IS 'Numeracion internacional'
/
COMMENT ON COLUMN  TA_NUMINTERNACIONAL.COD_PAIS IS 'Csdigo de Pais'
/
COMMENT ON COLUMN  TA_NUMINTERNACIONAL.IND_ORDEN IS 'Indice de Orden'
/
COMMENT ON COLUMN  TA_NUMINTERNACIONAL.NUM_TDESDE IS 'Numeracisn Desde'
/
COMMENT ON COLUMN  TA_NUMINTERNACIONAL.NUM_THASTA IS 'Numeracisn Hasta'
/
COMMENT ON COLUMN  TA_NUMINTERNACIONAL.NOM_USUARIO IS 'Almacena Nombre Usuario'
/
COMMENT ON COLUMN  TA_NUMINTERNACIONAL.FEC_ULTIMAMOD IS 'Fecha Ultima Modificacion'
/
