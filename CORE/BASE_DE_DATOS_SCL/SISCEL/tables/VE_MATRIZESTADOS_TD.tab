CREATE TABLE VE_MATRIZESTADOS_TD(
    COD_PROGRAMA VARCHAR2(3) NOT NULL 
 ,  IND_OFITER VARCHAR2(1) NOT NULL 
 ,  COD_PROCESO VARCHAR2(7) NOT NULL 
 ,  FORMULARIO VARCHAR2(50) NOT NULL 
 ,  COD_ESTADOORIGEN VARCHAR2(2) NOT NULL 
 ,  COD_ESTADODESTINO VARCHAR2(2) NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  IND_MODIF NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_MATRIZESTADOS_TD IS 'MATRIZ DE ESTADOS DE VENTA'
/
COMMENT ON COLUMN  VE_MATRIZESTADOS_TD.COD_PROGRAMA IS 'PROGRAMA A EJECUTAR'
/
COMMENT ON COLUMN  VE_MATRIZESTADOS_TD.IND_OFITER IS 'TIPO DE VENTA (O: OFICINA, T: TERRENO)'
/
COMMENT ON COLUMN  VE_MATRIZESTADOS_TD.COD_PROCESO IS 'PROCESO A EJECUTAR'
/
COMMENT ON COLUMN  VE_MATRIZESTADOS_TD.FORMULARIO IS 'FORMULARIO QUE EJECUTA'
/
COMMENT ON COLUMN  VE_MATRIZESTADOS_TD.COD_ESTADOORIGEN IS 'ESTADO ORIGEN'
/
COMMENT ON COLUMN  VE_MATRIZESTADOS_TD.COD_ESTADODESTINO IS 'ESTADO DESTINO'
/
COMMENT ON COLUMN  VE_MATRIZESTADOS_TD.FEC_DESDE IS 'FECHA DE INICIO VIGENCIA'
/
COMMENT ON COLUMN  VE_MATRIZESTADOS_TD.FEC_HASTA IS 'FECHA FIN VIGENCIA'
/
COMMENT ON COLUMN  VE_MATRIZESTADOS_TD.NOM_USUARIO IS 'USUARIO QUE REALIZA MODIFICACION'
/
COMMENT ON COLUMN  VE_MATRIZESTADOS_TD.FEC_ULTMOD IS 'FECHA ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  VE_MATRIZESTADOS_TD.IND_MODIF IS 'INDICA SI ES EL CAUSANTE DE LA MODIFICACION DE MATRIZ'
/
