CREATE TABLE AL_CAB_ES_EXTRAS(
    NUM_EXTRA NUMBER(9,0) NOT NULL 
 ,  COD_BODEGA NUMBER(6,0) NULL 
 ,  FEC_EXTRA DATE NOT NULL 
 ,  COD_MOTIVO NUMBER(2,0) NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) NOT NULL 
 ,  DES_OBSERVACION VARCHAR2(100) NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  IND_ENTSAL VARCHAR2(1) NOT NULL 
 ,  TIP_DOCU NUMBER(2,0) NULL 
 ,  NUM_DOCU NUMBER(10,0) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_CAB_ES_EXTRAS IS 'Cabecera de Entradas/Salidas Extras'
/
COMMENT ON COLUMN  AL_CAB_ES_EXTRAS.NUM_EXTRA IS 'Nzmero de Entrada/Salida Extra'
/
COMMENT ON COLUMN  AL_CAB_ES_EXTRAS.COD_BODEGA IS 'Bsdega de la que se efectua la E/S Extra'
/
COMMENT ON COLUMN  AL_CAB_ES_EXTRAS.FEC_EXTRA IS 'Fecha de Entrada/Salida Extra'
/
COMMENT ON COLUMN  AL_CAB_ES_EXTRAS.COD_MOTIVO IS 'Motivo de Entrada/Salida Extra'
/
COMMENT ON COLUMN  AL_CAB_ES_EXTRAS.COD_ESTADO IS 'Estado en el que se encuentra la entrada/salida extra'
/
COMMENT ON COLUMN  AL_CAB_ES_EXTRAS.DES_OBSERVACION IS 'Observaciones o Comentarios'
/
COMMENT ON COLUMN  AL_CAB_ES_EXTRAS.NOM_USUARIO IS 'Usuario que realizs la Entrada/Salida Extra'
/
COMMENT ON COLUMN  AL_CAB_ES_EXTRAS.IND_ENTSAL IS 'Indicativo de Entrada'
/
COMMENT ON COLUMN  AL_CAB_ES_EXTRAS.TIP_DOCU IS 'Tipo de Documento.'
/
COMMENT ON COLUMN  AL_CAB_ES_EXTRAS.NUM_DOCU IS 'Numero de Documento.'
/
