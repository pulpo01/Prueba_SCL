CREATE TABLE FA_CABFORCOB(
    COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  IND_ORDEN NUMBER(12,0) NOT NULL 
 ,  NUM_REGISTRO NUMBER(11,0) NOT NULL 
 ,  NOM_FICHERO VARCHAR2(40) NOT NULL 
 ,  ACUM_TOTAL NUMBER(17,4) NOT NULL 
 ,  FEC_ENVIO DATE NOT NULL 
 ,  TIP_REG NUMBER(1,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_CABFORCOB IS 'Tabla de cabecera de cargos por carrier- Retrofitted'
/
COMMENT ON COLUMN  FA_CABFORCOB.COD_OPERADOR IS 'Codigo de operador o carrier'
/
COMMENT ON COLUMN  FA_CABFORCOB.IND_ORDEN IS 'Orden para el mismo carrier'
/
COMMENT ON COLUMN  FA_CABFORCOB.NUM_REGISTRO IS 'Cantidad de registros incorporados en la cinta'
/
COMMENT ON COLUMN  FA_CABFORCOB.NOM_FICHERO IS 'Nombre de Fichero'
/
COMMENT ON COLUMN  FA_CABFORCOB.ACUM_TOTAL IS 'Valor Total, Valor Neto + Iva'
/
COMMENT ON COLUMN  FA_CABFORCOB.FEC_ENVIO IS 'Fecha de Envio de Cinta'
/
COMMENT ON COLUMN  FA_CABFORCOB.TIP_REG IS 'Tipo de Registro 0=Caratula'
/
