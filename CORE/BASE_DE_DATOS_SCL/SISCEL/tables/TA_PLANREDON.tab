CREATE TABLE TA_PLANREDON(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_PLANTARIF VARCHAR2(3) NOT NULL 
 ,  COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  CLS_TARIFA VARCHAR2(3) NOT NULL 
 ,  TIP_REDONDEO VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_PLANREDON IS 'Relacion Tipos de Redondeos a Planes tarifarios y Clases Tarifas'
/
COMMENT ON COLUMN  TA_PLANREDON.COD_PRODUCTO IS 'Csdigo de Producto'
/
COMMENT ON COLUMN  TA_PLANREDON.COD_PLANTARIF IS 'Codigo de Plan Tarifario'
/
COMMENT ON COLUMN  TA_PLANREDON.COD_OPERADOR IS 'Codigo de Operador'
/
COMMENT ON COLUMN  TA_PLANREDON.CLS_TARIFA IS 'Clase de Tarifa'
/
COMMENT ON COLUMN  TA_PLANREDON.TIP_REDONDEO IS 'Tipo de Redondeo'
/
