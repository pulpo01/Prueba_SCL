CREATE TABLE CM_CONCEPFACT_BONOCARTERA_TD(
    COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) NOT NULL 
 ,  COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  COD_CONCEPTOFACT NUMBER(4,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  TIP_CONCEPTO VARCHAR2(10) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_CONCEPFACT_BONOCARTERA_TD IS 'CONCEPTOS DE FACTURACION PARA EL CALCULO DE COMISION  POR BONO CARTERA'
/
COMMENT ON COLUMN  CM_CONCEPFACT_BONOCARTERA_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
COMMENT ON COLUMN  CM_CONCEPFACT_BONOCARTERA_TD.COD_PLANCOMIS IS 'CODIGO DEL PLAN DE COMISIONES '
/
COMMENT ON COLUMN  CM_CONCEPFACT_BONOCARTERA_TD.COD_CONCEPTO IS 'CODIGO DEL CONCEPTO'
/
COMMENT ON COLUMN  CM_CONCEPFACT_BONOCARTERA_TD.COD_CONCEPTOFACT IS 'CONCEPTO DE FACTURACION '
/
COMMENT ON COLUMN  CM_CONCEPFACT_BONOCARTERA_TD.FEC_ULTMOD IS 'FECHA ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  CM_CONCEPFACT_BONOCARTERA_TD.NOM_USUARIO IS 'USUARIO ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  CM_CONCEPFACT_BONOCARTERA_TD.TIP_CONCEPTO IS 'TIPO DE CONCEPTO DE FACTURACION'
/
