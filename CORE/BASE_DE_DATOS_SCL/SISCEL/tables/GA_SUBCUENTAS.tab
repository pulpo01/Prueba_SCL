CREATE TABLE GA_SUBCUENTAS(
    COD_SUBCUENTA VARCHAR2(15) NOT NULL 
 ,  COD_CUENTA NUMBER(8,0) NOT NULL 
 ,  DES_SUBCUENTA VARCHAR2(30) NOT NULL 
 ,  COD_DIRECCION NUMBER(20,0) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_SUBCUENTAS IS 'Subcuentas de Clientes'
/
COMMENT ON COLUMN  GA_SUBCUENTAS.COD_SUBCUENTA IS 'Codigo de Subcuenta'
/
COMMENT ON COLUMN  GA_SUBCUENTAS.COD_CUENTA IS 'Codigo de cuenta'
/
COMMENT ON COLUMN  GA_SUBCUENTAS.DES_SUBCUENTA IS 'Descripcion de Subcuenta'
/
COMMENT ON COLUMN  GA_SUBCUENTAS.COD_DIRECCION IS 'Codigo de Direccion'
/
COMMENT ON COLUMN  GA_SUBCUENTAS.FEC_ALTA IS 'Fecha de Alta'
/
