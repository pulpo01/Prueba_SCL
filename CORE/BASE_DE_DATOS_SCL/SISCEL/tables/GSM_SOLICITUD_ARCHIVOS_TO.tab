CREATE TABLE GSM_SOLICITUD_ARCHIVOS_TO(
    COD_ARCHIVO VARCHAR2(6) NOT NULL 
 ,  NUM_VERSION NUMBER(10,0) NOT NULL 
 ,  NUM_SEQ_SOLICITUD NUMBER(10,0) NOT NULL 
 ,  NOM_ARCHIVO VARCHAR2(50) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_SOLICITUD_ARCHIVOS_TO IS 'Tabla de relacion de Solicitud y sus archivos.'
/
COMMENT ON COLUMN  GSM_SOLICITUD_ARCHIVOS_TO.COD_ARCHIVO IS 'Codigo del Archivo.'
/
COMMENT ON COLUMN  GSM_SOLICITUD_ARCHIVOS_TO.NUM_VERSION IS 'Version del Archivo.'
/
COMMENT ON COLUMN  GSM_SOLICITUD_ARCHIVOS_TO.NUM_SEQ_SOLICITUD IS 'Numero de Solicitud.'
/
COMMENT ON COLUMN  GSM_SOLICITUD_ARCHIVOS_TO.NOM_ARCHIVO IS 'Nombre fisico del archivo.'
/
