CREATE TABLE GE_DATCLIFACTURA_TO (  
   COD_CLIENTE                   NUMBER(8)             NOT NULL
 , COD_TIPIDENT                  VARCHAR2(2)               NULL
 , NUM_IDENT                     VARCHAR2(20)              NULL
 , NOMBRE                        VARCHAR2(50)          NOT NULL
 , NOM_APELLIDO1                 VARCHAR2(30)          NOT NULL
 , NOM_APELLIDO2                 VARCHAR2(30)              NULL
 , TIPO_FACTURA                  VARCHAR2(1)           NOT NULL
 , COD_TIPDOCUM                  NUMBER(2)                 NULL
 , NUM_VENTA                     NUMBER(8)                 NULL
 , FEC_DESDE                     DATE                  NOT NULL
 , FEC_HASTA                     DATE                      NULL
 , NOM_USUARORA                  VARCHAR2(30)          NOT NULL
 , FEC_MODIFICACION              DATE                  NOT NULL
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  GE_DATCLIFACTURA_TO                      IS '[ GE_CLIFAC ] Terceros asociados a clientesTerceros asociados a clientes'
/

COMMENT ON COLUMN GE_DATCLIFACTURA_TO.COD_CLIENTE                    IS 'codigo cliente SCL'
/
COMMENT ON COLUMN GE_DATCLIFACTURA_TO.COD_TIPIDENT                   IS 'tipo identificación de tercero'
/
COMMENT ON COLUMN GE_DATCLIFACTURA_TO.NUM_IDENT                      IS 'num_ident de tercero'
/
COMMENT ON COLUMN GE_DATCLIFACTURA_TO.NOMBRE                         IS 'nombre de tercero'
/
COMMENT ON COLUMN GE_DATCLIFACTURA_TO.NOM_APELLIDO1                  IS 'primer apellido de tercero'
/
COMMENT ON COLUMN GE_DATCLIFACTURA_TO.NOM_APELLIDO2                  IS 'segundo apellido de tercero'
/
COMMENT ON COLUMN GE_DATCLIFACTURA_TO.TIPO_FACTURA                   IS 'tipo factura generada (I: inmediata) (C:ciclo)'
/
COMMENT ON COLUMN GE_DATCLIFACTURA_TO.COD_TIPDOCUM                   IS 'tipo de documento: factura o boleta'
/
COMMENT ON COLUMN GE_DATCLIFACTURA_TO.NUM_VENTA                      IS 'numero de venta'
/
COMMENT ON COLUMN GE_DATCLIFACTURA_TO.FEC_DESDE                      IS 'fecha de ingreso'
/
COMMENT ON COLUMN GE_DATCLIFACTURA_TO.FEC_HASTA                      IS 'fecha fin de vigencia'
/
COMMENT ON COLUMN GE_DATCLIFACTURA_TO.NOM_USUARORA                   IS 'usuario de modificación'
/
COMMENT ON COLUMN GE_DATCLIFACTURA_TO.FEC_MODIFICACION               IS 'fecha de modificación'
/

