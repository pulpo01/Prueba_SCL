CREATE TABLE GE_INTERLOCUTORES(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NOM_INTER VARCHAR2(50) NOT NULL 
 ,  PASSWORD VARCHAR2(10) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NUM_IDENT VARCHAR2(20) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_INTERLOCUTORES IS 'Interlocutores.'
/
COMMENT ON COLUMN  GE_INTERLOCUTORES.COD_CLIENTE IS 'Codigo del Cliente..'
/
COMMENT ON COLUMN  GE_INTERLOCUTORES.NOM_INTER IS 'Nombre, apellidos del Interlocutor.'
/
COMMENT ON COLUMN  GE_INTERLOCUTORES.PASSWORD IS 'Password del Interlocutor.'
/
COMMENT ON COLUMN  GE_INTERLOCUTORES.NOM_USUARIO IS 'Identificador unico del usuario.'
/
COMMENT ON COLUMN  GE_INTERLOCUTORES.FEC_ULTMOD IS 'Fecha de Ultima Modificacion..'
/
