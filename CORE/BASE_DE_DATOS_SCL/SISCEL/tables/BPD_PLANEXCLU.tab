CREATE TABLE BPD_PLANEXCLU(
    COD_PLANA VARCHAR2(5) NOT NULL 
 ,  COD_PLANB VARCHAR2(5) NOT NULL 
 ,  PLAN_APLICAR VARCHAR2(5) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_DESDEAPLI DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  BPD_PLANEXCLU IS 'Bpd_Planexclu.'
/
COMMENT ON COLUMN  BPD_PLANEXCLU.COD_PLANA IS 'Codigo de plan de beneficio A.'
/
COMMENT ON COLUMN  BPD_PLANEXCLU.COD_PLANB IS 'Codigo de plan de beneficio B.'
/
COMMENT ON COLUMN  BPD_PLANEXCLU.PLAN_APLICAR IS 'Plan que se debe aplicar.'
/
COMMENT ON COLUMN  BPD_PLANEXCLU.FEC_ULTMOD IS 'Fecha ultima actualizacion.'
/
COMMENT ON COLUMN  BPD_PLANEXCLU.NOM_USUARIO IS 'Nombre usuario responsable.'
/
COMMENT ON COLUMN  BPD_PLANEXCLU.FEC_DESDEAPLI IS 'Desde que fecha se puede empezar a aplicar el Beneficio'
/
