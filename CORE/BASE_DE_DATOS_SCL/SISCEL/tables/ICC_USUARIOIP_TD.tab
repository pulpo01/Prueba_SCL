CREATE TABLE ICC_USUARIOIP_TD(
    IP VARCHAR2(15) NOT NULL 
 ,  USUARIO VARCHAR2(10) NOT NULL 
 ,  STATUS NUMBER(1,0) NULL 
 ,  FEC_INGRESO DATE NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICC_USUARIOIP_TD IS 'Permite Validar IP y User asociada'
/
COMMENT ON COLUMN  ICC_USUARIOIP_TD.IP IS 'Direccion IP'
/
COMMENT ON COLUMN  ICC_USUARIOIP_TD.USUARIO IS 'Usuario asociado a la IP'
/
COMMENT ON COLUMN  ICC_USUARIOIP_TD.STATUS IS 'Estado de la Direccion IP 0-Inactivo 1-Activo'
/
COMMENT ON COLUMN  ICC_USUARIOIP_TD.FEC_INGRESO IS 'Fecha de Ingreso de la IP'
/
