CREATE TABLE GE_OPERADORA_SCL(
    COD_OPERADORA_SCL VARCHAR2(5) NOT NULL 
 ,  DES_OPERADORA_SCL VARCHAR2(50) NOT NULL 
 ,  COD_DIRECCION NUMBER(20,0) NULL 
 ,  IND_OPER_PRINCIPAL NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  COD_BODEGANODO NUMBER(6,0) NULL 
 ,  COD_EMPRESA VARCHAR2(2) NULL 
 ,  COD_PROVEEDOR NUMBER NULL 
 ,  COD_CLIENTE NUMBER(8,0) NULL 
 ,  IND_VTASINFOLIO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_OPERADORA_SCL IS 'Operadoras De Scl Siscel.'
/
COMMENT ON COLUMN  GE_OPERADORA_SCL.COD_OPERADORA_SCL IS 'Codigo de Operadora de SCL Siscel.'
/
COMMENT ON COLUMN  GE_OPERADORA_SCL.DES_OPERADORA_SCL IS 'Nombre de Operadora de SCL Siscel.'
/
COMMENT ON COLUMN  GE_OPERADORA_SCL.COD_DIRECCION IS 'Codigo de Direccion de la Operadora.'
/
COMMENT ON COLUMN  GE_OPERADORA_SCL.IND_OPER_PRINCIPAL IS 'Indicador de operadora principal.'
/
COMMENT ON COLUMN  GE_OPERADORA_SCL.COD_BODEGANODO IS 'Codigo de bodega nodo.'
/
COMMENT ON COLUMN  GE_OPERADORA_SCL.COD_EMPRESA IS 'Codigo de empresa.'
/
COMMENT ON COLUMN  GE_OPERADORA_SCL.COD_PROVEEDOR IS 'Codigo de proveedor.'
/
COMMENT ON COLUMN  GE_OPERADORA_SCL.COD_CLIENTE IS 'Codigo de cliente.'
/
COMMENT ON COLUMN  GE_OPERADORA_SCL.IND_VTASINFOLIO IS 'Indicador de continuacion de ventas sin folio- 0 No continua ; 1 continua'
/
