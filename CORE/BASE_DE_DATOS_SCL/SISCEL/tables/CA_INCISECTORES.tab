CREATE TABLE CA_INCISECTORES(
    NUM_INCIDENCIA NUMBER(9,0) NOT NULL 
 ,  FEC_DERIVACION DATE NOT NULL 
 ,  COD_SECTOR NUMBER(2,0) NOT NULL 
 ,  NOM_RESPONSABLE VARCHAR2(40) NOT NULL 
 ,  USU_RECEPTOR VARCHAR2(30) NULL 
 ,  COD_ESTADO VARCHAR2(2) NULL 
 ,  FEC_SALIDA DATE NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_INCISECTORES IS 'Incidencias - Sectores'
/
COMMENT ON COLUMN  CA_INCISECTORES.NUM_INCIDENCIA IS 'Numero Incidencia'
/
COMMENT ON COLUMN  CA_INCISECTORES.FEC_DERIVACION IS 'Fecha Derivacion'
/
COMMENT ON COLUMN  CA_INCISECTORES.COD_SECTOR IS 'Codigo Sector'
/
COMMENT ON COLUMN  CA_INCISECTORES.NOM_RESPONSABLE IS 'Responsable Sector'
/
COMMENT ON COLUMN  CA_INCISECTORES.USU_RECEPTOR IS 'Usuario Receptor'
/
COMMENT ON COLUMN  CA_INCISECTORES.COD_ESTADO IS 'Estado en que deja la Incidencia'
/
COMMENT ON COLUMN  CA_INCISECTORES.FEC_SALIDA IS 'Fecha Salida'
/
