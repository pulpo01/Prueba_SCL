CREATE TABLE ERD_CAMPOS(
    COD_CAMPO NUMBER(6,0) NOT NULL 
 ,  NOM_CAMPO VARCHAR2(30) NOT NULL 
 ,  NOM_USUARIO_MODI VARCHAR2(30) NOT NULL 
 ,  FEC_MODI_H DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERD_CAMPOS IS 'Definicion De Campos De Informacion.'
/
COMMENT ON COLUMN  ERD_CAMPOS.COD_CAMPO IS 'Identificador unico para el campo de informacion.'
/
COMMENT ON COLUMN  ERD_CAMPOS.NOM_CAMPO IS 'Nombre del campo de informacion.'
/
COMMENT ON COLUMN  ERD_CAMPOS.NOM_USUARIO_MODI IS 'Nombre del usuario que creo/actualizo este registro.'
/
COMMENT ON COLUMN  ERD_CAMPOS.FEC_MODI_H IS 'fecha de creacion/modificacion de este registro.'
/
