CREATE TABLE CA_BUZONES(
    NUM_LLAMADA NUMBER(9,0) NOT NULL 
 ,  FEC_BUZON DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  TIP_INTER NUMBER(2,0) NULL 
 ,  COD_INTERLOCUTOR NUMBER(10,0) NULL 
 ,  TIP_INCIDENCIA NUMBER(3,0) NULL 
 ,  COD_SECTOR NUMBER(2,0) NULL 
 ,  PER_CONTACTO VARCHAR2(40) NULL 
 ,  IND_URGENTE NUMBER(1,0) NULL 
 ,  COMENTARIO VARCHAR2(500) NULL 
 ,  IDEN_SESION NUMBER(1,0) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_BUZONES IS 'Buzon particular del operador para incidencias'
/
COMMENT ON COLUMN  CA_BUZONES.NUM_LLAMADA IS 'Numero de llamada'
/
COMMENT ON COLUMN  CA_BUZONES.FEC_BUZON IS 'Fecha y Hora grabacion'
/
COMMENT ON COLUMN  CA_BUZONES.NOM_USUARIO IS 'Usuario Oracle'
/
COMMENT ON COLUMN  CA_BUZONES.TIP_INTER IS 'Tipo de Interlocutor'
/
COMMENT ON COLUMN  CA_BUZONES.COD_INTERLOCUTOR IS 'Codigo de interlocutor'
/
COMMENT ON COLUMN  CA_BUZONES.TIP_INCIDENCIA IS 'Tipo de incidencia'
/
COMMENT ON COLUMN  CA_BUZONES.COD_SECTOR IS 'Codigo de sector'
/
COMMENT ON COLUMN  CA_BUZONES.PER_CONTACTO IS 'Persona de Contacto'
/
COMMENT ON COLUMN  CA_BUZONES.IND_URGENTE IS 'Indicativo de Urgencia'
/
COMMENT ON COLUMN  CA_BUZONES.COMENTARIO IS 'Comentario'
/
