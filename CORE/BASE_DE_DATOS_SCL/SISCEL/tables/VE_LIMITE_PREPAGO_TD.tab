CREATE TABLE VE_LIMITE_PREPAGO_TD(
    COD_LIMITE NUMBER(6,0) NOT NULL 
 ,  COD_RELACION NUMBER(1,0) NOT NULL 
 ,  COD_VALOR VARCHAR2(5) NULL 
 ,  VALOR_LIMITE NUMBER(6,0) NOT NULL 
 ,  FECHA_VIGENCIA_DESDE DATE NOT NULL 
 ,  FECHA_VIGENCIA_HASTA DATE NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_LIMITE_PREPAGO_TD IS '[ LIPR ] Limite de lineas de prepago por calidad de cliente o tipo de identificacion'
/
COMMENT ON COLUMN  VE_LIMITE_PREPAGO_TD.COD_LIMITE IS 'Codigo limite.'
/
COMMENT ON COLUMN  VE_LIMITE_PREPAGO_TD.COD_RELACION IS '1: Calidad de Cliente, 2: Tipo de Identificacion, 3:Valor por defecto.'
/
COMMENT ON COLUMN  VE_LIMITE_PREPAGO_TD.COD_VALOR IS 'Codigo calidad de cliente o tipo de identificacion.'
/
COMMENT ON COLUMN  VE_LIMITE_PREPAGO_TD.VALOR_LIMITE IS 'Valor de limite de lineas.'
/
COMMENT ON COLUMN  VE_LIMITE_PREPAGO_TD.FECHA_VIGENCIA_DESDE IS 'Fecha de entrada en vigencia del limite de lineas.'
/
COMMENT ON COLUMN  VE_LIMITE_PREPAGO_TD.FECHA_VIGENCIA_HASTA IS 'Fecha de fin de vigencia del limite de lineas.'
/
