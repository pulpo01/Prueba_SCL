CREATE TABLE CO_TRANSACINT(
    NUM_TRANSACCION NUMBER(9,0) NOT NULL 
 ,  COD_RETORNO NUMBER(2,0) NOT NULL 
 ,  DES_CADENA VARCHAR2(255) NULL 
 ,  MTO_INTERESES NUMBER(14,4) NULL 
 ,  FAC_COBRO NUMBER(8,5) NULL 
 ,  NUM_DIAS NUMBER(6,0) NULL 
 ,  COD_CONCFACT NUMBER(4,0) DEFAULT -1 NOT NULL 
 ,  IMP_CARGO NUMBER(14,4) DEFAULT 0 NULL 
 ) TABLESPACE    TS_RECYCOB_LDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_TRANSACINT IS 'Registro de Error de la Tansaccion'
/
COMMENT ON COLUMN  CO_TRANSACINT.NUM_TRANSACCION IS 'Numero  de  la Transaccion'
/
COMMENT ON COLUMN  CO_TRANSACINT.COD_RETORNO IS 'Csdigo Retorno'
/
COMMENT ON COLUMN  CO_TRANSACINT.DES_CADENA IS 'Descripcisn Cadena'
/
COMMENT ON COLUMN  CO_TRANSACINT.MTO_INTERESES IS 'Monto interes'
/
COMMENT ON COLUMN  CO_TRANSACINT.FAC_COBRO IS 'Factor de cobro por mora'
/
COMMENT ON COLUMN  CO_TRANSACINT.NUM_DIAS IS 'Cantidad de dias de mora'
/
COMMENT ON COLUMN  CO_TRANSACINT.COD_CONCFACT IS 'Contiene el concepto de facturacion de mora para generar el cargo'
/
COMMENT ON COLUMN  CO_TRANSACINT.IMP_CARGO IS 'Importe de la deuda'
/
