CREATE TABLE VE_DTOLLAM(
    COD_DTOLLAM NUMBER(6,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  DES_DTOLLAM VARCHAR2(30) NOT NULL 
 ,  COD_MATRICFRANJA VARCHAR2(2000) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_MINVIGENCIA DATE NOT NULL 
 ,  FEC_MAXVIGENCIA DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_DTOLLAM IS 'Plan de Descuento de Llamadas'
/
COMMENT ON COLUMN  VE_DTOLLAM.COD_DTOLLAM IS 'Codigo Plan Descuento Llamadas'
/
COMMENT ON COLUMN  VE_DTOLLAM.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  VE_DTOLLAM.DES_DTOLLAM IS 'Descripcion Plan Descuento Llamadas'
/
COMMENT ON COLUMN  VE_DTOLLAM.COD_MATRICFRANJA IS 'Matricula de franjas del Plan'
/
COMMENT ON COLUMN  VE_DTOLLAM.NOM_USUARIO IS 'Usuario que crea el Plan D.Ll.'
/
COMMENT ON COLUMN  VE_DTOLLAM.FEC_MINVIGENCIA IS 'Fecha Inicio Vigencia'
/
COMMENT ON COLUMN  VE_DTOLLAM.FEC_MAXVIGENCIA IS 'Fecha Fin Vigencia'
/
