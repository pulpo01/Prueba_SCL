CREATE TABLE CMD_BLOQUES(
    COD_BLOQUE VARCHAR2(15) NOT NULL 
 ,  DES_BLOQUE VARCHAR2(50) NOT NULL 
 ,  COD_LINEA_PROC CHAR(2) NOT NULL 
 ,  ORD_BLOQUE NUMBER(2,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMD_BLOQUES IS 'Definicion De Bloques Para Lineas De Proceso De Comisiones.'
/
COMMENT ON COLUMN  CMD_BLOQUES.COD_BLOQUE IS 'Codigo de Bloque de procesos.'
/
COMMENT ON COLUMN  CMD_BLOQUES.DES_BLOQUE IS 'Descripcion de Bloque.'
/
COMMENT ON COLUMN  CMD_BLOQUES.COD_LINEA_PROC IS 'Linea de proceso a la que pertenece el bloque.'
/
COMMENT ON COLUMN  CMD_BLOQUES.ORD_BLOQUE IS 'Orden de ejecucion del Bloque.'
/
COMMENT ON COLUMN  CMD_BLOQUES.FEC_ULTMOD IS 'Fecha de ultima modificacion del registro..'
/
COMMENT ON COLUMN  CMD_BLOQUES.NOM_USUARIO IS 'Usuario que hizo la insercion del registro en la tabla..'
/
