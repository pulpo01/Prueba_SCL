CREATE TABLE VE_CASOS_MIGRACION_TD(
    COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  NOM_VENDEDOR VARCHAR2(40) NOT NULL 
 ,  DES_MOTIVO VARCHAR2(256) NOT NULL 
 ,  DES_SOLUCION VARCHAR2(256) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CASOS_MIGRACION_TD IS 'CASOS DE MIGRACION MASIVA CON PROBLEMAS'
/
COMMENT ON COLUMN  VE_CASOS_MIGRACION_TD.COD_VENDEDOR IS 'CODIGO AGENTE DE VENTAS O VENDEDOR'
/
COMMENT ON COLUMN  VE_CASOS_MIGRACION_TD.NOM_VENDEDOR IS 'NOMBRE AGENTE DE VENTAS'
/
COMMENT ON COLUMN  VE_CASOS_MIGRACION_TD.DES_MOTIVO IS 'MOTIVO DE FALLA'
/
COMMENT ON COLUMN  VE_CASOS_MIGRACION_TD.DES_SOLUCION IS 'SOLUCION POSIBLE'
/
