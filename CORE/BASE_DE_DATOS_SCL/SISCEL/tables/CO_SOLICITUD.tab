CREATE TABLE CO_SOLICITUD(
    NUM_SOLICITUD NUMBER(8,0) NOT NULL 
 ,  TIPO_SOLICITUD VARCHAR2(5) NOT NULL 
 ,  FECHA_SOLICITUD DATE NOT NULL 
 ,  ESTADO_SOLICITUD VARCHAR2(5) NOT NULL 
 ,  NUM_IDENT VARCHAR2(20) NOT NULL 
 ,  UNIDAD_PROPIETARIA VARCHAR2(3) NOT NULL 
 ,  OBSERVACIONES VARCHAR2(40) NOT NULL 
 ,  COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(20) NOT NULL 
 ,  FEC_ULT_MOVIMIENTO DATE NOT NULL 
 ,  COD_TIPIDENT VARCHAR2(2) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_SOLICITUD IS 'Registro De Todas Las Solicitudes .'
/
COMMENT ON COLUMN  CO_SOLICITUD.NUM_SOLICITUD IS 'Numero de la Solicitud.'
/
COMMENT ON COLUMN  CO_SOLICITUD.TIPO_SOLICITUD IS 'Tipo de Solicitud.'
/
COMMENT ON COLUMN  CO_SOLICITUD.FECHA_SOLICITUD IS 'Fecha de la Solicitud.'
/
COMMENT ON COLUMN  CO_SOLICITUD.ESTADO_SOLICITUD IS 'Estado de la Solicitud.'
/
COMMENT ON COLUMN  CO_SOLICITUD.NUM_IDENT IS 'Rut de la persona que la Solicita.'
/
COMMENT ON COLUMN  CO_SOLICITUD.UNIDAD_PROPIETARIA IS 'Unidad a la que pertenece la Informacion.'
/
COMMENT ON COLUMN  CO_SOLICITUD.OBSERVACIONES IS 'Observaciones.'
/
COMMENT ON COLUMN  CO_SOLICITUD.COD_OFICINA IS 'Codigo Oficina.'
/
COMMENT ON COLUMN  CO_SOLICITUD.NOM_USUARIO IS 'Identificacion del Usuario.'
/
COMMENT ON COLUMN  CO_SOLICITUD.FEC_ULT_MOVIMIENTO IS 'Fecha Ultimo Movimiento.'
/
COMMENT ON COLUMN  CO_SOLICITUD.COD_TIPIDENT IS 'tipo de identificacion  .'
/
