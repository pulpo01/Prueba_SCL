CREATE TABLE ER_CLIVETADOS(
    NOMBRE_CLIENTE VARCHAR2(40) NOT NULL 
 ,  COD_RESPONSABLE VARCHAR2(12) NULL 
 ,  FECHA_IMPEDIMENTO NUMBER(8,0) NULL 
 ,  CAUSAL_IMPEDIMENTO NUMBER(2,0) NULL 
 ,  OBS_IMPEDIMENTO VARCHAR2(40) NULL 
 ,  FECHA DATE NULL 
 ,  NUM_IDENT_CLIENTE VARCHAR2(20) NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ER_CLIVETADOS IS 'Clientes vetados'
/
COMMENT ON COLUMN  ER_CLIVETADOS.NOMBRE_CLIENTE IS 'Nombre del cliente'
/
COMMENT ON COLUMN  ER_CLIVETADOS.COD_RESPONSABLE IS 'Codigo del operador de riesgo'
/
COMMENT ON COLUMN  ER_CLIVETADOS.FECHA_IMPEDIMENTO IS 'Fecha de impedimento'
/
COMMENT ON COLUMN  ER_CLIVETADOS.CAUSAL_IMPEDIMENTO IS 'Causa del impedimento'
/
COMMENT ON COLUMN  ER_CLIVETADOS.OBS_IMPEDIMENTO IS 'Observaciones al impedimento'
/
COMMENT ON COLUMN  ER_CLIVETADOS.FECHA IS 'Fecha impedimiento de cliente'
/
