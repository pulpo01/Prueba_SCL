CREATE TABLE ERD_ACRED_TIPTARJETAS(
    COD_TIPTARJETA VARCHAR2(3) NOT NULL 
 ,  EDAD_INI NUMBER(3,0) NOT NULL 
 ,  EDAD_FIN NUMBER(3,0) NOT NULL 
 ,  MTO_RENTA NUMBER(14,4) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  ERD_ACRED_TIPTARJETAS.COD_TIPTARJETA IS 'Codigo del Tipo de Tarjeta.'
/
COMMENT ON COLUMN  ERD_ACRED_TIPTARJETAS.EDAD_INI IS 'Edad inicial del Rango para esta acreditacion.'
/
COMMENT ON COLUMN  ERD_ACRED_TIPTARJETAS.EDAD_FIN IS 'Edad final del Rango para esta acreditacion.'
/
COMMENT ON COLUMN  ERD_ACRED_TIPTARJETAS.MTO_RENTA IS 'Monto de Renta asignado para esta acreditacion.'
/
COMMENT ON COLUMN  ERD_ACRED_TIPTARJETAS.FEC_ULTMOD IS 'Fecha de la ultima modificacion.'
/
COMMENT ON COLUMN  ERD_ACRED_TIPTARJETAS.NOM_USUARIO IS 'Nombre usuario que crea o modifica el registro.'
/
