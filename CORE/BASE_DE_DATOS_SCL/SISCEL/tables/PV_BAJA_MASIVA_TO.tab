CREATE TABLE PV_BAJA_MASIVA_TO(
    NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  ID_PROCESO NUMBER(15,0) DEFAULT 0 NOT NULL 
 ,  COD_ESTADO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  FEC_INGRESO DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_SQLCODE VARCHAR2(15) NULL 
 ,  COD_SQLERRM VARCHAR2(1000) NULL 
 ,  DESC_ERR VARCHAR2(200) NULL 
 ,  NOM_TABLA VARCHAR2(50) NULL 
 ,  COD_ACT VARCHAR2(1) NULL 
 ,  NOM_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ,  NUM_OS NUMBER(10,0) NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_BAJA_MASIVA_TO IS '[ PV_BM_TO ] Números de Celular para la Baja Masiva Batch PostpagoNúmeros de Celular para la Baja Masiva Batch Postpago'
/
COMMENT ON COLUMN  PV_BAJA_MASIVA_TO.NUM_CELULAR IS 'Numero de Celular'
/
COMMENT ON COLUMN  PV_BAJA_MASIVA_TO.ID_PROCESO IS 'Id de Proceso'
/
COMMENT ON COLUMN  PV_BAJA_MASIVA_TO.COD_ESTADO IS 'Estado de Proceso 0: ingresado 1:en proceso 2: procesado 3:Correcto 4:error'
/
COMMENT ON COLUMN  PV_BAJA_MASIVA_TO.FEC_INGRESO IS 'Fecha de Proceso'
/
COMMENT ON COLUMN  PV_BAJA_MASIVA_TO.COD_SQLCODE IS 'Error Sql Code'
/
COMMENT ON COLUMN  PV_BAJA_MASIVA_TO.COD_SQLERRM IS 'Error Sql Errm'
/
COMMENT ON COLUMN  PV_BAJA_MASIVA_TO.DESC_ERR IS 'Descripcion de Error'
/
COMMENT ON COLUMN  PV_BAJA_MASIVA_TO.NOM_TABLA IS 'Nombre de tabla donde ocurre el error'
/
COMMENT ON COLUMN  PV_BAJA_MASIVA_TO.COD_ACT IS 'Tipo de actuación sobre la tabla'
/
COMMENT ON COLUMN  PV_BAJA_MASIVA_TO.NOM_USUARIO IS 'Usuario de Proceso'
/
COMMENT ON COLUMN  PV_BAJA_MASIVA_TO.NUM_OS IS 'Identificador Orden de servicio'
/
