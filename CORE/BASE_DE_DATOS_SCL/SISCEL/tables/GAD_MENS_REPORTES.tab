CREATE TABLE GAD_MENS_REPORTES(
    COD_TITULO NUMBER(6,0) NOT NULL 
 ,  NOM_PROGRAMA VARCHAR2(20) NOT NULL 
 ,  NOM_REPORTE VARCHAR2(30) NOT NULL 
 ,  DES_TITULO VARCHAR2(80) NOT NULL 
 ,  TIP_TITULO NUMBER(4,0) NULL 
 ,  IND_ORDEN NUMBER(4,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAD_MENS_REPORTES IS 'Tabla De Parámetros .'
/
COMMENT ON COLUMN  GAD_MENS_REPORTES.COD_TITULO IS 'Código que identifica al título.'
/
COMMENT ON COLUMN  GAD_MENS_REPORTES.NOM_PROGRAMA IS 'Nombre del ejecutable que utiliza el reporte.'
/
COMMENT ON COLUMN  GAD_MENS_REPORTES.NOM_REPORTE IS 'Nombre del reporte.'
/
COMMENT ON COLUMN  GAD_MENS_REPORTES.DES_TITULO IS 'Descripción del título.'
/
COMMENT ON COLUMN  GAD_MENS_REPORTES.TIP_TITULO IS 'Tipo de título (GAD_PARAMETROS).'
/
COMMENT ON COLUMN  GAD_MENS_REPORTES.IND_ORDEN IS 'Orden en que van los títulos en el reporte.'
/
