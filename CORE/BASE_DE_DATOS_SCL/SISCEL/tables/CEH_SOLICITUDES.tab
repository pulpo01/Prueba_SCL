CREATE TABLE CEH_SOLICITUDES(
    FEC_SOLICITUD DATE NOT NULL 
 ,  COD_TIPCLAVE VARCHAR2(5) NOT NULL 
 ,  MTO_CLAVE NUMBER(14,4) NOT NULL 
 ,  CANT_CLAVES NUMBER(6,0) NOT NULL 
 ,  NUM_VIGENCIA NUMBER(6,0) NOT NULL 
 ,  NUM_DIASEXP NUMBER(6,0) NOT NULL 
 ,  NOM_USUARESP1 VARCHAR2(30) NOT NULL 
 ,  NOM_USUARESP2 VARCHAR2(30) NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CEH_SOLICITUDES IS 'Histsrico de solicitudes'
/
COMMENT ON COLUMN  CEH_SOLICITUDES.FEC_SOLICITUD IS 'Fecha de solicitud'
/
COMMENT ON COLUMN  CEH_SOLICITUDES.COD_TIPCLAVE IS 'Codigo identificacion clave'
/
COMMENT ON COLUMN  CEH_SOLICITUDES.MTO_CLAVE IS 'Monto de carga asociado'
/
COMMENT ON COLUMN  CEH_SOLICITUDES.CANT_CLAVES IS 'Cantidad de claves solicitadas'
/
COMMENT ON COLUMN  CEH_SOLICITUDES.NUM_VIGENCIA IS 'Numero de dias de vigencia para solicitud'
/
COMMENT ON COLUMN  CEH_SOLICITUDES.NUM_DIASEXP IS 'Numero de dias, para calculo de fecha expiracion'
/
COMMENT ON COLUMN  CEH_SOLICITUDES.NOM_USUARESP1 IS 'Usuario responsable'
/
COMMENT ON COLUMN  CEH_SOLICITUDES.NOM_USUARESP2 IS 'Usuario responsable'
/
COMMENT ON COLUMN  CEH_SOLICITUDES.FEC_ULTMOD IS 'Fecha ultima modificacion'
/
COMMENT ON COLUMN  CEH_SOLICITUDES.NOM_USUARIO IS 'Responsable ultima modificacion'
/
