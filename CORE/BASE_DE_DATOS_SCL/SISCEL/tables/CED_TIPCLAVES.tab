CREATE TABLE CED_TIPCLAVES(
    COD_TIPCLAVE VARCHAR2(5) NOT NULL 
 ,  DES_TIPCLAVE VARCHAR2(30) NOT NULL 
 ,  MTO_CLAVE NUMBER(14,4) NOT NULL 
 ,  IND_VIGENTE VARCHAR2(1) NOT NULL 
 ,  NUM_SOLICITUD NUMBER(10,0) NULL 
 ,  NUM_ACTIVAR NUMBER(10,0) NULL 
 ,  NUM_VIGENCIA NUMBER(6,0) NULL 
 ,  NUM_DIASEXP NUMBER(6,0) NULL 
 ,  FEC_ULTMOD DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CED_TIPCLAVES IS 'Tipos de claves comverse'
/
COMMENT ON COLUMN  CED_TIPCLAVES.COD_TIPCLAVE IS 'Codigo identificacion clave'
/
COMMENT ON COLUMN  CED_TIPCLAVES.DES_TIPCLAVE IS 'Descripcion tipo de clave'
/
COMMENT ON COLUMN  CED_TIPCLAVES.MTO_CLAVE IS 'Monto de carga asociado'
/
COMMENT ON COLUMN  CED_TIPCLAVES.IND_VIGENTE IS 'Indicador de vigencia  1: vigente 0: no Vigente'
/
COMMENT ON COLUMN  CED_TIPCLAVES.NUM_SOLICITUD IS 'Numero de claves por solicitud emitida'
/
COMMENT ON COLUMN  CED_TIPCLAVES.NUM_ACTIVAR IS 'Numero de claves a activar por proceso'
/
COMMENT ON COLUMN  CED_TIPCLAVES.NUM_VIGENCIA IS 'Numero de dias de vigencia para solicitud'
/
COMMENT ON COLUMN  CED_TIPCLAVES.NUM_DIASEXP IS 'Numero de dias, para calculo de fecha expiracion'
/
COMMENT ON COLUMN  CED_TIPCLAVES.FEC_ULTMOD IS 'Fecha ultima modificacion'
/
COMMENT ON COLUMN  CED_TIPCLAVES.NOM_USUARIO IS 'Responsable ultima modificacion'
/
