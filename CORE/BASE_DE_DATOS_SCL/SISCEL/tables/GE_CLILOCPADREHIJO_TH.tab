CREATE TABLE GE_CLILOCPADREHIJO_TH (  
   COD_CLIENTE                   NUMBER(8)          NOT NULL
 , COD_CLI_PADRE                 NUMBER(8)          NOT NULL
 , TIPO_CLIENTE                  VARCHAR(1)             NULL
 , FEC_INI                       DATE               NOT NULL
 , FEC_FIN                       DATE               NOT NULL
 , NOM_USUARIO                   VARCHAR(30)        NOT NULL
 , FEC_ULTMOD                    DATE               NOT NULL
 , NOM_USUARIOULT                VARCHAR(30)        NOT NULL
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  GE_CLILOCPADREHIJO_TH                      IS '[ GA_CLILOCH ] Relacion  histórica clientes locutoriosRelacion  histórica clientes locutorios'
/

COMMENT ON COLUMN GE_CLILOCPADREHIJO_TH.COD_CLIENTE                    IS 'Código de identificacion del cliente'
/
COMMENT ON COLUMN GE_CLILOCPADREHIJO_TH.COD_CLI_PADRE                  IS 'Código de identificacion del cliente padre locutorio'
/
COMMENT ON COLUMN GE_CLILOCPADREHIJO_TH.TIPO_CLIENTE                   IS 'Clasificacion de cliente locutorio (P: cliente Pabre; H: Cliente Hijo)'
/
COMMENT ON COLUMN GE_CLILOCPADREHIJO_TH.FEC_INI                        IS 'Fecha inicio vigencia de la relacion'
/
COMMENT ON COLUMN GE_CLILOCPADREHIJO_TH.FEC_FIN                        IS 'Fecha fin vigencia de la relacion'
/
COMMENT ON COLUMN GE_CLILOCPADREHIJO_TH.NOM_USUARIO                    IS 'Nombre del usuario que modifica el registro'
/
COMMENT ON COLUMN GE_CLILOCPADREHIJO_TH.FEC_ULTMOD                     IS 'Fecha de modifcacion del registro '
/
COMMENT ON COLUMN GE_CLILOCPADREHIJO_TH.NOM_USUARIOULT                 IS 'Nombre del usuario que modifica el registro'
/

