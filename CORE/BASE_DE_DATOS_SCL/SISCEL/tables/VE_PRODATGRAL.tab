CREATE TABLE VE_PRODATGRAL(
    COD_NORECHAZO VARCHAR2(2) NOT NULL 
 ,  COD_ENTRECHAZO VARCHAR2(2) NOT NULL 
 ,  IND_DELAUTOCLI NUMBER(1,0) NOT NULL 
 ,  COD_TIPID_NUM_IDENT VARCHAR2(2) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_PRODATGRAL IS 'Datos Generales para Prospectos'
/
COMMENT ON COLUMN  VE_PRODATGRAL.COD_NORECHAZO IS 'Codigo por defecto para NO RECHAZO de Prospecto'
/
COMMENT ON COLUMN  VE_PRODATGRAL.COD_ENTRECHAZO IS 'Codigo de RECHAZO de Prospecto (causas)'
/
COMMENT ON COLUMN  VE_PRODATGRAL.IND_DELAUTOCLI IS 'Indicativo de borrado automatico cuando un Prospecto pasa a Cliente'
/
