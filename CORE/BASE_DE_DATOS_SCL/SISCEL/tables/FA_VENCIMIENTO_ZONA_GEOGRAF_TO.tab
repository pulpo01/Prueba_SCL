CREATE TABLE FA_VENCIMIENTO_ZONA_GEOGRAF_TO(
    COD_REGION VARCHAR2(3) NOT NULL 
 ,  COD_PROVINCIA VARCHAR2(5) NOT NULL 
 ,  COD_VENCIME VARCHAR2(5) NOT NULL 
 ,  COD_PLANVENCIMIE VARCHAR2(5) NOT NULL 
 ,  IND_ESTADO NUMBER(1,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_VENCIMIENTO_ZONA_GEOGRAF_TO IS '[ FVCO ] Vencimientos por zona geografica'
/
COMMENT ON COLUMN  FA_VENCIMIENTO_ZONA_GEOGRAF_TO.COD_REGION IS 'Código de la región que identifica la ubicación del vencimiento'
/
COMMENT ON COLUMN  FA_VENCIMIENTO_ZONA_GEOGRAF_TO.COD_PROVINCIA IS 'Código de la provincia que identifica la ubicación del vencimiento'
/
COMMENT ON COLUMN  FA_VENCIMIENTO_ZONA_GEOGRAF_TO.COD_VENCIME IS 'Código del vencimiento definido geográficamente '
/
COMMENT ON COLUMN  FA_VENCIMIENTO_ZONA_GEOGRAF_TO.COD_PLANVENCIMIE IS 'Código del plan de vencimiento asignado a la zona'
/
COMMENT ON COLUMN  FA_VENCIMIENTO_ZONA_GEOGRAF_TO.IND_ESTADO IS 'Indicador del estado de vigencia para la aplicación del plan, 1activo; 0 inactivo'
/
COMMENT ON COLUMN  FA_VENCIMIENTO_ZONA_GEOGRAF_TO.FEC_ULTMOD IS 'Fecha de modificación del registro'
/
COMMENT ON COLUMN  FA_VENCIMIENTO_ZONA_GEOGRAF_TO.NOM_USUARIO IS 'Nombre del usuario de la modificación'
/
