CREATE TABLE IC_SUBPARAMETRO_TD(
    COD_LENGUAJE NUMBER(3,0) NOT NULL 
 ,  NOM_PARAMETRO VARCHAR2(15) NOT NULL 
 ,  COD_SUBPARAMETRO NUMBER(3,0) NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  IC_SUBPARAMETRO_TD IS '[ SBPR ] [ IC_SUBPAM ] Subparametros para movimientos'
/
COMMENT ON COLUMN  IC_SUBPARAMETRO_TD.COD_LENGUAJE IS 'Código de Lenguaje'
/
COMMENT ON COLUMN  IC_SUBPARAMETRO_TD.NOM_PARAMETRO IS 'Nombre del parámetro'
/
COMMENT ON COLUMN  IC_SUBPARAMETRO_TD.COD_SUBPARAMETRO IS 'Código de subparametro'
/
