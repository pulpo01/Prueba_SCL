CREATE TABLE SC_DOMINIO_CTO(
    COD_DOMINIO_CTO NUMBER(2,0) NOT NULL 
 ,  DES_DOMINIO_CTO VARCHAR2(30) NOT NULL 
 ,  IND_GRP_DOMINIO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  LEN_CONCEPTO NUMBER(2,0) DEFAULT 6 NULL 
 ,  IND_MAPEO_GRP NUMBER(1,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SC_DOMINIO_CTO IS 'DOMINIOS DE CONCEPTOS CONTABLES'
/
COMMENT ON COLUMN  SC_DOMINIO_CTO.COD_DOMINIO_CTO IS 'CODIGO DE DOMINIO DE CONCEPTOS'
/
COMMENT ON COLUMN  SC_DOMINIO_CTO.DES_DOMINIO_CTO IS 'DESCRIPCION DE DOMINIO DE CONCEPTOS'
/
COMMENT ON COLUMN  SC_DOMINIO_CTO.IND_GRP_DOMINIO IS 'indica si el dominio es simple (0) o si se trata de un grupo (1).'
/
COMMENT ON COLUMN  SC_DOMINIO_CTO.LEN_CONCEPTO IS 'LARGO DEL CODIGO DE CONCEPTOS'
/
COMMENT ON COLUMN  SC_DOMINIO_CTO.IND_MAPEO_GRP IS 'Indicador de mapeo 0: cuando la tabla no requiera agrupacion de conceptos y 1:cuando la tabla requiera agrupacion de conceptos'
/
