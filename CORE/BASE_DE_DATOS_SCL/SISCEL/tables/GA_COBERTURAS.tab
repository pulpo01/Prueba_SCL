 ,  DES_NODO VARCHAR2(30) NOT NULL 
 ,  TIP_COBERTURA NUMBER(2,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) DEFAULT 2 NOT NULL 
 ,  COD_ZONA NUMBER(2,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_COBERTURAS IS 'Coberturas Beeper'
/
COMMENT ON COLUMN  GA_COBERTURAS.COD_NODO IS 'Codigo de Cobertura'
/
COMMENT ON COLUMN  GA_COBERTURAS.DES_NODO IS 'Descripcion de cobertura'
/
COMMENT ON COLUMN  GA_COBERTURAS.TIP_COBERTURA IS 'Tipo de Cobertura'
/
COMMENT ON COLUMN  GA_COBERTURAS.COD_CENTRAL IS 'Codigo de Central'
/
COMMENT ON COLUMN  GA_COBERTURAS.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_COBERTURAS.COD_ZONA IS 'Codigo de Zona asociado'
/
