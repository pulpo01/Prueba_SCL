CREATE TABLE GA_HMODSUBCUENTAS(
    COD_SUBCUENTA VARCHAR2(15) NOT NULL 
 ,  FEC_MODIFICA DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  DES_SUBCUENTA VARCHAR2(30) NULL 
 ,  COD_DIRECCION NUMBER(20,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_HMODSUBCUENTAS IS 'Historico Modificaciones de Subcuentas'
/
COMMENT ON COLUMN  GA_HMODSUBCUENTAS.COD_SUBCUENTA IS 'Codigo de Subcuenta'
/
COMMENT ON COLUMN  GA_HMODSUBCUENTAS.FEC_MODIFICA IS 'Fecha de Modificacion'
/
COMMENT ON COLUMN  GA_HMODSUBCUENTAS.NOM_USUARORA IS 'Usuario que Modifica'
/
COMMENT ON COLUMN  GA_HMODSUBCUENTAS.FEC_HISTORICO IS 'Fecha de Historico'
/
COMMENT ON COLUMN  GA_HMODSUBCUENTAS.DES_SUBCUENTA IS 'Descripcion de Subuenta'
/
COMMENT ON COLUMN  GA_HMODSUBCUENTAS.COD_DIRECCION IS 'Codigo de Direccion'
/
