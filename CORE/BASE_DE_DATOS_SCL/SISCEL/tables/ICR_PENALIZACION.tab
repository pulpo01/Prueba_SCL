CREATE TABLE ICR_PENALIZACION(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_MODULO VARCHAR2(2) NOT NULL 
 ,  COD_SUSPREHA VARCHAR2(3) NOT NULL 
 ,  FEC_INGRESO DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_SERVICIO NUMBER(2,0) NOT NULL 
 ,  COD_NIVEL NUMBER(4,0) NOT NULL 
 ,  TIP_SUSPENSION VARCHAR2(2) NOT NULL 
 ,  COD_NIVEL_REA NUMBER(4,0) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICR_PENALIZACION IS 'Penalizaciones del Abonado Radio y Control'
/
COMMENT ON COLUMN  ICR_PENALIZACION.NUM_ABONADO IS 'Numero de Abonado'
/
COMMENT ON COLUMN  ICR_PENALIZACION.COD_MODULO IS 'Codigo de Modulo'
/
COMMENT ON COLUMN  ICR_PENALIZACION.COD_SUSPREHA IS 'Codigo de Susp/Reha'
/
COMMENT ON COLUMN  ICR_PENALIZACION.FEC_INGRESO IS 'Fecha de Ingreso'
/
COMMENT ON COLUMN  ICR_PENALIZACION.COD_SERVICIO IS 'Codigo de Sevicio'
/
COMMENT ON COLUMN  ICR_PENALIZACION.COD_NIVEL IS 'Codigo de Nivel'
/
COMMENT ON COLUMN  ICR_PENALIZACION.TIP_SUSPENSION IS ' - Retrofitted - Retrofitted'
/
COMMENT ON COLUMN  ICR_PENALIZACION.COD_NIVEL_REA IS 'Codigo de Nivel Rehabilitacion'
/
