CREATE TABLE GSM_ARCHIVOS_TO(
    COD_ARCHIVO VARCHAR2(6) NOT NULL 
 ,  NUM_VERSION NUMBER(10,0) NOT NULL 
 ,  NOM_ARCHIVO VARCHAR2(50) NULL 
 ,  FEC_CREACION DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_ARCHIVO_DEFAULT VARCHAR2(2) NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) DEFAULT 1 NULL 
 ,  FEC_ACTUALIZACION DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_ARCHIVOS_TO IS 'Tabla en la que se define el Archivo.'
/
COMMENT ON COLUMN  GSM_ARCHIVOS_TO.COD_ARCHIVO IS 'Codigo de archivo.'
/
COMMENT ON COLUMN  GSM_ARCHIVOS_TO.NUM_VERSION IS 'Numero de version del archivo.'
/
COMMENT ON COLUMN  GSM_ARCHIVOS_TO.NOM_ARCHIVO IS 'Nombre del archivo.'
/
COMMENT ON COLUMN  GSM_ARCHIVOS_TO.FEC_CREACION IS 'Fecha de creacion del archivo.'
/
COMMENT ON COLUMN  GSM_ARCHIVOS_TO.COD_ARCHIVO_DEFAULT IS 'Codigo que indica se el archivo es default o no.'
/
COMMENT ON COLUMN  GSM_ARCHIVOS_TO.FEC_ACTUALIZACION IS 'Fecha de actualizacion.'
/
COMMENT ON COLUMN  GSM_ARCHIVOS_TO.COD_USUARIO IS 'Codigo de usuario.'
/
