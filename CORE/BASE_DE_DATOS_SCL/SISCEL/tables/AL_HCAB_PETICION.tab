CREATE TABLE AL_HCAB_PETICION(
    NUM_PETICION NUMBER(9,0) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  COD_BODEGA_ORI NUMBER(6,0) NOT NULL 
 ,  COD_BODEGA_DEST NUMBER(6,0) NOT NULL 
 ,  FEC_PETICION DATE NOT NULL 
 ,  USU_PETICION VARCHAR2(30) NOT NULL 
 ,  COD_MOTIVO NUMBER(2,0) NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) NOT NULL 
 ,  DES_OBSERVACION VARCHAR2(100) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_HCAB_PETICION IS 'Historico de Cabeceras de Peticion de Traspaso'
/
COMMENT ON COLUMN  AL_HCAB_PETICION.NUM_PETICION IS 'Numero de Peticion'
/
COMMENT ON COLUMN  AL_HCAB_PETICION.FEC_HISTORICO IS 'Fecha de historico'
/
COMMENT ON COLUMN  AL_HCAB_PETICION.COD_BODEGA_ORI IS 'Bodega a la que se realiza la peticion'
/
COMMENT ON COLUMN  AL_HCAB_PETICION.COD_BODEGA_DEST IS 'Bodega que realiza la peticion'
/
COMMENT ON COLUMN  AL_HCAB_PETICION.FEC_PETICION IS 'Fecha de Peticion'
/
COMMENT ON COLUMN  AL_HCAB_PETICION.USU_PETICION IS 'Usuario Peticionario'
/
COMMENT ON COLUMN  AL_HCAB_PETICION.COD_MOTIVO IS 'Motivo peticion'
/
COMMENT ON COLUMN  AL_HCAB_PETICION.COD_ESTADO IS 'Estado'
/
COMMENT ON COLUMN  AL_HCAB_PETICION.DES_OBSERVACION IS 'Observaciones o Comentarios'
/
