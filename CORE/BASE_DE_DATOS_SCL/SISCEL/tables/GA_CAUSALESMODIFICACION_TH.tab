CREATE TABLE GA_CAUSALESMODIFICACION_TH(
    ID_RESPALDO NUMBER(4,0) NOT NULL 
 ,  COD_CAUSA NUMBER(4,0) NOT NULL 
 ,  NOMBRE_CAUSA VARCHAR2(40) NOT NULL 
 ,  FECHA_DESDE DATE NOT NULL 
 ,  FECHA_HASTA DATE NULL 
 ,  USUARIO VARCHAR2(40) NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_CAUSALESMODIFICACION_TH IS '[ CMDPC ] Tabla que contiene la historia de modificaciones de causa de modificación. Todos sus registros coreesponden a causales de modificación  no activas o a estados ya no validos de causales activas.'
/
COMMENT ON COLUMN  GA_CAUSALESMODIFICACION_TH.ID_RESPALDO IS 'Identificador de causas de modificación respaldada'
/
COMMENT ON COLUMN  GA_CAUSALESMODIFICACION_TH.COD_CAUSA IS 'Código de causa de Modificación'
/
COMMENT ON COLUMN  GA_CAUSALESMODIFICACION_TH.NOMBRE_CAUSA IS 'Texto que describe la causa de modificación'
/
COMMENT ON COLUMN  GA_CAUSALESMODIFICACION_TH.FECHA_DESDE IS 'Fecha de inicio de la vigencia de la causa de modificación'
/
COMMENT ON COLUMN  GA_CAUSALESMODIFICACION_TH.FECHA_HASTA IS 'Fecha de termino de la vigencia de la causa de modificación'
/
COMMENT ON COLUMN  GA_CAUSALESMODIFICACION_TH.USUARIO IS 'Usuario quien crea un registro de causa de modificación'
/
