CREATE TABLE FA_SCHEDULER(
    PID_PROCESO NUMBER(8,0) NOT NULL 
 ,  FEC_PROCESO DATE NOT NULL 
 ,  COD_ESTSCHED CHAR(1) NOT NULL 
 ,  EXE_COMANDO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_SCHEDULER IS 'Scheduler de Facturacion.'
/
COMMENT ON COLUMN  FA_SCHEDULER.PID_PROCESO IS 'PID Proceso Scheduler. [PK 1/1]'
/
COMMENT ON COLUMN  FA_SCHEDULER.FEC_PROCESO IS 'Fecha de Ejecucion del Proceso.'
/
COMMENT ON COLUMN  FA_SCHEDULER.COD_ESTSCHED IS 'Estado de Activacion: A=Activado | D=Desactivado'
/
COMMENT ON COLUMN  FA_SCHEDULER.EXE_COMANDO IS 'Comando de ejecucion del Scheduler.'
/
