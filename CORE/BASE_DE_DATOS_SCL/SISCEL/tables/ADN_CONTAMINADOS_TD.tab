CREATE TABLE ADN_CONTAMINADOS_TD(
    NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  COD_SUBALM VARCHAR2(5) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  COD_CAT NUMBER(2,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_OPERADORA VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ADN_CONTAMINADOS_TD IS 'Numeros Contaminados'
/
COMMENT ON COLUMN  ADN_CONTAMINADOS_TD.NUM_CELULAR IS 'Numero de Celular'
/
COMMENT ON COLUMN  ADN_CONTAMINADOS_TD.COD_SUBALM IS 'Codigo de Subalm'
/
COMMENT ON COLUMN  ADN_CONTAMINADOS_TD.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  ADN_CONTAMINADOS_TD.COD_CENTRAL IS 'Codigo de Central'
/
COMMENT ON COLUMN  ADN_CONTAMINADOS_TD.COD_CAT IS 'Codigo de Categoria'
/
COMMENT ON COLUMN  ADN_CONTAMINADOS_TD.COD_USO IS 'Codigo de Uso'
/
COMMENT ON COLUMN  ADN_CONTAMINADOS_TD.COD_OPERADORA IS 'Codigo de Operadora'
/
