CREATE TABLE CO_PROMESAPAGO(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  DEU_INGRESO NUMBER(14,4) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_PROMESAPAGO IS 'Promesas De Pago.'
/
COMMENT ON COLUMN  CO_PROMESAPAGO.COD_CLIENTE IS 'Codigo Cliente.'
/
COMMENT ON COLUMN  CO_PROMESAPAGO.FEC_INGRESO IS 'Fecha Ingreso de la Promesa.'
/
COMMENT ON COLUMN  CO_PROMESAPAGO.DEU_INGRESO IS 'Deuda Pactada en Promesa.'
/
COMMENT ON COLUMN  CO_PROMESAPAGO.NOM_USUARIO IS 'Usuario que registro la Promesa.'
/
