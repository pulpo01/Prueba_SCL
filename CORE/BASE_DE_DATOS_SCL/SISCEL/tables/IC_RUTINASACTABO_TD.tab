CREATE TABLE IC_RUTINASACTABO_TD(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_MODULO VARCHAR2(2) NOT NULL 
 ,  COD_ACTABO VARCHAR2(2) NOT NULL 
 ,  COD_TECNOLOGIA VARCHAR2(7) NOT NULL 
 ,  COD_RUTINA VARCHAR2(20) NOT NULL 
 ,  NUM_ORDEN NUMBER(2,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(20) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  FLG_ESTADO VARCHAR2(5) DEFAULT 'TRUE' NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  IC_RUTINASACTABO_TD IS 'ASOCIACISN DE RUTINAS POR ACTUACION COMERCIAL'
/
COMMENT ON COLUMN  IC_RUTINASACTABO_TD.COD_PRODUCTO IS 'PRODUCTO SOBRE EL QUE APLICA LA ACTUACION'
/
COMMENT ON COLUMN  IC_RUTINASACTABO_TD.COD_MODULO IS 'MODULO QUE GENERA EL MOVIMIENTO'
/
COMMENT ON COLUMN  IC_RUTINASACTABO_TD.COD_ACTABO IS 'ACTUACION COMERCIAL'
/
COMMENT ON COLUMN  IC_RUTINASACTABO_TD.COD_TECNOLOGIA IS 'TECNOLOGIA ASOCIADA AL ABONADO'
/
COMMENT ON COLUMN  IC_RUTINASACTABO_TD.COD_RUTINA IS 'RUTINA QUE SE EJECUTARA'
/
COMMENT ON COLUMN  IC_RUTINASACTABO_TD.NUM_ORDEN IS 'ORDEN DE EJECUCION'
/
COMMENT ON COLUMN  IC_RUTINASACTABO_TD.NOM_USUARIO IS 'USUARIO DE CREACION'
/
COMMENT ON COLUMN  IC_RUTINASACTABO_TD.FEC_ULTMOD IS 'FECHA DE CREACION'
/
