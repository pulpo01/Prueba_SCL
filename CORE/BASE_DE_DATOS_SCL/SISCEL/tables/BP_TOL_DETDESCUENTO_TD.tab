CREATE TABLE BP_TOL_DETDESCUENTO_TD (  
   TIP_DCTO                      VARCHAR2(5)                                     NOT NULL 
 , COD_DCTO                      VARCHAR2(5)                                     NOT NULL 
 , COD_AGRULLAM                  VARCHAR2(5)                                     NOT NULL 
 , IND_ARRASTRE                  VARCHAR2(1)                                     NOT NULL 
 , IND_UNIDAD                    VARCHAR2(5)                                     NOT NULL 
 , CNT_VALOR                     NUMBER(14,4)                                    NOT NULL 
 , DIA_VALI                      NUMBER(8)                                       NOT NULL 
 , NOM_USUARIO                   VARCHAR2(30)                                    NOT NULL 
 , FEC_UMOD                      DATE                                            NOT NULL 
 , DIA_EVAL                      NUMBER(2)                                       NOT NULL 
 ) TABLESPACE     INSTALL_DATA
/

COMMENT ON TABLE  BP_TOL_DETDESCUENTO_TD                      IS '[ BPTD ] Agrupaciones de llamadas de evaluación para Promocines DinamicasAgrupaciones de llamadas de evaluación para Promocines Dinamicas'
/

COMMENT ON COLUMN BP_TOL_DETDESCUENTO_TD.TIP_DCTO                       IS 'Tipo de descuento'
/
COMMENT ON COLUMN BP_TOL_DETDESCUENTO_TD.COD_DCTO                       IS 'Codigo de descuento'
/
COMMENT ON COLUMN BP_TOL_DETDESCUENTO_TD.COD_AGRULLAM                   IS 'Codigo de Agrupacion de llamadas'
/
COMMENT ON COLUMN BP_TOL_DETDESCUENTO_TD.IND_ARRASTRE                   IS 'Indicador de arrastre'
/
COMMENT ON COLUMN BP_TOL_DETDESCUENTO_TD.IND_UNIDAD                     IS 'Unidad de medida'
/
COMMENT ON COLUMN BP_TOL_DETDESCUENTO_TD.CNT_VALOR                      IS 'Valor de la bolsa de servicio'
/
COMMENT ON COLUMN BP_TOL_DETDESCUENTO_TD.DIA_VALI                       IS 'Representa dias validos para consumo'
/
COMMENT ON COLUMN BP_TOL_DETDESCUENTO_TD.NOM_USUARIO                    IS 'Codigo de usuario'
/
COMMENT ON COLUMN BP_TOL_DETDESCUENTO_TD.FEC_UMOD                       IS 'Ultima fecha de modificacion'
/
COMMENT ON COLUMN BP_TOL_DETDESCUENTO_TD.DIA_EVAL                       IS 'Dias a evaluar antes de la fecha de aplicación  de  la promoción'
/



