CREATE TABLE AL_ERROR_SERIES_TO(
    NUM_ORDEN NUMBER(9,0) NOT NULL 
 ,  NUM_LINEA NUMBER(2,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  COD_ERROR NUMBER(2,0) NOT NULL 
 ,  FEC_CREACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_ERROR_SERIES_TO IS 'CONTIENE LAS SERIES RECHAZADAS EN SU VALIDACION PARA INGRESO DE MERCADERIA  MASIVO '
/
COMMENT ON COLUMN  AL_ERROR_SERIES_TO.NUM_ORDEN IS 'NUMERO DE ORDEN DE INGRESO'
/
COMMENT ON COLUMN  AL_ERROR_SERIES_TO.NUM_LINEA IS 'NUMERO DE LINEA DE ORDEN DE INGRESO'
/
COMMENT ON COLUMN  AL_ERROR_SERIES_TO.NUM_SERIE IS 'NUMERO DE SERIE RECHAZADA POR PRO*C'
/
COMMENT ON COLUMN  AL_ERROR_SERIES_TO.COD_ERROR IS 'CODIGO DE ERROR(GED_CODIGOS)'
/
COMMENT ON COLUMN  AL_ERROR_SERIES_TO.FEC_CREACION IS 'FECHA DE CREACION DE ERROR'
/
COMMENT ON COLUMN  AL_ERROR_SERIES_TO.NOM_USUARIO IS 'NOMBRE DE USUARIO'
/
