CREATE TABLE CET_CLAVASIG(
    NUM_CLAVE VARCHAR2(16) NOT NULL 
 ,  NUM_SERIE VARCHAR2(16) NOT NULL 
 ,  FEC_EXPIRACION DATE NOT NULL 
 ,  COD_TIPCLAVE VARCHAR2(5) NOT NULL 
 ,  MTO_CLAVE NUMBER(10,0) NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) NOT NULL 
 ,  NUM_CELULAR NUMBER(15,0) NULL 
 ,  COD_SCRIPT VARCHAR2(2) NOT NULL 
 ,  NUM_LOTE NUMBER(6,0) NOT NULL 
 ,  FEC_HABILITA DATE NULL 
 ,  FEC_CIERRE DATE NULL 
 ,  COD_MOTERROR VARCHAR2(2) NULL 
 ,  NUM_OPACTIVA NUMBER(6,0) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CET_CLAVASIG IS 'Claves Asignadas a transacciones'
/
COMMENT ON COLUMN  CET_CLAVASIG.NUM_CLAVE IS 'Numero de clave'
/
COMMENT ON COLUMN  CET_CLAVASIG.NUM_SERIE IS 'Numero de serie'
/
COMMENT ON COLUMN  CET_CLAVASIG.FEC_EXPIRACION IS 'Fecha de expiracion de la clave'
/
COMMENT ON COLUMN  CET_CLAVASIG.COD_TIPCLAVE IS 'Tipo de clave ( tipos de tarjeta )'
/
COMMENT ON COLUMN  CET_CLAVASIG.MTO_CLAVE IS 'Monto de recarga de prepago'
/
COMMENT ON COLUMN  CET_CLAVASIG.COD_ESTADO IS 'Estado de la clave  P: Proceso A: Asignada H: Habilitada E: Error V: Vencida'
/
COMMENT ON COLUMN  CET_CLAVASIG.NUM_CELULAR IS 'Numero celular asignacion'
/
COMMENT ON COLUMN  CET_CLAVASIG.COD_SCRIPT IS 'Tipo de scriptacion utilizada'
/
COMMENT ON COLUMN  CET_CLAVASIG.NUM_LOTE IS 'Numero de lote informacion de clave'
/
COMMENT ON COLUMN  CET_CLAVASIG.FEC_HABILITA IS 'Fecha de habilitacion'
/
COMMENT ON COLUMN  CET_CLAVASIG.FEC_CIERRE IS 'Fecha de Cierre'
/
COMMENT ON COLUMN  CET_CLAVASIG.COD_MOTERROR IS 'Motivo de error'
/
COMMENT ON COLUMN  CET_CLAVASIG.NUM_OPACTIVA IS 'Numero de operacion de habilitacion'
/
