CREATE TABLE GA_HDIRECUSUAR(
    COD_USUARIO NUMBER(8,0) NOT NULL 
 ,  COD_TIPDIRECCION VARCHAR2(1) NOT NULL 
 ,  COD_DIRECCION NUMBER(20,0) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_HDIRECUSUAR IS 'Historico Tabla de Direcciones de Usuario'
/
COMMENT ON COLUMN  GA_HDIRECUSUAR.COD_USUARIO IS 'Codigo de Usuario'
/
COMMENT ON COLUMN  GA_HDIRECUSUAR.COD_TIPDIRECCION IS 'Codigo de Tipo de Direccion'
/
COMMENT ON COLUMN  GA_HDIRECUSUAR.COD_DIRECCION IS 'Codigo de Direccion'
/
COMMENT ON COLUMN  GA_HDIRECUSUAR.FEC_HISTORICO IS 'Fecha de Historico'
/
