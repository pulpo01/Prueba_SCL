CREATE TABLE GSM_SECUENCIA_PROVEEDOR_TD(
    COD_PROVEEDOR NUMBER(10,0) NOT NULL 
 ,  COD_SIG_PROVEEDOR VARCHAR2(3) NOT NULL 
 ,  NUM_VALOR_MINIMO NUMBER NOT NULL 
 ,  NUM_VALOR_MAXIMO NUMBER(18,0) NOT NULL 
 ,  NUM_SECUENCIA NUMBER NOT NULL 
 ,  NUM_LARGO_SECUENCIA NUMBER NOT NULL 
 ,  FEC_ACTUALIZACION DATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_SECUENCIA_PROVEEDOR_TD IS 'Tabla de secuencias para proveedores'
/
COMMENT ON COLUMN  GSM_SECUENCIA_PROVEEDOR_TD.COD_PROVEEDOR IS 'Codigo del proveedor.'
/
COMMENT ON COLUMN  GSM_SECUENCIA_PROVEEDOR_TD.COD_SIG_PROVEEDOR IS 'Sigla del proveedor.'
/
COMMENT ON COLUMN  GSM_SECUENCIA_PROVEEDOR_TD.NUM_VALOR_MINIMO IS 'Nombre de la secuencia.'
/
COMMENT ON COLUMN  GSM_SECUENCIA_PROVEEDOR_TD.NUM_VALOR_MAXIMO IS 'Valor minimo de la secuencia.'
/
COMMENT ON COLUMN  GSM_SECUENCIA_PROVEEDOR_TD.NUM_SECUENCIA IS 'Valor maximo de la secuencia.'
/
COMMENT ON COLUMN  GSM_SECUENCIA_PROVEEDOR_TD.NUM_LARGO_SECUENCIA IS 'Valor actual de la secuencia.'
/
COMMENT ON COLUMN  GSM_SECUENCIA_PROVEEDOR_TD.FEC_ACTUALIZACION IS 'Fecha de actualizacion.'
/
COMMENT ON COLUMN  GSM_SECUENCIA_PROVEEDOR_TD.COD_USUARIO IS 'Usuario que actualizo.'
/
