CREATE TABLE FA_REGISIMPRESION_TD(
    COD_TIPDOCUM VARCHAR2(5) NOT NULL 
 ,  COD_REGISTRO VARCHAR2(5) NOT NULL 
 ,  IND_IMPRESION NUMBER(1,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_REGISIMPRESION_TD IS 'Registros del proceso de impresion.'
/
COMMENT ON COLUMN  FA_REGISIMPRESION_TD.COD_TIPDOCUM IS 'Codigo de Documento.'
/
COMMENT ON COLUMN  FA_REGISIMPRESION_TD.COD_REGISTRO IS 'Codigo de Registro.'
/
COMMENT ON COLUMN  FA_REGISIMPRESION_TD.IND_IMPRESION IS 'Indicador de impresion.'
/
COMMENT ON COLUMN  FA_REGISIMPRESION_TD.FEC_ULTMOD IS 'Fecha de Ultima Modificacion.'
/
COMMENT ON COLUMN  FA_REGISIMPRESION_TD.NOM_USUARIO IS 'Nombre de Usuario.'
/
