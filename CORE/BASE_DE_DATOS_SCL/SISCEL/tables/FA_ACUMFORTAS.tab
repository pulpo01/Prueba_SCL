CREATE TABLE FA_ACUMFORTAS(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  IND_ALQUILER NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  COD_TIPCONCE NUMBER(2,0) NOT NULL 
 ,  IMP_CONSUMIDO NUMBER(12,4) NOT NULL 
 ,  SEG_CONSUMIDO NUMBER(9,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_PROCESO NUMBER(8,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_ACUMFORTAS IS 'Tabla de Acumulados FORTAS'
/
COMMENT ON COLUMN  FA_ACUMFORTAS.NUM_PROCESO IS 'Numero de Proceso'
/
COMMENT ON COLUMN  FA_ACUMFORTAS.NUM_ABONADO IS 'Numero de Abonado'
/
COMMENT ON COLUMN  FA_ACUMFORTAS.IND_ALQUILER IS 'Indicativo de Alquiler'
/
COMMENT ON COLUMN  FA_ACUMFORTAS.COD_PERIODO IS 'Periodo Alquiler (Si Ind_Alquiler = 1) o Ciclo Facturacion (Si Ind_Alquiler = 0)'
/
COMMENT ON COLUMN  FA_ACUMFORTAS.COD_OPERADOR IS 'Codigo de Operador'
/
COMMENT ON COLUMN  FA_ACUMFORTAS.COD_TIPCONCE IS '1: Tipo Impuesto 4: Tipo Consumo Carrier'
/
COMMENT ON COLUMN  FA_ACUMFORTAS.IMP_CONSUMIDO IS 'Importe Consumido'
/
COMMENT ON COLUMN  FA_ACUMFORTAS.SEG_CONSUMIDO IS 'Segundos Consumidos'
/
COMMENT ON COLUMN  FA_ACUMFORTAS.COD_CLIENTE IS 'Codigo de Cliente'
/
