CREATE TABLE FAD_TIPPLANVENCIMIE(
    COD_TIPPLANVENC VARCHAR2(5) NOT NULL 
 ,  DES_PLANVENCIMIE VARCHAR2(40) NOT NULL 
 ,  NUM_DIASDESDE NUMBER(3,0) NOT NULL 
 ,  NUM_DIASHASTA NUMBER(3,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAD_TIPPLANVENCIMIE IS 'Tipos De Planes De Vencimiento.'
/
COMMENT ON COLUMN  FAD_TIPPLANVENCIMIE.COD_TIPPLANVENC IS 'Codigo de Tipo de Plan de Vencimiento.'
/
COMMENT ON COLUMN  FAD_TIPPLANVENCIMIE.DES_PLANVENCIMIE IS 'Descripcion del Tipo de Plan de vencimiento.'
/
COMMENT ON COLUMN  FAD_TIPPLANVENCIMIE.NUM_DIASDESDE IS 'Cantidad de Dias Minimo.'
/
COMMENT ON COLUMN  FAD_TIPPLANVENCIMIE.NUM_DIASHASTA IS 'Cantidad de Dias Maximo.'
/
COMMENT ON COLUMN  FAD_TIPPLANVENCIMIE.FEC_ULTMOD IS 'Fecha de modificacin.'
/
COMMENT ON COLUMN  FAD_TIPPLANVENCIMIE.NOM_USUARIO IS 'Nombre de Usuario.'
/
