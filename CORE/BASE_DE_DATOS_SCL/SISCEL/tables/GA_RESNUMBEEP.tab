CREATE TABLE GA_RESNUMBEEP(
    NUM_TRANSACCION NUMBER(10,0) NOT NULL 
 ,  NUM_LINEA NUMBER(3,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(3,0) NOT NULL 
 ,  NUM_BEEPER NUMBER(8,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  FEC_RESERVA DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  IND_PROCNUM VARCHAR2(1) NOT NULL 
 ,  FEC_BAJA DATE NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_RESNUMBEEP IS 'Numeracion beeper reservada en la venta'
/
COMMENT ON COLUMN  GA_RESNUMBEEP.NUM_TRANSACCION IS 'NUMERO DE TRANSACCION.'
/
COMMENT ON COLUMN  GA_RESNUMBEEP.NUM_LINEA IS 'Numero Interno de Linea'
/
COMMENT ON COLUMN  GA_RESNUMBEEP.NUM_ORDEN IS 'Numero de orden'
/
COMMENT ON COLUMN  GA_RESNUMBEEP.NUM_BEEPER IS 'Numero de Beeper'
/
COMMENT ON COLUMN  GA_RESNUMBEEP.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_RESNUMBEEP.COD_CENTRAL IS 'Codigo de Central'
/
COMMENT ON COLUMN  GA_RESNUMBEEP.COD_USO IS 'Codigo de Uso'
/
COMMENT ON COLUMN  GA_RESNUMBEEP.FEC_RESERVA IS 'Fecha de Reserva'
/
COMMENT ON COLUMN  GA_RESNUMBEEP.NOM_USUARIO IS 'Usuario que realiza la reserva'
/
COMMENT ON COLUMN  GA_RESNUMBEEP.IND_PROCNUM IS 'Indicativo de Procedencia del numero; (L)ibres, (R)eutilizable'
/
COMMENT ON COLUMN  GA_RESNUMBEEP.FEC_BAJA IS 'Fecha de baja del numero reutilizable'
/
