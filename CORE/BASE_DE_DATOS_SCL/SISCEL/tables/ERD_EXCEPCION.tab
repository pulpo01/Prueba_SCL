CREATE TABLE ERD_EXCEPCION(
    FEC_DESDE_H DATE NULL 
 ,  FEC_HASTA_H DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD_H DATE NOT NULL 
 ,  COD_RESTRIC NUMBER(4,0) NULL 
 ,  COD_TIPIDENT VARCHAR2(2) DEFAULT '01' NOT NULL 
 ,  NUM_IDENT VARCHAR2(20) NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERD_EXCEPCION IS 'Rut Para Excepcion.'
/
COMMENT ON COLUMN  ERD_EXCEPCION.FEC_DESDE_H IS 'Fecha inicio vigencia.'
/
COMMENT ON COLUMN  ERD_EXCEPCION.FEC_HASTA_H IS 'Fecha termino vigencia.'
/
COMMENT ON COLUMN  ERD_EXCEPCION.NOM_USUARIO IS 'Nombre usuario modificacion.'
/
COMMENT ON COLUMN  ERD_EXCEPCION.FEC_ULTMOD_H IS 'Fecha Modificacion.'
/
COMMENT ON COLUMN  ERD_EXCEPCION.COD_RESTRIC IS 'Codigo de restriccion para ventas.'
/
COMMENT ON COLUMN  ERD_EXCEPCION.COD_TIPIDENT IS 'Tipo de Identificacion.'
/
