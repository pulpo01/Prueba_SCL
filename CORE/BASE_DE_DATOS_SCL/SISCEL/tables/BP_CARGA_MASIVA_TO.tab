CREATE TABLE BP_CARGA_MASIVA_TO(
    COD_TRANSACCION NUMBER(10,0) NOT NULL 
 ,  NUM_CELULAR NUMBER(15,0) DEFAULT 0 NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) DEFAULT 0 NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) DEFAULT -1 NULL 
 ,  COD_PLAN VARCHAR2(5) NULL 
 ,  FEC_DESDEAPLI DATE NULL 
 ,  FEC_HASTAAPLI DATE NULL 
 ,  COD_ERROR NUMBER(1,0) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_MDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  BP_CARGA_MASIVA_TO IS 'TABLA PASO DE CARGAS MASIVA'
/
COMMENT ON COLUMN  BP_CARGA_MASIVA_TO.COD_TRANSACCION IS 'CODIGO DE CARGA.'
/
COMMENT ON COLUMN  BP_CARGA_MASIVA_TO.NUM_CELULAR IS 'CELULAR ABONADO'
/
COMMENT ON COLUMN  BP_CARGA_MASIVA_TO.COD_CLIENTE IS 'CODIGO DE CLIENTE.'
/
COMMENT ON COLUMN  BP_CARGA_MASIVA_TO.NUM_ABONADO IS 'NUMERO ABONADO'
/
COMMENT ON COLUMN  BP_CARGA_MASIVA_TO.COD_PLAN IS 'CODIGO DE PLAN DE BENEFICIOS'
/
COMMENT ON COLUMN  BP_CARGA_MASIVA_TO.FEC_DESDEAPLI IS 'FECHA INICIO VIGENCIA COMERCIAL'
/
COMMENT ON COLUMN  BP_CARGA_MASIVA_TO.FEC_HASTAAPLI IS 'FECHA FIN VIGENCIA COMERCIAL'
/
COMMENT ON COLUMN  BP_CARGA_MASIVA_TO.COD_ERROR IS 'CODIGO ERROR: 0 - NO ERROR 1 - NO EXISTE EN SCL 2 - PLAN POSPAGO/CLIENTE PREPAGO 3 - PLAN PREPAGO/CLIENTE POSPAGO 4 - PLAN HIBRIDO/CLIENTE POSPAGO ML 5 - PLAN POSPAGO  ML/CLIENTE HIBRIDO'
/
