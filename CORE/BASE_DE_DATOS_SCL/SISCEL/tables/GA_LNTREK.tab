CREATE TABLE GA_LNTREK(
    NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  IND_PROCEQUI VARCHAR2(1) NOT NULL 
 ,  COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  COD_FABRICANTE NUMBER(2,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  NUM_SERIEMEC VARCHAR2(20) NULL 
 ,  NUM_ABONADO NUMBER(8,0) NULL 
 ,  NUM_MAN NUMBER(8,0) NULL 
 ,  COD_CLIENTE NUMBER(8,0) NULL 
 ,  FEC_ALTA DATE NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_LNTREK IS 'Lista Negra de Equipos Trek'
/
COMMENT ON COLUMN  GA_LNTREK.NUM_ABONADO IS 'Numero de Abonado STARTEL'
/
COMMENT ON COLUMN  GA_LNTREK.NUM_MAN IS 'Numero de Man Trek'
/
COMMENT ON COLUMN  GA_LNTREK.COD_CLIENTE IS 'Cliente al que pertenece el Equipo'
/
COMMENT ON COLUMN  GA_LNTREK.FEC_ALTA IS 'Fecha de ingreso en lista negra'
/
COMMENT ON COLUMN  GA_LNTREK.NUM_SERIE IS 'Numero de Serie'
/
COMMENT ON COLUMN  GA_LNTREK.IND_PROCEQUI IS 'Indicativo de Procedencia del Equipo (E)xterno/(I)nterno'
/
COMMENT ON COLUMN  GA_LNTREK.COD_OPERADOR IS 'Operador de procedencia del Equipo'
/
COMMENT ON COLUMN  GA_LNTREK.COD_FABRICANTE IS 'Fabricante del articulo'
/
COMMENT ON COLUMN  GA_LNTREK.COD_ARTICULO IS 'Operador de procedencia del Equipo'
/
COMMENT ON COLUMN  GA_LNTREK.NUM_SERIEMEC IS 'Numero de Serie Mecanica'
/
