 ,  IND_TIPDOCUM NUMBER(2,0) NOT NULL 
 ,  EST_SOLIC NUMBER(5,0) NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
