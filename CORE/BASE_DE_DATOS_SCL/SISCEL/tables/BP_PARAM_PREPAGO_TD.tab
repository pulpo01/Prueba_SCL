CREATE TABLE BP_PARAM_PREPAGO_TD(
    COD_PLAN VARCHAR2(5) NOT NULL 
 ,  FEC_DESDEAPLI DATE NOT NULL 
 ,  VAL_RECARGA_PER NUMBER(14,4) DEFAULT 0 NOT NULL 
 ,  TIP_UNIDAD_EVAL VARCHAR2(5) DEFAULT 'M' NOT NULL 
 ,  VAL_EVAL_MINIMO NUMBER(14,4) DEFAULT 0 NOT NULL 
 ,  VAL_EVAL_MAXIMO NUMBER(14,4) DEFAULT 0 NOT NULL 
 ,  IND_PROCEQUI CHAR(1) DEFAULT 'A' NOT NULL 
 ,  COD_CANAL NUMBER(1,0) NULL 
 ,  COD_ACTABO VARCHAR2(2) NOT NULL 
 ,  DUR_RECARGA NUMBER(3,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  BP_PARAM_PREPAGO_TD IS 'Parametros Promociones Prepago'
/
COMMENT ON COLUMN  BP_PARAM_PREPAGO_TD.COD_PLAN IS 'Codigo de Plan de BPD_PLANES'
/
COMMENT ON COLUMN  BP_PARAM_PREPAGO_TD.FEC_DESDEAPLI IS 'Fecha Inicio Vigencia del Plan'
/
COMMENT ON COLUMN  BP_PARAM_PREPAGO_TD.VAL_RECARGA_PER IS 'Valor de carga periodica. Segunda vez en adelante'
/
COMMENT ON COLUMN  BP_PARAM_PREPAGO_TD.TIP_UNIDAD_EVAL IS 'Tipo de unidad a evaluar. GED_CODIGOS'
/
COMMENT ON COLUMN  BP_PARAM_PREPAGO_TD.VAL_EVAL_MINIMO IS 'Valor minimo para evaluar'
/
COMMENT ON COLUMN  BP_PARAM_PREPAGO_TD.VAL_EVAL_MAXIMO IS 'Valor maximo para evaluar'
/
COMMENT ON COLUMN  BP_PARAM_PREPAGO_TD.IND_PROCEQUI IS 'Indicador procedencia del equipo. GED_CODIGOS'
/
COMMENT ON COLUMN  BP_PARAM_PREPAGO_TD.COD_CANAL IS 'Canal de venta. VE_TIPCOMIS.IND_VTA_EXTERNA'
/
COMMENT ON COLUMN  BP_PARAM_PREPAGO_TD.COD_ACTABO IS 'Codigo de actuacion de abonados'
/
COMMENT ON COLUMN  BP_PARAM_PREPAGO_TD.DUR_RECARGA IS 'Duracion de la Recarga en dias'
/
COMMENT ON COLUMN  BP_PARAM_PREPAGO_TD.COD_ARTICULO IS 'Codigo de articulo'
/
