CREATE TABLE ICG_SISTEMA(
    COD_SISTEMA NUMBER(2,0) NOT NULL 
 ,  COD_LENGUAJE NUMBER(3,0) NOT NULL 
 ,  COD_INTERFAZ NUMBER(2,0) NOT NULL 
 ,  COD_ESTADO NUMBER(2,0) NOT NULL 
 ,  IND_TIPO NUMBER(1,0) NOT NULL 
 ,  DES_SISTEMA VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICG_SISTEMA IS 'Sistemas de Conexion'
/
COMMENT ON COLUMN  ICG_SISTEMA.COD_SISTEMA IS 'codigo de sistema'
/
COMMENT ON COLUMN  ICG_SISTEMA.COD_LENGUAJE IS 'codigo de lenguaje'
/
COMMENT ON COLUMN  ICG_SISTEMA.COD_INTERFAZ IS 'codigo de interfaz'
/
COMMENT ON COLUMN  ICG_SISTEMA.COD_ESTADO IS 'estado del sistema'
/
COMMENT ON COLUMN  ICG_SISTEMA.IND_TIPO IS 'tipo de sistema'
/
COMMENT ON COLUMN  ICG_SISTEMA.DES_SISTEMA IS 'descripcion del sistema'
/
