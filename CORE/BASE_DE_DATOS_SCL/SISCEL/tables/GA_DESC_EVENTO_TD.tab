CREATE TABLE GA_DESC_EVENTO_TD
(
  COD_EVENTO    VARCHAR2(30 BYTE)               NOT NULL,
  COD_MODULO    VARCHAR2(2 BYTE)                NOT NULL,
  COD_PROGRAMA  VARCHAR2(3 BYTE)                NOT NULL,
  COD_PROCESO   VARCHAR2(7 BYTE)                NOT NULL,
  DES_OBJETO    VARCHAR2(100 BYTE)              NOT NULL
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   16
MAXTRANS   255
STORAGE    (
            INITIAL          160K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
NOMONITORING
/

COMMENT ON TABLE GA_DESC_EVENTO_TD IS 'Dominio de Eventos a Registrar'
/
COMMENT ON COLUMN GA_DESC_EVENTO_TD.COD_EVENTO IS 'Código de evento  generado.'
/
COMMENT ON COLUMN GA_DESC_EVENTO_TD.COD_MODULO IS 'Código de modulo'
/
COMMENT ON COLUMN GA_DESC_EVENTO_TD.COD_PROGRAMA IS 'Código de programa auditado'
/
COMMENT ON COLUMN GA_DESC_EVENTO_TD.COD_PROCESO IS 'Código de funcionalidad auditada.'
/
COMMENT ON COLUMN GA_DESC_EVENTO_TD.DES_OBJETO IS 'Descripción de evento auditado'
/