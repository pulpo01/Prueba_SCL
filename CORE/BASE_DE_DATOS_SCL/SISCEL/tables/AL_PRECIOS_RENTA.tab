CREATE TABLE AL_PRECIOS_RENTA(
    TIP_STOCK NUMBER(2,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_ESTADO NUMBER(2,0) NOT NULL 
 ,  COD_PERIODO VARCHAR2(2) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  PRC_RENTA NUMBER(14,4) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_PRECIOS_RENTA IS 'Tarifas Rentaphone de Articulos'
/
COMMENT ON COLUMN  AL_PRECIOS_RENTA.TIP_STOCK IS 'Tipo de Stock'
/
COMMENT ON COLUMN  AL_PRECIOS_RENTA.COD_ARTICULO IS 'Codigo de Articulo'
/
COMMENT ON COLUMN  AL_PRECIOS_RENTA.COD_USO IS 'Codigo de Uso'
/
COMMENT ON COLUMN  AL_PRECIOS_RENTA.COD_ESTADO IS 'Codigo de Estado'
/
COMMENT ON COLUMN  AL_PRECIOS_RENTA.COD_PERIODO IS 'Codigo periodo Rentaphone'
/
COMMENT ON COLUMN  AL_PRECIOS_RENTA.COD_MONEDA IS 'Codigo de Moneda'
/
COMMENT ON COLUMN  AL_PRECIOS_RENTA.PRC_RENTA IS 'Precio Rentaphone'
/
