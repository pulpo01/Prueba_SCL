CREATE GLOBAL TEMPORARY TABLE SISCEL.VE_SERVSUPL_TEMP_TT
(
  SECUENCIA        NUMBER(8)                    NOT NULL,
  COD_SERVICIO     VARCHAR2(3 BYTE),
  COD_SERVSUPL     NUMBER(2),
  DES_SERVICIO     VARCHAR2(40 BYTE),
  COD_NIVEL        NUMBER(4),
  DES_TIPSS        VARCHAR2(20 BYTE),
  CARGO_CONEXION   NUMBER(14,4),
  MONEDA_CONEXION  VARCHAR2(20 BYTE),
  TARIFA_MENSUAL   NUMBER(14,4),
  MONEDA_MENSUAL   VARCHAR2(20 BYTE),
  IND_IP           NUMBER(1)                    DEFAULT '0',
  TIP_RED          VARCHAR2(1 BYTE),
  TIP_COBRO        VARCHAR2(1 BYTE)
)
ON COMMIT PRESERVE ROWS
NOCACHE;

COMMENT ON TABLE SISCEL.VE_SERVSUPL_TEMP_TT IS '[ SESU ] Tabla transacional temporal contiene SS por defecto al plan, actuación y adicionales';

COMMENT ON COLUMN SISCEL.VE_SERVSUPL_TEMP_TT.IND_IP IS 'Tipo de IP: Dinámica: 0, Fija:1 ';

COMMENT ON COLUMN SISCEL.VE_SERVSUPL_TEMP_TT.TIP_RED IS 'Tipo de Red F:Fija M:Movil';

COMMENT ON COLUMN SISCEL.VE_SERVSUPL_TEMP_TT.TIP_COBRO IS 'A: Cobro Adelantado V: Cobro Vencido';

COMMENT ON COLUMN SISCEL.VE_SERVSUPL_TEMP_TT.SECUENCIA IS 'Secuencia de consulta';

COMMENT ON COLUMN SISCEL.VE_SERVSUPL_TEMP_TT.COD_SERVICIO IS 'Código de Servicio';

COMMENT ON COLUMN SISCEL.VE_SERVSUPL_TEMP_TT.COD_SERVSUPL IS 'Código de identificación del SS de central';

COMMENT ON COLUMN SISCEL.VE_SERVSUPL_TEMP_TT.DES_SERVICIO IS 'Descripción del servicio suplementario';

COMMENT ON COLUMN SISCEL.VE_SERVSUPL_TEMP_TT.COD_NIVEL IS 'Nivel de Servicio Suplementario';

COMMENT ON COLUMN SISCEL.VE_SERVSUPL_TEMP_TT.DES_TIPSS IS 'Tipo de Servicio: Default, Tecnología u Opcional';

COMMENT ON COLUMN SISCEL.VE_SERVSUPL_TEMP_TT.CARGO_CONEXION IS 'Importe Tarifa conexión SS';

COMMENT ON COLUMN SISCEL.VE_SERVSUPL_TEMP_TT.MONEDA_CONEXION IS 'Descripción de Moneda';

COMMENT ON COLUMN SISCEL.VE_SERVSUPL_TEMP_TT.TARIFA_MENSUAL IS 'Importe Tarifa Mensual SS';

COMMENT ON COLUMN SISCEL.VE_SERVSUPL_TEMP_TT.MONEDA_MENSUAL IS 'Descripción de Moneda';

