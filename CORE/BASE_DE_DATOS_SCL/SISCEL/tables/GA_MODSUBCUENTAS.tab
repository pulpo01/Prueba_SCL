CREATE TABLE GA_MODSUBCUENTAS(
    COD_SUBCUENTA VARCHAR2(15) NOT NULL 
 ,  FEC_MODIFICA DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  DES_SUBCUENTA VARCHAR2(30) NULL 
 ,  COD_DIRECCION NUMBER(20,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_MODSUBCUENTAS IS 'Modificaciones de Subcuentas'
/
COMMENT ON COLUMN  GA_MODSUBCUENTAS.COD_SUBCUENTA IS 'Codigo de Subcuenta'
/
COMMENT ON COLUMN  GA_MODSUBCUENTAS.FEC_MODIFICA IS 'Fecha de Modificacion'
/
COMMENT ON COLUMN  GA_MODSUBCUENTAS.NOM_USUARORA IS 'Usuario que Modifica'
/
COMMENT ON COLUMN  GA_MODSUBCUENTAS.DES_SUBCUENTA IS 'Descripcion de Subuenta'
/
COMMENT ON COLUMN  GA_MODSUBCUENTAS.COD_DIRECCION IS 'Codigo de Direccion'
/
