CREATE TABLE CMD_DETMETAS(
    NUM_METAPARCIAL NUMBER(6,0) NOT NULL 
 ,  NUM_META NUMBER(6,0) NOT NULL 
 ,  VAL_METAPARCIAL NUMBER(6,0) NOT NULL 
 ,  COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMD_DETMETAS IS 'Detalle De Evaluacion De Metas Anuales.'
/
COMMENT ON COLUMN  CMD_DETMETAS.NUM_METAPARCIAL IS 'Identificador unico del registro de detalle de meta.'
/
COMMENT ON COLUMN  CMD_DETMETAS.NUM_META IS 'Identificador del registro de meta al que detalla.'
/
COMMENT ON COLUMN  CMD_DETMETAS.VAL_METAPARCIAL IS 'Valor de la meta parcial.'
/
COMMENT ON COLUMN  CMD_DETMETAS.COD_PERIODO IS 'Perriodo sobre el que se aplicara la meta parcial.'
/
COMMENT ON COLUMN  CMD_DETMETAS.FEC_ULTMOD IS 'Fecha de ultima modificacion del registro..'
/
COMMENT ON COLUMN  CMD_DETMETAS.NOM_USUARIO IS 'Usuario que hizo la ultima modificacion del registro en la tabla..'
/
