CREATE TABLE TA_CLASTARIOPER(
    COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  CLS_TARIFA VARCHAR2(3) NOT NULL 
 ,  DES_TARIFA VARCHAR2(30) NOT NULL 
 ,  TIP_CLASE NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  TIP_VALOR NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  VALOR NUMBER(12,4) DEFAULT 0 NOT NULL 
 ,  NOM_USUARIO VARCHAR2(40) DEFAULT 'TARIFICA' NOT NULL 
 ,  FEC_ULTIMAMOD DATE DEFAULT '01-jan-99' NOT NULL 
 ,  VALOR_SEG NUMBER(12,4) DEFAULT 0 NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_CLASTARIOPER IS 'Clases de Tarifas asociadas a los operadores'
/
COMMENT ON COLUMN  TA_CLASTARIOPER.COD_OPERADOR IS 'Codigo de Operador'
/
COMMENT ON COLUMN  TA_CLASTARIOPER.CLS_TARIFA IS 'Cls. Tarifa'
/
COMMENT ON COLUMN  TA_CLASTARIOPER.DES_TARIFA IS 'Descripcion Tarifa'
/
COMMENT ON COLUMN  TA_CLASTARIOPER.TIP_CLASE IS 'Indica si va en Estructura Tarifa'
/
COMMENT ON COLUMN  TA_CLASTARIOPER.TIP_VALOR IS 'Indica si Usuario Cambia Valor'
/
COMMENT ON COLUMN  TA_CLASTARIOPER.VALOR IS 'Valor de la Clase Tarifa'
/
COMMENT ON COLUMN  TA_CLASTARIOPER.NOM_USUARIO IS 'Almacena Nombre Usuario'
/
COMMENT ON COLUMN  TA_CLASTARIOPER.FEC_ULTIMAMOD IS 'Fecha Ultima Modificacion'
/
