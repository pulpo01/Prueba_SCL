CREATE TABLE GA_PLANUSO(
    COD_PLANTARIF VARCHAR2(3) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL
 ,  FLG_CONTRATOFIJO VARCHAR2(10 BYTE) default 'N' 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL

COMMENT ON COLUMN GA_PLANUSO.FLG_CONTRATOFIJO IS 'indica si la venta tiene abonados portados. 1 = solo tiene abonados portados, 0 solo abonados normales';

