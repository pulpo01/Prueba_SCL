CREATE TABLE CB_CASCADA_BAJA(
    TIP_ACCION VARCHAR2(20) NOT NULL 
 ,  FEC_ACCION DATE NOT NULL 
 ,  FEC_ORIGEN DATE NOT NULL 
 ,  TIP_CASCADA CHAR(1) NOT NULL 
 ,  CAN_VENTA VARCHAR2(30) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  CANTIDAD NUMBER(6,0) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CB_CASCADA_BAJA IS 'Cascada De Bajas.'
/
COMMENT ON COLUMN  CB_CASCADA_BAJA.TIP_ACCION IS 'Tipo de Accion.'
/
COMMENT ON COLUMN  CB_CASCADA_BAJA.FEC_ACCION IS 'Fecha de Accion.'
/
COMMENT ON COLUMN  CB_CASCADA_BAJA.FEC_ORIGEN IS 'Fecha de Origen.'
/
COMMENT ON COLUMN  CB_CASCADA_BAJA.TIP_CASCADA IS 'Identifica el tipo de cascada: B=Bajas | A=Altas.'
/
COMMENT ON COLUMN  CB_CASCADA_BAJA.CAN_VENTA IS 'Canal por el cual se realiza la venta.'
/
COMMENT ON COLUMN  CB_CASCADA_BAJA.COD_VENDEDOR IS 'Codigo del Vendedor.'
/
COMMENT ON COLUMN  CB_CASCADA_BAJA.CANTIDAD IS 'Cantidad Total del Periodo.'
/
