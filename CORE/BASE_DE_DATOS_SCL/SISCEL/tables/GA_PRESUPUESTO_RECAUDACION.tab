 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COLUMNA NUMBER(6,0) NOT NULL 
 ,  NUM_CELULAR VARCHAR2(15) NULL 
 ,  NUM_UNIDADES NUMBER(6,0) NULL 
 ,  MTO_GARANTIA NUMBER(14,4) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
