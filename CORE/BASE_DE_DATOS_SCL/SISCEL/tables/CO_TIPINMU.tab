CREATE TABLE CO_TIPINMU(
    COD_TIPINMU NUMBER(3,0) NOT NULL 
 ,  DES_TIPINMU VARCHAR2(255) NOT NULL 
 ,  IND_SUSPEN NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_TIPINMU IS 'Tipos de inmunidades en suspension en Cobros'
/
COMMENT ON COLUMN  CO_TIPINMU.COD_TIPINMU IS 'Codigo de inmunidad'
/
COMMENT ON COLUMN  CO_TIPINMU.DES_TIPINMU IS 'Descripcion de inmunidad'
/
COMMENT ON COLUMN  CO_TIPINMU.IND_SUSPEN IS 'Indicativo de suspension 1 o de rehabilitacion 0'
/
