CREATE TABLE FA_TIPMOVIMIEN(
    COD_TIPMOVIMIEN NUMBER(2,0) NOT NULL 
 ,  COD_CATRIBUT CHAR(1) NOT NULL 
 ,  COD_TIPIMPOSITIVA NUMBER(1,0) NOT NULL 
 ,  COD_TIPDOCUM NUMBER(2,0) NOT NULL 
 ,  COD_MODVENTA NUMBER(2,0) DEFAULT 1 NOT NULL 
 ,  TIP_FOLIACION VARCHAR2(1) DEFAULT 'P' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_TIPMOVIMIEN IS 'Tipos de Movimientos de Facturacion'
/
COMMENT ON COLUMN  FA_TIPMOVIMIEN.COD_TIPMOVIMIEN IS 'Tipo de movimiento de facturacion:  Venta | Miscelanea | Ciclo | Nota de Credito. [PK 1/3]'
/
COMMENT ON COLUMN  FA_TIPMOVIMIEN.COD_CATRIBUT IS 'Codigo de categoria tributaria:  F=Factura | B=Boleta.  [PK 2/3] '
/
COMMENT ON COLUMN  FA_TIPMOVIMIEN.COD_TIPIMPOSITIVA IS 'Tipo de categoria impositiva: 1=Afecto | 0=Exento. [PK 3/3] '
/
COMMENT ON COLUMN  FA_TIPMOVIMIEN.COD_TIPDOCUM IS 'Tipo de documento de facturacion'
/
COMMENT ON COLUMN  FA_TIPMOVIMIEN.COD_MODVENTA IS ' Modalidad de la venta.'
/
