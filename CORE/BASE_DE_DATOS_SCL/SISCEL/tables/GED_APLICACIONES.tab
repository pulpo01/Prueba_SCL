CREATE TABLE GED_APLICACIONES(
    COD_APLIC VARCHAR2(3) NOT NULL 
 ,  DESCRIPCION VARCHAR2(30) NOT NULL 
 ,  COD_MODULO VARCHAR2(2) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GED_APLICACIONES IS 'Aplicaciones Disponibles.'
/
COMMENT ON COLUMN  GED_APLICACIONES.COD_APLIC IS 'Código de Interfaz.'
/
COMMENT ON COLUMN  GED_APLICACIONES.DESCRIPCION IS 'Interfaz.'
/
COMMENT ON COLUMN  GED_APLICACIONES.COD_MODULO IS 'Modulo asociado a interfaz.'
/
