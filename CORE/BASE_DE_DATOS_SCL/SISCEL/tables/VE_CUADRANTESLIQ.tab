CREATE TABLE VE_CUADRANTESLIQ(
    COD_CUADRANTE NUMBER(8,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_CTOLIQ NUMBER(6,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  IND_PROCEDENCIA NUMBER(1,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_EFECTIVIDAD DATE NOT NULL 
 ,  IMP_BASE NUMBER(12,4) NULL 
 ,  NUM_LIMITE NUMBER(4,0) NULL 
 ,  FEC_FINEFECTIVIDAD DATE NULL 
 ,  COD_MONEDA VARCHAR2(3) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CUADRANTESLIQ IS 'Cabecera de Cuadrantes de Liquidacion de Comisiones'
/
COMMENT ON COLUMN  VE_CUADRANTESLIQ.COD_CUADRANTE IS 'Codigo de Cuadrante'
/
COMMENT ON COLUMN  VE_CUADRANTESLIQ.COD_TIPCOMIS IS 'Codigo de Tipo de Comisionista'
/
COMMENT ON COLUMN  VE_CUADRANTESLIQ.COD_CTOLIQ IS 'Codigo de Concepto Liquidacion'
/
COMMENT ON COLUMN  VE_CUADRANTESLIQ.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  VE_CUADRANTESLIQ.IND_PROCEDENCIA IS 'Indicativo de procedencia del comisionista (Interno/Externo)'
/
COMMENT ON COLUMN  VE_CUADRANTESLIQ.NOM_USUARIO IS 'Usuario que define el cuadrante'
/
COMMENT ON COLUMN  VE_CUADRANTESLIQ.FEC_EFECTIVIDAD IS 'Fecha de entrada en vigor del cuadrante'
/
COMMENT ON COLUMN  VE_CUADRANTESLIQ.IMP_BASE IS 'Importe Base'
/
COMMENT ON COLUMN  VE_CUADRANTESLIQ.NUM_LIMITE IS 'Limite (bono de movilizacion)'
/
COMMENT ON COLUMN  VE_CUADRANTESLIQ.FEC_FINEFECTIVIDAD IS 'Fecha de expiracion del cuadrante'
/
COMMENT ON COLUMN  VE_CUADRANTESLIQ.COD_MONEDA IS 'Moneda a utlizar por un concepto liquidacion.'
/
