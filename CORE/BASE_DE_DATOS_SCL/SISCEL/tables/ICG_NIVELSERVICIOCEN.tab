CREATE TABLE ICG_NIVELSERVICIOCEN(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_SERVICIOCEN NUMBER(2,0) NOT NULL 
 ,  COD_LENGUAJE NUMBER(3,0) NOT NULL 
 ,  COD_NIVELCEN NUMBER(4,0) NOT NULL 
 ,  DES_NIVELCEN VARCHAR2(50) NOT NULL 
 ,  CAD_NIVELCEN VARCHAR2(50) NULL 
 ,  ACT_NIVELCEN VARCHAR2(50) NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICG_NIVELSERVICIOCEN IS 'Niveles de Servicio en Central'
/
COMMENT ON COLUMN  ICG_NIVELSERVICIOCEN.COD_PRODUCTO IS 'codigo de producto'
/
COMMENT ON COLUMN  ICG_NIVELSERVICIOCEN.COD_SERVICIOCEN IS 'codigo de servicio en central'
/
COMMENT ON COLUMN  ICG_NIVELSERVICIOCEN.COD_LENGUAJE IS 'codigo del lenguaje de la central'
/
COMMENT ON COLUMN  ICG_NIVELSERVICIOCEN.COD_NIVELCEN IS 'codigo del nivel en central'
/
COMMENT ON COLUMN  ICG_NIVELSERVICIOCEN.DES_NIVELCEN IS 'descripcion del nivel de servicio'
/
COMMENT ON COLUMN  ICG_NIVELSERVICIOCEN.CAD_NIVELCEN IS 'cadena para modificar el nivel del servicio'
/
COMMENT ON COLUMN  ICG_NIVELSERVICIOCEN.ACT_NIVELCEN IS 'cadena para activar o desacivar el servicio'
/
