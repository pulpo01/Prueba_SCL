CREATE TABLE VE_DTOLLAM_FRANJAS(
    COD_DTOLLAM NUMBER(6,0) NOT NULL 
 ,  COD_FRANJA NUMBER(6,0) NOT NULL 
 ,  FEC_EFECTIVIDAD DATE NOT NULL 
 ,  FEC_FINEFECTIVIDAD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_DTOLLAM_FRANJAS IS 'Franjas de Descuento asociadas a Plan de Descuento de Llamadas'
/
COMMENT ON COLUMN  VE_DTOLLAM_FRANJAS.COD_DTOLLAM IS 'Codigo Plan Descuento Llamadas'
/
COMMENT ON COLUMN  VE_DTOLLAM_FRANJAS.COD_FRANJA IS 'Codigo de Franja horaria'
/
COMMENT ON COLUMN  VE_DTOLLAM_FRANJAS.FEC_EFECTIVIDAD IS 'Fecha Inicio Efectividad'
/
COMMENT ON COLUMN  VE_DTOLLAM_FRANJAS.FEC_FINEFECTIVIDAD IS 'Fecha Fin Efectividad'
/
COMMENT ON COLUMN  VE_DTOLLAM_FRANJAS.NOM_USUARIO IS 'Usuario que asocia las franjas'
/
