CREATE TABLE CO_PAGOENTIDAD(
    COD_TIPDOCUM NUMBER(2,0) NOT NULL 
 ,  COD_CENTREMI NUMBER(4,0) NOT NULL 
 ,  NUM_SECUENCI NUMBER(8,0) NOT NULL 
 ,  COD_VENDEDOR_AGENTE NUMBER(6,0) NOT NULL 
 ,  LETRA VARCHAR2(1) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  IMP_PAGO NUMBER(14,4) NOT NULL 
 ,  NUM_EJERCICIO VARCHAR2(12) NOT NULL 
 ,  COD_CAJA NUMBER(4,0) NOT NULL 
 ,  COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  FEC_EFECTIVIDAD DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_PAGOENTIDAD IS 'Pagos asociados a entidades'
/
COMMENT ON COLUMN  CO_PAGOENTIDAD.COD_TIPDOCUM IS 'TIPO DE DOCUMENTO'
/
COMMENT ON COLUMN  CO_PAGOENTIDAD.COD_CENTREMI IS 'CODIGO DEL CENTRO EMISOR'
/
COMMENT ON COLUMN  CO_PAGOENTIDAD.NUM_SECUENCI IS 'NUMERO DE SECUENCIA'
/
COMMENT ON COLUMN  CO_PAGOENTIDAD.COD_VENDEDOR_AGENTE IS 'CODIGO DE AGENTE'
/
COMMENT ON COLUMN  CO_PAGOENTIDAD.LETRA IS 'LETRA'
/
COMMENT ON COLUMN  CO_PAGOENTIDAD.COD_CLIENTE IS 'CODIGO DE CLIENTE'
/
COMMENT ON COLUMN  CO_PAGOENTIDAD.IMP_PAGO IS 'IMPORTE DEL PAGO'
/
COMMENT ON COLUMN  CO_PAGOENTIDAD.NUM_EJERCICIO IS 'Numero de ejecicio de movimientos de caja'
/
COMMENT ON COLUMN  CO_PAGOENTIDAD.COD_CAJA IS 'Codigo de Caja - Retrofitted'
/
COMMENT ON COLUMN  CO_PAGOENTIDAD.COD_OFICINA IS 'Codigo de oficina'
/
COMMENT ON COLUMN  CO_PAGOENTIDAD.FEC_EFECTIVIDAD IS 'FECHA DE EFECTIVIDAD'
/
COMMENT ON COLUMN  CO_PAGOENTIDAD.NOM_USUARORA IS 'NOMBRE DE USUARIO'
/
