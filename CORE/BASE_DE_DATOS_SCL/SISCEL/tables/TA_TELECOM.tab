CREATE TABLE TA_TELECOM(
    COD_TIPCENTRAL NUMBER(2,0) NOT NULL 
 ,  NUM_BLOQUE VARCHAR2(9) NOT NULL 
 ,  SEC_TICKETS NUMBER(10,0) NOT NULL 
 ,  NUM_MOVIL1 VARCHAR2(18) NOT NULL 
 ,  NUM_NSE1 VARCHAR2(10) NULL 
 ,  NUM_MOVIL2 VARCHAR2(18) NOT NULL 
 ,  IND_ENTSAL NUMBER(1,0) NOT NULL 
 ,  FEC_LLAMADA DATE NOT NULL 
 ,  HOR_LLAMADA VARCHAR2(6) NOT NULL 
 ,  TIE_DURTASA NUMBER(6,0) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_TELECOM IS 'Llamadas que deberian tasarse a abonados Telecom'
/
COMMENT ON COLUMN  TA_TELECOM.COD_TIPCENTRAL IS 'Codigo de tipo de central'
/
COMMENT ON COLUMN  TA_TELECOM.NUM_BLOQUE IS 'Numero de Bloque'
/
COMMENT ON COLUMN  TA_TELECOM.SEC_TICKETS IS 'Secuencia de tickets'
/
COMMENT ON COLUMN  TA_TELECOM.NUM_MOVIL1 IS 'Numero de telefono que paga'
/
COMMENT ON COLUMN  TA_TELECOM.NUM_NSE1 IS 'Numero electronico de serie'
/
COMMENT ON COLUMN  TA_TELECOM.NUM_MOVIL2 IS 'Numero de telefono que no paga'
/
COMMENT ON COLUMN  TA_TELECOM.IND_ENTSAL IS 'Si es entrante, saliente o trasferencia'
/
COMMENT ON COLUMN  TA_TELECOM.FEC_LLAMADA IS 'Fecha de la llamada'
/
COMMENT ON COLUMN  TA_TELECOM.HOR_LLAMADA IS 'Hora de la llamada'
/
COMMENT ON COLUMN  TA_TELECOM.TIE_DURTASA IS 'Duracion de la tasacion'
/
