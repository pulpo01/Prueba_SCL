CREATE TABLE GA_MOV_TRASPASO_TD(
    COD_PRODUCTO NUMBER(1,0) DEFAULT 1 NOT NULL 
 ,  COD_ACTABO VARCHAR2(2) NOT NULL 
 ,  TIP_MOVIMIENTO VARCHAR2(2) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_MOV_TRASPASO_TD IS 'Tipos de Movmientos de Traspaso Permitidos'
/
COMMENT ON COLUMN  GA_MOV_TRASPASO_TD.COD_PRODUCTO IS 'Código de Producto'
/
COMMENT ON COLUMN  GA_MOV_TRASPASO_TD.COD_ACTABO IS 'Actuación comercial de la postventa'
/
COMMENT ON COLUMN  GA_MOV_TRASPASO_TD.TIP_MOVIMIENTO IS 'Sus valores son cualquier combinación de las letras E, I y F. Se interpretan de la sgte forma:I = IndivualE = EmpresaF = FamiliarDe izquierda a derecha el primer carácter representa el tipo de plan tarifario del origen y el segundo el del destino.'
/
COMMENT ON COLUMN  GA_MOV_TRASPASO_TD.FEC_DESDE IS 'Inicio de la vigencia'
/
COMMENT ON COLUMN  GA_MOV_TRASPASO_TD.FEC_HASTA IS 'Término de la vigencia, un null indica que esta vigente a la fecha'
/
COMMENT ON COLUMN  GA_MOV_TRASPASO_TD.NOM_USUARORA IS 'Usuario oracle que modifica'
/
