CREATE TABLE AL_LIN_MOVI_LOCA(
    NUM_MOVI NUMBER(9,0) NOT NULL 
 ,  NUM_LINEA NUMBER(3,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  CAN_MOVI NUMBER(9,0) NOT NULL 
 ,  NUM_SEC_ANT NUMBER(10,0) NULL 
 ,  NUM_SEC_NUE NUMBER(10,0) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_LIN_MOVI_LOCA IS 'Linea De Detalle De Relocalizacion.'
/
COMMENT ON COLUMN  AL_LIN_MOVI_LOCA.NUM_MOVI IS 'Numero de movimiento de Relocalizacion.'
/
COMMENT ON COLUMN  AL_LIN_MOVI_LOCA.NUM_LINEA IS 'Numero de linea.'
/
COMMENT ON COLUMN  AL_LIN_MOVI_LOCA.COD_ARTICULO IS 'Codigo de articulo.'
/
COMMENT ON COLUMN  AL_LIN_MOVI_LOCA.CAN_MOVI IS 'Cantidad de movimiento de articulo.'
/
COMMENT ON COLUMN  AL_LIN_MOVI_LOCA.NUM_SEC_ANT IS 'Identificador de Localizacion Anterior.'
/
COMMENT ON COLUMN  AL_LIN_MOVI_LOCA.NUM_SEC_NUE IS 'Identificador de Localizacion Nueva.'
/
