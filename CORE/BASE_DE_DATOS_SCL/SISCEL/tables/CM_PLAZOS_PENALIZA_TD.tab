CREATE TABLE CM_PLAZOS_PENALIZA_TD(
    COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  TIP_PLAN VARCHAR2(5) NOT NULL 
 ,  COD_CATEGCLIENTE VARCHAR2(10) NOT NULL 
 ,  DIAS_DESDE NUMBER(6,0) NOT NULL 
 ,  DIAS_HASTA NUMBER(6,0) NOT NULL 
 ,  IMP_PENALIZA NUMBER(14,4) DEFAULT '0' NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) DEFAULT 'C' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_PLAZOS_PENALIZA_TD IS 'Detalle De Plazos De Penalizacion .'
/
COMMENT ON COLUMN  CM_PLAZOS_PENALIZA_TD.COD_CONCEPTO IS 'CONCEPTO DE PENALIZACION.'
/
COMMENT ON COLUMN  CM_PLAZOS_PENALIZA_TD.TIP_PLAN IS 'TIPO DE PLAN A COMISIONAR.'
/
COMMENT ON COLUMN  CM_PLAZOS_PENALIZA_TD.COD_CATEGCLIENTE IS 'CATEGORIA DE CLIENTE A COMISIONAR.'
/
COMMENT ON COLUMN  CM_PLAZOS_PENALIZA_TD.DIAS_DESDE IS 'IMPORTE DE COMISION.'
/
COMMENT ON COLUMN  CM_PLAZOS_PENALIZA_TD.DIAS_HASTA IS 'SOBRE IMPORTE DE EQUIPO.'
/
COMMENT ON COLUMN  CM_PLAZOS_PENALIZA_TD.IMP_PENALIZA IS 'FECHA DE ULTIMA MODIFICACION.'
/
COMMENT ON COLUMN  CM_PLAZOS_PENALIZA_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
COMMENT ON COLUMN  CM_PLAZOS_PENALIZA_TD.COD_PLANCOMIS IS 'PLAN DE COMISIONES QUE CONTIENEN AL CONCEPTO.'
/
