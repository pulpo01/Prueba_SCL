CREATE TABLE VE_VENDCLIENTE(
    COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  FEC_ASIGNACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_DESASIGNAC DATE NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_VENDCLIENTE IS 'EGCs asociados a Clientes por clasificacion de Cuenta'
/
COMMENT ON COLUMN  VE_VENDCLIENTE.COD_VENDEDOR IS 'Codigo de Comisionista'
/
COMMENT ON COLUMN  VE_VENDCLIENTE.COD_CLIENTE IS 'Codigo de Cliente al que se asocia el Vendedor'
/
COMMENT ON COLUMN  VE_VENDCLIENTE.FEC_ASIGNACION IS 'Fecha de asignacion'
/
COMMENT ON COLUMN  VE_VENDCLIENTE.NOM_USUARIO IS 'Usuario que realiza la asignacion'
/
COMMENT ON COLUMN  VE_VENDCLIENTE.FEC_DESASIGNAC IS 'Fecha de desasignacion'
/
