CREATE TABLE CRT_INGCORRESPONDENCIA(
    NUM_INGRESO NUMBER(10,0) NOT NULL 
 ,  COD_TIPCOR NUMBER(3,0) NOT NULL 
 ,  COD_AREA NUMBER(2,0) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NULL 
 ,  NOM_CLIENTE VARCHAR2(60) NOT NULL 
 ,  NUM_TERMINAL NUMBER(10,0) NULL 
 ,  DESCRIPCION VARCHAR2(100) NOT NULL 
 ,  FEC_INI_GESTION DATE NULL 
 ,  FEC_FIN_GESTION_ESTIMADA DATE NULL 
 ,  FEC_FIN_GESTION_REAL DATE NULL 
 ,  NUM_RECLAMO NUMBER(10,0) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CRT_INGCORRESPONDENCIA IS 'Correspondencia Entrante.'
/
COMMENT ON COLUMN  CRT_INGCORRESPONDENCIA.NUM_INGRESO IS 'Numero de ingreso de correspondencia.'
/
COMMENT ON COLUMN  CRT_INGCORRESPONDENCIA.COD_TIPCOR IS 'Tipo de correspondencia.'
/
COMMENT ON COLUMN  CRT_INGCORRESPONDENCIA.COD_AREA IS 'Area donde se deriva la correspondencia.'
/
COMMENT ON COLUMN  CRT_INGCORRESPONDENCIA.FEC_INGRESO IS 'Fecha de ingreso de correspondencia.'
/
COMMENT ON COLUMN  CRT_INGCORRESPONDENCIA.COD_CLIENTE IS 'Codigo de cliente.'
/
COMMENT ON COLUMN  CRT_INGCORRESPONDENCIA.NOM_CLIENTE IS 'Nombre del cliente.'
/
COMMENT ON COLUMN  CRT_INGCORRESPONDENCIA.NUM_TERMINAL IS 'Numero de terminal.'
/
COMMENT ON COLUMN  CRT_INGCORRESPONDENCIA.DESCRIPCION IS 'Descripcion de la correspondencia entrante.'
/
COMMENT ON COLUMN  CRT_INGCORRESPONDENCIA.FEC_INI_GESTION IS 'Fecha de inicio de gestion.'
/
COMMENT ON COLUMN  CRT_INGCORRESPONDENCIA.FEC_FIN_GESTION_ESTIMADA IS 'Fecha de fin de gestion estimada.'
/
COMMENT ON COLUMN  CRT_INGCORRESPONDENCIA.FEC_FIN_GESTION_REAL IS 'Fecha de fin de gestion real.'
/
COMMENT ON COLUMN  CRT_INGCORRESPONDENCIA.NUM_RECLAMO IS 'Numero de reclamo de la correspondencia.'
/
