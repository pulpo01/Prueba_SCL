CREATE TABLE GAT_RECARGAS(
    NUM_SECUENCIA NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  FEC_INGRESO DATE DEFAULT sysdate NOT NULL 
 ,  FEC_RECARGA DATE NOT NULL 
 ,  MTO_RECARGA NUMBER(14,4) NOT NULL 
 ,  COD_ORIGEN VARCHAR2(5) NOT NULL 
 ,  COD_SERVICIO VARCHAR2(5) NULL 
 ,  NUM_SERIE VARCHAR2(25) NULL 
 ,  NUM_CLAVE VARCHAR2(16) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAT_RECARGAS IS 'Recargas De Tarjetas.'
/
COMMENT ON COLUMN  GAT_RECARGAS.NUM_SECUENCIA IS 'Secuencia de Recarga.'
/
COMMENT ON COLUMN  GAT_RECARGAS.NUM_ABONADO IS 'Abonado.'
/
COMMENT ON COLUMN  GAT_RECARGAS.NUM_CELULAR IS 'Celular.'
/
COMMENT ON COLUMN  GAT_RECARGAS.FEC_INGRESO IS 'Fecha de ingreso a la tabla.'
/
COMMENT ON COLUMN  GAT_RECARGAS.FEC_RECARGA IS 'fecha de la recarga.'
/
COMMENT ON COLUMN  GAT_RECARGAS.MTO_RECARGA IS 'Monto de recarga.'
/
COMMENT ON COLUMN  GAT_RECARGAS.COD_ORIGEN IS 'Origen de la recarga.'
/
COMMENT ON COLUMN  GAT_RECARGAS.COD_SERVICIO IS 'Servicio asociado a la recarga.'
/
COMMENT ON COLUMN  GAT_RECARGAS.NUM_SERIE IS 'pk de cet_transaccion.'
/
COMMENT ON COLUMN  GAT_RECARGAS.NUM_CLAVE IS 'pk de cet_transaccion.'
/
