CREATE TABLE GA_DET_MOVIVOX(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  NUM_TELEFONO VARCHAR2(15) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  IND_PROCESO CHAR(1) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_DET_MOVIVOX IS 'Detalle de cada telefono MoviVox que posea el abonado'
/
COMMENT ON COLUMN  GA_DET_MOVIVOX.NUM_ABONADO IS 'Numero de abonado MoviVox'
/
COMMENT ON COLUMN  GA_DET_MOVIVOX.NUM_TELEFONO IS 'Numero de telefono MoviVox'
/
COMMENT ON COLUMN  GA_DET_MOVIVOX.FEC_ALTA IS 'Fecha de ingreso'
/
COMMENT ON COLUMN  GA_DET_MOVIVOX.FEC_ULTMOD IS 'Fecha de ultima modificacion'
/
COMMENT ON COLUMN  GA_DET_MOVIVOX.IND_PROCESO IS 'Indicador de procesado (en plataforma): I=Ingresado, M=Modificado, E=Eliminado, N=Normal'
/
