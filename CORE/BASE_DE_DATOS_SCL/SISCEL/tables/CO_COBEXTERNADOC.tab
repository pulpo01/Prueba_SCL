CREATE TABLE CO_COBEXTERNADOC
(
  NUM_IDENT        VARCHAR2(20 BYTE)            NOT NULL,
  COD_TIPIDENT     VARCHAR2(2 BYTE)             NOT NULL,
  COD_CLIENTE      NUMBER(8)                    NOT NULL,
  NUM_ABONADO      NUMBER(8)                    NOT NULL,
  NUM_FOLIO        NUMBER(9)                    NOT NULL,
  COD_TIPDOCUM     NUMBER(2)                    NOT NULL,
  NUM_CUOTAS       NUMBER(2)                    NOT NULL,
  SEC_CUOTA        NUMBER(2)                    NOT NULL,
  FEC_VENCIMIE     DATE                         NOT NULL,
  FEC_EFECTIVIDAD  DATE                         NOT NULL,
  IMPORTE_DEBE     NUMBER(14,4)                 NOT NULL,
  IMPORTE_HABER    NUMBER(14,4)                 NOT NULL,
  IND_INFORMADO    CHAR(1 BYTE),
  COD_ENTIDAD      VARCHAR2(5 BYTE),
  COD_CONCEPTO     NUMBER(4),
  PREF_PLAZA       VARCHAR2(25 BYTE)
)
TABLESPACE TS_RECYCOB_SDAT
PCTUSED    0
PCTFREE    5
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          33800K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON COLUMN CO_COBEXTERNADOC.COD_CLIENTE IS 'Codigo de Cliente';

COMMENT ON COLUMN CO_COBEXTERNADOC.IND_INFORMADO IS 'Indicador de documento informado a cobranza externa';

COMMENT ON COLUMN CO_COBEXTERNADOC.COD_ENTIDAD IS 'Entidad de Cobranza';

COMMENT ON COLUMN CO_COBEXTERNADOC.COD_CONCEPTO IS 'Concepto de Documento';

COMMENT ON COLUMN CO_COBEXTERNADOC.PREF_PLAZA IS 'Prefijo Plaza';

