CREATE TABLE SP_REPORTES(
    NOM_REPORTE VARCHAR2(15) DEFAULT '0' NOT NULL 
 ,  COD_OPERAD VARCHAR2(5) DEFAULT '0' NOT NULL 
 ,  COD_IDIOMA VARCHAR2(5) DEFAULT '0' NOT NULL 
 ,  NOM_CAMPO VARCHAR2(20) DEFAULT '0' NOT NULL 
 ,  NOM_FORMULA VARCHAR2(20) DEFAULT '0' NULL 
 ,  TEX_CAMPO VARCHAR2(4000) DEFAULT '0' NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  SP_REPORTES.NOM_REPORTE IS 'NOMBRE DEL REPORTE'
/
COMMENT ON COLUMN  SP_REPORTES.COD_OPERAD IS 'OPERADORA'
/
COMMENT ON COLUMN  SP_REPORTES.COD_IDIOMA IS 'IDIOMA'
/
COMMENT ON COLUMN  SP_REPORTES.NOM_CAMPO IS 'CAMPO EN EL REPORTE'
/
COMMENT ON COLUMN  SP_REPORTES.NOM_FORMULA IS 'FORMULA EN EL REPORTE'
/
COMMENT ON COLUMN  SP_REPORTES.TEX_CAMPO IS 'VALOR DE LA FORMULA EN EL REPORTE'
/
