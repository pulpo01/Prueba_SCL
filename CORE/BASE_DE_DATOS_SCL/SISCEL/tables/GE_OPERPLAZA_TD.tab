CREATE TABLE GE_OPERPLAZA_TD
(
  COD_OPERPLAZA      NUMBER(8)                  NOT NULL,
  COD_OPERADORA_SCL  VARCHAR2(5 BYTE)           NOT NULL,
  COD_PLAZA          VARCHAR2(5 BYTE)           NOT NULL,
  COD_DIRECCION      NUMBER(20)                 NOT NULL,
  PREF_PLAZA         VARCHAR2(25 BYTE),
  COD_CLIENTE        NUMBER(8),
  COD_OFICINA        VARCHAR2(2 BYTE)
)
TABLESPACE TS_GENERAL_SDAT
PCTUSED    0
PCTFREE    5
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          40K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
CACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE GE_OPERPLAZA_TD IS 'Plazas por OPeradora.';

COMMENT ON COLUMN GE_OPERPLAZA_TD.COD_OPERPLAZA IS 'Codigo de Operadora-Plaza.';

COMMENT ON COLUMN GE_OPERPLAZA_TD.COD_OPERADORA_SCL IS 'Codigo de Operadora.';

COMMENT ON COLUMN GE_OPERPLAZA_TD.COD_PLAZA IS 'Codigo de Plaza.';

COMMENT ON COLUMN GE_OPERPLAZA_TD.COD_DIRECCION IS 'Codigo de direccion de Operadora-Plaza.';

COMMENT ON COLUMN GE_OPERPLAZA_TD.PREF_PLAZA IS 'Prefijo de la plaza.';

COMMENT ON COLUMN GE_OPERPLAZA_TD.COD_OFICINA IS 'CENTRO EMISOR';

