CREATE TABLE AL_DETALLE_CAL_PMP_TH(
    COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  FEC_PERIODO DATE NOT NULL 
 ,  COD_OPERADORA_SCL VARCHAR2(5) NOT NULL 
 ,  COD_BODEGA NUMBER(6,0) NOT NULL 
 ,  FEC_DIA_PMP DATE NOT NULL 
 ,  PRC_UNIDAD NUMBER(14,4) NOT NULL 
 ,  CAN_STOCK NUMBER(14,0) NULL 
 ,  CAN_SALIDAS NUMBER(14,0) NULL 
 ,  CAN_ENTRADAS NUMBER(14,0) NULL 
 ,  CAN_INGRESOS NUMBER(14,0) NULL 
 ) TABLESPACE    TS_LOGISTICA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_DETALLE_CAL_PMP_TH IS '[ DECA ] Mantiene el detalle del calculo del PMP diferenciado por bodegas'
/
COMMENT ON COLUMN  AL_DETALLE_CAL_PMP_TH.COD_ARTICULO IS 'Código Articulo'
/
COMMENT ON COLUMN  AL_DETALLE_CAL_PMP_TH.FEC_PERIODO IS 'Fecha PMP'
/
COMMENT ON COLUMN  AL_DETALLE_CAL_PMP_TH.COD_OPERADORA_SCL IS 'Código Operadora'
/
COMMENT ON COLUMN  AL_DETALLE_CAL_PMP_TH.COD_BODEGA IS 'Codigo Bodega'
/
COMMENT ON COLUMN  AL_DETALLE_CAL_PMP_TH.FEC_DIA_PMP IS 'Fecha del PMP'
/
COMMENT ON COLUMN  AL_DETALLE_CAL_PMP_TH.PRC_UNIDAD IS 'Precio Unitario del Articulo'
/
COMMENT ON COLUMN  AL_DETALLE_CAL_PMP_TH.CAN_STOCK IS 'Cantidad de Stock al momento del calculo del PMP'
/
COMMENT ON COLUMN  AL_DETALLE_CAL_PMP_TH.CAN_SALIDAS IS 'Cantidad de mercadería sacada de inventario en la fecha de proceso'
/
COMMENT ON COLUMN  AL_DETALLE_CAL_PMP_TH.CAN_ENTRADAS IS 'Cantidad de mercadería ingresada a inventario en la fecha de proceso'
/
COMMENT ON COLUMN  AL_DETALLE_CAL_PMP_TH.CAN_INGRESOS IS 'Cantidad de mercadería ingresada a inventario en la fecha de proceso, a través, O/I y D/I'
/
