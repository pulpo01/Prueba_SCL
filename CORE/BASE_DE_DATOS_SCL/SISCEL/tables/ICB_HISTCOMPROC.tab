CREATE TABLE ICB_HISTCOMPROC(
    NUM_MOVIMIENTO NUMBER(9,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(3,0) NOT NULL 
 ,  FEC_HISTORICO DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_LENGUAJE NUMBER(3,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  COD_SISTEMA NUMBER(2,0) NOT NULL 
 ,  COD_ACT VARCHAR2(2) NULL 
 ,  COD_COMANDO NUMBER(3,0) NOT NULL 
 ,  DES_COMANDO VARCHAR2(512) NOT NULL 
 ,  FEC_EJECUCION DATE NULL 
 ,  FEC_INGRESO DATE DEFAULT SYSDATE NOT NULL 
 ,  DES_RESPUESTA VARCHAR2(80) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICB_HISTCOMPROC IS 'Historico comandos procesados beepers'
/
COMMENT ON COLUMN  ICB_HISTCOMPROC.NUM_MOVIMIENTO IS 'Numero de Movimiento Beepers'
/
COMMENT ON COLUMN  ICB_HISTCOMPROC.NUM_ORDEN IS 'Numero de Orden de Ejecucion'
/
COMMENT ON COLUMN  ICB_HISTCOMPROC.FEC_HISTORICO IS 'Fecha de Ingreso en la Tabla'
/
COMMENT ON COLUMN  ICB_HISTCOMPROC.COD_LENGUAJE IS 'Lenguaje del Comando Utilizado'
/
COMMENT ON COLUMN  ICB_HISTCOMPROC.COD_CENTRAL IS 'Central en la que se ejecuto el comando'
/
COMMENT ON COLUMN  ICB_HISTCOMPROC.COD_SISTEMA IS 'Sistema en el que se ejecuto el comando'
/
COMMENT ON COLUMN  ICB_HISTCOMPROC.COD_ACT IS 'Usado para el paso a las tablas mensuales de historicos'
/
COMMENT ON COLUMN  ICB_HISTCOMPROC.COD_COMANDO IS 'Codigo de comando utilizado'
/
COMMENT ON COLUMN  ICB_HISTCOMPROC.DES_COMANDO IS 'Comando utilizado en la ejecucion'
/
COMMENT ON COLUMN  ICB_HISTCOMPROC.FEC_EJECUCION IS 'Fecha de Ejecucion del comando'
/
COMMENT ON COLUMN  ICB_HISTCOMPROC.FEC_INGRESO IS 'Fecha de Peticion del Movimiento'
/
COMMENT ON COLUMN  ICB_HISTCOMPROC.DES_RESPUESTA IS 'Respuesta enviada al comando ejecutado'
/
