CREATE TABLE CO_TABPAGCFIN(
    COD_NEGO VARCHAR2(5) NOT NULL 
 ,  NRT_CLIE VARCHAR2(20) NOT NULL 
 ,  DGV_CLIE VARCHAR2(1) NOT NULL 
 ,  NUM_CONT VARCHAR2(20) NOT NULL 
 ,  SEC_PAGO VARCHAR2(20) NOT NULL 
 ,  FEC_PAGO VARCHAR2(20) NOT NULL 
 ,  MTO_PAGO VARCHAR2(20) NOT NULL 
 ,  LOG_USUA VARCHAR2(20) NULL 
 ,  GLS_OBS VARCHAR2(20) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_TABPAGCFIN IS 'Tabpagcfin.'
/
COMMENT ON COLUMN  CO_TABPAGCFIN.COD_NEGO IS 'CODIGO DEL NEGOCIO.'
/
COMMENT ON COLUMN  CO_TABPAGCFIN.NRT_CLIE IS 'NUMERO DE CLIENTE(RUT).'
/
COMMENT ON COLUMN  CO_TABPAGCFIN.DGV_CLIE IS 'DIGITO VERIFICADOR RUT CLIENTE.'
/
COMMENT ON COLUMN  CO_TABPAGCFIN.NUM_CONT IS 'NUMERO DEL CONTRATO DEL CLIENTE.'
/
COMMENT ON COLUMN  CO_TABPAGCFIN.SEC_PAGO IS 'SECUENCIA DE PAGO.'
/
COMMENT ON COLUMN  CO_TABPAGCFIN.FEC_PAGO IS 'FECHA DE PAGO.'
/
COMMENT ON COLUMN  CO_TABPAGCFIN.MTO_PAGO IS 'MONTO DE PAGO.'
/
COMMENT ON COLUMN  CO_TABPAGCFIN.LOG_USUA IS 'LOGIN USUARIO.'
/
COMMENT ON COLUMN  CO_TABPAGCFIN.GLS_OBS IS 'OBSERVACIONES GENERALES.'
/
