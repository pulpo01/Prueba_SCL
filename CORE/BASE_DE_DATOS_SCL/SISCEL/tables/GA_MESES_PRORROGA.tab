CREATE TABLE GA_MESES_PRORROGA(
    NUM_MESES NUMBER(3,0) NOT NULL 
 ,  DES_PRORROGA VARCHAR2(40) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(40) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_MESES_PRORROGA IS 'Tabla de Meses de Prorroga de Contrato'
/
COMMENT ON COLUMN  GA_MESES_PRORROGA.NUM_MESES IS 'Numero del Mes a Prorogar'
/
COMMENT ON COLUMN  GA_MESES_PRORROGA.FEC_DESDE IS 'Fecha Vigencia Desde'
/
COMMENT ON COLUMN  GA_MESES_PRORROGA.FEC_HASTA IS 'Fecha Vigencia Hasta'
/
COMMENT ON COLUMN  GA_MESES_PRORROGA.NOM_USUARORA IS 'Nombre Usuario'
/
