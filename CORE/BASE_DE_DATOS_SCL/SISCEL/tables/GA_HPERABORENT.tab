CREATE TABLE GA_HPERABORENT(
    NUM_ALQUILER NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ,  COD_PERIODO VARCHAR2(2) NOT NULL 
 ,  FEC_HISTORICO DATE NULL 
 ,  FEC_BAJA DATE NULL 
 ,  NUM_MOVSS NUMBER(9,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_HPERABORENT IS 'Historico Periodos de Renta de Abonados Rent a Phone'
/
COMMENT ON COLUMN  GA_HPERABORENT.NUM_ALQUILER IS 'Numero de Alquiler'
/
COMMENT ON COLUMN  GA_HPERABORENT.NUM_ABONADO IS 'Numero de Abonado/Producto'
/
COMMENT ON COLUMN  GA_HPERABORENT.FEC_ALTA IS 'Fecha de Alta'
/
COMMENT ON COLUMN  GA_HPERABORENT.COD_PERIODO IS 'Codigo de Periodo Contratado'
/
COMMENT ON COLUMN  GA_HPERABORENT.FEC_HISTORICO IS 'Fecha de Historico'
/
COMMENT ON COLUMN  GA_HPERABORENT.FEC_BAJA IS 'Fecha de Baja'
/
COMMENT ON COLUMN  GA_HPERABORENT.NUM_MOVSS IS 'Numero movimiento servicio suplementarios'
/
