CREATE TABLE FA_INTERIMPRESION_TD(
    NUM_PROCESO NUMBER(8,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  FEC_ENVIO DATE NOT NULL 
 ,  DIR_WEB VARCHAR2(100) NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_INTERIMPRESION_TD IS 'Interfaz entre la generacion y el retorno de DOC-1.'
/
COMMENT ON COLUMN  FA_INTERIMPRESION_TD.NUM_PROCESO IS 'Numero proceso del documento.'
/
COMMENT ON COLUMN  FA_INTERIMPRESION_TD.COD_CLIENTE IS 'Codigo de Cliente.'
/
COMMENT ON COLUMN  FA_INTERIMPRESION_TD.FEC_ENVIO IS 'Fecha y Hora de la transaccion.'
/
COMMENT ON COLUMN  FA_INTERIMPRESION_TD.DIR_WEB IS 'Direccion WEB.'
/
COMMENT ON COLUMN  FA_INTERIMPRESION_TD.COD_ESTADO IS 'Estado de la Transaccion.'
/
COMMENT ON COLUMN  FA_INTERIMPRESION_TD.FEC_ULTMOD IS 'Fecha y hora Movimiento.'
/
COMMENT ON COLUMN  FA_INTERIMPRESION_TD.NOM_USUARIO IS 'Nombre de Usuario.'
/
