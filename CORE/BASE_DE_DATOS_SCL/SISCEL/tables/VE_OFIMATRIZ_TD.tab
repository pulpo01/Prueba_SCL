CREATE TABLE VE_OFIMATRIZ_TD(
    COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  COD_MATRIZ VARCHAR2(2) NOT NULL 
 ,  USU_CREACION VARCHAR2(100) DEFAULT 'USER' NOT NULL 
 ,  FEC_CREACION DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_OFIMATRIZ_TD IS '[ VE_OFIMAT_TD ] TABLA DE RELACION MATRIZ POR OFICINATABLA DE RELACION MATRIZ POR OFICINA'
/
COMMENT ON COLUMN  VE_OFIMATRIZ_TD.COD_OFICINA IS 'CODIGO DE OFICINA'
/
COMMENT ON COLUMN  VE_OFIMATRIZ_TD.COD_MATRIZ IS 'CODIGO DE MATRIZ'
/
COMMENT ON COLUMN  VE_OFIMATRIZ_TD.USU_CREACION IS 'USUARIO CREADOR'
/
COMMENT ON COLUMN  VE_OFIMATRIZ_TD.FEC_CREACION IS 'FECHA DE CREACION'
/
