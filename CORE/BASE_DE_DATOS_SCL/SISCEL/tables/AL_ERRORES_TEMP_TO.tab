CREATE TABLE AL_ERRORES_TEMP_TO(
    COD_MODULO VARCHAR2(2) NOT NULL 
 ,  PROC_INVOCADOR VARCHAR2(50) NOT NULL 
 ,  COD_ERROR NUMBER(2,0) NOT NULL 
 ,  DES_ERROR VARCHAR2(150) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_ERRORES_TEMP_TO IS 'TABLA CON ERRORES PARA CARGA MASIVA'
/
COMMENT ON COLUMN  AL_ERRORES_TEMP_TO.COD_MODULO IS 'MODULO'
/
COMMENT ON COLUMN  AL_ERRORES_TEMP_TO.PROC_INVOCADOR IS 'PROCESO INVOCADOR 1)"RSV" RESERVA'
/
COMMENT ON COLUMN  AL_ERRORES_TEMP_TO.COD_ERROR IS 'CODIGO ERROR 0 SIN ERROR'
/
COMMENT ON COLUMN  AL_ERRORES_TEMP_TO.DES_ERROR IS 'DESCRIPCION ERROR.'
/
