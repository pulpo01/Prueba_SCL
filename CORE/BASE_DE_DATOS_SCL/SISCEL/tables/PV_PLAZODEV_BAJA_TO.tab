CREATE TABLE PV_PLAZODEV_BAJA_TO(
    COD_TIPLAN VARCHAR2(5) NOT NULL 
 ,  DES_TIPLAN VARCHAR2(30) NOT NULL 
 ,  NUM_PLAZODEV VARCHAR2(3) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_PLAZODEV_BAJA_TO IS 'Plazo De Devolucion Para Realizar Una Anulacion De Baja..'
/
COMMENT ON COLUMN  PV_PLAZODEV_BAJA_TO.COD_TIPLAN IS 'Codigo Tipo de Plan.'
/
COMMENT ON COLUMN  PV_PLAZODEV_BAJA_TO.DES_TIPLAN IS 'Descripcion Tipo de Plan.'
/
COMMENT ON COLUMN  PV_PLAZODEV_BAJA_TO.NUM_PLAZODEV IS 'Numero de dias de plazo para devolucion .'
/
COMMENT ON COLUMN  PV_PLAZODEV_BAJA_TO.FEC_ULTMOD IS 'Fecha Modificacion.'
/
COMMENT ON COLUMN  PV_PLAZODEV_BAJA_TO.NOM_USUARIO IS 'Nombre de Usuario.'
/
