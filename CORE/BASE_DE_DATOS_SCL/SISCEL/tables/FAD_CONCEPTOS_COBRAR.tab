CREATE TABLE FAD_CONCEPTOS_COBRAR(
    COD_OPERACION VARCHAR2(5) NOT NULL 
 ,  IND_CAUSA NUMBER(1,0) NOT NULL 
 ,  COD_CAUSA VARCHAR2(2) NULL 
 ,  COD_CANAL_VTA VARCHAR2(5) NOT NULL 
 ,  COD_ENTIDAD VARCHAR2(5) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  IND_COBRANZA NUMBER(1,0) NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAD_CONCEPTOS_COBRAR IS 'Dominio De Conceptos Por Cobrar.'
/
COMMENT ON COLUMN  FAD_CONCEPTOS_COBRAR.COD_OPERACION IS 'Codigo de Operacion Origen.'
/
COMMENT ON COLUMN  FAD_CONCEPTOS_COBRAR.IND_CAUSA IS 'Indica si la operacion es basada en la causa 0: Sin Causa; 1: Con Causa.'
/
COMMENT ON COLUMN  FAD_CONCEPTOS_COBRAR.COD_CAUSA IS 'Codigo de la causa..'
/
COMMENT ON COLUMN  FAD_CONCEPTOS_COBRAR.COD_CANAL_VTA IS '.'
/
COMMENT ON COLUMN  FAD_CONCEPTOS_COBRAR.COD_ENTIDAD IS 'Entidad a la cual se cobrara el concepto..'
/
COMMENT ON COLUMN  FAD_CONCEPTOS_COBRAR.COD_CONCEPTO IS 'Codigo de concepto facturable.'
/
COMMENT ON COLUMN  FAD_CONCEPTOS_COBRAR.FEC_DESDE IS 'Fecha Inicio Vigencia.'
/
COMMENT ON COLUMN  FAD_CONCEPTOS_COBRAR.IND_COBRANZA IS 'Indica si es cobrable o no. 0:No Cobrable; 1: Si Covbrable.'
/
COMMENT ON COLUMN  FAD_CONCEPTOS_COBRAR.FEC_HASTA IS 'Fecha final de vigencia.'
/
COMMENT ON COLUMN  FAD_CONCEPTOS_COBRAR.NOM_USUARIO IS 'Usuario responsable de creacion.'
/
