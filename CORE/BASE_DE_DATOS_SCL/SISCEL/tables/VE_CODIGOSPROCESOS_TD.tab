CREATE TABLE VE_CODIGOSPROCESOS_TD(
    COD_MODULO VARCHAR2(2) NOT NULL 
 ,  NOM_TABLA VARCHAR2(30) NOT NULL 
 ,  NOM_COLUMNA VARCHAR2(50) NOT NULL 
 ,  COD_VALOR VARCHAR2(50) NOT NULL 
 ,  DES_VALOR VARCHAR2(512) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CODIGOSPROCESOS_TD IS 'GENERAL CODIGOS DE PROCESO'
/
COMMENT ON COLUMN  VE_CODIGOSPROCESOS_TD.COD_MODULO IS 'MODULO DEL DOMINIO'
/
COMMENT ON COLUMN  VE_CODIGOSPROCESOS_TD.NOM_TABLA IS 'TABLA ASOCIADA EL NUEVO CODIGO'
/
COMMENT ON COLUMN  VE_CODIGOSPROCESOS_TD.NOM_COLUMNA IS 'COLUMNA ASOCIADA AL NUEVO CODIGO'
/
COMMENT ON COLUMN  VE_CODIGOSPROCESOS_TD.COD_VALOR IS 'CODIGO ASIGNADO'
/
COMMENT ON COLUMN  VE_CODIGOSPROCESOS_TD.DES_VALOR IS 'DESCRIPCION DE CODIGO ASIGNADO'
/
COMMENT ON COLUMN  VE_CODIGOSPROCESOS_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  VE_CODIGOSPROCESOS_TD.NOM_USUARIO IS 'USUARIOS RESPONSABLE'
/
