 ,  NUM_TELEFONO NUMBER(15,0) NOT NULL 
 ,  COD_BANCO VARCHAR2(15) NOT NULL 
 ,  COD_BCOI NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
