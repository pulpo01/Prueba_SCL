CREATE TABLE CO_CTACTE_VALOR_TD(
    COD_BANCO VARCHAR2(15) NOT NULL 
 ,  NUM_CTA_CTE VARCHAR2(18) NOT NULL 
 ,  TIP_VALOR NUMBER(2,0) NOT NULL 
 ,  FEC_ACTUALIZA DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_CTACTE_VALOR_TD IS 'ASOCIA NUMERO DE CTA CTE CON TIPOS DE VALOR'
/
COMMENT ON COLUMN  CO_CTACTE_VALOR_TD.COD_BANCO IS 'CODIGO DE BANCO'
/
COMMENT ON COLUMN  CO_CTACTE_VALOR_TD.NUM_CTA_CTE IS 'NUMERO CUENTA CORRIENTE'
/
COMMENT ON COLUMN  CO_CTACTE_VALOR_TD.TIP_VALOR IS 'TIPO DE VALOR'
/
COMMENT ON COLUMN  CO_CTACTE_VALOR_TD.FEC_ACTUALIZA IS 'FECHA ACTUALIZACION'
/
COMMENT ON COLUMN  CO_CTACTE_VALOR_TD.NOM_USUARORA IS 'NOMBRE USUARIO'
/
