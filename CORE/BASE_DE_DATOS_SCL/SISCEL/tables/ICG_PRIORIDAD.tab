CREATE TABLE ICG_PRIORIDAD(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_ACCION NUMBER(3,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  NUM_PRIORIDAD NUMBER(3,0) NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICG_PRIORIDAD IS 'Prioridades de Ejecucion de Movimientos'
/
COMMENT ON COLUMN  ICG_PRIORIDAD.COD_PRODUCTO IS 'codigo de producto'
/
COMMENT ON COLUMN  ICG_PRIORIDAD.COD_ACCION IS 'Codigo de Accion - Retrofitted'
/
COMMENT ON COLUMN  ICG_PRIORIDAD.COD_CENTRAL IS 'Codigo de Central - Retrofitted'
/
COMMENT ON COLUMN  ICG_PRIORIDAD.NUM_PRIORIDAD IS 'Valor de la Prioridad - Retrofitted'
/
