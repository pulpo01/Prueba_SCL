CREATE TABLE TA_TIPOHORARIO(
    COD_TIPOHORARIO CHAR(1) NOT NULL 
 ,  DES_TIPOHORARIO VARCHAR2(30) NOT NULL 
 ,  IND_TIPOHORARIO NUMBER(1,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(40) NOT NULL 
 ,  FEC_ULTIMAMOD DATE NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  TA_TIPOHORARIO.COD_TIPOHORARIO IS 'Codigo Tipo Horario'
/
COMMENT ON COLUMN  TA_TIPOHORARIO.DES_TIPOHORARIO IS 'Descripcion Tipo Horario'
/
COMMENT ON COLUMN  TA_TIPOHORARIO.IND_TIPOHORARIO IS 'Indicador Tipo Horario'
/
COMMENT ON COLUMN  TA_TIPOHORARIO.NOM_USUARIO IS 'Almacena Nombre Usuario'
/
COMMENT ON COLUMN  TA_TIPOHORARIO.FEC_ULTIMAMOD IS 'Fecha Ultima Modificacion'
/
