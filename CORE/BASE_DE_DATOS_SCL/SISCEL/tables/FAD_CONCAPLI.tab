CREATE TABLE FAD_CONCAPLI(
    COD_GRUPO NUMBER(6,0) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  COD_CONREL NUMBER(4,0) NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAD_CONCAPLI IS 'Grupo de Conceptos de Aplicacisn de Descuentos'
/
COMMENT ON COLUMN  FAD_CONCAPLI.COD_GRUPO IS 'Codigo de grupo'
/
COMMENT ON COLUMN  FAD_CONCAPLI.COD_CONCEPTO IS 'Codigo concepto'
/
COMMENT ON COLUMN  FAD_CONCAPLI.FEC_DESDE IS 'Fecha Inicio de Vigencia'
/
COMMENT ON COLUMN  FAD_CONCAPLI.FEC_HASTA IS 'Fecha termino de Vigencia'
/
COMMENT ON COLUMN  FAD_CONCAPLI.COD_CONREL IS 'Codigo de concepto relacionado a generar'
/
COMMENT ON COLUMN  FAD_CONCAPLI.FEC_ULTMOD IS 'Fecha ultima actualizacion'
/
COMMENT ON COLUMN  FAD_CONCAPLI.NOM_USUARIO IS 'Nombre usuario responsable'
/
