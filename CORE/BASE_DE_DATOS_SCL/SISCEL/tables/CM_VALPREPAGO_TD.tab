CREATE TABLE CM_VALPREPAGO_TD(
    COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  IMP_COMISION NUMBER(14,4) NOT NULL 
 ,  IND_PROCEQUI VARCHAR2(2) DEFAULT 'E' NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) DEFAULT 'C' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_VALPREPAGO_TD IS 'Valorizacion Del Prepago.'
/
COMMENT ON COLUMN  CM_VALPREPAGO_TD.COD_PLANCOMIS IS 'PLAN DE COMISIONES QUE CONTIENEN AL CONCEPTO.'
/
COMMENT ON COLUMN  CM_VALPREPAGO_TD.COD_CONCEPTO IS 'CONCEPTO DE PENALIZACION.'
/
COMMENT ON COLUMN  CM_VALPREPAGO_TD.IMP_COMISION IS 'IMPORTE COMISION.'
/
COMMENT ON COLUMN  CM_VALPREPAGO_TD.IND_PROCEQUI IS 'INDICADOR .'
/
COMMENT ON COLUMN  CM_VALPREPAGO_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION.'
/
COMMENT ON COLUMN  CM_VALPREPAGO_TD.NOM_USUARIO IS 'NOMBRE USUARIO.'
/
COMMENT ON COLUMN  CM_VALPREPAGO_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
