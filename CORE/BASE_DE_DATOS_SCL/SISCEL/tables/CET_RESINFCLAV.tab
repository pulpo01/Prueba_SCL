CREATE TABLE CET_RESINFCLAV(
    NUM_LOTE NUMBER(16,0) NOT NULL 
 ,  FEC_EXPIRACION DATE NOT NULL 
 ,  NUM_CLAVES NUMBER(6,0) NULL 
 ,  NUM_CLAVERR NUMBER(6,0) NULL 
 ,  NUM_VIGENCIA NUMBER(4,0) NULL 
 ,  COD_TIPCLAVE VARCHAR2(5) NOT NULL 
 ,  MTO_CLAVE NUMBER(14,4) NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) NOT NULL 
 ,  COD_SCRIPT VARCHAR2(2) NOT NULL 
 ,  FEC_INFORME DATE NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CET_RESINFCLAV IS 'Resumen Informe de Claves (carga de archivos)'
/
COMMENT ON COLUMN  CET_RESINFCLAV.NUM_LOTE IS 'Numero de Lote'
/
COMMENT ON COLUMN  CET_RESINFCLAV.FEC_EXPIRACION IS 'Fecha de expiracion de la clave'
/
COMMENT ON COLUMN  CET_RESINFCLAV.NUM_CLAVES IS 'Numero de claves aceptadas'
/
COMMENT ON COLUMN  CET_RESINFCLAV.NUM_CLAVERR IS 'Numero de claves rechazadas'
/
COMMENT ON COLUMN  CET_RESINFCLAV.NUM_VIGENCIA IS 'Numero dias de vigencia carga'
/
COMMENT ON COLUMN  CET_RESINFCLAV.COD_TIPCLAVE IS 'Tipo de clave ( tipos de tarjeta )'
/
COMMENT ON COLUMN  CET_RESINFCLAV.MTO_CLAVE IS 'Monto de recarga de prepago'
/
COMMENT ON COLUMN  CET_RESINFCLAV.COD_ESTADO IS 'Estado de la clave  I: informada  A: Activada  P: Proceso'
/
COMMENT ON COLUMN  CET_RESINFCLAV.COD_SCRIPT IS 'Tipo de scriptacion utilizada'
/
COMMENT ON COLUMN  CET_RESINFCLAV.FEC_INFORME IS 'Fecha de informe de archivo'
/
