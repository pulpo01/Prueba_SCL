CREATE TABLE AL_ERRORES_WEB_TH(
    NUM_ERROR NUMBER(9,0) NOT NULL 
 ,  COD_PEDIDO NUMBER(12,0) NOT NULL 
 ,  LIN_PEDIDO NUMBER(2,0) NULL 
 ,  NUM_SERIE VARCHAR2(25) NULL 
 ,  COD_ARTICULO NUMBER(6,0) NULL 
 ,  FEC_ERROR DATE NOT NULL 
 ,  NUM_VENTA NUMBER(8,0) NULL 
 ,  NUM_TRASPASO_MASIVO NUMBER(9,0) NULL 
 ,  GLOSA_ERROR_TRASPASO VARCHAR2(1024) NULL 
 ,  GLOSA_ERROR_VENTA VARCHAR2(1024) NULL 
 ,  COD_ESTADO_ERROR VARCHAR2(50) NULL 
 ) TABLESPACE    TS_LOGISTICA_MDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_ERRORES_WEB_TH IS '[ ERWE ] Tabla historica de Errores Encontrados En Web DistribuidoresTabla historica de Errores Encontrados En Web Distribuidores'
/
COMMENT ON COLUMN  AL_ERRORES_WEB_TH.NUM_ERROR IS 'Número de error generado.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB_TH.COD_PEDIDO IS 'Código de pedido web.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB_TH.LIN_PEDIDO IS 'Línea de pedido.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB_TH.NUM_SERIE IS 'Identificador numérico de un articulo.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB_TH.COD_ARTICULO IS 'Código del articulo.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB_TH.FEC_ERROR IS 'Fecha y hora en que se registro el error.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB_TH.NUM_VENTA IS 'Número de venta.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB_TH.NUM_TRASPASO_MASIVO IS 'Número de traspaso masivo.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB_TH.GLOSA_ERROR_TRASPASO IS 'Glosa de error para traspaso.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB_TH.GLOSA_ERROR_VENTA IS 'Glosa de error para venta.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB_TH.COD_ESTADO_ERROR IS 'Código de estado error PR = en error SO = solucionado.'
/
