CREATE TABLE CO_LIMCONSUMO(
    COD_CREDCON NUMBER(3,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CALCLIEN VARCHAR2(2) NOT NULL 
 ,  IND_PORCEN NUMBER(5,2) NOT NULL 
 ,  IMP_CONSUMO NUMBER(14,4) NOT NULL 
 ,  DES_CREDCON VARCHAR2(40) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_LIMCONSUMO IS 'LIMITE DE CONSUMO'
/
COMMENT ON COLUMN  CO_LIMCONSUMO.COD_CREDCON IS 'CODIGO DE CREDITO DE CONSUMO'
/
COMMENT ON COLUMN  CO_LIMCONSUMO.COD_PRODUCTO IS 'CODIGO DE PRODUCTO'
/
COMMENT ON COLUMN  CO_LIMCONSUMO.COD_CALCLIEN IS 'CALIDAD DE CLIENTE'
/
COMMENT ON COLUMN  CO_LIMCONSUMO.IND_PORCEN IS 'PORCENTAJE DE CREDITO'
/
COMMENT ON COLUMN  CO_LIMCONSUMO.IMP_CONSUMO IS 'IMPORTE DE MOROSIDAD DE CONSUMO'
/
COMMENT ON COLUMN  CO_LIMCONSUMO.DES_CREDCON IS 'DESCRIPCION DE LIMITE DE CONSUMO'
/
