CREATE TABLE TA_CTLFORMATO(
    NUM_BLOQUE VARCHAR2(9) NOT NULL 
 ,  NUM_TICKETS NUMBER(9,0) NOT NULL 
 ,  NUM_EXITOS NUMBER(8,0) NOT NULL 
 ,  NUM_NEXITOS NUMBER(8,0) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_CTLFORMATO IS 'Control ficheros enviados y recibidos en formato alternativo'
/
COMMENT ON COLUMN  TA_CTLFORMATO.NUM_BLOQUE IS 'Numero de Bloque'
/
COMMENT ON COLUMN  TA_CTLFORMATO.NUM_TICKETS IS 'Numero de Tickets'
/
COMMENT ON COLUMN  TA_CTLFORMATO.NUM_EXITOS IS 'Numero de Exitos'
/
COMMENT ON COLUMN  TA_CTLFORMATO.NUM_NEXITOS IS 'Numero de No Exitos'
/
