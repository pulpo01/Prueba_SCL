CREATE TABLE CED_ACTUACION(
    COD_COMANDO VARCHAR2(5) NOT NULL 
 ,  DES_COMANDO VARCHAR2(30) NOT NULL 
 ,  COD_ACTUACION NUMBER(5,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CED_ACTUACION IS 'Codigos de actuacisn comandos comverse '
/
COMMENT ON COLUMN  CED_ACTUACION.COD_COMANDO IS 'Codigo de comando a ejecutar'
/
COMMENT ON COLUMN  CED_ACTUACION.DES_COMANDO IS 'Descripcion del comando'
/
COMMENT ON COLUMN  CED_ACTUACION.COD_ACTUACION IS 'Codigo actuacion'
/
COMMENT ON COLUMN  CED_ACTUACION.FEC_ULTMOD IS 'Fecha ultima actualizacion'
/
COMMENT ON COLUMN  CED_ACTUACION.NOM_USUARIO IS 'Nombre usuario responsable'
/
