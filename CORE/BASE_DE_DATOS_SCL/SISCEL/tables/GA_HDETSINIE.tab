CREATE TABLE GA_HDETSINIE(
    NUM_SINIESTRO NUMBER(8,0) NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) NOT NULL 
 ,  FEC_DETALLE DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  OBS_DETALLE VARCHAR2(255) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_HDETSINIE IS 'Historico Detalles de Siniestro de Abonados de Producto'
/
COMMENT ON COLUMN  GA_HDETSINIE.NUM_SINIESTRO IS 'Numero de Siniestro'
/
COMMENT ON COLUMN  GA_HDETSINIE.COD_ESTADO IS 'Codigo de Estado del Siniestro'
/
COMMENT ON COLUMN  GA_HDETSINIE.FEC_DETALLE IS 'Fecha Detalle Siniestro'
/
COMMENT ON COLUMN  GA_HDETSINIE.NOM_USUARORA IS 'Usuario Oracle que Realiza el Detalle'
/
COMMENT ON COLUMN  GA_HDETSINIE.FEC_HISTORICO IS 'Fecha Historico'
/
COMMENT ON COLUMN  GA_HDETSINIE.OBS_DETALLE IS 'Observaciones del Detalle del Siniestro'
/
