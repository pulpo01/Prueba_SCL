CREATE TABLE AL_CAB_COR_MONE(
    NUM_CORRE NUMBER(9,0) NOT NULL 
 ,  FEC_CORRE DATE NOT NULL 
 ,  FEC_SOLI DATE NOT NULL 
 ,  TIP_CORRE NUMBER(1,0) NOT NULL 
 ,  IND_IMPU NUMBER(1,0) NOT NULL 
 ,  COD_USUA VARCHAR2(30) NOT NULL 
 ,  COD_ESTA NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_CAB_COR_MONE IS 'Tabla De Cabecera De Correccion Monetaria.'
/
COMMENT ON COLUMN  AL_CAB_COR_MONE.NUM_CORRE IS 'Secuencial de la correccion monetaria.'
/
COMMENT ON COLUMN  AL_CAB_COR_MONE.FEC_CORRE IS 'Fecha de la Correccion Solicitada.'
/
COMMENT ON COLUMN  AL_CAB_COR_MONE.FEC_SOLI IS 'Fecha de Solicitud de Correccion.'
/
COMMENT ON COLUMN  AL_CAB_COR_MONE.TIP_CORRE IS 'Tipo de Correccion Monetaria.'
/
COMMENT ON COLUMN  AL_CAB_COR_MONE.IND_IMPU IS 'Tipo de Imputacion Contable.'
/
COMMENT ON COLUMN  AL_CAB_COR_MONE.COD_USUA IS 'Usuario de Solicitud.'
/
COMMENT ON COLUMN  AL_CAB_COR_MONE.COD_ESTA IS 'Estado de la Solicitud de Correccion Monetaria.'
/
