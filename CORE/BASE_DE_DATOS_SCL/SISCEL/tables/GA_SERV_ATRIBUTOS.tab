 ,  COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  COD_ATRIBUTO NUMBER(3,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  GA_SERV_ATRIBUTOS.COD_PRODUCTO IS 'Codigo de producto, celular, beeper,etc.'
/
COMMENT ON COLUMN  GA_SERV_ATRIBUTOS.COD_SERVICIO IS 'Codigo de servicio suplementario.'
/
