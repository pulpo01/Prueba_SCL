CREATE TABLE RE_CATEGRECL(
    COD_CATEGRECL NUMBER(3,0) NOT NULL 
 ,  DES_CATEGRECL VARCHAR2(60) NOT NULL 
 ,  TIP_CATEGORIA VARCHAR2(1) NOT NULL 
 ,  COD_VIGENCIA VARCHAR2(1) NOT NULL 
 ,  DIAS_DUPLICIDAD NUMBER(2,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  RE_CATEGRECL.DIAS_DUPLICIDAD IS 'Cantidad de dias anteriores a la fecha actual en que se controlaran posibles duplicidades de reclamos al ingresar uno.'
/
