CREATE TABLE CA_RESULTADOS(
    NUM_LLAMADA NUMBER(9,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_LLAMADA DATE NOT NULL 
 ,  COD_ATENCION NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_RESULTADOS IS 'Resultados Llamadas'
/
COMMENT ON COLUMN  CA_RESULTADOS.NUM_LLAMADA IS 'Numero Llamada'
/
COMMENT ON COLUMN  CA_RESULTADOS.NOM_USUARIO IS 'Usuario creador'
/
COMMENT ON COLUMN  CA_RESULTADOS.FEC_LLAMADA IS 'Fecha Llamada'
/
COMMENT ON COLUMN  CA_RESULTADOS.COD_ATENCION IS 'Tipo de atencion'
/
