CREATE TABLE CO_INCIDENCIAS(
    NUM_INCIDENCIA NUMBER(9,0) NOT NULL 
 ,  TIP_SUSPENSION NUMBER(1,0) NOT NULL 
 ,  TIP_INCIDENCIA NUMBER(3,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  USU_SOLUCION VARCHAR2(30) NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_INCIDENCIAS IS 'TIPOS DE INCIDENCIAS NO SUSPENDIBLES'
/
COMMENT ON COLUMN  CO_INCIDENCIAS.NUM_INCIDENCIA IS 'NUMERO DE INCIDENCIA'
/
COMMENT ON COLUMN  CO_INCIDENCIAS.TIP_SUSPENSION IS 'Tipo de suspensisn (0) Morosidad, (1) Limite de credito - Retrofitted'
/
COMMENT ON COLUMN  CO_INCIDENCIAS.TIP_INCIDENCIA IS 'Tipo de incidencia en CA_TIPINICENCIAS'
/
COMMENT ON COLUMN  CO_INCIDENCIAS.COD_CLIENTE IS 'CODIGO DE CLIENTE'
/
COMMENT ON COLUMN  CO_INCIDENCIAS.NUM_ABONADO IS 'Numero de abonado - Retrofitted'
/
COMMENT ON COLUMN  CO_INCIDENCIAS.USU_SOLUCION IS 'Usuario solucion - Retrofitted'
/
COMMENT ON COLUMN  CO_INCIDENCIAS.COD_PRODUCTO IS 'CODIGO DE PRODUCTO'
/
