CREATE TABLE PV_RESTRIC_CONVEROOSS_TD(
    COD_OS_ANT VARCHAR2(5) NOT NULL 
 ,  COD_OS_NUE VARCHAR2(5) NOT NULL 
 ,  COD_ACTUACION VARCHAR2(2) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  TIPO_MOVIMIENTO VARCHAR2(20) NOT NULL 
 ,  COD_ACTUACION_WEB VARCHAR2(2) NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_RESTRIC_CONVEROOSS_TD IS '[ RECO ] Conversión de orden de servicio'
/
COMMENT ON COLUMN  PV_RESTRIC_CONVEROOSS_TD.COD_OS_ANT IS 'Código de Orden de Servicio Antigua (Batch o En Línea)'
/
COMMENT ON COLUMN  PV_RESTRIC_CONVEROOSS_TD.COD_OS_NUE IS 'Código de Orden de Servicio Nueva'
/
COMMENT ON COLUMN  PV_RESTRIC_CONVEROOSS_TD.COD_ACTUACION IS 'Código de Actuación de la orden de servicio antigua'
/
COMMENT ON COLUMN  PV_RESTRIC_CONVEROOSS_TD.FEC_DESDE IS 'Fecha inicial de vigencia del código de orden de servicio nuevo'
/
COMMENT ON COLUMN  PV_RESTRIC_CONVEROOSS_TD.FEC_HASTA IS 'Fecha final de vigencia del código de orden de servicio nuevo'
/
COMMENT ON COLUMN  PV_RESTRIC_CONVEROOSS_TD.TIPO_MOVIMIENTO IS 'Tipo Movimiento de Combinatoria Origen Destino (Ejemplo: POSPAGOPOSPAGO, PREPAGOPOSPAGO)'
/
COMMENT ON COLUMN  PV_RESTRIC_CONVEROOSS_TD.COD_ACTUACION_WEB IS 'Código de Actuación WEB'
/
