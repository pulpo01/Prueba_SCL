CREATE TABLE CMD_CATEGCLIENTES(
    COD_CATEGCLIENTE VARCHAR2(10) NOT NULL 
 ,  DES_CATEGCLIENTE VARCHAR2(50) NOT NULL 
 ,  FEC_CREACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMD_CATEGCLIENTES IS 'Definicion De Categorias De Clientes Desde El Punto De Vista Comisional.'
/
COMMENT ON COLUMN  CMD_CATEGCLIENTES.COD_CATEGCLIENTE IS 'Tipo (categoria) de cliente desde el punto de vista comisional..'
/
COMMENT ON COLUMN  CMD_CATEGCLIENTES.DES_CATEGCLIENTE IS 'Descripcion de la categoria de cliente..'
/
COMMENT ON COLUMN  CMD_CATEGCLIENTES.FEC_CREACION IS 'Fecha de insercion del registro en la tabla..'
/
COMMENT ON COLUMN  CMD_CATEGCLIENTES.NOM_USUARIO IS 'Usuario que hizo la creacion del registro en la tabla..'
/
