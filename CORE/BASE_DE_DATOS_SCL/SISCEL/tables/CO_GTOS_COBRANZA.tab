CREATE TABLE CO_GTOS_COBRANZA(
    NUM_SECUENCI NUMBER(8,0) NOT NULL 
 ,  TRAMO_DESDE VARCHAR2(15) NOT NULL 
 ,  TRAMO_HASTA VARCHAR2(15) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  PORC_APLICADO NUMBER(8,5) NOT NULL 
 ,  FEC_VIGENCIA_DD_DH DATE NOT NULL 
 ,  FEC_VIGENCIA_HH_DH DATE NOT NULL 
 ,  FECHA_CREACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(20) NOT NULL 
 ,  COD_ENTIDAD VARCHAR2(5) NOT NULL 
 ,  TIP_DATO VARCHAR2(5) NOT NULL 
 ,  DES_MATRIZ VARCHAR2(80) NOT NULL 
 ,  TIP_COMISION VARCHAR2(10) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
