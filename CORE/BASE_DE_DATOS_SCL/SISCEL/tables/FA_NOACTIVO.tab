 ,  DES_CONCEPTO VARCHAR2(60) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_TIPCONCE NUMBER(2,0) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_NOACTIVO IS 'Conceptos de Facturacion NO Activos'
/
COMMENT ON COLUMN  FA_NOACTIVO.COD_CONCEPTO IS 'Codigo de Concepto no activo'
/
COMMENT ON COLUMN  FA_NOACTIVO.DES_CONCEPTO IS 'Descripcion de Concepto'
/
COMMENT ON COLUMN  FA_NOACTIVO.COD_PRODUCTO IS 'Producto al que pertenece el concepto no activo'
