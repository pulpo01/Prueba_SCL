CREATE TABLE CO_ARCHIVOPAGOS_TO(
    NUM_SECUENCIA NUMBER(8,0) NOT NULL 
 ,  COD_CAJA NUMBER(5,0) NOT NULL 
 ,  COD_OFICINA VARCHAR2(3) NOT NULL 
 ,  NUM_EJERCICIO VARCHAR2(12) NOT NULL 
 ,  NOM_ARCHIVO VARCHAR2(255) NOT NULL 
 ,  NUM_REGISTROS NUMBER(8,0) DEFAULT 0 NOT NULL 
 ,  IMP_REGISTROS NUMBER(14,4) DEFAULT 0 NOT NULL 
 ,  NUM_ANOMALIAS NUMBER(8,0) DEFAULT 0 NULL 
 ,  FEC_PROCESO DATE NOT NULL 
 ,  COD_VENDEALER NUMBER(6,0) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_ARCHIVOPAGOS_TO IS '[ ARCH ] Archivos de Pagos DealersArchivos de Pagos Dealers'
/
COMMENT ON COLUMN  CO_ARCHIVOPAGOS_TO.NUM_SECUENCIA IS 'Numero de la secuencia co_seq_pagosdealer'
/
COMMENT ON COLUMN  CO_ARCHIVOPAGOS_TO.COD_CAJA IS 'Codigo de caja que efectua el pago'
/
COMMENT ON COLUMN  CO_ARCHIVOPAGOS_TO.COD_OFICINA IS 'Codigo de oficina donde se efectua el pago'
/
COMMENT ON COLUMN  CO_ARCHIVOPAGOS_TO.NUM_EJERCICIO IS 'Fecha del ejercicio realizado. En fto yyyymmddhhmm(sysdatE)'
/
COMMENT ON COLUMN  CO_ARCHIVOPAGOS_TO.NOM_ARCHIVO IS 'Nombre del archivo procesado'
/
COMMENT ON COLUMN  CO_ARCHIVOPAGOS_TO.NUM_REGISTROS IS 'Cantidad total de registros procesados'
/
COMMENT ON COLUMN  CO_ARCHIVOPAGOS_TO.IMP_REGISTROS IS 'Importe total de registros procesados'
/
COMMENT ON COLUMN  CO_ARCHIVOPAGOS_TO.NUM_ANOMALIAS IS 'Cantidad de anomalias encontradas en el archivo'
/
COMMENT ON COLUMN  CO_ARCHIVOPAGOS_TO.FEC_PROCESO IS 'Fecha efectiva del proceso'
/
COMMENT ON COLUMN  CO_ARCHIVOPAGOS_TO.COD_VENDEALER IS 'Codigo del vendedor dealer'
/
COMMENT ON COLUMN  CO_ARCHIVOPAGOS_TO.NOM_USUARORA IS 'Nombre del usuario oracle que efectuo el pago'
/
