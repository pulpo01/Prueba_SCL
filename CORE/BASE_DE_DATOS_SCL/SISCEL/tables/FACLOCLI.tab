CREATE TABLE SISCEL.FA_CICLOCLI
(
  COD_CLIENTE       NUMBER(8)                   NOT NULL,
  COD_CICLO         NUMBER(2)                   NOT NULL,
  COD_PRODUCTO      NUMBER(1)                   NOT NULL,
  NUM_ABONADO       NUMBER(8)                   NOT NULL,
  NUM_PROCESO       NUMBER(8)                   DEFAULT 0                     NOT NULL,
  COD_CALCLIEN      VARCHAR2(2 BYTE)            NOT NULL,
  IND_CAMBIO        NUMBER(1)                   NOT NULL,
  NOM_USUARIO       VARCHAR2(20 BYTE)           NOT NULL,
  NOM_APELLIDO1     VARCHAR2(20 BYTE)           NOT NULL,
  NOM_APELLIDO2     VARCHAR2(20 BYTE),
  COD_CREDMOR       NUMBER(3),
  IND_DEBITO        VARCHAR2(1 BYTE),
  COD_CICLONUE      NUMBER(2),
  FEC_ALTA          DATE                        NOT NULL,
  NUM_TERMINAL      NUMBER(15),
  FEC_ULTFACT       DATE,
  IND_MASCARA       NUMBER(2),
  COD_DESPACHO      VARCHAR2(5 BYTE),
  COD_PRIORIDAD     NUMBER(3),
  COD_COURRIER      VARCHAR2(10 BYTE),
  COD_ZONACOURRIER  VARCHAR2(10 BYTE)
)  TABLESPACE TS_FACTURACION_MDAT
/


COMMENT ON TABLE SISCEL.FA_CICLOCLI IS 'Relacion entre cliente ciclo y abonados';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.COD_ZONACOURRIER IS 'Código Zona Courrier Distribución';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.COD_COURRIER IS 'Código Courrier Distribución';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.COD_CLIENTE IS 'Codigo de Cliente';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.COD_CICLO IS 'Ciclo al que pertenece el cliente';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.COD_PRODUCTO IS 'Producto al que pertenece el concepto';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.NUM_ABONADO IS 'Secuencia producto/abonado';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.NUM_PROCESO IS 'Numero de Proceso que ejecuta el ciclo - Retrofitted';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.COD_CALCLIEN IS 'Calidad de cliente - Retrofitted';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.IND_CAMBIO IS 'Indicador de Cambio en el Periodo';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.NOM_USUARIO IS 'Nombre del abonado - Retrofitted';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.NOM_APELLIDO1 IS 'Primer apellido de abonado - Retrofitted';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.NOM_APELLIDO2 IS 'Primer apellido de abonado - Retrofitted';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.COD_CREDMOR IS 'Codigo del credito de morosidad - Retrofitted';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.IND_DEBITO IS ' - Retrofitted';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.COD_CICLONUE IS ' - Retrofitted';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.FEC_ULTFACT IS 'Fecha ultima facturacion abonado';

COMMENT ON COLUMN SISCEL.FA_CICLOCLI.IND_MASCARA IS 'Indicador de en proceso de facturacisn';

