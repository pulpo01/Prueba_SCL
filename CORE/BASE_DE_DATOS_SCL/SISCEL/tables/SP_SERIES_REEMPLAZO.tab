CREATE TABLE SP_SERIES_REEMPLAZO(
    NUM_ORDEN NUMBER(9,0) NOT NULL 
 ,  NUM_LINEA NUMBER(2,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  NUM_SERIEMEC VARCHAR2(20) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SP_SERIES_REEMPLAZO IS 'Series de los accesorios seriados entregados en prestamo al cliente'
/
COMMENT ON COLUMN  SP_SERIES_REEMPLAZO.NUM_ORDEN IS 'Numero de Orden de Reparacion a que pertenece el Numero de serie deaccesorio de reemplazo seriado'
/
COMMENT ON COLUMN  SP_SERIES_REEMPLAZO.NUM_LINEA IS 'Numero de linea de accesorios reemplazo'
/
COMMENT ON COLUMN  SP_SERIES_REEMPLAZO.NUM_SERIE IS 'Numero de serie de accesorio de reemplazo seriado'
/
COMMENT ON COLUMN  SP_SERIES_REEMPLAZO.NUM_SERIEMEC IS 'numero de serie mecanico de la serie de reemplazo'
/
