CREATE TABLE BPT_RECARGAS_BENEF(
    NUM_SECUENCIA NUMBER(8,0) NOT NULL 
 ,  COD_PLAN VARCHAR2(5) NOT NULL 
 ,  FEC_PROCESO DATE NOT NULL 
 ,  MTO_PROMOCION NUMBER(14,4) NOT NULL 
 ,  MTO_SALDO NUMBER(14,4) NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  BPT_RECARGAS_BENEF IS 'Beneficios Por Recargas De Tarjetas.'
/
COMMENT ON COLUMN  BPT_RECARGAS_BENEF.NUM_SECUENCIA IS 'Secuencia de Recarga.'
/
COMMENT ON COLUMN  BPT_RECARGAS_BENEF.COD_PLAN IS 'Plan de Beneficio.'
/
COMMENT ON COLUMN  BPT_RECARGAS_BENEF.FEC_PROCESO IS 'Fecha en que se proceso.'
/
COMMENT ON COLUMN  BPT_RECARGAS_BENEF.MTO_PROMOCION IS 'Monto de la Promocion.'
/
COMMENT ON COLUMN  BPT_RECARGAS_BENEF.MTO_SALDO IS 'Saldo relacionado .'
/
