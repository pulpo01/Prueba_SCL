CREATE TABLE AL_ERRORES_WEB(
    NUM_ERROR NUMBER(9,0) NOT NULL 
 ,  COD_PEDIDO NUMBER(12,0) NOT NULL 
 ,  LIN_PEDIDO NUMBER(2,0) NULL 
 ,  NUM_SERIE VARCHAR2(25) NULL 
 ,  COD_ARTICULO NUMBER(6,0) NULL 
 ,  FEC_ERROR DATE NOT NULL 
 ,  NUM_VENTA NUMBER(8,0) NULL 
 ,  NUM_TRASPASO_MASIVO NUMBER(9,0) NULL 
 ,  GLOSA_ERROR_TRASPASO VARCHAR2(1024) NULL 
 ,  GLOSA_ERROR_VENTA VARCHAR2(1024) NULL 
 ,  COD_ESTADO_ERROR VARCHAR2(50) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_ERRORES_WEB IS 'Errores Encontrados En Web Distribuidores.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB.NUM_ERROR IS 'Correlativo de error sequencia.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB.COD_PEDIDO IS 'Codigo de pedido o numero de pedido.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB.LIN_PEDIDO IS 'Linea de pedido.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB.NUM_SERIE IS 'Numero de serie.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB.COD_ARTICULO IS 'Codigo de articulo.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB.FEC_ERROR IS 'Fecha y hora en que se registro el error.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB.NUM_VENTA IS 'Numero de venta.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB.NUM_TRASPASO_MASIVO IS 'Numero de traspaso masivo.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB.GLOSA_ERROR_TRASPASO IS 'Glosa de error para traspaso.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB.GLOSA_ERROR_VENTA IS 'Glosa de error para venta.'
/
COMMENT ON COLUMN  AL_ERRORES_WEB.COD_ESTADO_ERROR IS 'Codigo de estado error PR = en error SO = solucionado.'
/
