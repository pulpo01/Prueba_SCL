 ,  FEC_MODIFICACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_DCIFRA VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CLASIF_CREDCLI_PLANES_TD IS '[ VE_CLPL ] Relación Clasificación Clientes con planes tarifarios'
/
