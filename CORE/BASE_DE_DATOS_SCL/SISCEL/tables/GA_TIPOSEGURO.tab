 ,  DES_TIPSEGU VARCHAR2(30) NOT NULL 
 ,  COD_ASEG NUMBER(3,0) NOT NULL 
 ,  TIP_FRANQUICIA NUMBER(1,0) NOT NULL 
 ,  IMP_FRANQUICIA NUMBER(14,4) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NULL 
 ,  COD_SERVICIO VARCHAR2(3) NULL 
 ,  COD_TIPCONTRATO VARCHAR2(3) NULL 
 ,  COD_SERVICIODES VARCHAR2(3) NULL 
 ,  FEC_DESDE DATE NULL 
 ,  FEC_HASTA DATE NULL 
 ,  NOM_USUARORA VARCHAR2(30) NULL 
 ,  NUM_REEMPLAZOS NUMBER(4,0) NULL 
 ,  NUM_MESES_REEMP NUMBER(4,0) NULL 
 ,  IMP_TOPE NUMBER(14,4) NULL 
