CREATE TABLE SC_PROCARCH(
    COD_EVENTO NUMBER(2,0) NOT NULL 
 ,  ID_LOTE VARCHAR2(50) NOT NULL 
 ,  IND_ACCION CHAR(1) NOT NULL 
 ,  NOM_ARCHIVO VARCHAR2(100) NOT NULL 
 ,  FEC_TRANSAC DATE NOT NULL 
 ,  IND_ESTADO CHAR(1) NOT NULL 
 ,  COD_OPERADORA_SCL VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SC_PROCARCH IS 'PROCESO DE ARCHIVOS PARA CONTABILIDAD'
/
COMMENT ON COLUMN  SC_PROCARCH.COD_EVENTO IS 'CODIGO DE EVENTO'
/
COMMENT ON COLUMN  SC_PROCARCH.ID_LOTE IS 'IDENTIFICACION DE LOTE'
/
COMMENT ON COLUMN  SC_PROCARCH.IND_ACCION IS 'INDICADOR DE ACCION C/E (CREACION/ELIMINACION)'
/
COMMENT ON COLUMN  SC_PROCARCH.NOM_ARCHIVO IS 'UBICACISN Y NOMBRE DELARCHIVO'
/
COMMENT ON COLUMN  SC_PROCARCH.FEC_TRANSAC IS 'FECHA DE CREACION/ELIMINACION DEL ARCHIVO'
/
COMMENT ON COLUMN  SC_PROCARCH.IND_ESTADO IS 'INDICADOR DE ESTADO C/P (CREADO/EN PROCESO)'
/
COMMENT ON COLUMN  SC_PROCARCH.COD_OPERADORA_SCL IS 'CODIGO DE OPERADORA'
/
