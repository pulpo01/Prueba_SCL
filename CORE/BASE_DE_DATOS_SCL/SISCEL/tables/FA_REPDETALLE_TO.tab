CREATE TABLE FA_REPDETALLE_TO(
    NUM_PROCPASOCOBRO NUMBER(8,0) NOT NULL 
 ,  COD_OPERADORA_SCL VARCHAR2(5) NOT NULL 
 ,  NUM_DOCOPERADORA NUMBER(7,0) NOT NULL 
 ,  TOT_OPERADORA NUMBER(16,4) NOT NULL 
 ,  MTO_AJUSTE NUMBER(10,4) NOT NULL 
 ,  FEC_GENINTCON DATE NULL 
 ,  FEC_IMPUTCONTABLE DATE NULL 
 ,  FEC_IMPCONTPAGOS DATE DEFAULT NULL NULL 
 ,  FEC_IMPCONTSTAFF DATE DEFAULT NULL NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_REPDETALLE_TO IS 'Detalle de Reportes de Procesos de Paso a Cobros.'
/
COMMENT ON COLUMN  FA_REPDETALLE_TO.NUM_PROCPASOCOBRO IS 'Numero de proceso de Intercalacion.'
/
COMMENT ON COLUMN  FA_REPDETALLE_TO.COD_OPERADORA_SCL IS 'Codigo de Operadora.'
/
COMMENT ON COLUMN  FA_REPDETALLE_TO.NUM_DOCOPERADORA IS 'Cantidad de documentos.'
/
COMMENT ON COLUMN  FA_REPDETALLE_TO.TOT_OPERADORA IS 'Monto de documentos intercalados.'
/
COMMENT ON COLUMN  FA_REPDETALLE_TO.MTO_AJUSTE IS 'Monto de ajuste.'
/
COMMENT ON COLUMN  FA_REPDETALLE_TO.FEC_GENINTCON IS 'Fecha generacion Interfaz Contable.'
/
COMMENT ON COLUMN  FA_REPDETALLE_TO.FEC_IMPUTCONTABLE IS 'Fecha Imputacion Contable.'
/
COMMENT ON COLUMN  FA_REPDETALLE_TO.FEC_IMPCONTPAGOS IS 'Fecha Imputacion contable de Pagos.'
/
COMMENT ON COLUMN  FA_REPDETALLE_TO.FEC_IMPCONTSTAFF IS 'Fecha Imputacion contable del Pago Staff.'
/
