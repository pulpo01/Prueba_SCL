CREATE TABLE SC_TUPLA(
    ID_TUPLA NUMBER(10,0) NOT NULL 
 ,  DOMINIO VARCHAR2(32) NULL 
 ,  CONCEPTO VARCHAR2(76) NULL 
 ,  DESCRIPCION VARCHAR2(200) NULL 
 ,  CONTABLE NUMBER(1,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_RECYCOB_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SC_TUPLA IS 'Auxiliar Para Formar Los Detalles De Grupos De Dominios (Tuplas).'
/
COMMENT ON COLUMN  SC_TUPLA.ID_TUPLA IS 'IDENTIFICACION DE LA TUPLA.'
/
COMMENT ON COLUMN  SC_TUPLA.DOMINIO IS 'COMBINACION DE DOMINIOS QUE FORMAN LA TUPLA.'
/
COMMENT ON COLUMN  SC_TUPLA.CONCEPTO IS 'COMBINACION DE CONCEPTOS DE LOS DOMINIOS QUE FORMAN LA TUPLA.'
/
COMMENT ON COLUMN  SC_TUPLA.DESCRIPCION IS 'Descripcion del concepto de grupo ( conceptos combinados).'
/
COMMENT ON COLUMN  SC_TUPLA.CONTABLE IS 'Indica si el concepto es contabilizable (0) o no lo es (1).'
/
