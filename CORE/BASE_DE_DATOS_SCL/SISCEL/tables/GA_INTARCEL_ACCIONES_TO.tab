CREATE TABLE GA_INTARCEL_ACCIONES_TO(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  IND_NUMERO NUMBER(1,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  COD_ACTABO VARCHAR2(2) NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_INTARCEL_ACCIONES_TO IS 'Causa Comercial de registro en GA_INTARCEL.'
/
COMMENT ON COLUMN  GA_INTARCEL_ACCIONES_TO.COD_CLIENTE IS 'Codigo de Cliente'
/
COMMENT ON COLUMN  GA_INTARCEL_ACCIONES_TO.NUM_ABONADO IS 'Numero de Abonado'
/
COMMENT ON COLUMN  GA_INTARCEL_ACCIONES_TO.IND_NUMERO IS 'Indicador de numeracion'
/
COMMENT ON COLUMN  GA_INTARCEL_ACCIONES_TO.FEC_DESDE IS 'Fecha Inicio Vigencia del Concepto en ga_intarcel'
/
COMMENT ON COLUMN  GA_INTARCEL_ACCIONES_TO.COD_ACTABO IS 'Codigo de Actuacion Comercial'
/
COMMENT ON COLUMN  GA_INTARCEL_ACCIONES_TO.NOM_USUARORA IS 'Usuario Oracle que ingresa el registro'
/
COMMENT ON COLUMN  GA_INTARCEL_ACCIONES_TO.FEC_INGRESO IS 'Fecha de Ingreso del Registro'
/
