CREATE TABLE GSM_SECARCH_CAMPOS_TO(
    COD_CAMPO VARCHAR2(10) NOT NULL 
 ,  SEC_ARCHIVO_SECCION NUMBER(10,0) NOT NULL 
 ,  NUM_SEQ_CAMPO NUMBER NOT NULL 
 ,  IND_TABLA VARCHAR2(1) NOT NULL 
 ,  IND_MODIF NUMBER(1,0) DEFAULT 1 NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_SECARCH_CAMPOS_TO IS 'Tabla de relacion seccion de un Archivo con sus campos.'
/
COMMENT ON COLUMN  GSM_SECARCH_CAMPOS_TO.COD_CAMPO IS 'Codigo de Campo.'
/
COMMENT ON COLUMN  GSM_SECARCH_CAMPOS_TO.SEC_ARCHIVO_SECCION IS 'Codigo unico de relacion archivo y su s secciones.'
/
COMMENT ON COLUMN  GSM_SECARCH_CAMPOS_TO.NUM_SEQ_CAMPO IS 'Secuencial de posicion de campo en al seccion del archivo.'
/
COMMENT ON COLUMN  GSM_SECARCH_CAMPOS_TO.IND_TABLA IS 'Indicador de campo o prefijo.'
/
COMMENT ON COLUMN  GSM_SECARCH_CAMPOS_TO.IND_MODIF IS 'Indicador si el campo es modificable (0) o no modificable (1)'
/
