CREATE TABLE AL_ASIG_DOCUMENTOST
(
  COD_OFICINA     VARCHAR2(2 BYTE)              NOT NULL,
  COD_TIPDOCUM    NUMBER(2)                     NOT NULL,
  FEC_ASIGNA      DATE                          NOT NULL,
  RAN_DESDE       NUMBER(9)                     NOT NULL,
  RAN_HASTA       NUMBER(9)                     NOT NULL,
  RAN_USADO       NUMBER(9)                     DEFAULT 0                     NOT NULL,
  PREF_PLAZA      VARCHAR2(25 BYTE)             DEFAULT ' '                   NOT NULL,
  COD_OPERADORA   VARCHAR2(5 BYTE)              NOT NULL,
  RESOLUCION      VARCHAR2(25 BYTE),
  FEC_RESOLUCION  DATE,
  SERIE           VARCHAR2(10 BYTE),
  ETIQUETA        VARCHAR2(10 BYTE),
  FEC_CREAFOLIO   DATE,
  NUM_SOLFOLIOS   NUMBER(15),
  COD_ESTADOSOL   VARCHAR2(2 BYTE)
)
TABLESPACE TS_LOGISTICA_SDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          1144K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE AL_ASIG_DOCUMENTOST IS 'Tabla donde se guardan los Rangos de Documentos que seran Asignados posteriormente';

COMMENT ON COLUMN AL_ASIG_DOCUMENTOST.FEC_CREAFOLIO IS 'Fecha de creación de la solicitud de folios';

COMMENT ON COLUMN AL_ASIG_DOCUMENTOST.NUM_SOLFOLIOS IS 'Nro de solicitud de folios';

COMMENT ON COLUMN AL_ASIG_DOCUMENTOST.COD_ESTADOSOL IS 'Estado sol. SO=Solicitada, AN=Anulada,AS=Asignada,AU=Autorizada,RE=Rechazada';

COMMENT ON COLUMN AL_ASIG_DOCUMENTOST.COD_OFICINA IS 'Oficina o sucursal a la que se hace la asignacion de un rango';

COMMENT ON COLUMN AL_ASIG_DOCUMENTOST.COD_TIPDOCUM IS 'Tipo de Documento';

COMMENT ON COLUMN AL_ASIG_DOCUMENTOST.FEC_ASIGNA IS 'Mes de Asignacion.';

COMMENT ON COLUMN AL_ASIG_DOCUMENTOST.RAN_DESDE IS 'Desde Numero de Folio';

COMMENT ON COLUMN AL_ASIG_DOCUMENTOST.RAN_HASTA IS 'Hasta numero de folio';

COMMENT ON COLUMN AL_ASIG_DOCUMENTOST.RAN_USADO IS 'Ultimo numero de folio usado';

COMMENT ON COLUMN AL_ASIG_DOCUMENTOST.RESOLUCION IS 'Resolución';

COMMENT ON COLUMN AL_ASIG_DOCUMENTOST.FEC_RESOLUCION IS 'Fecha de resolución';

COMMENT ON COLUMN AL_ASIG_DOCUMENTOST.SERIE IS 'Serie de folios';

COMMENT ON COLUMN AL_ASIG_DOCUMENTOST.ETIQUETA IS 'Etiqueta de folios';

COMMENT ON COLUMN AL_ASIG_DOCUMENTOST.PREF_PLAZA IS 'Prefijo de folio de la plaza';

COMMENT ON COLUMN AL_ASIG_DOCUMENTOST.COD_OPERADORA IS 'Codigo de Operadora';
