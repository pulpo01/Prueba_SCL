CREATE TABLE CO_CONVENIO_AVAL(
    NUM_CONVENIO NUMBER(8,0) NOT NULL 
 ,  NUM_IDENT_AVAL_1 VARCHAR2(20) NULL 
 ,  NOMBRE_AVAL_1 VARCHAR2(40) NULL 
 ,  PROFESION_AVAL_1 VARCHAR2(30) NULL 
 ,  DOMICILIO_AVAL_1 VARCHAR2(40) NULL 
 ,  COMUNA_AVAL_1 VARCHAR2(20) NULL 
 ,  REGION_AVAL_1 VARCHAR2(20) NULL 
 ,  NUM_IDENT_AVAL_2 VARCHAR2(20) NULL 
 ,  NOMBRE_AVAL_2 VARCHAR2(40) NULL 
 ,  PROFESION_AVAL_2 VARCHAR2(30) NULL 
 ,  DOMICILIO_AVAL_2 VARCHAR2(40) NULL 
 ,  COMUNA_AVAL_2 VARCHAR2(20) NULL 
 ,  REGION_AVAL_2 VARCHAR2(20) NULL 
 ,  COD_TIPIDENT_AVAL_1 VARCHAR2(2) NULL 
 ,  COD_TIPIDENT_AVAL_2 VARCHAR2(2) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
