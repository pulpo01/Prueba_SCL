/
COMMENT ON COLUMN  CO_CONVENIO_AVAL.COMUNA_AVAL_1 IS 'Comuna del Primer Aval.'
/
COMMENT ON COLUMN  CO_CONVENIO_AVAL.REGION_AVAL_1 IS 'Region del Primer Aval.'
/
COMMENT ON COLUMN  CO_CONVENIO_AVAL.NUM_IDENT_AVAL_2 IS 'Rut del Segundo Aval.'
