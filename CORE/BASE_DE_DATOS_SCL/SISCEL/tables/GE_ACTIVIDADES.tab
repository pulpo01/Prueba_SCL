CREATE TABLE GE_ACTIVIDADES(
    COD_ACTIVIDAD NUMBER(3,0) NOT NULL 
 ,  DES_ACTIVIDAD VARCHAR2(40) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON COLUMN  GE_ACTIVIDADES.COD_ACTIVIDAD IS 'Codigo de actividad'
/
COMMENT ON COLUMN  GE_ACTIVIDADES.DES_ACTIVIDAD IS 'Descripcion de la actividad'
/
