CREATE TABLE GSM_DOMINIOS_PREFIJOS_TD(
    COD_CAMPO VARCHAR2(10) NOT NULL 
 ,  COD_PARAMETRO VARCHAR2(10) NOT NULL 
 ,  NOM_DOMINIO VARCHAR2(50) NULL 
 ,  VAL_DOMINIO VARCHAR2(20) NOT NULL 
 ,  NUM_LARGO NUMBER NULL 
 ,  COD_RELLENO VARCHAR2(1) NULL 
 ,  FEC_ACTUALIZACION DATE DEFAULT SYSDATE NULL 
 ,  COD_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_DOMINIOS_PREFIJOS_TD IS 'Tabla de dominios (valores) para los prefijos.'
/
COMMENT ON COLUMN  GSM_DOMINIOS_PREFIJOS_TD.COD_CAMPO IS 'Codigo de Campo.'
/
COMMENT ON COLUMN  GSM_DOMINIOS_PREFIJOS_TD.COD_PARAMETRO IS 'Codigo del parametro.'
/
COMMENT ON COLUMN  GSM_DOMINIOS_PREFIJOS_TD.NOM_DOMINIO IS 'Nombre del Dominio.'
/
COMMENT ON COLUMN  GSM_DOMINIOS_PREFIJOS_TD.VAL_DOMINIO IS 'Valor del Dominio.'
/
COMMENT ON COLUMN  GSM_DOMINIOS_PREFIJOS_TD.NUM_LARGO IS 'Largo del campo.'
/
COMMENT ON COLUMN  GSM_DOMINIOS_PREFIJOS_TD.COD_RELLENO IS 'Valor de relleno.'
/
COMMENT ON COLUMN  GSM_DOMINIOS_PREFIJOS_TD.FEC_ACTUALIZACION IS 'Fecha de la ultima actualizacion.'
/
COMMENT ON COLUMN  GSM_DOMINIOS_PREFIJOS_TD.COD_USUARIO IS 'Codigo del usuario que actualizo.'
/
