CREATE TABLE VE_CUADCOMIS(
    COD_CUADRANTE NUMBER(8,0) NOT NULL 
 ,  IMP_DESDE NUMBER(12,4) NOT NULL 
 ,  IMP_HASTA NUMBER(12,4) NOT NULL 
 ,  IMP_RANGO NUMBER(12,4) NOT NULL 
 ,  COD_CATEGORIA VARCHAR2(3) NOT NULL 
 ,  NUM_DESDE NUMBER(4,0) NOT NULL 
 ,  NUM_HASTA NUMBER(4,0) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CUADCOMIS IS 'Contenido de Cuadrante de Comisiones'
/
COMMENT ON COLUMN  VE_CUADCOMIS.COD_CUADRANTE IS 'Codigo de Cuadrante de Liquidacion'
/
COMMENT ON COLUMN  VE_CUADCOMIS.IMP_DESDE IS 'Importe minimo del rango'
/
COMMENT ON COLUMN  VE_CUADCOMIS.IMP_HASTA IS 'Importe maximo del rango'
/
COMMENT ON COLUMN  VE_CUADCOMIS.IMP_RANGO IS 'Importe de la comision en el rango (IMP_DESDE-IMP_HASTA)'
/
COMMENT ON COLUMN  VE_CUADCOMIS.COD_CATEGORIA IS 'Categoria asociada al rango'
/
COMMENT ON COLUMN  VE_CUADCOMIS.NUM_DESDE IS 'Numero inicio de rango de la subcategoria'
/
COMMENT ON COLUMN  VE_CUADCOMIS.NUM_HASTA IS 'Numero fin de rango de la subcategoria'
/
