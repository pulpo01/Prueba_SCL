CREATE TABLE CA_PREGUNTAS(
    COD_CUESTIONARIO VARCHAR2(6) NOT NULL 
 ,  NUM_PREGUNTA NUMBER(2,0) NOT NULL 
 ,  DES_PREGUNTA VARCHAR2(80) NOT NULL 
 ,  NUM_EXPLICACION NUMBER(4,0) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_PREGUNTAS IS 'Preguntas'
/
COMMENT ON COLUMN  CA_PREGUNTAS.COD_CUESTIONARIO IS 'Codigo Cueationario'
/
COMMENT ON COLUMN  CA_PREGUNTAS.NUM_PREGUNTA IS 'Numero Pregunta'
/
COMMENT ON COLUMN  CA_PREGUNTAS.DES_PREGUNTA IS 'Texto pregunta'
/
COMMENT ON COLUMN  CA_PREGUNTAS.NUM_EXPLICACION IS 'Numero explicacion'
/
