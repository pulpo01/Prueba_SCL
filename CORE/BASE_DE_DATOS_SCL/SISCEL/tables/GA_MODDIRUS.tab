CREATE TABLE GA_MODDIRUS(
    COD_USUARIO NUMBER(8,0) NOT NULL 
 ,  FEC_MODIFICA DATE NOT NULL 
 ,  COD_TIPDIRECCION VARCHAR2(1) NOT NULL 
 ,  COD_DIRECANT NUMBER(20,0) NOT NULL 
 ,  COD_DIRECNUE NUMBER(20,0) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_MODDIRUS IS 'Modificaciones de Direcciones de Usuarios'
/
COMMENT ON COLUMN  GA_MODDIRUS.COD_USUARIO IS 'Codigo de Usuario'
/
COMMENT ON COLUMN  GA_MODDIRUS.FEC_MODIFICA IS 'Fecha de Modificacion'
/
COMMENT ON COLUMN  GA_MODDIRUS.COD_TIPDIRECCION IS 'Codigo de Tipo de Direccion'
/
COMMENT ON COLUMN  GA_MODDIRUS.COD_DIRECANT IS 'Codigo de Direccion Anterior'
/
COMMENT ON COLUMN  GA_MODDIRUS.COD_DIRECNUE IS 'Codigo de Direccion Nueva'
/
COMMENT ON COLUMN  GA_MODDIRUS.NOM_USUARORA IS 'Codigo de Usuario Oracle'
/
