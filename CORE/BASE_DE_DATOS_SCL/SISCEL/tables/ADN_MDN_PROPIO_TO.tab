CREATE TABLE ADN_MDN_PROPIO_TO(
    NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  COD_SUBALM VARCHAR2(5) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  COD_CAT NUMBER(2,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  FEC_BAJA DATE NOT NULL 
 ,  IND_EQUIPADO NUMBER(1,0) NOT NULL 
 ,  USO_ANTERIOR NUMBER(2,0) NULL 
 ,  FEC_CONSUMO DATE NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  ADN_MDN_PROPIO_TO.NUM_CELULAR IS 'Numero de Celular'
/
COMMENT ON COLUMN  ADN_MDN_PROPIO_TO.COD_SUBALM IS 'Codigo de Subalm'
/
COMMENT ON COLUMN  ADN_MDN_PROPIO_TO.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  ADN_MDN_PROPIO_TO.COD_CENTRAL IS 'Codigo de Central'
/
COMMENT ON COLUMN  ADN_MDN_PROPIO_TO.COD_CAT IS 'Codigo de Categoria'
/
COMMENT ON COLUMN  ADN_MDN_PROPIO_TO.COD_USO IS 'Codigo de Uso'
/
COMMENT ON COLUMN  ADN_MDN_PROPIO_TO.FEC_BAJA IS 'Fecha en que se da de baja el num celular '
/
COMMENT ON COLUMN  ADN_MDN_PROPIO_TO.IND_EQUIPADO IS 'Indicador de si es equipado'
/
COMMENT ON COLUMN  ADN_MDN_PROPIO_TO.USO_ANTERIOR IS 'Indicador del Uso que se le dio anteriormente'
/
COMMENT ON COLUMN  ADN_MDN_PROPIO_TO.FEC_CONSUMO IS 'Fecha de consumo'
/
