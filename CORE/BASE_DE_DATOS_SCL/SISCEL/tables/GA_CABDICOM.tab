CREATE TABLE GA_CABDICOM(
    FEC_CONSULTA DATE NOT NULL 
 ,  NUM_CONSULTA NUMBER(8,0) NOT NULL 
 ,  IND_DETALLE VARCHAR2(1) NOT NULL 
 ,  NUM_CHEQUES NUMBER(4,0) NOT NULL 
 ,  NUM_LETRAS NUMBER(4,0) NOT NULL 
 ,  NUM_PAGARES NUMBER(4,0) NOT NULL 
 ,  NUM_OTROS NUMBER(4,0) NOT NULL 
 ,  NUM_TOTAL NUMBER(4,0) NOT NULL 
 ,  NUM_IDENT VARCHAR2(20) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_CABDICOM IS 'Tabla de cabecera de consultas al Dicom'
/
COMMENT ON COLUMN  GA_CABDICOM.FEC_CONSULTA IS 'Fecha en que se realizo la consulta al dicom'
/
COMMENT ON COLUMN  GA_CABDICOM.NUM_CONSULTA IS 'Numero de la Consulta procedente de la secuencia GA_SEQ_CONSDICOM'
/
COMMENT ON COLUMN  GA_CABDICOM.IND_DETALLE IS 'indicativo de detalle (s)i ,(n)o'
/
COMMENT ON COLUMN  GA_CABDICOM.NUM_CHEQUES IS 'numero de  cheques protestados'
/
COMMENT ON COLUMN  GA_CABDICOM.NUM_LETRAS IS 'numero de letras protestadas'
/
COMMENT ON COLUMN  GA_CABDICOM.NUM_PAGARES IS 'numero de pagares protestados'
/
COMMENT ON COLUMN  GA_CABDICOM.NUM_OTROS IS 'otros protestos'
/
COMMENT ON COLUMN  GA_CABDICOM.NUM_TOTAL IS 'numero total de protestos'
/
COMMENT ON COLUMN  GA_CABDICOM.NUM_IDENT IS 'Rut del que se ha hecho la consulta'
/
