 ,  COD_IMPREAL NUMBER(2,0) NOT NULL 
 ,  VAL_COMI NUMBER(14,4) NOT NULL 
 ,  VAL_FACTOR NUMBER(14,4) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_RNGCOMI IS 'RANGO DE COMISIONES'
/
COMMENT ON COLUMN  CO_RNGCOMI.COD_TIPCOMISION IS 'TIPO DE COMISION'
/
COMMENT ON COLUMN  CO_RNGCOMI.COD_IMPASIG IS 'CODIGO DEL RANGO DE IMPORTE ASIGNADO'
/
COMMENT ON COLUMN  CO_RNGCOMI.COD_IMPREAL IS 'CODIGO DEL RANGO DEL IMPORTE REAL'
/
COMMENT ON COLUMN  CO_RNGCOMI.VAL_COMI IS 'VALOR DE LA COMISION'
/
COMMENT ON COLUMN  CO_RNGCOMI.VAL_FACTOR IS 'VALOR DEL FACTOR DE LA COMISION'
/
