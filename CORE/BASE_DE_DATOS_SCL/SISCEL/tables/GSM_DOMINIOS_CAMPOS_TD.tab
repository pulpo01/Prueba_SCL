CREATE TABLE GSM_DOMINIOS_CAMPOS_TD
(
  COD_CAMPO          VARCHAR2(10 BYTE)          NOT NULL,
  COD_DOMINIO        VARCHAR2(10 BYTE)          NOT NULL,
  NOM_DOMINO         VARCHAR2(50 BYTE),
  VAL_DOMINIO        VARCHAR2(50 BYTE)          NOT NULL,
  COD_VIGENCIA       VARCHAR2(1 BYTE)           DEFAULT 1                     NOT NULL,
  FEC_ACTUALIZACION  DATE                       DEFAULT SYSDATE               NOT NULL,
  COD_USUARIO        VARCHAR2(30 BYTE)          NOT NULL,
  IND_TIPO           NUMBER(1)
)
TABLESPACE TS_LOGISTICA_SDAT
PCTUSED    0
PCTFREE    20
INITRANS   16
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE GSM_DOMINIOS_CAMPOS_TD IS 'Tabla de dominios (valores) para los CAMPOS.';

COMMENT ON COLUMN GSM_DOMINIOS_CAMPOS_TD.IND_TIPO IS 'Indica tipificacion: 1=Postpago,2=prepago,nulo=no tiene tipificacion';

COMMENT ON COLUMN GSM_DOMINIOS_CAMPOS_TD.COD_CAMPO IS 'Codigo de Campo.';

COMMENT ON COLUMN GSM_DOMINIOS_CAMPOS_TD.COD_DOMINIO IS 'Codigo de Dominio.';

COMMENT ON COLUMN GSM_DOMINIOS_CAMPOS_TD.NOM_DOMINO IS 'Nombre del dominio.';

COMMENT ON COLUMN GSM_DOMINIOS_CAMPOS_TD.VAL_DOMINIO IS 'Valor asociado al dominio del campo.';

COMMENT ON COLUMN GSM_DOMINIOS_CAMPOS_TD.COD_VIGENCIA IS 'Codigo de Vigencia  por default 1 y 0  No vigente.';

COMMENT ON COLUMN GSM_DOMINIOS_CAMPOS_TD.FEC_ACTUALIZACION IS 'Fecha de la ultima actualizacion.';

COMMENT ON COLUMN GSM_DOMINIOS_CAMPOS_TD.COD_USUARIO IS 'Codigo del usuario que actualizo.';
