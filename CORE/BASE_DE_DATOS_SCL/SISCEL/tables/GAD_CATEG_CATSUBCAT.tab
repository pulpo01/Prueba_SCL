CREATE TABLE GAD_CATEG_CATSUBCAT(
    COD_CATEGORIA NUMBER(4,0) NOT NULL 
 ,  COD_SUBCATEGORIA VARCHAR2(5) NOT NULL 
 ,  IND_DEFAULT VARCHAR2(1) NOT NULL 
 ,  IND_VIGENCIA VARCHAR2(1) NOT NULL 
 ,  FEC_IND_VIGENCIA DATE NOT NULL 
 ,  NOM_USU_INDVIGENCIA VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAD_CATEG_CATSUBCAT IS 'Relacion Entre Las Categorias Y Las Subcategorias, Indicando Una Por Default..'
/
COMMENT ON COLUMN  GAD_CATEG_CATSUBCAT.COD_CATEGORIA IS 'Codigo de Categoria de Cuenta.'
/
COMMENT ON COLUMN  GAD_CATEG_CATSUBCAT.COD_SUBCATEGORIA IS 'Codigo de Subcategoria de cuenta.'
/
COMMENT ON COLUMN  GAD_CATEG_CATSUBCAT.IND_DEFAULT IS 'Indicador de subcategoria por default asociada a la categoria.'
/
COMMENT ON COLUMN  GAD_CATEG_CATSUBCAT.IND_VIGENCIA IS 'Indicador de vigencia de la relacion entre categoria y subcategoria.'
/
COMMENT ON COLUMN  GAD_CATEG_CATSUBCAT.FEC_IND_VIGENCIA IS 'Fecha en que se modifico el indicador de vigencia.'
/
COMMENT ON COLUMN  GAD_CATEG_CATSUBCAT.NOM_USU_INDVIGENCIA IS 'Usuario que modifico el indicador de vigencia.'
/
COMMENT ON COLUMN  GAD_CATEG_CATSUBCAT.FEC_ULTMOD IS 'Fecha de la ultima modificacion realizada al registro.'
/
COMMENT ON COLUMN  GAD_CATEG_CATSUBCAT.NOM_USUARIO IS 'Usuario que realizo la ultima modificacion al registro.'
/
