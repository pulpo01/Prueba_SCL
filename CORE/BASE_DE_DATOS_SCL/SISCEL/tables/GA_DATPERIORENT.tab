CREATE TABLE GA_DATPERIORENT(
    COD_PERIODO VARCHAR2(2) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) DEFAULT 1 NOT NULL 
 ,  NUM_DIAS NUMBER(3,0) NOT NULL 
 ,  COD_PLANTARIF_AN VARCHAR2(3) NOT NULL 
 ,  COD_PLANTARIF_DG VARCHAR2(3) NOT NULL 
 ,  COD_PLANSERV VARCHAR2(3) NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_DATPERIORENT IS 'Periodos de Contratos de rentaphon'
/
COMMENT ON COLUMN  GA_DATPERIORENT.FEC_DESDE IS 'Fecha Desde Vigencia datos'
/
COMMENT ON COLUMN  GA_DATPERIORENT.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_DATPERIORENT.NUM_DIAS IS 'Numero de Dias del Periodo'
/
COMMENT ON COLUMN  GA_DATPERIORENT.COD_PLANTARIF_AN IS 'Codigo de Plan Tarifario Analogico asociado al Periodo'
/
COMMENT ON COLUMN  GA_DATPERIORENT.COD_PLANTARIF_DG IS 'Codigo de Plan Tarifario Digital asociado al Periodo'
/
COMMENT ON COLUMN  GA_DATPERIORENT.COD_PLANSERV IS 'Codigo de Plan de Servicios asociado al Periodo'
/
COMMENT ON COLUMN  GA_DATPERIORENT.FEC_HASTA IS 'Fecha Hasta Vigencia datos'
/
COMMENT ON COLUMN  GA_DATPERIORENT.COD_PERIODO IS 'Codigo de Periodo'
/
