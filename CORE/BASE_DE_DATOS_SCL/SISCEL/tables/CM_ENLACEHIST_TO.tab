CREATE TABLE CM_ENLACEHIST_TO(
    COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  NOM_LOGICO VARCHAR2(30) NOT NULL 
 ,  NOM_FISICO VARCHAR2(30) NULL 
 ,  ID_PERIODO VARCHAR2(10) DEFAULT 'NN' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_ENLACEHIST_TO IS 'Tabla De Enlace Historico.'
/
COMMENT ON COLUMN  CM_ENLACEHIST_TO.COD_PERIODO IS 'CODIGO DEL PERIODO DE COMISIONES.'
/
COMMENT ON COLUMN  CM_ENLACEHIST_TO.NOM_LOGICO IS 'NOMBRE LOGICO DE LA TABLA..'
/
COMMENT ON COLUMN  CM_ENLACEHIST_TO.NOM_FISICO IS 'NOMBRE FISICO DE LA TABLA, EN FUNCION DEL ESTADO DEL PERIODO..'
/
COMMENT ON COLUMN  CM_ENLACEHIST_TO.ID_PERIODO IS 'IDENTIFICADOR DEL CICLO DE COMISIONES'
/
