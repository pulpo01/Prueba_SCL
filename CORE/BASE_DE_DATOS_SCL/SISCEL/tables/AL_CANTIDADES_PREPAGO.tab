CREATE TABLE AL_CANTIDADES_PREPAGO
(
  COD_PREPAGO  NUMBER(2)                        NOT NULL,
  CAN_PREPAGO  NUMBER(14,4)                     NOT NULL,
  DES_PREPAGO  VARCHAR2(100 BYTE)               NOT NULL
)
TABLESPACE TS_LOGISTICA_SDAT
PCTUSED    0
PCTFREE    20
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          40K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE AL_CANTIDADES_PREPAGO IS 'Valores de carga para Prepago.';

COMMENT ON COLUMN AL_CANTIDADES_PREPAGO.COD_PREPAGO IS 'Codigo identificativo del registro.';

COMMENT ON COLUMN AL_CANTIDADES_PREPAGO.CAN_PREPAGO IS 'Cantidad de la carga de Prepago.';

COMMENT ON COLUMN AL_CANTIDADES_PREPAGO.DES_PREPAGO IS 'Descripcion asociada a la carga de Prepago.';
