CREATE TABLE CO_RECUPERO_COMISION_TO(
    COD_ENTIDAD VARCHAR2(5) NOT NULL 
 ,  MONTO_ASIGNADO NUMBER(14,4) NOT NULL 
 ,  MONTO_RECUPERADO NUMBER(14,4) NOT NULL 
 ,  PORC_RECUPERADO NUMBER(8,4) NOT NULL 
 ,  PORC_APLICADO NUMBER(8,4) NOT NULL 
 ,  COMISION NUMBER(14,4) NOT NULL 
 ,  FECHA_DESDE DATE NOT NULL 
 ,  FECHA_HASTA DATE NOT NULL 
 ,  FECHA_COMISION DATE NOT NULL 
 ,  USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  CO_RECUPERO_COMISION_TO.COD_ENTIDAD IS 'Entidad de Cobranza Externa.'
/
COMMENT ON COLUMN  CO_RECUPERO_COMISION_TO.MONTO_ASIGNADO IS 'Monto Total Asignado.'
/
COMMENT ON COLUMN  CO_RECUPERO_COMISION_TO.MONTO_RECUPERADO IS 'Monto recuperado.'
/
COMMENT ON COLUMN  CO_RECUPERO_COMISION_TO.PORC_RECUPERADO IS 'Porcentaje de recuperacion.'
/
COMMENT ON COLUMN  CO_RECUPERO_COMISION_TO.PORC_APLICADO IS 'Porcentaje de Calculo.'
/
COMMENT ON COLUMN  CO_RECUPERO_COMISION_TO.COMISION IS 'Monto de Comision.'
/
COMMENT ON COLUMN  CO_RECUPERO_COMISION_TO.FECHA_DESDE IS 'Fecha desde para periodo comisionable.'
/
COMMENT ON COLUMN  CO_RECUPERO_COMISION_TO.FECHA_HASTA IS 'Fecha hasta para periodo comisionable.'
/
COMMENT ON COLUMN  CO_RECUPERO_COMISION_TO.FECHA_COMISION IS 'Fecha de calculo de Comision.'
/
COMMENT ON COLUMN  CO_RECUPERO_COMISION_TO.USUARIO IS 'Usuario.'
/
