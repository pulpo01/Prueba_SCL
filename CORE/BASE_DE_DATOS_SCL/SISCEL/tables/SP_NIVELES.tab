CREATE TABLE SP_NIVELES(
    COD_NIVEL NUMBER(2,0) NOT NULL 
 ,  DES_NIVEL VARCHAR2(15) NOT NULL 
 ,  PRC_NIVEL NUMBER(15,4) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SP_NIVELES IS 'Elementos de Chequeo'
/
COMMENT ON COLUMN  SP_NIVELES.COD_NIVEL IS 'Codigo del nivel de aprobacion de Presupuesto'
/
COMMENT ON COLUMN  SP_NIVELES.DES_NIVEL IS 'Descripcion del nivel'
/
COMMENT ON COLUMN  SP_NIVELES.PRC_NIVEL IS 'Cantidad en pesos aprobada para el nivel'
/
COMMENT ON COLUMN  SP_NIVELES.COD_MONEDA IS 'Codigo de la moneda del monto de preaprobacion'
/
