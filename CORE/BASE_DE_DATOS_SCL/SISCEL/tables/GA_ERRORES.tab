CREATE TABLE GA_ERRORES(
    COD_ACTABO VARCHAR2(2) NOT NULL 
 ,  CODIGO NUMBER(9,0) NOT NULL 
 ,  FEC_ERROR DATE NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  NOM_PROC VARCHAR2(50) NULL 
 ,  NOM_TABLA VARCHAR2(50) NULL 
 ,  COD_ACT VARCHAR2(1) NULL 
 ,  COD_SQLCODE VARCHAR2(15) NULL 
 ,  COD_SQLERRM VARCHAR2(60) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_ERRORES IS 'Errores producidos durante la ejecucion de PLs'
/
COMMENT ON COLUMN  GA_ERRORES.COD_ACTABO IS 'Codigo de Actuacion de Abonados'
/
COMMENT ON COLUMN  GA_ERRORES.CODIGO IS 'Numero del cliente sobre el cual da el error'
/
COMMENT ON COLUMN  GA_ERRORES.FEC_ERROR IS 'Fecha de cuando fue el error'
/
COMMENT ON COLUMN  GA_ERRORES.COD_PRODUCTO IS 'Codigo del producto'
/
COMMENT ON COLUMN  GA_ERRORES.NOM_PROC IS 'Nombre del procedimiento que da el error'
/
COMMENT ON COLUMN  GA_ERRORES.NOM_TABLA IS 'Nombre de la tabla donde ocurre el error'
/
COMMENT ON COLUMN  GA_ERRORES.COD_ACT IS 'Tipo de actuacion sobre la tabla'
/
COMMENT ON COLUMN  GA_ERRORES.COD_SQLCODE IS 'Codigo de sql que devuelve el oracle'
/
COMMENT ON COLUMN  GA_ERRORES.COD_SQLERRM IS 'Mensaje de error que devuelve oracle'
/
