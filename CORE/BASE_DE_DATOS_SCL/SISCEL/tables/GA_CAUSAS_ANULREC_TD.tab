CREATE TABLE GA_CAUSAS_ANULREC_TD(
    COD_CAUSA_ANULREC VARCHAR2(2) NOT NULL 
 ,  DES_CAUSA_ANULREC VARCHAR2(30) NOT NULL 
 ,  IND_COBRO NUMBER(1,0) NOT NULL 
 ,  IND_ACTIVO NUMBER(1,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_CAUSAS_ANULREC_TD IS 'Causas De Anulacion De Rechazo.'
/
COMMENT ON COLUMN  GA_CAUSAS_ANULREC_TD.COD_CAUSA_ANULREC IS 'Codigo anulacion de rechazo de venta..'
/
COMMENT ON COLUMN  GA_CAUSAS_ANULREC_TD.DES_CAUSA_ANULREC IS 'Descripcion de anulacion de rechazo de venta..'
/
COMMENT ON COLUMN  GA_CAUSAS_ANULREC_TD.IND_COBRO IS 'Indica si por el motivo de anulacion de rechazo corresponde cobro del trafico a algun cliente (distribuidor): 1 = Si | 0 = No.'
/
COMMENT ON COLUMN  GA_CAUSAS_ANULREC_TD.IND_ACTIVO IS 'Indicador de causal de anulacion de rechazo activa por operadora (0:inactiva 1:activa).'
/
