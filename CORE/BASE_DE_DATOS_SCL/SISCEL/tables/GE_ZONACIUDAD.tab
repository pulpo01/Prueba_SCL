CREATE TABLE GE_ZONACIUDAD(
    COD_REGION VARCHAR2(3) NOT NULL 
 ,  COD_PROVINCIA VARCHAR2(5) NOT NULL 
 ,  COD_CIUDAD VARCHAR2(5) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  COD_ZONAIMPO NUMBER(2,0) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_ZONACIUDAD IS 'Relacion de ciudades con zonas impositivas'
/
COMMENT ON COLUMN  GE_ZONACIUDAD.COD_REGION IS 'Codigo de Region'
/
COMMENT ON COLUMN  GE_ZONACIUDAD.COD_PROVINCIA IS 'Codigo de provincia'
/
COMMENT ON COLUMN  GE_ZONACIUDAD.COD_CIUDAD IS 'Codigo de Ciudad'
/
COMMENT ON COLUMN  GE_ZONACIUDAD.FEC_DESDE IS 'Fecha Desde'
/
COMMENT ON COLUMN  GE_ZONACIUDAD.FEC_HASTA IS 'Fecha Hasta'
/
COMMENT ON COLUMN  GE_ZONACIUDAD.COD_ZONAIMPO IS 'Division en zonas impositivas del pais'
/
