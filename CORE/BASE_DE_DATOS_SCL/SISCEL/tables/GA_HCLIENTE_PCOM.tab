CREATE TABLE GA_HCLIENTE_PCOM(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_PLANCOM NUMBER(6,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_HCLIENTE_PCOM IS 'Historico Planes Comerciales de Clientes'
/
COMMENT ON COLUMN  GA_HCLIENTE_PCOM.COD_CLIENTE IS 'Codigo de Cliente'
/
COMMENT ON COLUMN  GA_HCLIENTE_PCOM.COD_PLANCOM IS 'Codigo de Plan Comercial'
/
COMMENT ON COLUMN  GA_HCLIENTE_PCOM.FEC_DESDE IS 'Fecha de Suscripcion al Plan'
/
COMMENT ON COLUMN  GA_HCLIENTE_PCOM.NOM_USUARORA IS 'Usuario Asignador del Plan'
/
COMMENT ON COLUMN  GA_HCLIENTE_PCOM.FEC_HISTORICO IS 'Fecha de Historico'
/
COMMENT ON COLUMN  GA_HCLIENTE_PCOM.FEC_HASTA IS 'Fecha de Baja al Plan'
/
