CREATE TABLE RET_DETALLES_OFERTAS_TD(
    COD_TIPO_OFERTA NUMBER(2,0) NOT NULL 
 ,  COD_DETALLE_OFERTA NUMBER(5,0) NOT NULL 
 ,  DES_DETALLE_OFERTA VARCHAR2(60) NOT NULL 
 ,  IND_VIGENCIA CHAR(1) NOT NULL 
 ,  COD_OOSS VARCHAR2(5) NULL 
 ,  IND_DETALLE_OFERTA CHAR(2) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  RET_DETALLES_OFERTAS_TD IS 'DETALLE DE OFERTAS TIPO'
/
COMMENT ON COLUMN  RET_DETALLES_OFERTAS_TD.COD_TIPO_OFERTA IS 'CÓDIGO DE TIPO DE OFERTA'
/
COMMENT ON COLUMN  RET_DETALLES_OFERTAS_TD.COD_DETALLE_OFERTA IS 'CÓDIGO DE DETALLE DE OFERTA'
/
COMMENT ON COLUMN  RET_DETALLES_OFERTAS_TD.DES_DETALLE_OFERTA IS 'DESCRIPCIÓN DE DETALLE DE OFERTA'
/
COMMENT ON COLUMN  RET_DETALLES_OFERTAS_TD.IND_VIGENCIA IS 'INDICADOR DE VIGENCIA'
/
COMMENT ON COLUMN  RET_DETALLES_OFERTAS_TD.COD_OOSS IS 'CÓDIGO DE ORDEN DE SERVICIOS'
/
COMMENT ON COLUMN  RET_DETALLES_OFERTAS_TD.IND_DETALLE_OFERTA IS 'INDICADOR DEL TIPO DE DETALLE (BYP, OOSS)'
/
