CREATE TABLE CI_REG_ATENCION_CLIENTES(
    FEC_INICIO DATE NOT NULL 
 ,  FEC_TERMINO DATE NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_ATENCION NUMBER(3,0) NOT NULL 
 ,  OBS_ATENCION VARCHAR2(300) NULL 
 ,  NUM_ATENCION NUMBER(10,0) NOT NULL 
 ,  COD_OFICINA VARCHAR2(2) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CI_REG_ATENCION_CLIENTES IS ' Tabla Que Registra Las Las Necesidades Del Cliente.'
/
COMMENT ON COLUMN  CI_REG_ATENCION_CLIENTES.FEC_INICIO IS 'Fecha y Hora de inicio de la busqueda.'
/
COMMENT ON COLUMN  CI_REG_ATENCION_CLIENTES.FEC_TERMINO IS 'Fecha y Hora de termino de acciones en la busqueda. (cada vez que existe)'
/
COMMENT ON COLUMN  CI_REG_ATENCION_CLIENTES.NOM_USUARORA IS 'User Id del ejecutivo que atiende.'
/
COMMENT ON COLUMN  CI_REG_ATENCION_CLIENTES.NUM_ABONADO IS 'Codigo del Cliente.'
/
COMMENT ON COLUMN  CI_REG_ATENCION_CLIENTES.COD_ATENCION IS 'ID del Requerimiento.'
/
COMMENT ON COLUMN  CI_REG_ATENCION_CLIENTES.OBS_ATENCION IS 'Alguna Observacisn.'
/
COMMENT ON COLUMN  CI_REG_ATENCION_CLIENTES.NUM_ATENCION IS 'Nzmero de Identificacisn del registro de Atencisn al Cliente'
/
COMMENT ON COLUMN  CI_REG_ATENCION_CLIENTES.COD_OFICINA IS 'Oficina asociada al usuario que registro una atencion.'
/
