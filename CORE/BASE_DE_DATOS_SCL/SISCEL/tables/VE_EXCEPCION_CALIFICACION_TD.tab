CREATE TABLE VE_EXCEPCION_CALIFICACION_TD
(
  COD_CLIENTE    NUMBER(8),
  COD_PLANTARIF  VARCHAR2(3 BYTE),
  NOM_USUARIO    VARCHAR2(50 BYTE),
  COD_PASSWORD   VARCHAR2(10 BYTE),
  LIMITE_CREDITO  VARCHAR2(15 BYTE),
  FEC_CREACION    DATE
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          1M
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING
/
COMMENT ON TABLE VE_EXCEPCION_CALIFICACION_TD IS 'Tabla que contiene los datos del cliente al que se le hizo una excepcion de calificacion';
/
COMMENT ON COLUMN VE_EXCEPCION_CALIFICACION_TD.COD_CLIENTE IS 'Codigo Cliente';
/
COMMENT ON COLUMN VE_EXCEPCION_CALIFICACION_TD.COD_PLANTARIF IS 'Codigo Plan Tarifario';
/
COMMENT ON COLUMN VE_EXCEPCION_CALIFICACION_TD.NOM_USUARIO IS 'Nombre Usuario';
/
COMMENT ON COLUMN VE_EXCEPCION_CALIFICACION_TD.COD_PASSWORD IS 'Codigo Password';
/
COMMENT ON COLUMN VE_EXCEPCION_CALIFICACION_TD.LIMITE_CREDITO IS 'Limite de Credito';
/
COMMENT ON COLUMN VE_EXCEPCION_CALIFICACION_TD.FEC_CREACION IS 'Fecha de Creacion';
/