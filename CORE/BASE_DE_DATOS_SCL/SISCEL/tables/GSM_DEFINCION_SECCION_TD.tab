CREATE TABLE GSM_DEFINCION_SECCION_TD(
    COD_SECCION VARCHAR2(10) NOT NULL 
 ,  COD_SEPARADOR_CAMPO VARCHAR2(10) NOT NULL 
 ,  IND_LISTA VARCHAR2(2) NOT NULL 
 ,  FEC_ACTUALIZACION DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) NOT NULL 
 ,  DES_COMENTARIO VARCHAR2(255) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_DEFINCION_SECCION_TD IS 'Tabla de definicion de seccion'
/
COMMENT ON COLUMN  GSM_DEFINCION_SECCION_TD.COD_SECCION IS 'Codigo de Seccion'
/
COMMENT ON COLUMN  GSM_DEFINCION_SECCION_TD.COD_SEPARADOR_CAMPO IS 'Separador de Campo'
/
COMMENT ON COLUMN  GSM_DEFINCION_SECCION_TD.IND_LISTA IS 'Indica si el comportamiento es de VAR_IN o VAR_IN_LIST'
/
COMMENT ON COLUMN  GSM_DEFINCION_SECCION_TD.FEC_ACTUALIZACION IS 'Fecha de actualizacion del parametro.'
/
COMMENT ON COLUMN  GSM_DEFINCION_SECCION_TD.COD_USUARIO IS 'Usuario con que se actualizo la ultima vez el parametro.'
/
