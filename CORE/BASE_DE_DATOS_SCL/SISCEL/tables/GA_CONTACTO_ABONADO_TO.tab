CREATE TABLE GA_CONTACTO_ABONADO_TO
(
  NUM_ABONADO         NUMBER(8),
  COD_SERVICIO        VARCHAR2(3 BYTE),
  NUM_CONTACTO        NUMBER(2),
  NOMBRE_CONTACTO     VARCHAR2(50 BYTE),
  APELLIDO1_CONTACTO  VARCHAR2(20 BYTE),
  APELLIDO2_CONTACTO  VARCHAR2(20 BYTE),
  COD_PARENTESCO      VARCHAR2(5 BYTE),
  COD_DIRECCION       NUMBER(20),
  PLACA_VEHICULAR     VARCHAR2(10 BYTE),
  COLOR_VEHICULO      VARCHAR2(30 BYTE),
  ANIO_VEHICULO       VARCHAR2(4 BYTE),
  OBSERVACION         VARCHAR2(50 BYTE),
  IND_VIGENTE         VARCHAR2(1 BYTE),
  FEC_MODIFICACION    DATE,
  NOM_USUAORA         VARCHAR2(30 BYTE),
  NUM_TELEFONO        NUMBER(15)
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          1M
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE GA_CONTACTO_ABONADO_TO IS '[ GA_CONTACTO ] Contactos Asociados al Abonado';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.NUM_ABONADO IS 'Numero de Abonado Asociado al Contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.COD_SERVICIO IS 'Codigo de Servicio asociado al Abonado (GA_SERVSUPL)';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.NUM_CONTACTO IS 'Número de contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.NOMBRE_CONTACTO IS 'Nombre del Contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.APELLIDO1_CONTACTO IS 'Primer apellido del contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.APELLIDO2_CONTACTO IS 'Segundo apellido del contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.COD_PARENTESCO IS 'Codigo de Parentesco (GED_CODIGOS)';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.COD_DIRECCION IS 'Código de Direccion asociado al contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.PLACA_VEHICULAR IS 'Placa Vehicular asociada al contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.COLOR_VEHICULO IS 'Color del Vehiculo asociado al contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.ANIO_VEHICULO IS 'Año del Vehiculo asociado al contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.OBSERVACION IS 'Observacion';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.IND_VIGENTE IS 'Indicador de Vigencia (1: Vigente 0:No vigente)';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.FEC_MODIFICACION IS 'Fecha de Modificación';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.NOM_USUAORA IS 'Nombre de Usuario Oracle';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TO.NUM_TELEFONO IS 'numero de telefono';
