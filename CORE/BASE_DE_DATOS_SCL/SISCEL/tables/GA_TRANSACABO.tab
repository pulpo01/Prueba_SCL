CREATE TABLE GA_TRANSACABO(
    NUM_TRANSACCION NUMBER(9,0) NOT NULL 
 ,  COD_RETORNO NUMBER(1,0) NOT NULL 
 ,  DES_CADENA VARCHAR2(255) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_TRANSACABO IS 'Resultados de Transacciones Realizadas en PL/SQL llamados desde Visual'
/
COMMENT ON COLUMN  GA_TRANSACABO.NUM_TRANSACCION IS 'Numero de Transaccion'
/
COMMENT ON COLUMN  GA_TRANSACABO.COD_RETORNO IS 'Codigo de Retorno de la Transaccion; Retorno <> 0 = Error'
/
COMMENT ON COLUMN  GA_TRANSACABO.DES_CADENA IS 'Cadena de Parametros Devueltos por la Transaccion; Separador = "/"'
/
