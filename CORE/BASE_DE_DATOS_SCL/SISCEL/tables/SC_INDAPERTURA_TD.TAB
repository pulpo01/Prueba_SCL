CREATE TABLE SC_INDAPERTURA_TD(
    COD_MODULO VARCHAR2(2) NOT NULL 
 ,  APER_TECNOLOGIA CHAR(1) NOT NULL 
 ,  APER_OFICINA CHAR(1) NOT NULL 
 ,  APER_BODEGA CHAR(1) NOT NULL 
 ,  APER_COMISIONISTA CHAR(1) NOT NULL 
 ,  VAL_TECNOLOGIA VARCHAR2(7) NOT NULL 
 ,  VAL_OFICINA VARCHAR2(2) NOT NULL 
 ,  VAL_BODEGA NUMBER(6,0) NOT NULL 
 ,  VAL_COMISIONISTA VARCHAR2(2) NOT NULL 
 ,  IND_MODPRORRATEO VARCHAR2(5) NOT NULL 
 ,  APER_ARTICULO CHAR(1) DEFAULT 'N' NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SC_INDAPERTURA_TD IS 'Aperturas Contables por defecto'
/
COMMENT ON COLUMN  SC_INDAPERTURA_TD.COD_MODULO IS 'Codigo del modulo'
/
COMMENT ON COLUMN  SC_INDAPERTURA_TD.APER_TECNOLOGIA IS 'Apertura por Tecnologia, N=No, S=Si'
/
COMMENT ON COLUMN  SC_INDAPERTURA_TD.APER_OFICINA IS 'Apertura por Oficina, N=No, S =Si'
/
COMMENT ON COLUMN  SC_INDAPERTURA_TD.APER_BODEGA IS 'Apertura por Bodega, N=No, S =Si'
/
COMMENT ON COLUMN  SC_INDAPERTURA_TD.APER_COMISIONISTA IS 'Apertura por Comisionista, N=No, S =Si'
/
COMMENT ON COLUMN  SC_INDAPERTURA_TD.VAL_TECNOLOGIA IS 'Valor por defecto de Apertura por Tecnologia'
/
COMMENT ON COLUMN  SC_INDAPERTURA_TD.VAL_OFICINA IS 'Valor por defecto de Apertura por Oficina'
/
COMMENT ON COLUMN  SC_INDAPERTURA_TD.VAL_BODEGA IS 'Valor por defecto de Apertura por Bodega'
/
COMMENT ON COLUMN  SC_INDAPERTURA_TD.VAL_COMISIONISTA IS 'Valor por defecto de Apertura por Comisionista'
/
COMMENT ON COLUMN  SC_INDAPERTURA_TD.IND_MODPRORRATEO IS 'Indicador de Prorrateo en Facturacion, N=No, S =Si'
/
COMMENT ON COLUMN  SC_INDAPERTURA_TD.APER_ARTICULO IS 'Apertura por Articulo, N=No, S =Si'
/
