CREATE TABLE AL_HLIN_PETICION(
    NUM_PETICION NUMBER(9,0) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  CAN_TRASPASO NUMBER(6,0) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_HLIN_PETICION IS 'Historico de Lineas de Peticion de Traspasos'
/
COMMENT ON COLUMN  AL_HLIN_PETICION.NUM_PETICION IS 'Numero de peticion'
/
COMMENT ON COLUMN  AL_HLIN_PETICION.FEC_HISTORICO IS 'Fecha de Historico'
/
COMMENT ON COLUMN  AL_HLIN_PETICION.COD_ARTICULO IS 'Codigo de articulo'
/
COMMENT ON COLUMN  AL_HLIN_PETICION.CAN_TRASPASO IS 'Cantidad solicitada'
/
