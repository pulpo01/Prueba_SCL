CREATE TABLE AL_SERIES_ACTIVAS(
    NUM_TELEFONO NUMBER(15,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  TIP_TERMINAL VARCHAR2(1) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  NUM_SERHEX VARCHAR2(8) NOT NULL 
 ,  FEC_ENTRADA DATE NULL 
 ,  NOM_USUARORA VARCHAR2(25) NULL 
 ,  IND_PROCESADO CHAR(1) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_SERIES_ACTIVAS IS 'Numeros de Series de los articulos existentes en stock'
/
COMMENT ON COLUMN  AL_SERIES_ACTIVAS.NUM_TELEFONO IS 'Numero de telefono'
/
COMMENT ON COLUMN  AL_SERIES_ACTIVAS.NUM_SERIE IS 'Numero de serie'
/
COMMENT ON COLUMN  AL_SERIES_ACTIVAS.COD_PRODUCTO IS 'Producto'
/
COMMENT ON COLUMN  AL_SERIES_ACTIVAS.TIP_TERMINAL IS 'Tipo de Terminal'
/
COMMENT ON COLUMN  AL_SERIES_ACTIVAS.COD_CENTRAL IS 'Central'
/
COMMENT ON COLUMN  AL_SERIES_ACTIVAS.NUM_SERHEX IS 'Numero de serie en formato hexadecimal'
/
COMMENT ON COLUMN  AL_SERIES_ACTIVAS.FEC_ENTRADA IS 'Fecha de ingreso de la serie en la tabla'
/
COMMENT ON COLUMN  AL_SERIES_ACTIVAS.NOM_USUARORA IS 'Usuario que hizo el ingreso de la serie'
/
COMMENT ON COLUMN  AL_SERIES_ACTIVAS.IND_PROCESADO IS 'Indicativo de migrado'
/
