CREATE TABLE GA_RESNUMTREK(
    NUM_TRANSACCION NUMBER(10,0) NOT NULL 
 ,  NUM_LINEA NUMBER(3,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(3,0) NOT NULL 
 ,  NUM_MAN NUMBER(8,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  IND_TIPSUSC VARCHAR2(1) NOT NULL 
 ,  FEC_RESERVA DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  IND_PROCNUM VARCHAR2(1) NOT NULL 
 ,  FEC_BAJA DATE NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_RESNUMTREK IS 'Numeracion trek reservada en la venta'
/
COMMENT ON COLUMN  GA_RESNUMTREK.NUM_TRANSACCION IS 'NUMERO DE TRANSACCION.'
/
COMMENT ON COLUMN  GA_RESNUMTREK.NUM_LINEA IS 'Numero Interno de Linea'
/
COMMENT ON COLUMN  GA_RESNUMTREK.NUM_ORDEN IS 'Numero de orden'
/
COMMENT ON COLUMN  GA_RESNUMTREK.NUM_MAN IS 'Numero Man'
/
COMMENT ON COLUMN  GA_RESNUMTREK.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_RESNUMTREK.COD_CENTRAL IS 'Codigo de Central'
/
COMMENT ON COLUMN  GA_RESNUMTREK.COD_CLIENTE IS 'Codigo de cliente'
/
COMMENT ON COLUMN  GA_RESNUMTREK.IND_TIPSUSC IS 'Tipo de Suscripcion'
/
COMMENT ON COLUMN  GA_RESNUMTREK.FEC_RESERVA IS 'Fecha de Reserva'
/
COMMENT ON COLUMN  GA_RESNUMTREK.NOM_USUARIO IS 'Usuario que realiza la reserva'
/
COMMENT ON COLUMN  GA_RESNUMTREK.IND_PROCNUM IS 'Indicativo de Procedencia del numero; (L)ibres, (R)eutilizable'
/
COMMENT ON COLUMN  GA_RESNUMTREK.FEC_BAJA IS 'Fecha de baja del numero reutilizable'
/
