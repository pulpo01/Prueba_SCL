CREATE TABLE GA_HMODDIRUS(
    COD_USUARIO NUMBER(8,0) NOT NULL 
 ,  FEC_MODIFICA DATE NOT NULL 
 ,  COD_TIPDIRECCION VARCHAR2(1) NOT NULL 
 ,  COD_DIRECANT NUMBER(20,0) NOT NULL 
 ,  COD_DIRECNUE NUMBER(20,0) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_HMODDIRUS IS 'Historico Modificacion Direcciones de Usuarios'
/
COMMENT ON COLUMN  GA_HMODDIRUS.COD_DIRECNUE IS 'Codigo de Direccion Nueva'
/
COMMENT ON COLUMN  GA_HMODDIRUS.NOM_USUARORA IS 'Codigo de Usuario Oracle'
/
COMMENT ON COLUMN  GA_HMODDIRUS.FEC_HISTORICO IS 'Fecha de Historic'
/
COMMENT ON COLUMN  GA_HMODDIRUS.COD_USUARIO IS 'Codigo de Usuario'
/
COMMENT ON COLUMN  GA_HMODDIRUS.FEC_MODIFICA IS 'Fecha de Modificacion'
/
COMMENT ON COLUMN  GA_HMODDIRUS.COD_TIPDIRECCION IS 'Codigo de Tipo de Direccion'
/
COMMENT ON COLUMN  GA_HMODDIRUS.COD_DIRECANT IS 'Codigo de Direccion Anterior'
/
