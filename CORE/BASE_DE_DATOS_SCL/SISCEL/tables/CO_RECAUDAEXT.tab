CREATE TABLE CO_RECAUDAEXT(
    COD_RECEXT NUMBER(8,0) NOT NULL 
 ,  NOM_FICHERO VARCHAR2(80) NOT NULL 
 ,  FEC_VALOR DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  NUM_REGISTROS NUMBER(8,0) NOT NULL 
 ,  NUM_ANOMALIAS NUMBER(8,0) NOT NULL 
 ,  COD_ORIPAGO NUMBER(2,0) NULL 
 ,  IMP_VALOR_ANOM NUMBER(15,4) NULL 
 ,  FEC_IMPUTCONTABLE DATE NULL 
 ,  IMP_VALOR NUMBER(15,4) NULL 
 ,  COD_OPERADORA VARCHAR2(5) NOT NULL 
 ,  COD_PLAZA VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
