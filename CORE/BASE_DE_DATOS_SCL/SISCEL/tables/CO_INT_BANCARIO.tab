CREATE TABLE CO_INT_BANCARIO(
    NUM_SECUENCI NUMBER(8,0) NOT NULL 
 ,  TRAMO_DESDE NUMBER(14,4) NOT NULL 
 ,  TRAMO_HASTA NUMBER(14,4) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  PORCENTAJE NUMBER(8,5) NOT NULL 
 ,  FEC_VIGENCIA_DD_DH DATE NOT NULL 
 ,  FEC_VIGENCIA_HH_DH DATE NOT NULL 
 ,  FECHA_CREACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(20) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_INT_BANCARIO IS 'Interes Bancario.'
/
COMMENT ON COLUMN  CO_INT_BANCARIO.NUM_SECUENCI IS 'Numero de Secuencia de la Tabla.'
/
COMMENT ON COLUMN  CO_INT_BANCARIO.TRAMO_DESDE IS 'Monto Desde.'
/
COMMENT ON COLUMN  CO_INT_BANCARIO.TRAMO_HASTA IS 'Monto Hasta.'
/
COMMENT ON COLUMN  CO_INT_BANCARIO.COD_MONEDA IS 'Codigo Moneda.'
/
COMMENT ON COLUMN  CO_INT_BANCARIO.PORCENTAJE IS 'Porcentaje Aplicado.'
/
COMMENT ON COLUMN  CO_INT_BANCARIO.FEC_VIGENCIA_DD_DH IS 'Fecha Vigencia Desde.'
/
COMMENT ON COLUMN  CO_INT_BANCARIO.FEC_VIGENCIA_HH_DH IS 'Fecha Vigencia Hasta.'
/
COMMENT ON COLUMN  CO_INT_BANCARIO.FECHA_CREACION IS 'Fecha Creacion.'
/
COMMENT ON COLUMN  CO_INT_BANCARIO.NOM_USUARIO IS 'Identificacion del Usuario.'
/
