CREATE TABLE GA_TIPDOC_DIGITALIZADOS_TD (  
   COD_TIPO_DOC                  NUMBER(3)       NOT NULL
 , DES_TIPO_DOC                  VARCHAR2(150)   NOT NULL
 , IND_VIGENCIA                  NUMBER(1)       NOT NULL
 , NOM_USUARIO_ORA               VARCHAR2(30)    NOT NULL
 , FECHA_MODIFICA                DATE                                          
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  GA_TIPDOC_DIGITALIZADOS_TD                      IS '[ GA_TIPDIG ] Tipos de Documentos que podran ser asociados en SCL'
/

COMMENT ON COLUMN GA_TIPDOC_DIGITALIZADOS_TD.COD_TIPO_DOC                   IS 'Codigo de Tipo de Documento'
/
COMMENT ON COLUMN GA_TIPDOC_DIGITALIZADOS_TD.DES_TIPO_DOC                   IS 'Descripcion de Tipo de Documento'
/
COMMENT ON COLUMN GA_TIPDOC_DIGITALIZADOS_TD.IND_VIGENCIA                   IS 'Indicador de Vigencia(1:Vigente 0: No Vigente)'
/
COMMENT ON COLUMN GA_TIPDOC_DIGITALIZADOS_TD.NOM_USUARIO_ORA                IS 'Nombre de Usuario Oracle'
/
COMMENT ON COLUMN GA_TIPDOC_DIGITALIZADOS_TD.FECHA_MODIFICA                 IS 'Fecha de Modificación'
/

