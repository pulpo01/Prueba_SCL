CREATE TABLE TA_SEGUNDOS(
    COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  CLS_TARIFA VARCHAR2(3) NOT NULL 
 ,  IND_ORDEN NUMBER(2,0) NOT NULL 
 ,  SEG_FRACCION NUMBER(4,0) NOT NULL 
 ,  DES_FRACCION VARCHAR2(30) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(40) DEFAULT 'TARIFICA' NOT NULL 
 ,  FEC_ULTIMAMOD DATE DEFAULT '01-jan-99' NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_SEGUNDOS IS 'Fracciones de segundos que implica un paso o pulso'
/
COMMENT ON COLUMN  TA_SEGUNDOS.COD_OPERADOR IS 'Csdigo de Operador'
/
COMMENT ON COLUMN  TA_SEGUNDOS.CLS_TARIFA IS 'Cls. Tarifa'
/
COMMENT ON COLUMN  TA_SEGUNDOS.IND_ORDEN IS 'Indice de Orden'
/
COMMENT ON COLUMN  TA_SEGUNDOS.SEG_FRACCION IS 'Fraccisn en milisegundos'
/
COMMENT ON COLUMN  TA_SEGUNDOS.DES_FRACCION IS 'Descripcisn Fraccisn'
/
COMMENT ON COLUMN  TA_SEGUNDOS.NOM_USUARIO IS 'Almacena Nombre Usuario'
/
COMMENT ON COLUMN  TA_SEGUNDOS.FEC_ULTIMAMOD IS 'Fecha Ultima Modificacion'
/
