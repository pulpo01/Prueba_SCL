CREATE TABLE GSM_ESTADO_SOLICITUD_TO(
    COD_ESTADO VARCHAR2(2) NOT NULL 
 ,  NUM_SEQ_SOLICITUD NUMBER(10,0) NOT NULL 
 ,  GLS_OBSERVACION VARCHAR2(256) NOT NULL 
 ,  FEC_ACTUALIZACION DATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_ESTADO_SOLICITUD_TO IS 'Tabla de estados de solicitud y observaciones de esta..'
/
COMMENT ON COLUMN  GSM_ESTADO_SOLICITUD_TO.COD_ESTADO IS 'Numero secuencia de Solicitud.'
/
COMMENT ON COLUMN  GSM_ESTADO_SOLICITUD_TO.NUM_SEQ_SOLICITUD IS 'Estado de la solicitud.'
/
COMMENT ON COLUMN  GSM_ESTADO_SOLICITUD_TO.GLS_OBSERVACION IS 'Observacion.'
/
COMMENT ON COLUMN  GSM_ESTADO_SOLICITUD_TO.FEC_ACTUALIZACION IS 'Fecha de la ultima actualizacion.'
/
