CREATE TABLE GA_DOC_DIGITALIZADOS_TO
(
  NUM_CORRELATIVO     NUMBER(20)                NOT NULL,
  NUM_VENTA           NUMBER(8),
  COD_TIPO_DOC        NUMBER(3),
  OBSERVACION         VARCHAR2(200 BYTE),
  NOM_USUARORA        VARCHAR2(30 BYTE),
  BIN_ARCHIVO         BLOB,
  VALOR_CONTENT_TYPE  VARCHAR2(100 BYTE),
  NOMBRE_ARCHIVO      VARCHAR2(256 BYTE),
  FEC_CREACION        DATE,
  NUM_SOLSCORING      NUMBER(10),
  COD_DOCSCORING      VARCHAR2(20 BYTE),
  DES_DOCSCORING      VARCHAR2(200 BYTE),
  REQUERIDO_SCORING   CHAR(1 BYTE)
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          1M
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
LOB (BIN_ARCHIVO) STORE AS 
      ( TABLESPACE  INSTALL_DATA 
        ENABLE      STORAGE IN ROW
        CHUNK       8192
        RETENTION
        NOCACHE
        NOLOGGING
        INDEX       (
          TABLESPACE INSTALL_DATA
          STORAGE    (
                      INITIAL          1M
                      NEXT             1
                      MINEXTENTS       1
                      MAXEXTENTS       UNLIMITED
                      PCTINCREASE      0
                      BUFFER_POOL      DEFAULT
                     ))
        STORAGE    (
                    INITIAL          1M
                    NEXT             1M
                    MINEXTENTS       1
                    MAXEXTENTS       UNLIMITED
                    PCTINCREASE      0
                    BUFFER_POOL      DEFAULT
                   )
      )
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE GA_DOC_DIGITALIZADOS_TO IS '[ GA_DOC_DIG ] Documentos digitalizados asociados a la Venta';

COMMENT ON COLUMN GA_DOC_DIGITALIZADOS_TO.NUM_CORRELATIVO IS 'Correlativo del Documento';

COMMENT ON COLUMN GA_DOC_DIGITALIZADOS_TO.NUM_VENTA IS 'Número de Venta';

COMMENT ON COLUMN GA_DOC_DIGITALIZADOS_TO.COD_TIPO_DOC IS 'Código de Tipo de Documento';

COMMENT ON COLUMN GA_DOC_DIGITALIZADOS_TO.OBSERVACION IS 'Observacion';

COMMENT ON COLUMN GA_DOC_DIGITALIZADOS_TO.NOM_USUARORA IS 'Usuario SCL que realiza la asociacion';

COMMENT ON COLUMN GA_DOC_DIGITALIZADOS_TO.BIN_ARCHIVO IS 'Archivo Digitalizado';

COMMENT ON COLUMN GA_DOC_DIGITALIZADOS_TO.VALOR_CONTENT_TYPE IS 'Tipo de Archivo ';

COMMENT ON COLUMN GA_DOC_DIGITALIZADOS_TO.NOMBRE_ARCHIVO IS 'Nombre del Archivo';

COMMENT ON COLUMN GA_DOC_DIGITALIZADOS_TO.FEC_CREACION IS 'Fecha de Creacion';

COMMENT ON COLUMN GA_DOC_DIGITALIZADOS_TO.NUM_SOLSCORING IS 'Numero de Solicitud Scoring Asociado';

COMMENT ON COLUMN GA_DOC_DIGITALIZADOS_TO.COD_DOCSCORING IS 'Codigo Documento Scoring';

COMMENT ON COLUMN GA_DOC_DIGITALIZADOS_TO.DES_DOCSCORING IS 'Descripcion Documento Scoring';

COMMENT ON COLUMN GA_DOC_DIGITALIZADOS_TO.REQUERIDO_SCORING IS 'Indica si es requerido por el Scoring';

