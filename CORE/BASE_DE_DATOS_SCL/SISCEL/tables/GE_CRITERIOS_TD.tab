CREATE TABLE GE_CRITERIOS_TD(
    COD_MODULO VARCHAR2(2) NOT NULL 
 ,  COD_CRITERIO NUMBER(3,0) NOT NULL 
 ,  NOM_CRITERIO VARCHAR2(50) NOT NULL 
 ,  DES_CRITERIO VARCHAR2(100) NOT NULL 
 ,  UNIDAD_MEDIDA VARCHAR2(30) NOT NULL 
 ,  TIPO_UNIDAD VARCHAR2(2) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_CRITERIOS_TD IS 'CRITERIOS DE RETENCION'
/
COMMENT ON COLUMN  GE_CRITERIOS_TD.COD_MODULO IS 'MODULO DE APLICACION'
/
COMMENT ON COLUMN  GE_CRITERIOS_TD.COD_CRITERIO IS 'CODIGO DE CRITERIO'
/
COMMENT ON COLUMN  GE_CRITERIOS_TD.NOM_CRITERIO IS 'NOMBRE DE CRITERIO'
/
COMMENT ON COLUMN  GE_CRITERIOS_TD.DES_CRITERIO IS 'DESCRIPCION DEL CRITERIO'
/
COMMENT ON COLUMN  GE_CRITERIOS_TD.UNIDAD_MEDIDA IS 'UNIDAD DE MEDIDA '
/
COMMENT ON COLUMN  GE_CRITERIOS_TD.TIPO_UNIDAD IS 'TIPO DE UNIDAD (DISCRETA, CONTINUA)'
/
