CREATE TABLE GA_CAMBIO_DN_TO (  
   NUM_ABONADO_SCL               NUMBER(10)     NOT NULL
 , NUM_ABONADO_VAN               NUMBER(10)     NOT NULL
 , NUM_CELULAR_SCL               NUMBER(15)     NOT NULL
 , NUM_CELULAR_VAN               NUMBER(15)     NOT NULL
 , FECHA                         DATE           NOT NULL 
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  GA_CAMBIO_DN_TO                      IS '[ CADN ] Detalle de cambios de abonados por cambio de sistemaDetalle de cambios de abonados por cambio de sistema'
/

COMMENT ON COLUMN GA_CAMBIO_DN_TO.NUM_ABONADO_SCL                IS 'Numero Abonado SCL'
/
COMMENT ON COLUMN GA_CAMBIO_DN_TO.NUM_ABONADO_VAN                IS 'Numer Abonado Vantive'
/
COMMENT ON COLUMN GA_CAMBIO_DN_TO.NUM_CELULAR_SCL                IS 'Numero Celular SCL'
/
COMMENT ON COLUMN GA_CAMBIO_DN_TO.NUM_CELULAR_VAN                IS 'Numero Celular Vantive'
/
COMMENT ON COLUMN GA_CAMBIO_DN_TO.FECHA                          IS 'Fecha'
/

