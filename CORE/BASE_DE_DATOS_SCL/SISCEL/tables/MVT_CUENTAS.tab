CREATE TABLE MVT_CUENTAS(
    FEC_PROCESO DATE NOT NULL 
 ,  COD_CUENTA NUMBER(8,0) NOT NULL 
 ,  COD_CATEGORIA NUMBER(4,0) NOT NULL 
 ,  NUM_ABONCTA NUMBER(8,0) NOT NULL 
 ,  VAL_VC NUMBER(14,4) NOT NULL 
 ,  VAL_VCA NUMBER(14,4) NOT NULL 
 ,  IND_MOMENTUM CHAR(1) NULL 
 ,  IND_NOTA CHAR(4) NOT NULL 
 ,  IND_ALARMA NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  MVT_CUENTAS IS 'Tabla Cuentas Modelo Valor.'
/
COMMENT ON COLUMN  MVT_CUENTAS.FEC_PROCESO IS 'FECHA DE PROCESO.'
/
COMMENT ON COLUMN  MVT_CUENTAS.COD_CUENTA IS 'CODIGO DE UNA CUENTA.'
/
COMMENT ON COLUMN  MVT_CUENTAS.COD_CATEGORIA IS 'CODIGO DE LA CATEGORIA DE LA CUENTA.'
/
COMMENT ON COLUMN  MVT_CUENTAS.NUM_ABONCTA IS 'NUMERO DE ABONADOS PARA LA CUENTA.'
/
COMMENT ON COLUMN  MVT_CUENTAS.VAL_VC IS 'VALOR CLIENTE.'
/
COMMENT ON COLUMN  MVT_CUENTAS.VAL_VCA IS 'VALOR CLIENTE ACUMULADO.'
/
COMMENT ON COLUMN  MVT_CUENTAS.IND_MOMENTUM IS 'VARIACION DE LA NOTA.'
/
COMMENT ON COLUMN  MVT_CUENTAS.IND_NOTA IS 'NOTA DE LA CUENTA.'
/
COMMENT ON COLUMN  MVT_CUENTAS.IND_ALARMA IS 'INDICADOR DE ALARMA DE DESPERDICIO O DESVIACION.'
/
