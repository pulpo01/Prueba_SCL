CREATE TABLE AL_CIERRES_ALMA(
    FEC_INICIO DATE NOT NULL 
 ,  FEC_FIN DATE NOT NULL 
 ,  IND_CERRADO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  MES_CIERRE DATE NULL 
 ,  FEC_FIN_REAL DATE NULL 
 ,  COD_OPERADORA_SCL VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_CIERRES_ALMA IS 'Periodos de ejercicios para cierres de almacen'
/
COMMENT ON COLUMN  AL_CIERRES_ALMA.FEC_INICIO IS 'Fecha Inicio Ejercicio'
/
COMMENT ON COLUMN  AL_CIERRES_ALMA.FEC_FIN IS 'Fecha Fin Ejercicio'
/
COMMENT ON COLUMN  AL_CIERRES_ALMA.IND_CERRADO IS 'Indicativo de cerrado (0=No, 1=Si)'
/
COMMENT ON COLUMN  AL_CIERRES_ALMA.MES_CIERRE IS 'Ultimo mes cerrado'
/
COMMENT ON COLUMN  AL_CIERRES_ALMA.FEC_FIN_REAL IS 'Fecha real en la que se realiza el cierre anual'
/
COMMENT ON COLUMN  AL_CIERRES_ALMA.COD_OPERADORA_SCL IS 'Codigo de operadora SCL.'
/
