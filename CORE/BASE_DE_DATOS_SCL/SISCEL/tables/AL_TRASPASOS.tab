CREATE TABLE SISCEL.AL_TRASPASOS
(
  NUM_TRASPASO     NUMBER(9)                    NOT NULL,
  NUM_PETICION     NUMBER(9)                    NOT NULL,
  COD_BODEGA_ORI   NUMBER(6)                    NOT NULL,
  COD_BODEGA_DEST  NUMBER(6)                    NOT NULL,
  FEC_AUTORIZA     DATE                         NOT NULL,
  USU_AUTORIZA     VARCHAR2(30 BYTE)            NOT NULL,
  COD_ESTADO       VARCHAR2(2 BYTE)             NOT NULL,
  TIP_MOVIM_AUT    NUMBER(2)                    NOT NULL,
  TIP_MOVIM_ENV    NUMBER(2),
  TIP_MOVIM_REC    NUMBER(2),
  FEC_DESPACHO     DATE,
  USU_DESPACHO     VARCHAR2(30 BYTE),
  FEC_RECEPCION    DATE,
  USU_RECEPCION    VARCHAR2(30 BYTE),
  DES_OBSERVACION  VARCHAR2(100 BYTE),
  NUM_GUIA         VARCHAR2(25 BYTE)
)  TABLESPACE TS_LOGISTICA_SDAT
/

COMMENT ON TABLE SISCEL.AL_TRASPASOS IS 'Cabecera Ordenes de Traspasos';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.NUM_GUIA IS 'Folio para la guía en traspasos con bodega destino requiere guia remision';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.NUM_TRASPASO IS 'Numero de Orden de Traspaso';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.NUM_PETICION IS 'Numero de Peticion originaria del traspaso';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.COD_BODEGA_ORI IS 'Bodega origen del traspaso';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.COD_BODEGA_DEST IS 'Bodega destino del traspaso';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.FEC_AUTORIZA IS 'Fecha en la que fue autorizada la peticion';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.USU_AUTORIZA IS 'Usuario que autorizo la peticion';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.COD_ESTADO IS 'Estado';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.TIP_MOVIM_AUT IS 'Tipo de movimiento de autorizacisn';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.TIP_MOVIM_ENV IS 'Tipo de movimiento de envmo';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.TIP_MOVIM_REC IS 'Tipo de movimiento de recepcisn';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.FEC_DESPACHO IS 'Fecha salida mercancia';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.USU_DESPACHO IS 'Usuario despacho mercancia';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.FEC_RECEPCION IS 'Fecha recepcion mercancia.';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.USU_RECEPCION IS 'Usuario recepcion';

COMMENT ON COLUMN SISCEL.AL_TRASPASOS.DES_OBSERVACION IS 'Observaciones o Comentarios';
