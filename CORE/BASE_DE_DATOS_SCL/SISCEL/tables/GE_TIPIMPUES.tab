CREATE TABLE GE_TIPIMPUES(
    COD_TIPIMPUE NUMBER(2,0) NOT NULL 
 ,  DES_TIPIMPUE VARCHAR2(40) NOT NULL 
 ,  IMP_UMBRAL NUMBER(12,4) NOT NULL 
 ,  TIP_MONTO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  COD_CATEIMP NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  IMP_MAXIMO NUMBER(14,4) DEFAULT 0
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_TIPIMPUES IS 'Tipos de impuestos- Retrofitted'
/
COMMENT ON COLUMN  GE_TIPIMPUES.COD_TIPIMPUE IS 'Codigo de tipo de impuestos - Retrofitted'
/
COMMENT ON COLUMN  GE_TIPIMPUES.DES_TIPIMPUE IS 'Descripcion de tipo de impuesto - Retrofitted'
/
COMMENT ON COLUMN  GE_TIPIMPUES.IMP_UMBRAL IS 'Umbral del Iva Percepcion - Retrofitted'
/
COMMENT ON COLUMN  GE_TIPIMPUES.TIP_MONTO IS 'Indica porcentaje o monto fijo'
/
COMMENT ON COLUMN  GE_TIPIMPUES.COD_CATEIMP IS 'Indica categoria o naturaleza del tipo de impuesto o gravamen'
/
COMMENT ON COLUMN  GE_TIPIMPUES.IMP_MAXIMO IS 'Importe maximo de aplicacion'
/
