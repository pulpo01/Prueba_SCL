CREATE TABLE GA_HDTOSTARIF(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_CICLFACT NUMBER(6,0) NOT NULL 
 ,  NUM_MINUTOS NUMBER(6,0) NOT NULL 
 ,  TIP_PLANTARIF VARCHAR2(1) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_GRABACION DATE NOT NULL 
 ,  FEC_BAJA DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_HDTOSTARIF IS 'Numeros de minutos libres adiccionales por vendedor/abonado a los de su plan tarifario'
/
COMMENT ON COLUMN  GA_HDTOSTARIF.NUM_ABONADO IS 'Numero de Abonado - Retrofitted'
/
COMMENT ON COLUMN  GA_HDTOSTARIF.COD_CICLFACT IS 'codigo de Ciclo de Facturaciondurante el cual se van aplicar los descuentos - Retrofitted'
/
COMMENT ON COLUMN  GA_HDTOSTARIF.NUM_MINUTOS IS 'Numero de minutos Gratis para ese periodo de facturacion/abonado - Retrofitted'
/
COMMENT ON COLUMN  GA_HDTOSTARIF.TIP_PLANTARIF IS 'Tipo de Plantarifario del Abonado - Retrofitted'
/
COMMENT ON COLUMN  GA_HDTOSTARIF.COD_VENDEDOR IS 'Vendedor asociado al abonado - Retrofitted'
/
COMMENT ON COLUMN  GA_HDTOSTARIF.NOM_USUARORA IS 'Ususario que realizo el cambio - Retrofitted'
/
COMMENT ON COLUMN  GA_HDTOSTARIF.FEC_GRABACION IS 'Fecha en que se realizo la transaccion - Retrofitted'
/
COMMENT ON COLUMN  GA_HDTOSTARIF.FEC_BAJA IS 'Fecha de Baja en GA_DTOSTARIF - Retrofitted'
/
