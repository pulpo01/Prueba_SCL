CREATE TABLE GA_SEGUROABONADO_TO (  
   NUM_ABONADO                   NUMBER(8)                     NOT NULL
 , COD_SEGURO                    VARCHAR2(5)                   NOT NULL
 , NUM_EVENTOS                   NUMBER(2)         DEFAULT   0 NOT NULL
 , IMPORTE_EQUIPO                NUMBER(14,4)      DEFAULT   0 NOT NULL
 , FEC_ALTA                      DATE                          NOT NULL
 , FEC_FINCONTRATO               DATE                          NOT NULL
 , FEC_MODIFICACION              DATE                          NOT NULL
 , NOM_USUARORA                  VARCHAR2(30)                  NOT NULL
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  GA_SEGUROABONADO_TO                      IS '[ GA_SABO ] Registro de seguros contratados por AbonadoRegistro de seguros contratados por Abonado'
/

COMMENT ON COLUMN GA_SEGUROABONADO_TO.NUM_ABONADO                    IS 'Número de Abonado'
/
COMMENT ON COLUMN GA_SEGUROABONADO_TO.COD_SEGURO                     IS 'Código de Seguro'
/
COMMENT ON COLUMN GA_SEGUROABONADO_TO.NUM_EVENTOS                    IS 'Número de Eventos'
/
COMMENT ON COLUMN GA_SEGUROABONADO_TO.IMPORTE_EQUIPO                 IS 'Importe de Equipo'
/
COMMENT ON COLUMN GA_SEGUROABONADO_TO.FEC_ALTA                       IS 'Fecha de alta de Seguro'
/
COMMENT ON COLUMN GA_SEGUROABONADO_TO.FEC_FINCONTRATO                IS 'Fecha de Fin de contrato del seguro'
/
COMMENT ON COLUMN GA_SEGUROABONADO_TO.FEC_MODIFICACION               IS 'Fecha Ultima modificación'
/
COMMENT ON COLUMN GA_SEGUROABONADO_TO.NOM_USUARORA                   IS 'Nombre de Usuario '
/

