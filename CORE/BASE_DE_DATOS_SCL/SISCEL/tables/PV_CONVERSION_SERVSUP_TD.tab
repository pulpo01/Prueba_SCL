CREATE TABLE PV_CONVERSION_SERVSUP_TD(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_USO_ORIGEN VARCHAR2(3) NOT NULL 
 ,  COD_SERVICIO_ORIGEN VARCHAR2(3) NOT NULL 
 ,  COD_USO_DESTINO VARCHAR2(3) NOT NULL 
 ,  COD_SERVICIO_DESTINO VARCHAR2(3) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_CONVERSION_SERVSUP_TD IS '[ COSS ] Tabla de conversion de servicios suplementario'
/
COMMENT ON COLUMN  PV_CONVERSION_SERVSUP_TD.COD_PRODUCTO IS 'Código del producto'
/
COMMENT ON COLUMN  PV_CONVERSION_SERVSUP_TD.COD_USO_ORIGEN IS 'ATL: Atlántida, PRE: Prepago, POS: Postpagp, HIB: Hibrido'
/
COMMENT ON COLUMN  PV_CONVERSION_SERVSUP_TD.COD_SERVICIO_ORIGEN IS 'Servicio Origen'
/
COMMENT ON COLUMN  PV_CONVERSION_SERVSUP_TD.COD_USO_DESTINO IS 'ATL: Atlántida, PRE: Prepago, POS: Postpagp, HIB: Hibrido'
/
COMMENT ON COLUMN  PV_CONVERSION_SERVSUP_TD.COD_SERVICIO_DESTINO IS 'Servicio Destino'
/
COMMENT ON COLUMN  PV_CONVERSION_SERVSUP_TD.FEC_DESDE IS 'Fecha de Inicio  conversión'
/
COMMENT ON COLUMN  PV_CONVERSION_SERVSUP_TD.FEC_HASTA IS 'Fecha de Término de conversión'
/
COMMENT ON COLUMN  PV_CONVERSION_SERVSUP_TD.NOM_USUARIO IS 'Usuario que realizó conversión'
/
COMMENT ON COLUMN  PV_CONVERSION_SERVSUP_TD.FEC_ULTMOD IS 'Fecha última modificación'
/
