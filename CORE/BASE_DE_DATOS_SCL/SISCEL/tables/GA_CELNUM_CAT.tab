CREATE TABLE GA_CELNUM_CAT(
    NUM_DESDE NUMBER(15,0) NOT NULL 
 ,  NUM_HASTA NUMBER(15,0) NOT NULL 
 ,  COD_SUBALM VARCHAR2(5) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  COD_CAT NUMBER(2,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_CELNUM_CAT IS 'Numeracion celular abierta por categoria'
/
COMMENT ON COLUMN  GA_CELNUM_CAT.NUM_DESDE IS 'Numero desde del rango'
/
COMMENT ON COLUMN  GA_CELNUM_CAT.NUM_HASTA IS 'Numero hasta del rango'
/
COMMENT ON COLUMN  GA_CELNUM_CAT.COD_SUBALM IS 'Codigo de Subalm'
/
COMMENT ON COLUMN  GA_CELNUM_CAT.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_CELNUM_CAT.COD_CENTRAL IS 'Codigo de Central'
/
COMMENT ON COLUMN  GA_CELNUM_CAT.COD_CAT IS 'Codigo de Categoria'
/
