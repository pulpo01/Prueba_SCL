CREATE TABLE PVH_PARAM_ABOCEL_TH
(
  NUM_OS             NUMBER(10)                 NOT NULL,
  NUM_ABONADO        NUMBER(8)                  DEFAULT 1                     NOT NULL,
  COD_TIPMODI        VARCHAR2(2 BYTE)           NOT NULL,
  FEC_MODIFICA       DATE                       NOT NULL,
  NUOM_USUARORA      VARCHAR2(30 BYTE)          NOT NULL,
  NUM_CELULAR        NUMBER(15),
  COD_USO            VARCHAR2(2 BYTE),
  TIP_PLANTARIF      VARCHAR2(1 BYTE),
  TIP_TERMINAL       VARCHAR2(1 BYTE),
  COD_PLANTARIF      VARCHAR2(3 BYTE),
  COD_CARGOBASICO    VARCHAR2(3 BYTE),
  COD_PLANSERV       VARCHAR2(3 BYTE),
  COD_LIMCONSUMO     VARCHAR2(5 BYTE),
  NUM_SERIE          VARCHAR2(25 BYTE),
  NUM_SERIEHEX       VARCHAR2(8 BYTE),
  COD_EMPRESA        NUMBER(8),
  COD_GRPSERV        VARCHAR2(2 BYTE),
  COD_CARRIER        NUMBER(5),
  COD_CICLO          NUMBER(2),
  IND_FACTUR         NUMBER(1),
  COD_CAUSA          VARCHAR2(3 BYTE),
  NUM_MIN            VARCHAR2(3 BYTE),
  COD_TIPCONTRATO    VARCHAR2(3 BYTE),
  NUM_MESES          NUMBER(2),
  IND_EQPRESTADO     VARCHAR2(1 BYTE),
  FEC_PRORROGA       DATE,
  NUM_CICLOS         NUMBER(10),
  NUM_MINUTOS        NUMBER(10),
  FEC_FINCONTRATO    DATE,
  COD_SERVICIO       VARCHAR2(3 BYTE),
  OBS_DETALLE        VARCHAR2(255 BYTE),
  COD_TIPSEGU        NUMBER(2),
  NUM_CONTRATO       VARCHAR2(21 BYTE),
  NUM_ANEXO          VARCHAR2(21 BYTE),
  COD_PRODUCTO       NUMBER(1)                  DEFAULT 1,
  COD_PLANTARIF_NUE  VARCHAR2(3 BYTE),
  TIP_PLANTARIF_NUE  VARCHAR2(1 BYTE),
  NUM_DIA            NUMBER(3),
  COD_VENDEDOR       NUMBER(6),
  PARAM1_MENS        VARCHAR2(255 BYTE)
)
TABLESPACE TS_CRM_POSVENTA_MDAT
PCTUSED    0
PCTFREE    5
INITRANS   16
MAXTRANS   255
STORAGE    (
            INITIAL          15M
            NEXT             4M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.NUM_MESES IS 'NUMERO DE MESES';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.IND_EQPRESTADO IS 'INDICADOR DE EQUIPO PRESTADO';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.FEC_PRORROGA IS 'FECHA MODIFICACION PROGRAMACION';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.NUM_CICLOS IS 'NUMERO DE CICLOS PARA COBRAR';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.NUM_MINUTOS IS 'CANTIDAD DE MINUTOS AGTREGADOS AL ABONADO';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.FEC_FINCONTRATO IS 'FECHA DE TERMINO DE CONTRATO';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.COD_SERVICIO IS 'CODIGO DE SERVICIO';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.OBS_DETALLE IS 'OBSERVACION DE DETALLE';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.COD_TIPSEGU IS 'CODIGO DE TIPO DE SEGURO';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.NUM_CONTRATO IS 'NUMERO DE CONTRATO DEL SEGURO';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.NUM_ANEXO IS 'NUMERO DE ANEXO DEL SEGURO';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.NUM_OS IS 'NUMERO DE ORDEN DE SERVICIO.';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.NUM_ABONADO IS 'NUMERO DE ABONADO.';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.COD_TIPMODI IS 'CODIGO DE TIPO DE MODIFICACION';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.FEC_MODIFICA IS 'FECHA DE MODIFICACION.';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.NUOM_USUARORA IS 'USUARIO CREADOR DE ABONADO';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.NUM_CELULAR IS 'NUMERO DE CELULAR';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.COD_USO IS 'CODIGO DE USO';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.TIP_PLANTARIF IS 'TIPO DE PLAN TARIFARIO; INDIVIDUAL (I), EMPRESA (E), HOLDING (H)';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.TIP_TERMINAL IS 'TIPO DE TERMINAL; ANALOGICO (A), DIGITAL (D)';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.COD_PLANTARIF IS 'CODIGO DE PLAN TARIFARIO';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.COD_CARGOBASICO IS 'CODIGO DE CARGO BASICO';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.COD_PLANSERV IS 'CODIGO DE PLAN DE SERVICIOS';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.COD_LIMCONSUMO IS 'CODIGO DE LIMITE DE CONSUMO';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.NUM_SERIE IS 'NUMERO DE SERIE DECIMAL';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.NUM_SERIEHEX IS 'NUMERO DE SERIE HEXADECIMAL';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.COD_EMPRESA IS 'EMPRESA EN LA CUAL SE ENCUENTRA INCLUIDI';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.COD_GRPSERV IS 'GRUPO DE COBRO DE SERICIOS EN EL CUAL SE ENCUENTRA INCLUIDO EL ABONADO';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.COD_CARRIER IS 'OPERADOR DE CARRIER';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.COD_CICLO IS 'CICLO DEL ABONADO';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.IND_FACTUR IS 'INDICATIVO DE FACTURABLE';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.COD_CAUSA IS 'CAUSA CAMBIO EQUIPO';

COMMENT ON COLUMN PVH_PARAM_ABOCEL_TH.COD_TIPCONTRATO IS 'CODIGO DE TIPO DE CONTRATO';
