CREATE TABLE VE_HIJOS_CLIENTE_BURO_TO
(
  COD_REFERENCIA        NUMBER(8),
  NOMBRE_HIJO           VARCHAR2(50 BYTE),
  NOMBRE_COMPLETO_HIJO  VARCHAR2(100 BYTE),
  APELLIDO_PATERNO      VARCHAR2(50 BYTE),
  APELLIDO_MATERNO      VARCHAR2(50 BYTE),
  COD_PARENTESCO        VARCHAR2(5 BYTE),
  NUM_CEDULA_HIJO       VARCHAR2(20 BYTE),
  FALLECIDO             VARCHAR2(5 BYTE),
  COD_FALLECIDO         VARCHAR2(5 BYTE),
  EDAD_HIJO             VARCHAR2(25 BYTE),
  FECHA_NACIMIENTO      VARCHAR2(20 BYTE),
  SEXO_HIJO             VARCHAR2(5 BYTE)
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          1M
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING
/
COMMENT ON COLUMN VE_HIJOS_CLIENTE_BURO_TO.COD_REFERENCIA IS 'Codigo Referencia';
/
COMMENT ON COLUMN VE_HIJOS_CLIENTE_BURO_TO.NOMBRE_HIJO IS 'Nombre del Hijo';
/
COMMENT ON COLUMN VE_HIJOS_CLIENTE_BURO_TO.NOMBRE_COMPLETO_HIJO IS 'Solo el nombre completo del hijo';
/
COMMENT ON COLUMN VE_HIJOS_CLIENTE_BURO_TO.APELLIDO_PATERNO IS 'Primer apellido del hijo';
/
COMMENT ON COLUMN VE_HIJOS_CLIENTE_BURO_TO.APELLIDO_MATERNO IS 'Segundo Apellido';
/
COMMENT ON COLUMN VE_HIJOS_CLIENTE_BURO_TO.COD_PARENTESCO IS 'Codigo parentesco';
/
COMMENT ON COLUMN VE_HIJOS_CLIENTE_BURO_TO.NUM_CEDULA_HIJO IS 'Cedula del hijo';
/
COMMENT ON COLUMN VE_HIJOS_CLIENTE_BURO_TO.FALLECIDO IS 'Indica si el hijo esta fallecido ';
/
COMMENT ON COLUMN VE_HIJOS_CLIENTE_BURO_TO.COD_FALLECIDO IS 'codigo de fallecido';
/
COMMENT ON COLUMN VE_HIJOS_CLIENTE_BURO_TO.EDAD_HIJO IS 'Edad del hijo';
/
COMMENT ON COLUMN VE_HIJOS_CLIENTE_BURO_TO.FECHA_NACIMIENTO IS 'Fecha de nacimiento del hijo';
/
COMMENT ON COLUMN VE_HIJOS_CLIENTE_BURO_TO.SEXO_HIJO IS 'Genero del hijo';
/
