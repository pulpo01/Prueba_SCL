CREATE TABLE GSM_PROVEEDORES_ARTICULOS_TO(
    COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  COD_PROVEEDOR NUMBER NOT NULL 
 ,  FEC_ACTUALIZACION DATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_PROVEEDORES_ARTICULOS_TO IS 'Tabla de relacion de proveedores y sus articulos GSM.'
/
COMMENT ON COLUMN  GSM_PROVEEDORES_ARTICULOS_TO.COD_ARTICULO IS 'Codigo de Articulo.'
/
COMMENT ON COLUMN  GSM_PROVEEDORES_ARTICULOS_TO.COD_PROVEEDOR IS 'Codigo de proveedor.'
/
COMMENT ON COLUMN  GSM_PROVEEDORES_ARTICULOS_TO.FEC_ACTUALIZACION IS 'Fecha de la ultima actualizacion.'
/
COMMENT ON COLUMN  GSM_PROVEEDORES_ARTICULOS_TO.COD_USUARIO IS 'Codigo del usuario que actualizo.'
/
