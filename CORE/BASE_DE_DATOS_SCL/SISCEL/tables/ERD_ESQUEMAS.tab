CREATE TABLE ERD_ESQUEMAS(
    COD_ESQUEMA NUMBER(6,0) NOT NULL 
 ,  NOM_ESQUEMA VARCHAR2(30) NOT NULL 
 ,  NOM_USUARIO_MODI VARCHAR2(30) NOT NULL 
 ,  FEC_MODI_H DATE NOT NULL 
 ,  IND_VIGENCIA VARCHAR2(1) DEFAULT 'S' NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERD_ESQUEMAS IS 'Defincion De Esquemas De Evaluacion.'
/
COMMENT ON COLUMN  ERD_ESQUEMAS.COD_ESQUEMA IS 'Identificador unico para el esquema de evaluacion.'
/
COMMENT ON COLUMN  ERD_ESQUEMAS.NOM_ESQUEMA IS 'Nombre del esquema de evaluacion.'
/
COMMENT ON COLUMN  ERD_ESQUEMAS.NOM_USUARIO_MODI IS 'Nombre del usuario que creo/actualizo este registro.'
/
COMMENT ON COLUMN  ERD_ESQUEMAS.FEC_MODI_H IS 'fecha de creacion/modificacion de este registro.'
/
