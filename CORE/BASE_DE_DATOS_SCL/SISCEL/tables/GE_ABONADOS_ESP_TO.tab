CREATE TABLE GE_ABONADOS_ESP_TO(
    COD_MODULO VARCHAR2(2) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_TIPO_ABONESP NUMBER(3,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_MODIF DATE NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_ABONADOS_ESP_TO IS 'ABONADOS ESPECIALES'
/
COMMENT ON COLUMN  GE_ABONADOS_ESP_TO.COD_MODULO IS 'CODIGO DE MODULO (APLICACION)'
/
COMMENT ON COLUMN  GE_ABONADOS_ESP_TO.NUM_ABONADO IS 'NUMERO DE ABONADO'
/
COMMENT ON COLUMN  GE_ABONADOS_ESP_TO.COD_TIPO_ABONESP IS 'CODIGO DE TIPO DE ABONADO ESPECIAL'
/
COMMENT ON COLUMN  GE_ABONADOS_ESP_TO.NOM_USUARIO IS 'USUARIO'
/
COMMENT ON COLUMN  GE_ABONADOS_ESP_TO.FEC_MODIF IS 'FECHA DE MODIFICACION'
/
