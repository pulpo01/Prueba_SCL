CREATE TABLE CEH_COMISIONES(
    COD_EMPRESA VARCHAR2(5) NOT NULL 
 ,  COD_GRPSERV NUMBER(5,0) NOT NULL 
 ,  COD_PERIODO VARCHAR2(6) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) NOT NULL 
 ,  NUM_TRANSACCION NUMBER(10,0) NULL 
 ,  MTO_TRANSACCION NUMBER(14,4) NULL 
 ,  NUM_TRANSINFOR NUMBER(6,0) NULL 
 ,  MTO_TRANSINFOR NUMBER(14,4) NULL 
 ,  NUM_TRANSCOMIS NUMBER(6,0) NULL 
 ,  MTO_TRANSCOMIS NUMBER(14,4) NULL 
 ,  VAL_COMISION NUMBER(14,4) NULL 
 ,  MTO_COMISIONES NUMBER(14,4) NULL 
 ,  NUM_PROCESO NUMBER(8,0) NOT NULL 
 ,  FEC_PROCESO DATE NULL 
 ,  FEC_CONCILIACION DATE NULL 
 ,  FEC_PAGOCOMIS DATE NULL 
 ,  FEC_CIERRE DATE NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CEH_COMISIONES IS 'Histsrica de comisiones por empresa'
/
COMMENT ON COLUMN  CEH_COMISIONES.COD_EMPRESA IS 'Codigo Empresa'
/
COMMENT ON COLUMN  CEH_COMISIONES.COD_GRPSERV IS 'Codigo Grupo  de servicios'
/
COMMENT ON COLUMN  CEH_COMISIONES.COD_PERIODO IS 'Periodo cierre comisiones'
/
COMMENT ON COLUMN  CEH_COMISIONES.FEC_DESDE IS 'Fecha Inicio Periodo'
/
COMMENT ON COLUMN  CEH_COMISIONES.FEC_HASTA IS 'Fecha Termino Periodo'
/
COMMENT ON COLUMN  CEH_COMISIONES.COD_ESTADO IS 'Estado Comisiones :  GE: Generadas'
/
COMMENT ON COLUMN  CEH_COMISIONES.NUM_TRANSACCION IS 'Numero de transacciones efectuadas'
/
COMMENT ON COLUMN  CEH_COMISIONES.MTO_TRANSACCION IS 'Monto generado por transacciones'
/
COMMENT ON COLUMN  CEH_COMISIONES.NUM_TRANSINFOR IS 'Numero de transacciones informadas por la empresa'
/
COMMENT ON COLUMN  CEH_COMISIONES.MTO_TRANSINFOR IS 'Monto Informado por la empresa'
/
COMMENT ON COLUMN  CEH_COMISIONES.NUM_TRANSCOMIS IS 'Numero de transacciones comisionadas'
/
COMMENT ON COLUMN  CEH_COMISIONES.MTO_TRANSCOMIS IS 'Monto de transacciones comisionadas'
/
COMMENT ON COLUMN  CEH_COMISIONES.VAL_COMISION IS 'Valor comision para calculo'
/
COMMENT ON COLUMN  CEH_COMISIONES.MTO_COMISIONES IS 'Monto comisiones pagadas'
/
COMMENT ON COLUMN  CEH_COMISIONES.NUM_PROCESO IS 'Numero de proceso de comision'
/
COMMENT ON COLUMN  CEH_COMISIONES.FEC_PROCESO IS 'Fecha ejecucion de proceso de calculo'
/
COMMENT ON COLUMN  CEH_COMISIONES.FEC_CONCILIACION IS 'Fecha Conciliacion'
/
COMMENT ON COLUMN  CEH_COMISIONES.FEC_PAGOCOMIS IS 'Fecha Pago comisiones'
/
COMMENT ON COLUMN  CEH_COMISIONES.FEC_CIERRE IS 'Fecha de cierre total'
/
