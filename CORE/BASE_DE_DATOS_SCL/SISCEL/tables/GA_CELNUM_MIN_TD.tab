CREATE TABLE GA_CELNUM_MIN_TD(
    NUM_DESDE NUMBER(15,0) NOT NULL 
 ,  NUM_HASTA NUMBER(15,0) NOT NULL 
 ,  MIN_DESDE_ACTUAL VARCHAR2(15) NULL 
 ,  MIN_HASTA_ACTUAL VARCHAR2(15) NULL 
 ,  MIN_DESDE_NUEVO VARCHAR2(15) NOT NULL 
 ,  MIN_HASTA_NUEVO VARCHAR2(15) NOT NULL 
 ,  IND_VIGENCIA_MIN_NUEVO VARCHAR2(1) NOT NULL 
 ,  FEC_VIG_MIN_NUEVO DATE NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_CELNUM_MIN_TD IS 'RANGOS MINS PARA RANGOS DE LINEA MDN'
/
COMMENT ON COLUMN  GA_CELNUM_MIN_TD.NUM_DESDE IS 'Numero desde del rango'
/
COMMENT ON COLUMN  GA_CELNUM_MIN_TD.NUM_HASTA IS 'Numero hasta del rango'
/
COMMENT ON COLUMN  GA_CELNUM_MIN_TD.MIN_DESDE_ACTUAL IS 'Numero desde Rango MIN actual'
/
COMMENT ON COLUMN  GA_CELNUM_MIN_TD.MIN_HASTA_ACTUAL IS 'Numero hasta Rango MIN actual'
/
COMMENT ON COLUMN  GA_CELNUM_MIN_TD.MIN_DESDE_NUEVO IS 'Numero desde Rango MIN nuevo'
/
COMMENT ON COLUMN  GA_CELNUM_MIN_TD.MIN_HASTA_NUEVO IS 'Numero hasta Rango MIN nuevo'
/
COMMENT ON COLUMN  GA_CELNUM_MIN_TD.IND_VIGENCIA_MIN_NUEVO IS 'Indicador de Vigencia de Rango MIN Nuevo'
/
COMMENT ON COLUMN  GA_CELNUM_MIN_TD.FEC_VIG_MIN_NUEVO IS 'Fecha en que rango MIN Nuevo pasa a tener vigencia'
/
COMMENT ON COLUMN  GA_CELNUM_MIN_TD.FEC_ULTMOD IS 'Fecha de ultima modificacion'
/
COMMENT ON COLUMN  GA_CELNUM_MIN_TD.NOM_USUARIO IS 'Usario que realizo ultima modificacion'
/
