CREATE TABLE TA_OPEROAMING(
    COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  IND_AUTOMATICO NUMBER(1,0) NOT NULL 
 ,  IND_CONCILIACION NUMBER(1,0) NOT NULL 
 ,  IMP_FRACCION NUMBER(8,4) NULL 
 ,  SEG_FRACCION NUMBER(5,0) NULL 
 ,  COD_SID NUMBER(5,0) NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_OPEROAMING IS 'Datos operadores con acuerdo roaming.'
/
COMMENT ON COLUMN  TA_OPEROAMING.COD_OPERADOR IS 'Codigo de operador'
/
COMMENT ON COLUMN  TA_OPEROAMING.IND_AUTOMATICO IS 'Tipo de roaming 1 Automatico 0 Manual'
/
COMMENT ON COLUMN  TA_OPEROAMING.IND_CONCILIACION IS 'Tipo conciliacion Alternativo CIBER'
/
COMMENT ON COLUMN  TA_OPEROAMING.IMP_FRACCION IS 'Importe por fraccion'
/
COMMENT ON COLUMN  TA_OPEROAMING.SEG_FRACCION IS 'Segundos que es cada fraccion de tiempo'
/
COMMENT ON COLUMN  TA_OPEROAMING.COD_SID IS 'Numero identificativo del operador'
/
