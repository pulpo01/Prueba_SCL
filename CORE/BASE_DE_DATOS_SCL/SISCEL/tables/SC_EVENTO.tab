CREATE TABLE SC_EVENTO(
    COD_EVENTO NUMBER(2,0) NOT NULL 
 ,  DES_EVENTO VARCHAR2(40) NOT NULL 
 ,  COD_DOMINIO_CTO NUMBER(2,0) NOT NULL 
 ,  IND_IMPUTACION CHAR(1) NOT NULL 
 ,  DES_DIRECTORIO VARCHAR2(100) NOT NULL 
 ,  DES_ORIGEN VARCHAR2(25) NOT NULL 
 ,  COD_EVENTO_CTO NUMBER(2,0) NULL 
 ,  IND_AGRUPACION NUMBER(1,0) DEFAULT 0 NULL 
 ,  IND_GRP_DOMINIO NUMBER(1,0) DEFAULT 0 NULL 
 ,  IND_PLAZA VARCHAR2(1) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SC_EVENTO IS 'EVENTOS DE MOVIMIENTOS CONTABLES'
/
COMMENT ON COLUMN  SC_EVENTO.COD_EVENTO IS 'CODIGO DE EVENTO'
/
COMMENT ON COLUMN  SC_EVENTO.DES_EVENTO IS 'DESCRIPCION DE EVENTO'
/
COMMENT ON COLUMN  SC_EVENTO.COD_DOMINIO_CTO IS 'CODIGO DEL DOMINIO DE CONCEPTOS'
/
COMMENT ON COLUMN  SC_EVENTO.IND_IMPUTACION IS 'INDICADOR C:CREDITO/D:DEBITO'
/
COMMENT ON COLUMN  SC_EVENTO.DES_DIRECTORIO IS 'UBICACISN DEL ARCHIVO PARA CONTABILIDAD'
/
COMMENT ON COLUMN  SC_EVENTO.DES_ORIGEN IS 'ORIGEN CONTABLE DEL COMPROBANTE'
/
COMMENT ON COLUMN  SC_EVENTO.COD_EVENTO_CTO IS 'Codigo de evento para contabilizar conceptos.'
/
COMMENT ON COLUMN  SC_EVENTO.IND_AGRUPACION IS 'Agrupacion de evento contable: 0=Agrupa cuenta y contracuenta contables | 1=Agrupa solo contracuenta contable.'
/
COMMENT ON COLUMN  SC_EVENTO.IND_GRP_DOMINIO IS 'INDICADOR SI EL EVENTO SE RELACIONA CON UN DOMINIO (0) O CON UN GRUPO DE DOMINIOS(1)..'
/
COMMENT ON COLUMN  SC_EVENTO.IND_PLAZA IS 'INDICA SI SE INFORMA LA PLAZA EN EL INDENTIFICADOR DEL LOTE'
/
