CREATE TABLE VE_PROSTATUS(
    COD_PROSPECTO NUMBER(8,0) NOT NULL 
 ,  COD_ESTADOPRO VARCHAR2(2) NOT NULL 
 ,  FEC_STATUS DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_PROSTATUS IS 'Status de Prospectos por fechas'
/
COMMENT ON COLUMN  VE_PROSTATUS.COD_PROSPECTO IS 'Codigo de Prospecto'
/
COMMENT ON COLUMN  VE_PROSTATUS.COD_ESTADOPRO IS 'Codigo Estado de Prospecto'
/
COMMENT ON COLUMN  VE_PROSTATUS.FEC_STATUS IS 'Fecha de cambio'
/
COMMENT ON COLUMN  VE_PROSTATUS.NOM_USUARIO IS 'Usuario que efectuo el cambio'
/
