CREATE TABLE CM_ASIGNA_CUALIDAD_TD(
    COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  IMP_MAXIMO NUMBER(14,4) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) DEFAULT 'C' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_ASIGNA_CUALIDAD_TD IS 'asignacion de conceptos del tipo cualitativos.'
/
COMMENT ON COLUMN  CM_ASIGNA_CUALIDAD_TD.FEC_ULTMOD IS 'FECHA ULT. MODIFICACION.'
/
COMMENT ON COLUMN  CM_ASIGNA_CUALIDAD_TD.NOM_USUARIO IS 'NOMBRE USUARIO.'
/
COMMENT ON COLUMN  CM_ASIGNA_CUALIDAD_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
COMMENT ON COLUMN  CM_ASIGNA_CUALIDAD_TD.COD_PLANCOMIS IS 'PLAN DE COMISIONES QUE CONTIENEN AL CONCEPTO.'
/
COMMENT ON COLUMN  CM_ASIGNA_CUALIDAD_TD.COD_CONCEPTO IS 'CONCEPTO DE PENALIZACION.'
/
COMMENT ON COLUMN  CM_ASIGNA_CUALIDAD_TD.IMP_MAXIMO IS 'IMPORTE MAXIMO.'
/
