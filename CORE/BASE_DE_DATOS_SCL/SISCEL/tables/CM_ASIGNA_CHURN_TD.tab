CREATE TABLE CM_ASIGNA_CHURN_TD(
    COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_CREACION DATE NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) DEFAULT 'C' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_ASIGNA_CHURN_TD IS 'Detalle De Asignacion De Churn.'
/
COMMENT ON COLUMN  CM_ASIGNA_CHURN_TD.COD_CONCEPTO IS 'CONCEPTO DE PENALIZACION.'
/
COMMENT ON COLUMN  CM_ASIGNA_CHURN_TD.NOM_USUARIO IS 'NOMBRE USUARIO.'
/
COMMENT ON COLUMN  CM_ASIGNA_CHURN_TD.FEC_CREACION IS 'FECHA DE ULTIMA MODIFICACION.'
/
COMMENT ON COLUMN  CM_ASIGNA_CHURN_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
COMMENT ON COLUMN  CM_ASIGNA_CHURN_TD.COD_PLANCOMIS IS 'PLAN DE COMISIONES QUE CONTIENEN AL CONCEPTO.'
/
