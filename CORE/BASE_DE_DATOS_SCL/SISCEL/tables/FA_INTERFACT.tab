CREATE TABLE SISCEL.FA_INTERFACT
(
  NUM_PROCESO        NUMBER(8)                  NOT NULL,
  NUM_VENTA          NUMBER(8),
  COD_MODGENER       VARCHAR2(3 BYTE)           NOT NULL,
  COD_ESTADOC        NUMBER(3)                  NOT NULL,
  COD_ESTPROC        NUMBER(1)                  NOT NULL,
  COD_TIPMOVIMIEN    NUMBER(2)                  NOT NULL,
  COD_CATRIBUT       CHAR(1 BYTE)               NOT NULL,
  COD_CATIMPOSITIVA  NUMBER(1),
  COD_TIPDOCUM       NUMBER(2)                  DEFAULT 0                     NOT NULL,
  NUM_FOLIO          NUMBER(9),
  NUM_FOLIOREL       NUMBER(9),
  FEC_INGRESO        DATE,
  FEC_FACTURACION    DATE,
  FEC_IMPRESION      DATE,
  FEC_FOLIACION      DATE,
  FEC_VISACION       DATE,
  FEC_PASOCOBRO      DATE,
  FEC_CIERRE         DATE,
  FEC_VENCIMIENTO    DATE,
  NUM_CUOTAS         NUMBER(3)                  DEFAULT 0                     NOT NULL,
  COD_MODVENTA       NUMBER(2)                  DEFAULT 1                     NOT NULL,
  NOM_USUAELIM       VARCHAR2(30 BYTE),
  COD_CAUSAELIM      VARCHAR2(5 BYTE),
  COD_APLIC          VARCHAR2(3 BYTE)           DEFAULT 'FAC'                 NOT NULL,
  PREF_PLAZA         VARCHAR2(25 BYTE)          DEFAULT 'TMG'                 NOT NULL,
  PREF_PLAZAREL      VARCHAR2(25 BYTE)          DEFAULT 'TMG'                 NOT NULL,
  TIP_FOLIACION      VARCHAR2(1 BYTE)           DEFAULT 'P'                   NOT NULL
)  TABLESPACE TS_FACTURACION_SDAT
/

COMMENT ON TABLE SISCEL.FA_INTERFACT IS 'Interfaz de Facturacion.';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.NUM_PROCESO IS 'Numero de Proceso. [PK 1/1]';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.NUM_VENTA IS 'Numero de Venta.';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.COD_MODGENER IS 'Indicador de Generacion de Documentos. Segun tabla FA_MODGENER.';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.COD_ESTADOC IS 'Estado del Documento. Segun tabla FA_INTESTADOC.';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.COD_ESTPROC IS 'Estado del Proceso:  0=Sin Procesar|1=Procesando|2=Terminado con Error|3=Terminado OK';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.COD_TIPMOVIMIEN IS 'Tipo de Movimiento: Venta|Miscelanea|N. Credito|Ciclo';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.COD_CATRIBUT IS 'Categoria Tributaria:  F=Factura|B=Boleta';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.COD_CATIMPOSITIVA IS 'Codigo de categoria impositiva del cliente: 1=Afecto|0=Exento';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.COD_TIPDOCUM IS 'Tipo Documento a Generar';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.NUM_FOLIO IS 'Numero Folio del Documento';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.NUM_FOLIOREL IS 'Numero Folio de Factura Relacionada a la Nota de Credito.';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.FEC_INGRESO IS 'Fecha de Ingreso (HORA)';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.FEC_FACTURACION IS 'Fecha de Facturacion (HORA)';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.FEC_IMPRESION IS 'Fecha de impresion (HORA)';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.FEC_FOLIACION IS 'Fecha de Foliacion (HORA)';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.FEC_VISACION IS 'Fecha de Visacion (HORA)';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.FEC_PASOCOBRO IS 'Fecha de Paso a Cobros (HORA)';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.FEC_CIERRE IS 'Fecha de Cierre (HORA)';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.FEC_VENCIMIENTO IS 'Fecha de Vencimiento Factura';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.NUM_CUOTAS IS 'Numero de cuotas de la venta.';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.COD_MODVENTA IS 'Modalidad de venta.';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.NOM_USUAELIM IS 'Usuario responsable de la eliminacisn del documento';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.COD_CAUSAELIM IS 'Codigo causa de eliminacisn E: Error de digitacion; D: duplicidad';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.COD_APLIC IS 'Codigo de Interfaz.';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.PREF_PLAZA IS 'PREFIJO DE LA PLAZA';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.PREF_PLAZAREL IS 'PREFIJO DE LA PLAZA RELACIONADA';

COMMENT ON COLUMN SISCEL.FA_INTERFACT.TIP_FOLIACION IS 'TIPO DE FOLIACION A:AUTOFOLIATIVA P:PREFOLIADA';

