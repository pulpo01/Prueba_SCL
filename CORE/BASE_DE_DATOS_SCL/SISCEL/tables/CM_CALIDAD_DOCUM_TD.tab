CREATE TABLE CM_CALIDAD_DOCUM_TD(
    COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  TIP_EVALUACION CHAR(1) NOT NULL 
 ,  FEC_CREACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) DEFAULT 'C' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_CALIDAD_DOCUM_TD IS 'EVALUACION DE DOCUMENTO POR TIPO.'
/
COMMENT ON COLUMN  CM_CALIDAD_DOCUM_TD.COD_CONCEPTO IS 'CONCEPTO DE PENALIZACION.'
/
COMMENT ON COLUMN  CM_CALIDAD_DOCUM_TD.TIP_EVALUACION IS 'INDICADOR DE EVALUACION P:PLAZO/ C:CALIDAD.'
/
COMMENT ON COLUMN  CM_CALIDAD_DOCUM_TD.FEC_CREACION IS 'FECHA DE CREACION.'
/
COMMENT ON COLUMN  CM_CALIDAD_DOCUM_TD.NOM_USUARIO IS 'NOMBRE USUARIO.'
/
COMMENT ON COLUMN  CM_CALIDAD_DOCUM_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
COMMENT ON COLUMN  CM_CALIDAD_DOCUM_TD.COD_PLANCOMIS IS 'PLAN DE COMISIONES QUE CONTIENEN AL CONCEPTO.'
/
