CREATE TABLE AL_SER_MOVI_LOCA(
    NUM_MOVI NUMBER(9,0) NOT NULL 
 ,  NUM_LINEA NUMBER(3,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  TIP_STOCK NUMBER(2,0) NOT NULL 
 ,  COD_ESTADO NUMBER(2,0) NOT NULL 
 ,  NUM_SEC_ANT NUMBER(10,0) NULL 
 ,  NUM_SEC_NUE NUMBER(10,0) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_SER_MOVI_LOCA IS 'Series Involucradas En Relocalizacion.'
/
COMMENT ON COLUMN  AL_SER_MOVI_LOCA.NUM_MOVI IS 'Numero de movimiento de Relocalizacion.'
/
COMMENT ON COLUMN  AL_SER_MOVI_LOCA.NUM_LINEA IS 'Numero de Linea.'
/
COMMENT ON COLUMN  AL_SER_MOVI_LOCA.NUM_SERIE IS 'Numero de Serie.'
/
COMMENT ON COLUMN  AL_SER_MOVI_LOCA.COD_USO IS 'Codigo de Uso de Equipo.'
/
COMMENT ON COLUMN  AL_SER_MOVI_LOCA.TIP_STOCK IS 'Tipo de Stock.'
/
COMMENT ON COLUMN  AL_SER_MOVI_LOCA.COD_ESTADO IS 'Codigo de Estado del Equipo.'
/
COMMENT ON COLUMN  AL_SER_MOVI_LOCA.NUM_SEC_ANT IS 'Identificador de Localizacion Anterior.'
/
COMMENT ON COLUMN  AL_SER_MOVI_LOCA.NUM_SEC_NUE IS 'Identificador de Localizacion Nueva.'
/
