CREATE TABLE IC_SECTRANSACCION_TH(
    COD_PROD_CONTRATADO NUMBER(38,0) NOT NULL 
 ,  NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  ID_SECUENCIA NUMBER(10,0) NOT NULL 
 ,  NUM_MOV_ORIGEN NUMBER(9,0) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  IC_SECTRANSACCION_TH IS '[ IC_SECTRANTH ] Tabla historica de secuencias de Altamira'
/
COMMENT ON COLUMN  IC_SECTRANSACCION_TH.COD_PROD_CONTRATADO IS 'Código de Producto Contratado'
/
COMMENT ON COLUMN  IC_SECTRANSACCION_TH.NUM_CELULAR IS 'Número de Celular'
/
COMMENT ON COLUMN  IC_SECTRANSACCION_TH.ID_SECUENCIA IS 'Identificador de Secuencia'
/
COMMENT ON COLUMN  IC_SECTRANSACCION_TH.NUM_MOV_ORIGEN IS 'Número de Movimiento generador de la secuencia'
/
COMMENT ON COLUMN  IC_SECTRANSACCION_TH.FEC_INGRESO IS 'Fecha de ingreso del movimiento'
/
COMMENT ON COLUMN  IC_SECTRANSACCION_TH.FEC_HISTORICO IS 'Fecha de paso a Historico'
/
