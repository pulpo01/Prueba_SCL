CREATE TABLE GA_CAUSAREP(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CAUSAREP NUMBER(3,0) NOT NULL 
 ,  DES_CAUSAREP VARCHAR2(30) NOT NULL 
 ,  IND_VIGENCIA NUMBER(1,0) DEFAULT 1 NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_CREACION DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_CAUSAREP IS 'Causas de Rechazo Provisorio de Venta'
/
COMMENT ON COLUMN  GA_CAUSAREP.COD_PRODUCTO IS 'Codigo de Producto '
/
COMMENT ON COLUMN  GA_CAUSAREP.COD_CAUSAREP IS 'Codigo de Causa de Rechazo Provisorio'
/
COMMENT ON COLUMN  GA_CAUSAREP.DES_CAUSAREP IS 'Descripcion de Causa de Rechazo Provisorio'
/
COMMENT ON COLUMN  GA_CAUSAREP.IND_VIGENCIA IS 'Indica si la causa es o no vigente 0:No; 1:Si'
/
COMMENT ON COLUMN  GA_CAUSAREP.NOM_USUARIO IS 'Usuario de CreaciM-sn'
/
COMMENT ON COLUMN  GA_CAUSAREP.FEC_CREACION IS 'Fecha de CreaciM-sn'
/
