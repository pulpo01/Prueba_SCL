CREATE TABLE CO_DOC_NETEABLE(
    NUM_IDENT VARCHAR2(20) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_TIPDOCUM NUMBER(2,0) NOT NULL 
 ,  NUM_FOLIO NUMBER(9,0) NOT NULL 
 ,  TOTAL_DOCUMENTO NUMBER(14,4) NOT NULL 
 ,  SALDO_IMPAGO NUMBER(14,4) NOT NULL 
 ,  FEC_EMISION DATE NOT NULL 
 ,  FEC_VENCIMIE DATE NOT NULL 
 ,  NUM_FOLIOCTC VARCHAR2(11) NOT NULL 
 ,  ID_ARCHIVO_OFA NUMBER(6,0) NOT NULL 
 ,  IND_NETEO VARCHAR2(1) NULL 
 ,  MONTO_NETEADO NUMBER(14,4) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_DOC_NETEABLE IS 'DOCUMENTOS SELECCIONADOS PARA CANJE'
/
COMMENT ON COLUMN  CO_DOC_NETEABLE.NUM_IDENT IS 'RUT DEL CLIENTE_PROVEEDOR'
/
COMMENT ON COLUMN  CO_DOC_NETEABLE.COD_CLIENTE IS 'CODIGO CLIENTE'
/
COMMENT ON COLUMN  CO_DOC_NETEABLE.COD_TIPDOCUM IS 'CODIGO DE TIPO DE DOCUMENTO'
/
COMMENT ON COLUMN  CO_DOC_NETEABLE.NUM_FOLIO IS 'NUMERO DE FOLIO DE DOCUMENTO'
/
COMMENT ON COLUMN  CO_DOC_NETEABLE.TOTAL_DOCUMENTO IS 'MONTO TOTAL POR EL QUE FUE EMITIDO EL DOCUMENTO'
/
COMMENT ON COLUMN  CO_DOC_NETEABLE.SALDO_IMPAGO IS 'SALDO IMPAGO O PENDIENTE DEL DOCUMENTO'
/
COMMENT ON COLUMN  CO_DOC_NETEABLE.FEC_EMISION IS 'FECHA DE EMISION DEL DOCUMENTO'
/
COMMENT ON COLUMN  CO_DOC_NETEABLE.FEC_VENCIMIE IS 'FECHA DE VENCIMIENTO DEL DOCUMENTO'
/
COMMENT ON COLUMN  CO_DOC_NETEABLE.NUM_FOLIOCTC IS 'NUMERO DE FOLIO CTC ( NECESARIO PARA TECAS )'
/
COMMENT ON COLUMN  CO_DOC_NETEABLE.ID_ARCHIVO_OFA IS 'IDENTIFICACION DE ARCHIVO PARA ORACLE FINANTIALS'
/
COMMENT ON COLUMN  CO_DOC_NETEABLE.IND_NETEO IS 'INDICADOR DE NETEO (T:TOTAL; P:PARCIAL; N:NO_NETEADO)'
/
COMMENT ON COLUMN  CO_DOC_NETEABLE.MONTO_NETEADO IS 'MONTO QUE SE NETEO EFECTIVAMENTE'
/
