CREATE TABLE TA_REGISTROS(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_TIPCENTRAL NUMBER(2,0) NOT NULL 
 ,  COD_TIPREGISTRO NUMBER(2,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(3,0) NOT NULL 
 ,  NUM_IDENTIFICADOR NUMBER(3,0) NOT NULL 
 ,  IND_LONGITUD NUMBER(3,0) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_REGISTROS IS 'Asociacion campo a tipo de registro'
/
COMMENT ON COLUMN  TA_REGISTROS.COD_PRODUCTO IS 'Csdigo de Producto'
/
COMMENT ON COLUMN  TA_REGISTROS.COD_TIPCENTRAL IS 'Csdigo de Tipo de Central'
/
COMMENT ON COLUMN  TA_REGISTROS.COD_TIPREGISTRO IS 'Csdigo de Tipo de Registro'
/
COMMENT ON COLUMN  TA_REGISTROS.NUM_ORDEN IS 'Nzmero de Orden'
/
COMMENT ON COLUMN  TA_REGISTROS.NUM_IDENTIFICADOR IS 'Nzmero de Identificador'
/
COMMENT ON COLUMN  TA_REGISTROS.IND_LONGITUD IS 'Nzmero de Longitud'
/
