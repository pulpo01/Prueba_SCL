CREATE TABLE VE_EXCEPCION_PREPAGO_TD(
    COD_CAUSA NUMBER(3,0) NOT NULL 
 ,  DESC_CAUSA VARCHAR2(30) NOT NULL 
 ,  FECHA_VIGENCIA_DESDE DATE NULL 
 ,  FECHA_VIGENCIA_HASTA DATE NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_EXCEPCION_PREPAGO_TD IS '[ EXPR ] Excepciones para sobrepasar el limite de lineas de prepago'
/
COMMENT ON COLUMN  VE_EXCEPCION_PREPAGO_TD.COD_CAUSA IS 'Codigo de causa de excepcion.'
/
COMMENT ON COLUMN  VE_EXCEPCION_PREPAGO_TD.DESC_CAUSA IS 'Descripcion de causa de excepcion.'
/
COMMENT ON COLUMN  VE_EXCEPCION_PREPAGO_TD.FECHA_VIGENCIA_DESDE IS 'Fecha de entrada en vigencia de la causa de excepcion.'
