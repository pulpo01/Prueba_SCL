CREATE TABLE GE_CONTPAQUETE(
    COD_PAQUETE NUMBER(6,0) NOT NULL 
 ,  NUM_ITEM NUMBER(2,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NULL 
 ,  NUM_UNIDADES NUMBER(3,0) NULL 
 ,  COD_SERVICIO VARCHAR2(3) NULL 
 ,  NUM_DIASSERV NUMBER(4,0) NULL 
 ,  IND_EQUIACC VARCHAR2(1) NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_CONTPAQUETE IS 'Contenido del Paquete'
/
COMMENT ON COLUMN  GE_CONTPAQUETE.COD_PAQUETE IS 'Codigo de Paquete'
/
COMMENT ON COLUMN  GE_CONTPAQUETE.NUM_ITEM IS 'Identificador de item (PK)'
/
COMMENT ON COLUMN  GE_CONTPAQUETE.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GE_CONTPAQUETE.COD_ARTICULO IS 'Codigo de Articulo'
/
COMMENT ON COLUMN  GE_CONTPAQUETE.NUM_UNIDADES IS 'Numero de unidades para articulos'
/
COMMENT ON COLUMN  GE_CONTPAQUETE.COD_SERVICIO IS 'Codigo de Servicio'
/
COMMENT ON COLUMN  GE_CONTPAQUETE.NUM_DIASSERV IS 'Numero de dias de vigencia del Servicio'
/
COMMENT ON COLUMN  GE_CONTPAQUETE.IND_EQUIACC IS 'Indicativo de Equipo/Accesorio (E/A)'
/
