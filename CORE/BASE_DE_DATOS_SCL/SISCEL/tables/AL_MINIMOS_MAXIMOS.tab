CREATE TABLE AL_MINIMOS_MAXIMOS(
    COD_BODEGA NUMBER(6,0) NOT NULL 
 ,  TIP_STOCK NUMBER(2,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_ESTADO NUMBER(2,0) NOT NULL 
 ,  CAN_MINIMA NUMBER(9,0) NOT NULL 
 ,  CAN_MAXIMA NUMBER(9,0) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_MINIMOS_MAXIMOS IS 'Definiciones de stock minimo y maximo'
/
COMMENT ON COLUMN  AL_MINIMOS_MAXIMOS.COD_BODEGA IS 'Bodega'
/
COMMENT ON COLUMN  AL_MINIMOS_MAXIMOS.TIP_STOCK IS 'Tipo de Stock'
/
COMMENT ON COLUMN  AL_MINIMOS_MAXIMOS.COD_ARTICULO IS 'Artmculo'
/
COMMENT ON COLUMN  AL_MINIMOS_MAXIMOS.COD_USO IS 'Uso al que esta destinado el artmculo'
/
COMMENT ON COLUMN  AL_MINIMOS_MAXIMOS.COD_ESTADO IS 'Estado en el que se encuentra el artmculo'
/
COMMENT ON COLUMN  AL_MINIMOS_MAXIMOS.CAN_MINIMA IS 'Cantidad Mmnima Aconsejable'
/
COMMENT ON COLUMN  AL_MINIMOS_MAXIMOS.CAN_MAXIMA IS 'Cantidad Maxima aconsejable'
/
