CREATE TABLE FA_HISTARRIENDO(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  PRECIO_MES NUMBER(11,2) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_HISTARRIENDO IS 'Articulos en arriendo- Retrofitted'
/
COMMENT ON COLUMN  FA_HISTARRIENDO.COD_CLIENTE IS 'Codigo de Cliente'
/
COMMENT ON COLUMN  FA_HISTARRIENDO.NUM_ABONADO IS 'Secuencia de Abonado Producto'
/
COMMENT ON COLUMN  FA_HISTARRIENDO.FEC_DESDE IS 'Fecha desde inicio de arriendo'
/
COMMENT ON COLUMN  FA_HISTARRIENDO.FEC_HASTA IS 'Fecha fin del arriendo'
/
COMMENT ON COLUMN  FA_HISTARRIENDO.COD_PRODUCTO IS 'Codigo producto del  articulo'
/
COMMENT ON COLUMN  FA_HISTARRIENDO.COD_CONCEPTO IS 'Codigo de Concepto'
/
COMMENT ON COLUMN  FA_HISTARRIENDO.COD_ARTICULO IS 'Codigo del articulo arrendado'
/
COMMENT ON COLUMN  FA_HISTARRIENDO.PRECIO_MES IS 'Precios por mes del articulo arrendado'
/
COMMENT ON COLUMN  FA_HISTARRIENDO.COD_MONEDA IS 'Codigo moneda del articulo'
/
COMMENT ON COLUMN  FA_HISTARRIENDO.FEC_HISTORICO IS 'Fecha de Paso a Historicos'
/
