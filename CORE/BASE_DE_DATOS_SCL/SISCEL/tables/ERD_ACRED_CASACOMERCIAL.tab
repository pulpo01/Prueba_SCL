CREATE TABLE ERD_ACRED_CASACOMERCIAL(
    COD_CASACOMERCIAL VARCHAR2(3) NOT NULL 
 ,  EDAD_INI NUMBER(3,0) NOT NULL 
 ,  EDAD_FIN NUMBER(3,0) NOT NULL 
 ,  MTO_RENTA NUMBER(14,4) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  ERD_ACRED_CASACOMERCIAL.COD_CASACOMERCIAL IS 'Codigo de la Casa Comercial.'
/
COMMENT ON COLUMN  ERD_ACRED_CASACOMERCIAL.EDAD_INI IS 'Edad inicial del Rango para esta acreditacion.'
/
COMMENT ON COLUMN  ERD_ACRED_CASACOMERCIAL.EDAD_FIN IS 'Edad final del Rango para esta acreditacion.'
/
COMMENT ON COLUMN  ERD_ACRED_CASACOMERCIAL.MTO_RENTA IS 'Monto de Renta asignado para esta acreditacion.'
/
COMMENT ON COLUMN  ERD_ACRED_CASACOMERCIAL.FEC_ULTMOD IS 'Fecha de la ultima modificacion.'
/
COMMENT ON COLUMN  ERD_ACRED_CASACOMERCIAL.NOM_USUARIO IS 'Nombre usuario que crea o modifica el registro.'
/
