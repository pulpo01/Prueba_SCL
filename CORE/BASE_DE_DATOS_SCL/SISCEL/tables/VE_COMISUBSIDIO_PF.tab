CREATE TABLE VE_COMISUBSIDIO_PF(
    COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  COD_CATTIPOCONTR VARCHAR2(3) NOT NULL 
 ,  IND_FAMILIAR NUMBER(1,0) NOT NULL 
 ,  COD_CATPLANTARIF VARCHAR2(3) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  IMP_SUBSIDIO NUMBER(12,2) DEFAULT 0 NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  FEC_ULTMOD DATE NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  VE_COMISUBSIDIO_PF.COD_ARTICULO IS 'csdigo de artmculo'
/
COMMENT ON COLUMN  VE_COMISUBSIDIO_PF.COD_CATTIPOCONTR IS 'csd. categorma de tipo de contrato'
/
COMMENT ON COLUMN  VE_COMISUBSIDIO_PF.IND_FAMILIAR IS 'indicador plan familiar'
/
COMMENT ON COLUMN  VE_COMISUBSIDIO_PF.COD_CATPLANTARIF IS 'csd. categorma de plan tarifario'
/
COMMENT ON COLUMN  VE_COMISUBSIDIO_PF.FEC_DESDE IS 'fecha inicio vigencia valor subsidio'
/
COMMENT ON COLUMN  VE_COMISUBSIDIO_PF.FEC_HASTA IS 'fecha fin vigencia valor subsidio'
/
COMMENT ON COLUMN  VE_COMISUBSIDIO_PF.IMP_SUBSIDIO IS 'Valor de subsidio'
/
COMMENT ON COLUMN  VE_COMISUBSIDIO_PF.NOM_USUARIO IS 'Nombre del usuario'
/
COMMENT ON COLUMN  VE_COMISUBSIDIO_PF.FEC_ULTMOD IS 'fecha zltima modificacisn de la fila'
/
