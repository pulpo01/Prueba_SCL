CREATE TABLE FA_ACUMFORADI(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  IND_ALQUILER NUMBER(1,0) NOT NULL 
 ,  COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  COD_TIPCONCE NUMBER(2,0) NOT NULL 
 ,  IMP_CONSUMIDO NUMBER(12,4) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_PROCESO NUMBER(8,0) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_ACUMFORADI IS 'Trafico Acumulado de Foradi'
/
COMMENT ON COLUMN  FA_ACUMFORADI.NUM_ABONADO IS 'Numero de Abonado'
/
COMMENT ON COLUMN  FA_ACUMFORADI.IND_ALQUILER IS 'Indicativo de Alquiler'
/
COMMENT ON COLUMN  FA_ACUMFORADI.COD_PERIODO IS 'Codigo Periodo de Alquiler'
/
COMMENT ON COLUMN  FA_ACUMFORADI.COD_OPERADOR IS 'Codigo de Operador'
/
COMMENT ON COLUMN  FA_ACUMFORADI.COD_TIPCONCE IS '1: Tipo Impuesto 4: Tipo Carrier'
/
COMMENT ON COLUMN  FA_ACUMFORADI.IMP_CONSUMIDO IS 'Importe Consumido'
/
COMMENT ON COLUMN  FA_ACUMFORADI.COD_CLIENTE IS 'Codigo de Cliente'
/
COMMENT ON COLUMN  FA_ACUMFORADI.NUM_PROCESO IS 'Numero de Proceso'
/
