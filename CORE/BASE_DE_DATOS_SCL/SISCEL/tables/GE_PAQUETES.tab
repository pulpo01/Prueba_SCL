CREATE TABLE GE_PAQUETES(
    COD_PAQUETE NUMBER(6,0) NOT NULL 
 ,  DES_PAQUETE VARCHAR2(40) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  IND_TIPOPACK VARCHAR2(1) NOT NULL 
 ,  IND_EQUIPO NUMBER(1,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_PAQUETES IS 'Cabecera de Paquetes Promocionales'
/
COMMENT ON COLUMN  GE_PAQUETES.COD_PAQUETE IS 'Codigo de Paquete'
/
COMMENT ON COLUMN  GE_PAQUETES.DES_PAQUETE IS 'Descripcion de Paquete'
/
COMMENT ON COLUMN  GE_PAQUETES.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GE_PAQUETES.IND_TIPOPACK IS 'Indicativo de Tipo de Paquete (A-Articulos, S-Servicios, M-Mixto)'
/
COMMENT ON COLUMN  GE_PAQUETES.IND_EQUIPO IS 'Existe al menos un telefono en el Paquete'
/
COMMENT ON COLUMN  GE_PAQUETES.NOM_USUARIO IS 'Usuario que crea o modifica el paquete'
/
COMMENT ON COLUMN  GE_PAQUETES.COD_CONCEPTO IS 'Codigo de concepto facturable'
/
