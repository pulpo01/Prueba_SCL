CREATE TABLE EV_DOCUMENTOS_SCORING_TO (  
   NUM_SOLSCORING                NUMBER(10)          NOT NULL
 , COD_TIPDOCUMSCORING           VARCHAR2(5)         NOT NULL
 , REQUERIDO                     CHAR(1)             NOT NULL
 , DES_TIPDOCUMSCORING           VARCHAR2(2000)          NULL
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  EV_DOCUMENTOS_SCORING_TO                      IS '[ EDSC ] Documentos asociado a solicutid  scoringDocumentos asociado a solicutid  scoring'
/

COMMENT ON COLUMN EV_DOCUMENTOS_SCORING_TO.NUM_SOLSCORING                 IS 'Numero de Solicitud Scoring Asociado'
/
COMMENT ON COLUMN EV_DOCUMENTOS_SCORING_TO.COD_TIPDOCUMSCORING            IS 'Tipo documento scoring adjuntado'
/
COMMENT ON COLUMN EV_DOCUMENTOS_SCORING_TO.REQUERIDO                      IS 'Indicador obligatoriedad de documento'
/
COMMENT ON COLUMN EV_DOCUMENTOS_SCORING_TO.DES_TIPDOCUMSCORING            IS 'Descripcción tipo documento scoring'
/

