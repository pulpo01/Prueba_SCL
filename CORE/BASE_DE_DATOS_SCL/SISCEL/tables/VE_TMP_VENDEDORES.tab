 ,  DIG_VERIF CHAR(1) NULL 
 ,  NOM_VENDEDOR VARCHAR2(200) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_TMP_VENDEDORES IS 'Tabla Temporal De Vendedores.'
/
COMMENT ON COLUMN  VE_TMP_VENDEDORES.NUM_IDENT IS 'Numero de RUT.'
/
COMMENT ON COLUMN  VE_TMP_VENDEDORES.DIG_VERIF IS 'Digito verificador de RUT.'
/
COMMENT ON COLUMN  VE_TMP_VENDEDORES.NOM_VENDEDOR IS 'Nombre del vendedor.'
/
