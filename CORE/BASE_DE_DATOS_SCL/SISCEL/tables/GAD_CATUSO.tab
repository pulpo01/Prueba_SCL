CREATE TABLE GAD_CATUSO(
    COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_CATTARIF VARCHAR2(2) NOT NULL 
 ,  COD_CATCOMERCIAL NUMBER(2,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAD_CATUSO IS 'Relaciona Categorias Y Usos.'
/
COMMENT ON COLUMN  GAD_CATUSO.COD_USO IS 'Codigo identificativo del uso o destino del articulo.'
/
COMMENT ON COLUMN  GAD_CATUSO.COD_CATTARIF IS 'Codoigo indentificativo de categoria tarifaria, pre-pago, post-pago.'
/
COMMENT ON COLUMN  GAD_CATUSO.COD_CATCOMERCIAL IS 'Codigo de categoria comercial .'
/
COMMENT ON COLUMN  GAD_CATUSO.FEC_DESDE IS 'Fecha Inicio Vigendia.'
/
COMMENT ON COLUMN  GAD_CATUSO.FEC_HASTA IS 'Fecha Final de Vigencia.'
/
