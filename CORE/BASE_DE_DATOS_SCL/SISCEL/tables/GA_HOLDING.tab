CREATE TABLE GA_HOLDING(
    COD_HOLDING NUMBER(8,0) NOT NULL 
 ,  DES_HOLDING VARCHAR2(40) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_PLANTARIF VARCHAR2(3) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  COD_CICLO NUMBER(2,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NULL 
 ,  TIP_TERMINAL VARCHAR2(1) NULL 
 ,  NUM_ABONADOS NUMBER(4,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_HOLDING IS 'Tabla de Holdings de Abonados'
/
COMMENT ON COLUMN  GA_HOLDING.COD_HOLDING IS 'Codigo de Holding'
/
COMMENT ON COLUMN  GA_HOLDING.DES_HOLDING IS 'Descripcion de Holding'
/
COMMENT ON COLUMN  GA_HOLDING.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_HOLDING.COD_PLANTARIF IS 'Codigo de Plan Tarifario'
/
COMMENT ON COLUMN  GA_HOLDING.FEC_ALTA IS 'Fecha de Alta del Holding'
/
COMMENT ON COLUMN  GA_HOLDING.NOM_USUARORA IS 'Usuario Creador del Holding'
/
COMMENT ON COLUMN  GA_HOLDING.COD_CICLO IS 'Codigo de Ciclo de Facturacion'
/
COMMENT ON COLUMN  GA_HOLDING.COD_CLIENTE IS 'Codigo de Cliente'
/
COMMENT ON COLUMN  GA_HOLDING.TIP_TERMINAL IS 'Tipo de Terminal'
/
COMMENT ON COLUMN  GA_HOLDING.NUM_ABONADOS IS 'Numero de Abonados Incluidos en el Holding'
/
