CREATE TABLE GA_CLAVE_TO(
    TIP_SUJETO VARCHAR2(2) NOT NULL 
 ,  SUJETO NUMBER(8,0) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NULL 
 ,  CLAVE VARCHAR2(128) NULL 
 ,  TIP_CLAVE NUMBER(8,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_CLAVE_TO IS 'Claves de Servicios de Abonados.'
/
COMMENT ON COLUMN  GA_CLAVE_TO.TIP_SUJETO IS 'Tipo de Sujeto (A = Abonado, C = Cliente)'
/
COMMENT ON COLUMN  GA_CLAVE_TO.SUJETO IS 'Numero de Abonado o Cliente'
/
COMMENT ON COLUMN  GA_CLAVE_TO.NOM_USUARORA IS 'Usuario Oracle que ingresa o Modifica este registro'
/
COMMENT ON COLUMN  GA_CLAVE_TO.CLAVE IS 'Clave Encriptada'
/
COMMENT ON COLUMN  GA_CLAVE_TO.TIP_CLAVE IS 'Tipo de Clave'
/
