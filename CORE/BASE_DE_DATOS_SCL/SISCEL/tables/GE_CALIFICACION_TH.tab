CREATE TABLE GE_CALIFICACION_TH (  
   COD_CALIFICACION              VARCHAR2(5)                                     NOT NULL  
 , DES_CALIFICACION              VARCHAR2(30)                                     NOT NULL  
 , IND_VIGENCIA                  VARCHAR2(1)                                     NOT NULL  
 , FEC_CREACION                  DATE                                          DEFAULT  SYSDATE NOT NULL  
 , NOM_USUARIO                   VARCHAR2(30)                                     NOT NULL
 ) TABLESPACE INSTALL_DATA
/


COMMENT ON TABLE  GE_CALIFICACION_TH                      IS '[ CATH ] TABLA HISTOTICA CALIFICACION DE LOS CLIENTES'
/

COMMENT ON COLUMN GE_CALIFICACION_TH.COD_CALIFICACION               IS 'Código de la Calificacion del Cliente'
/
COMMENT ON COLUMN GE_CALIFICACION_TH.DES_CALIFICACION               IS 'Descripcion de la Calificacion del Cliente'
/
COMMENT ON COLUMN GE_CALIFICACION_TH.IND_VIGENCIA                   IS 'Indicador de vigencia'
/
COMMENT ON COLUMN GE_CALIFICACION_TH.FEC_CREACION                   IS 'Fecha de Creación del registro'
/
COMMENT ON COLUMN GE_CALIFICACION_TH.NOM_USUARIO                    IS 'Usuario que genero el registro'
/


