CREATE TABLE CO_INTERESES_PRO(
    NUM_SECUENCIA NUMBER(5,0) NOT NULL 
 ,  FACTOR_INT NUMBER(8,5) NOT NULL 
 ,  FACTOR_DIA NUMBER(3,0) NOT NULL 
 ,  IND_FEC_COBRO CHAR(1) NOT NULL 
 ,  DIAS_APLICACION NUMBER(3,0) NOT NULL 
 ,  FEC_VIGENCIA_DD_DH DATE NOT NULL 
 ,  FEC_VIGENCIA_HH_DH DATE NOT NULL 
 ,  FEC_CREACION_DH DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_INTERESES_PRO IS 'Intereses Por Prorroga.'
/
COMMENT ON COLUMN  CO_INTERESES_PRO.NUM_SECUENCIA IS 'Numero de Secuencia.'
/
COMMENT ON COLUMN  CO_INTERESES_PRO.FACTOR_INT IS 'Factor de Interes.'
/
COMMENT ON COLUMN  CO_INTERESES_PRO.FACTOR_DIA IS 'Factor Dia.'
/
COMMENT ON COLUMN  CO_INTERESES_PRO.IND_FEC_COBRO IS 'Indicador Fecha de Cobro.'
/
COMMENT ON COLUMN  CO_INTERESES_PRO.DIAS_APLICACION IS 'Dias de Aplicacion.'
/
COMMENT ON COLUMN  CO_INTERESES_PRO.FEC_VIGENCIA_DD_DH IS 'Fecha Vigencia Desde.'
/
COMMENT ON COLUMN  CO_INTERESES_PRO.FEC_VIGENCIA_HH_DH IS 'Fecha Vigencia Hasta.'
/
COMMENT ON COLUMN  CO_INTERESES_PRO.FEC_CREACION_DH IS 'Fecha Creacion.'
/
COMMENT ON COLUMN  CO_INTERESES_PRO.NOM_USUARIO IS 'Usuario Responsable.'
/
