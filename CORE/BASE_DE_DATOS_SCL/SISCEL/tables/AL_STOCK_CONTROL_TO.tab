CREATE TABLE AL_STOCK_CONTROL_TO(
    COD_BODEGA NUMBER(6,0) NOT NULL 
 ,  TIP_STOCK NUMBER(2,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_ESTADO NUMBER(2,0) NOT NULL 
 ,  NUM_DESDE NUMBER(9,0) NOT NULL 
 ,  COD_PLAZA VARCHAR2(5) NOT NULL 
 ,  IND_ENTSAL VARCHAR2(1) NOT NULL 
 ,  COD_PROCESO VARCHAR2(1) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  CAN_STOCK NUMBER(9,0) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_STOCK_CONTROL_TO IS '[ AL_STCK_CTRL ] Tabla de control de bloqueos en al_stock'
/
COMMENT ON COLUMN  AL_STOCK_CONTROL_TO.COD_PROCESO IS 'Codigo de estado de proceso'
/
COMMENT ON COLUMN  AL_STOCK_CONTROL_TO.FEC_INGRESO IS 'Fecha de Proceso'
/
COMMENT ON COLUMN  AL_STOCK_CONTROL_TO.CAN_STOCK IS 'Cantidad Stock'
/
COMMENT ON COLUMN  AL_STOCK_CONTROL_TO.COD_BODEGA IS 'Codigo de Bodega'
/
COMMENT ON COLUMN  AL_STOCK_CONTROL_TO.TIP_STOCK IS 'Tipo de Stock'
/
COMMENT ON COLUMN  AL_STOCK_CONTROL_TO.COD_ARTICULO IS 'Codigo de Articulo'
/
COMMENT ON COLUMN  AL_STOCK_CONTROL_TO.COD_USO IS 'Codigo de Uso'
/
COMMENT ON COLUMN  AL_STOCK_CONTROL_TO.COD_ESTADO IS 'Codigo de estado'
/
COMMENT ON COLUMN  AL_STOCK_CONTROL_TO.NUM_DESDE IS 'Numero de inicio Rango'
/
COMMENT ON COLUMN  AL_STOCK_CONTROL_TO.COD_PLAZA IS 'Codigo de Plaza'
/
COMMENT ON COLUMN  AL_STOCK_CONTROL_TO.IND_ENTSAL IS 'Indicador de Entrada/Salida'
/
