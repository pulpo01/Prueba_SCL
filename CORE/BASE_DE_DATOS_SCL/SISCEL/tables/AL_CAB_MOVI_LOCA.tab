CREATE TABLE AL_CAB_MOVI_LOCA(
    NUM_MOVI NUMBER(9,0) NOT NULL 
 ,  COD_BODEGA NUMBER(6,0) NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  GLS_OBS VARCHAR2(100) NULL 
 ,  FEC_SOLI DATE NULL 
 ,  COD_ESTADO VARCHAR2(2) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_CAB_MOVI_LOCA IS 'Cabecera De Solicitud De Relocalizacion.'
/
COMMENT ON COLUMN  AL_CAB_MOVI_LOCA.NUM_MOVI IS 'Numero de movimiento de Relocalizacion.'
/
COMMENT ON COLUMN  AL_CAB_MOVI_LOCA.COD_BODEGA IS 'Codigo de Bodega.'
/
COMMENT ON COLUMN  AL_CAB_MOVI_LOCA.NOM_USUARIO IS 'Login del Usuario de Solicitud de Relocalizacion.'
/
COMMENT ON COLUMN  AL_CAB_MOVI_LOCA.GLS_OBS IS 'Observacion de la Relocalizacion.'
/
COMMENT ON COLUMN  AL_CAB_MOVI_LOCA.FEC_SOLI IS 'Fecha de Solicitud de Relocalizacion.'
/
COMMENT ON COLUMN  AL_CAB_MOVI_LOCA.COD_ESTADO IS 'Estado del Movimiento de Relocalizacion.'
/
