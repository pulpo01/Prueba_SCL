CREATE TABLE AL_HACUM_PMP_AFIJO(
    NUM_ACTIVO NUMBER(10,0) NOT NULL 
 ,  FEC_CALCULO DATE NOT NULL 
 ,  FEC_EJERCICIO DATE NOT NULL 
 ,  IMP_CORRECCION NUMBER(14,4) NOT NULL 
 ,  IMP_DEPRECIACION NUMBER(14,4) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_HACUM_PMP_AFIJO IS 'Historico de acumulados de calculo del PMP del Activo Fijo'
/
COMMENT ON COLUMN  AL_HACUM_PMP_AFIJO.NUM_ACTIVO IS 'Numero de Activo'
/
COMMENT ON COLUMN  AL_HACUM_PMP_AFIJO.FEC_CALCULO IS 'Fecha de calculo'
/
COMMENT ON COLUMN  AL_HACUM_PMP_AFIJO.FEC_EJERCICIO IS 'Fecha de Ejercicio'
/
COMMENT ON COLUMN  AL_HACUM_PMP_AFIJO.IMP_CORRECCION IS 'Importe correccion'
/
COMMENT ON COLUMN  AL_HACUM_PMP_AFIJO.IMP_DEPRECIACION IS 'Importe Depreciacion'
/
