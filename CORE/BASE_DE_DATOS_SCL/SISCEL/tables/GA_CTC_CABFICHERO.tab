CREATE TABLE GA_CTC_CABFICHERO(
    COD_FICHERO NUMBER(8,0) NOT NULL 
 ,  FEC_LECTURA DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  NOM_FICHERO VARCHAR2(50) NOT NULL 
 ,  NUM_LINEAS NUMBER(5,0) NOT NULL 
 ,  IND_PROCESADO NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_CTC_CABFICHERO IS 'TABLA DE CABECERA DE FICHEROS PROCEDENTES DE CTC PARA SER INCORPORADOS AL SISTEMA'
/
COMMENT ON COLUMN  GA_CTC_CABFICHERO.COD_FICHERO IS 'CODIGO DE FICHERO'
/
COMMENT ON COLUMN  GA_CTC_CABFICHERO.FEC_LECTURA IS 'FECHA DE LECTURA'
/
COMMENT ON COLUMN  GA_CTC_CABFICHERO.NOM_USUARORA IS 'USUARIO QUE REALIZO EL PROCESO DE CARGA'
/
COMMENT ON COLUMN  GA_CTC_CABFICHERO.NOM_FICHERO IS 'NOMBRE DE FICHERO Y EXTENSION'
/
COMMENT ON COLUMN  GA_CTC_CABFICHERO.NUM_LINEAS IS 'NUMERO DE LINEAS(abonados) DENTRO DEL FICHERO'
/
