CREATE TABLE FA_CARGOSERVPUNTUAL_TO(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  COD_TIPIDENT VARCHAR2(2) NOT NULL 
 ,  IND_FACTURAELECTRONICA NUMBER(1,0) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  MTO_CARGO NUMBER(14,4) NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_CARGOSERVPUNTUAL_TO IS '[ CSP ] Configuracion cargos serrvicios puntuales'
/
COMMENT ON COLUMN  FA_CARGOSERVPUNTUAL_TO.COD_PRODUCTO IS 'Código del producto asociado al servicio'
/
COMMENT ON COLUMN  FA_CARGOSERVPUNTUAL_TO.COD_SERVICIO IS 'Código del servicio suplementario'
/
COMMENT ON COLUMN  FA_CARGOSERVPUNTUAL_TO.COD_TIPIDENT IS 'Código del tipode identificacion tributaria del cliente'
/
COMMENT ON COLUMN  FA_CARGOSERVPUNTUAL_TO.IND_FACTURAELECTRONICA IS 'Código de los tipos de despacho para la factura'
/
COMMENT ON COLUMN  FA_CARGOSERVPUNTUAL_TO.COD_CONCEPTO IS 'Código del concepto de cargos a generar'
/
COMMENT ON COLUMN  FA_CARGOSERVPUNTUAL_TO.COD_MONEDA IS 'Código de moneda del monto del cargo'
/
COMMENT ON COLUMN  FA_CARGOSERVPUNTUAL_TO.MTO_CARGO IS 'Monto del cargo a generar.'
/
