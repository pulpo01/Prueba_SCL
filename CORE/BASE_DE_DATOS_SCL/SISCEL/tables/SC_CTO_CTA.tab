CREATE TABLE SC_CTO_CTA(
    COD_DOMINIO_CTO NUMBER(2,0) NOT NULL 
 ,  COD_EVENTO_CTO NUMBER(2,0) NOT NULL 
 ,  COD_CONCEPTO VARCHAR2(15) NOT NULL 
 ,  COD_CUENTA VARCHAR2(60) NULL 
 ,  COD_CONTRAP VARCHAR2(60) NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  FEC_ACTUALIZ DATE NULL 
 ,  IND_GRP_DOMINIO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  COD_OPERADORA_SCL VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SC_CTO_CTA IS 'Relacion conceptos - cuentas contables.'
/
COMMENT ON COLUMN  SC_CTO_CTA.COD_CONCEPTO IS 'Codigo de concepto contable.'
/
COMMENT ON COLUMN  SC_CTO_CTA.COD_CUENTA IS 'Codigo de cuenta contable.'
/
COMMENT ON COLUMN  SC_CTO_CTA.COD_CONTRAP IS 'Codigo de cuenta contable de contrapartida.'
/
COMMENT ON COLUMN  SC_CTO_CTA.NOM_USUARIO IS 'Usuario que actualiza la fila.'
/
COMMENT ON COLUMN  SC_CTO_CTA.FEC_ACTUALIZ IS 'Fecha de actualizacion de la fila.'
/
COMMENT ON COLUMN  SC_CTO_CTA.COD_DOMINIO_CTO IS 'Codigo del dominio de conceptos.'
/
COMMENT ON COLUMN  SC_CTO_CTA.COD_EVENTO_CTO IS 'Codigo de evento para contabilizar conceptos.'
/
