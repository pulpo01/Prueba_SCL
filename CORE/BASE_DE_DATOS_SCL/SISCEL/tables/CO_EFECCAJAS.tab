CREATE TABLE CO_EFECCAJAS(
    COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  COD_CAJA NUMBER(4,0) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  EFEC_EGRESO NUMBER(14,4) NOT NULL 
 ,  EFEC_CAMBIO NUMBER(14,4) NOT NULL 
 ,  COD_OPERADORA_SCL VARCHAR2(5) NOT NULL 
 ,  COD_PLAZA VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_EFECCAJAS IS 'CAMBIO EFECTIVO DE LAS CAJAS'
/
COMMENT ON COLUMN  CO_EFECCAJAS.COD_OFICINA IS 'CODIGO DE OFICINA'
/
COMMENT ON COLUMN  CO_EFECCAJAS.COD_CAJA IS 'CODIGO DE CAJA'
/
COMMENT ON COLUMN  CO_EFECCAJAS.COD_MONEDA IS 'CODIGO DE MONEDA'
/
COMMENT ON COLUMN  CO_EFECCAJAS.EFEC_EGRESO IS 'IMPORTE EFECTIVO EGRESOS'
/
COMMENT ON COLUMN  CO_EFECCAJAS.EFEC_CAMBIO IS 'IMPORTE EFECTIVO PARA CAMBIOS O DEVOLUCIONES'
/
COMMENT ON COLUMN  CO_EFECCAJAS.COD_OPERADORA_SCL IS 'Codigo de operadora'
/
COMMENT ON COLUMN  CO_EFECCAJAS.COD_PLAZA IS 'Codigo de plaza'
/
