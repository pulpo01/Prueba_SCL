CREATE TABLE SC_PER_CONT(
    PER_CONTABLE NUMBER(6,0) NOT NULL 
 ,  IND_ESTTRANSIT CHAR(1) NOT NULL 
 ,  FEC_ESTTRANSIT DATE NOT NULL 
 ,  IND_ESTDEFINIT CHAR(1) NOT NULL 
 ,  FEC_ESTDEFINIT DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SC_PER_CONT IS 'PERIODOS CONTABLES'
/
COMMENT ON COLUMN  SC_PER_CONT.PER_CONTABLE IS 'PERIODO CONTABLE '
/
COMMENT ON COLUMN  SC_PER_CONT.IND_ESTTRANSIT IS 'INDICADOR ESTADO TRANSITORIO (A:ABIERTO/C:CERRADO)'
/
COMMENT ON COLUMN  SC_PER_CONT.FEC_ESTTRANSIT IS 'FECHA DE ESTADO TRANSITORIO'
/
COMMENT ON COLUMN  SC_PER_CONT.IND_ESTDEFINIT IS 'INDICADOR ESTADO DEFINITIVO (A:ABIERTO/C:CERRADO)'
/
COMMENT ON COLUMN  SC_PER_CONT.FEC_ESTDEFINIT IS 'FECHA DE ESTADO DEFINITIVO'
/
COMMENT ON COLUMN  SC_PER_CONT.NOM_USUARIO IS 'USUARIO QUE ABRE/CIERRA EL PERIODO'
/
