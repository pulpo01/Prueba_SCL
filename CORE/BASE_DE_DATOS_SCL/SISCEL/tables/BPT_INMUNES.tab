CREATE TABLE BPT_INMUNES(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_RUTINA VARCHAR2(5) NOT NULL 
 ,  COD_PARAMETRO VARCHAR2(5) NOT NULL 
 ,  NUM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  BPT_INMUNES IS 'Abonados Inmunes A Acciones De Promociones.'
/
COMMENT ON COLUMN  BPT_INMUNES.NUM_ABONADO IS 'Abonado.'
/
COMMENT ON COLUMN  BPT_INMUNES.COD_RUTINA IS 'Rutina.'
/
COMMENT ON COLUMN  BPT_INMUNES.COD_PARAMETRO IS 'Parametro.'
/
COMMENT ON COLUMN  BPT_INMUNES.NUM_USUARIO IS 'Usuario.'
/
COMMENT ON COLUMN  BPT_INMUNES.FEC_INGRESO IS 'Fecha de ingreso.'
/
COMMENT ON COLUMN  BPT_INMUNES.COD_CLIENTE IS 'Cliente.'
/
