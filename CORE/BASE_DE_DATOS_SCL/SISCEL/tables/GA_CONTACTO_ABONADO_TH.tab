CREATE TABLE GA_CONTACTO_ABONADO_TH
(
  NUM_ABONADO         NUMBER(8)                 NOT NULL,
  COD_SERVICIO        VARCHAR2(3 BYTE)          NOT NULL,
  NUM_CONTACTO        NUMBER(2)                 NOT NULL,
  NOMBRE_CONTACTO     VARCHAR2(50 BYTE),
  APELLIDO1_CONTACTO  VARCHAR2(20 BYTE),
  APELLIDO2_CONTACTO  VARCHAR2(20 BYTE),
  COD_PARENTESCO      VARCHAR2(5 BYTE),
  COD_DIRECCION       NUMBER(20),
  PLACA_VEHICULAR     VARCHAR2(10 BYTE),
  COLOR_VEHICULO      VARCHAR2(30 BYTE),
  ANIO_VEHICULO       CHAR(4 BYTE),
  OBSERVACION         VARCHAR2(50 BYTE),
  IND_VIGENTE         VARCHAR2(1 BYTE),
  FEC_MODIFICACION    DATE                      NOT NULL,
  NOM_USUAORA         VARCHAR2(30 BYTE)         NOT NULL,
  FEC_PASOHIST        DATE                      NOT NULL,
  USU_PASOHIST        VARCHAR2(30 BYTE)         NOT NULL,
  NUM_TELEFONO        NUMBER(15)
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    20
INITRANS   16
MAXTRANS   255
STORAGE    (
            INITIAL          685720K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE GA_CONTACTO_ABONADO_TH IS '[ GA_CONTH ] Tabla Historica de Contactos de Abonados';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.NUM_ABONADO IS 'Número de Abonado Asociado al Contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.COD_SERVICIO IS 'Codigo de Servicio asociado al Servicio de Contactos';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.NUM_CONTACTO IS 'Secuencia de Número de Contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.NOMBRE_CONTACTO IS 'Nombre del Contacto Asociado';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.APELLIDO1_CONTACTO IS 'Primer Apellido del Contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.APELLIDO2_CONTACTO IS 'Segundo Apellido del Contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.COD_PARENTESCO IS 'Codigo de Parentesco';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.COD_DIRECCION IS 'Direccion Contacto  (GE_DIRECCIONES)';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.PLACA_VEHICULAR IS 'Placa Vehicular asociada al Contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.COLOR_VEHICULO IS 'Color del Vehiculo asociada al contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.ANIO_VEHICULO IS 'Año del Vehiculo asociada al contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.OBSERVACION IS 'Observacion ';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.IND_VIGENTE IS 'Indicador de Vigencia 1:Vigente 0: No Vigente';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.FEC_MODIFICACION IS 'Fecha de Ingreso o Modificacion del Contacto';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.NOM_USUAORA IS 'Nombre de Usuario';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.FEC_PASOHIST IS 'Fecha de Paso a Historico';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.USU_PASOHIST IS 'Usuario que realizó el Paso a Historico';

COMMENT ON COLUMN GA_CONTACTO_ABONADO_TH.NUM_TELEFONO IS 'numero de telefono';

