CREATE TABLE AL_PROVEEDORES(
    COD_PROVEEDOR NUMBER NOT NULL 
 ,  NOM_PROVEEDOR VARCHAR2(40) NOT NULL 
 ,  DIR_PROVEEDOR VARCHAR2(60) NULL 
 ,  DES_COMUNA VARCHAR2(25) NULL 
 ,  DES_REGION VARCHAR2(25) NULL 
 ,  NUM_TELEFONO VARCHAR2(15) NULL 
 ,  COD_DIRECCION NUMBER(20,0) NULL 
 ,  COD_TIPIDENT NUMBER(2,0) NULL 
 ,  NUM_IDENT VARCHAR2(20) DEFAULT '2' NOT NULL 
 ,  IND_TIPROVEEDOR NUMBER(1,0) DEFAULT '0' NULL 
 ,  RUT VARCHAR2(12) DEFAULT '2' NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_PROVEEDORES IS 'Proveedores de articulos. Los datos seran suministrados por ORACLEFINANCIALS.'
/
COMMENT ON COLUMN  AL_PROVEEDORES.COD_PROVEEDOR IS 'Csdigo del proveedor'
/
COMMENT ON COLUMN  AL_PROVEEDORES.NOM_PROVEEDOR IS 'Nombre del proveedor'
/
COMMENT ON COLUMN  AL_PROVEEDORES.DIR_PROVEEDOR IS 'Direccion del proveedor'
/
COMMENT ON COLUMN  AL_PROVEEDORES.DES_COMUNA IS 'Comuna del Proveedor'
/
COMMENT ON COLUMN  AL_PROVEEDORES.DES_REGION IS 'Region'
/
COMMENT ON COLUMN  AL_PROVEEDORES.NUM_TELEFONO IS 'Telefono'
/
COMMENT ON COLUMN  AL_PROVEEDORES.COD_DIRECCION IS 'Codigo de Direccion del Cliente'
/
COMMENT ON COLUMN  AL_PROVEEDORES.COD_TIPIDENT IS 'Tipo de Identificador de Cliente'
/
COMMENT ON COLUMN  AL_PROVEEDORES.IND_TIPROVEEDOR IS 'Indicador Tipo Proveedor Interno/Externo'
/
COMMENT ON COLUMN  AL_PROVEEDORES.RUT IS 'Rut del proveedor'
/
