CREATE TABLE GAT_PLANDESCABO(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_SECUENCIA NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_CICLFACT NUMBER(6,0) NOT NULL 
 ,  COD_PLANDESC VARCHAR2(5) NOT NULL 
 ,  TIP_ENTIDAD VARCHAR2(5) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAT_PLANDESCABO IS 'Planes De Descuento Por Abonado.'
/
COMMENT ON COLUMN  GAT_PLANDESCABO.COD_CLIENTE IS 'Codigo Cliente.'
/
COMMENT ON COLUMN  GAT_PLANDESCABO.NUM_SECUENCIA IS 'Numero de secuencia beneficio.'
/
COMMENT ON COLUMN  GAT_PLANDESCABO.NUM_ABONADO IS 'numero de abonado.'
/
COMMENT ON COLUMN  GAT_PLANDESCABO.COD_CICLFACT IS 'codigo de ciclo de facturacion.'
/
COMMENT ON COLUMN  GAT_PLANDESCABO.COD_PLANDESC IS 'codigo de plan de descuento.'
/
COMMENT ON COLUMN  GAT_PLANDESCABO.TIP_ENTIDAD IS 'Tipo Entidad asociado al plan de beneficio'
/
