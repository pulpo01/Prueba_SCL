CREATE TABLE FA_IMP_SUBBLOQUE(
    COD_SUBBLOQUE NUMBER(3,0) NOT NULL 
 ,  COD_REGISTRO NUMBER(3,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(3,0) NOT NULL 
 ,  IND_FIJO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  POS_X NUMBER(3,0) NOT NULL 
 ,  POS_PARALELO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_IMP_SUBBLOQUE IS 'Tabla de Subloques dentro de un Bloque- Retrofitted'
/
COMMENT ON COLUMN  FA_IMP_SUBBLOQUE.COD_SUBBLOQUE IS 'Codigo de SubBloque - Retrofitted'
/
COMMENT ON COLUMN  FA_IMP_SUBBLOQUE.COD_REGISTRO IS 'Codigo de Registro - Retrofitted'
/
COMMENT ON COLUMN  FA_IMP_SUBBLOQUE.NUM_ORDEN IS 'Numero de Orden - Retrofitted'
/
COMMENT ON COLUMN  FA_IMP_SUBBLOQUE.IND_FIJO IS 'Indicativo de Fijo 0 => No 1=> Si - Retrofitted'
/
COMMENT ON COLUMN  FA_IMP_SUBBLOQUE.POS_X IS 'Posicion en el eje de las X"s o Coordenadas - Retrofitted'
/
COMMENT ON COLUMN  FA_IMP_SUBBLOQUE.POS_PARALELO IS '1 => Posicionamiento Paralelo 0 => No - Retrofitted'
/
