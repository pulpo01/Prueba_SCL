CREATE TABLE GSM_DET_SOLICITUD_AUC_TH(
    NUM_SEQ_SOLICITUD_AUC NUMBER(10,0) NOT NULL 
 ,  NUM_SIMCARD VARCHAR2(20) NOT NULL 
 ,  NUM_MOVIMIENTO NUMBER(9,0) NOT NULL 
 ,  NUM_INTENTO NUMBER(3,0) NOT NULL 
 ,  COD_ESTADO NUMBER(3,0) NULL 
 ,  DES_ERROR VARCHAR2(2000) NULL 
 ,  FEC_ACTUALIZACION DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_DET_SOLICITUD_AUC_TH IS 'TABLA HISTORICA TEMPORAL DE DETALLE DE SIMCARD A AUTENTIFICAR'
/
COMMENT ON COLUMN  GSM_DET_SOLICITUD_AUC_TH.NUM_SEQ_SOLICITUD_AUC IS 'NUMERO SOLICITUD DE AUC'
/
COMMENT ON COLUMN  GSM_DET_SOLICITUD_AUC_TH.NUM_SIMCARD IS 'NUMERO DE SIMCARD'
/
COMMENT ON COLUMN  GSM_DET_SOLICITUD_AUC_TH.NUM_MOVIMIENTO IS 'NUMERO DE MOVIMIENTO DE TABLA ICC_MOVIMIENTO'
/
COMMENT ON COLUMN  GSM_DET_SOLICITUD_AUC_TH.NUM_INTENTO IS 'NUMERO DE INTENTOS QUE SE HAN HECHO PARA AUTENTIFICAR LA ICC'
/
COMMENT ON COLUMN  GSM_DET_SOLICITUD_AUC_TH.COD_ESTADO IS 'CSDIGO DE ESTADO'
/
COMMENT ON COLUMN  GSM_DET_SOLICITUD_AUC_TH.DES_ERROR IS 'DESCRIPCISN DEL ERROR'
/
COMMENT ON COLUMN  GSM_DET_SOLICITUD_AUC_TH.FEC_ACTUALIZACION IS 'FECHA DE ACTUALIZACISN'
/
COMMENT ON COLUMN  GSM_DET_SOLICITUD_AUC_TH.COD_USUARIO IS 'CSDIGO DE USUARIO'
/
