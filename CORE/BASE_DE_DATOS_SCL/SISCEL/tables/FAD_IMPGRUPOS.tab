CREATE TABLE FAD_IMPGRUPOS(
    COD_GRUPO NUMBER(3,0) NOT NULL 
 ,  DES_GRUPO VARCHAR2(40) NOT NULL 
 ,  DES_GLOSA VARCHAR2(50) NULL 
 ,  COD_FORMULARIO NUMBER(3,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(5,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NIV_IMPRESION NUMBER(1,0) NULL 
 ,  TIP_UNIDAD VARCHAR2(5) NULL 
 ,  TIP_GRUPO VARCHAR2(5) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAD_IMPGRUPOS IS 'Grupos De Impresion.'
/
COMMENT ON COLUMN  FAD_IMPGRUPOS.COD_GRUPO IS 'Codigo de grupo de impresion..'
/
COMMENT ON COLUMN  FAD_IMPGRUPOS.DES_GRUPO IS 'Descripcion del codigo de grupo de impresion..'
/
COMMENT ON COLUMN  FAD_IMPGRUPOS.DES_GLOSA IS 'Glosa que aparecera fisicamente en el formulario..'
/
COMMENT ON COLUMN  FAD_IMPGRUPOS.COD_FORMULARIO IS 'Formulario donde se usara el grupo de impresion..'
/
COMMENT ON COLUMN  FAD_IMPGRUPOS.NUM_ORDEN IS 'Orden de aparicion del grupo dentro del formulario..'
/
COMMENT ON COLUMN  FAD_IMPGRUPOS.NOM_USUARIO IS 'ultimo usuario que realizo algun cambio en el registro..'
/
COMMENT ON COLUMN  FAD_IMPGRUPOS.FEC_ULTMOD IS 'Fecha de ultima modificacion..'
/
COMMENT ON COLUMN  FAD_IMPGRUPOS.NIV_IMPRESION IS 'NIVEL DE IMPRESION'
/
COMMENT ON COLUMN  FAD_IMPGRUPOS.TIP_UNIDAD IS 'TIPO DE UNIDAD'
/
COMMENT ON COLUMN  FAD_IMPGRUPOS.TIP_GRUPO IS 'TIPO DE GRUPO'
/
