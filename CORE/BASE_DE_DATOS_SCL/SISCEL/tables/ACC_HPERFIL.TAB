CREATE TABLE ACC_HPERFIL(
    COD_PERFIL VARCHAR2(5) NOT NULL 
 ,  GLS_PERFIL VARCHAR2(30) NULL 
 ,  USER_LOG_ORIG VARCHAR2(10) NOT NULL 
 ,  FEC_LOG_ORIG DATE NOT NULL 
 ,  USER_LOG VARCHAR2(10) NOT NULL 
 ,  FEC_LOG DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  ACC_HPERFIL.COD_PERFIL IS 'Codigo del perfil'
/
COMMENT ON COLUMN  ACC_HPERFIL.GLS_PERFIL IS 'Descripcion del perfil'
/
COMMENT ON COLUMN  ACC_HPERFIL.USER_LOG_ORIG IS 'Codigo del usuario que realizo la transaccion original'
/
COMMENT ON COLUMN  ACC_HPERFIL.FEC_LOG_ORIG IS 'Fecha en que se realizo la transaccion original'
/
COMMENT ON COLUMN  ACC_HPERFIL.USER_LOG IS 'Codigo del usuario logeado en el Sistema'
/
COMMENT ON COLUMN  ACC_HPERFIL.FEC_LOG IS 'Fecha en que el Usuario realizo la transaccion'
/
