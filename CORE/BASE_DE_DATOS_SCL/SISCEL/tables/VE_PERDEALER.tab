CREATE TABLE VE_PERDEALER(
    COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_EMPLEADO NUMBER(4,0) NOT NULL 
 ,  NOM_EMPLEADO VARCHAR2(40) NOT NULL 
 ,  DES_OCUPACION VARCHAR2(30) NOT NULL 
 ,  COD_TIPIDENT VARCHAR2(2) NOT NULL 
 ,  NUM_IDENT VARCHAR2(20) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_PERDEALER IS 'Personal de la Red de Distribucion'
/
COMMENT ON COLUMN  VE_PERDEALER.COD_VENDEDOR IS 'Codigo de Comisionista'
/
COMMENT ON COLUMN  VE_PERDEALER.COD_EMPLEADO IS 'Codigo de empleado'
/
COMMENT ON COLUMN  VE_PERDEALER.NOM_EMPLEADO IS 'Nombre del empleado'
/
COMMENT ON COLUMN  VE_PERDEALER.DES_OCUPACION IS 'Descripcion de la ocupacion'
/
COMMENT ON COLUMN  VE_PERDEALER.COD_TIPIDENT IS 'Codigo de Tipo de Identificacion'
/
COMMENT ON COLUMN  VE_PERDEALER.NUM_IDENT IS 'Numero de Identificacion'
/
