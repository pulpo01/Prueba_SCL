CREATE TABLE GA_CAUSA_LNCLI_TO (  
   COD_CAUSA                     VARCHAR2(3)      NOT NULL
 , DES_CAUSA                     VARCHAR2(50)     NOT NULL
 , FEC_DESDE                     DATE             NOT NULL
 , FEC_HASTA                     DATE             NOT NULL
 , NOM_USUARIO                   VARCHAR2(30)     NOT NULL
 , FEC_MODIFICA                  DATE             NOT NULL
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  GA_CAUSA_LNCLI_TO                      IS '[ GA_CALNCLI ] Causas Listas Negras ClientesCausas Listas Negras Clientes'
/

COMMENT ON COLUMN GA_CAUSA_LNCLI_TO.COD_CAUSA                      IS 'Causa Lista Negra Cliente'
/
COMMENT ON COLUMN GA_CAUSA_LNCLI_TO.DES_CAUSA                      IS 'Descripcion Causa'
/
COMMENT ON COLUMN GA_CAUSA_LNCLI_TO.FEC_DESDE                      IS 'Fecha Inicio'
/
COMMENT ON COLUMN GA_CAUSA_LNCLI_TO.FEC_HASTA                      IS 'Fecha Termino'
/
COMMENT ON COLUMN GA_CAUSA_LNCLI_TO.NOM_USUARIO                    IS 'Usuario modificación registro'
/
COMMENT ON COLUMN GA_CAUSA_LNCLI_TO.FEC_MODIFICA                   IS 'Fecha modificación registro'
/

