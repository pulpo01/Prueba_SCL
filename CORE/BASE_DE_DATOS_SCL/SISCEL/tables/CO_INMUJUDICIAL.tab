CREATE TABLE CO_INMUJUDICIAL(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_TIPINMUJUDICIAL NUMBER(3,0) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_VALOR DATE NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_INMUJUDICIAL IS 'INMUNIDAD EN COBROS AL PROCESO DE ASIGNACION JUDICIAL'
/
COMMENT ON COLUMN  CO_INMUJUDICIAL.NUM_ABONADO IS 'Codigo de abonado - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUJUDICIAL.COD_PRODUCTO IS 'Codigo de Producto - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUJUDICIAL.COD_CLIENTE IS 'Codigo de cliente - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUJUDICIAL.COD_TIPINMUJUDICIAL IS 'Tipo de inmunidad - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUJUDICIAL.NOM_USUARORA IS 'Usuario que realizo la inmunidad - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUJUDICIAL.FEC_VALOR IS 'Fecha de ingreso de la inmunidad - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUJUDICIAL.FEC_DESDE IS 'Fecha de comienzo de inmunidad - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUJUDICIAL.FEC_HASTA IS 'Fecha fin de inmunidad - Retrofitted'
/
