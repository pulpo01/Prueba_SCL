CREATE TABLE BPD_PLANES_PERFILES(
    COD_PLAN VARCHAR2(5) NOT NULL 
 ,  COD_PERFIL VARCHAR2(5) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_DESDEAPLI DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  BPD_PLANES_PERFILES IS 'Perfiles Asociados A Cada Plan De Beneficios.'
/
COMMENT ON COLUMN  BPD_PLANES_PERFILES.COD_PLAN IS 'Plan de Bendeficio.'
/
COMMENT ON COLUMN  BPD_PLANES_PERFILES.COD_PERFIL IS 'Perfil de Usuarios que lo pueden asignar.'
/
COMMENT ON COLUMN  BPD_PLANES_PERFILES.NOM_USUARORA IS 'Usuario que lo Habilito.'
/
COMMENT ON COLUMN  BPD_PLANES_PERFILES.FEC_DESDEAPLI IS 'Desde que fecha se puede empezar a aplicar el Beneficio'
/
