CREATE TABLE FAT_PLANVENCICTA(
    COD_CUENTA NUMBER(8,0) NOT NULL 
 ,  COD_TIPMOVIMIENT NUMBER(2,0) NOT NULL 
 ,  COD_PLANVENCIMIE VARCHAR2(5) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAT_PLANVENCICTA IS 'Plan De Vencimiento Por Cuenta.'
/
COMMENT ON COLUMN  FAT_PLANVENCICTA.COD_CUENTA IS 'Codigo de Cuenta .'
/
COMMENT ON COLUMN  FAT_PLANVENCICTA.COD_TIPMOVIMIENT IS 'Codigo del tipo de movimiento.'
/
COMMENT ON COLUMN  FAT_PLANVENCICTA.COD_PLANVENCIMIE IS 'Codigo de Plan de Vencimiento.'
/
COMMENT ON COLUMN  FAT_PLANVENCICTA.FEC_DESDE IS 'Fecha de Inicio del periodo.'
/
COMMENT ON COLUMN  FAT_PLANVENCICTA.FEC_HASTA IS 'Fecha de termino del periodo.'
/
COMMENT ON COLUMN  FAT_PLANVENCICTA.FEC_ULTMOD IS 'Fecha de modificacion.'
/
COMMENT ON COLUMN  FAT_PLANVENCICTA.NOM_USUARIO IS 'Nombre de Usuario.'
/
