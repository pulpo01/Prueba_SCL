CREATE TABLE CMT_VALORIZADOS(
    NUM_GENERAL NUMBER(10,0) NOT NULL 
 ,  COD_UNIVERSO VARCHAR2(6) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_COMISIONISTA NUMBER(6,0) NOT NULL 
 ,  COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  ID_PERIODO VARCHAR2(10) NOT NULL 
 ,  IMP_CONCEPTO NUMBER(11,4) NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMT_VALORIZADOS IS 'Tabla De Valorizacion De Eventos.'
/
COMMENT ON COLUMN  CMT_VALORIZADOS.NUM_GENERAL IS 'Correlativo General de Universos de seleccion..'
/
COMMENT ON COLUMN  CMT_VALORIZADOS.COD_UNIVERSO IS 'Codigo de universo de seleccion..'
/
COMMENT ON COLUMN  CMT_VALORIZADOS.COD_CONCEPTO IS 'Codigo de concepto de comision..'
/
COMMENT ON COLUMN  CMT_VALORIZADOS.COD_TIPCOMIS IS 'Codigo del canal por el cual se hizo la venta (10, 26, 29, ...)..'
/
COMMENT ON COLUMN  CMT_VALORIZADOS.COD_COMISIONISTA IS 'Codigo del comisionista que hizo la venta (Master dealer o vendedor raiz)..'
/
COMMENT ON COLUMN  CMT_VALORIZADOS.COD_PERIODO IS 'Periodo de Comisiones (Formato Corto YYYYMM).'
/
COMMENT ON COLUMN  CMT_VALORIZADOS.ID_PERIODO IS 'Periodo de Comisiones (Formato Largo YYYYMM<D|N><M|QM|S><#SEQ>).'
/
COMMENT ON COLUMN  CMT_VALORIZADOS.IMP_CONCEPTO IS 'Importe del concepto de comision aplicado al registro..'
/
COMMENT ON COLUMN  CMT_VALORIZADOS.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
