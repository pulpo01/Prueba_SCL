CREATE TABLE GSM_RANGOS_PREFIJOS_TO(
    COD_PREFIJO VARCHAR2(10) NOT NULL 
 ,  NUM_SECUENCIA NUMBER(18,0) NOT NULL 
 ,  NUM_VALOR_MINIMO NUMBER NOT NULL 
 ,  NUM_VALOR_MAXIMO NUMBER(18,0) NOT NULL 
 ,  NUM_VALOR_INTERVALO NUMBER(18,0) NOT NULL 
 ,  FEC_ACTUALIZACION DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_RANGOS_PREFIJOS_TO IS 'Tabla de Rangos de prefijos ICC e IMSI'
/
COMMENT ON COLUMN  GSM_RANGOS_PREFIJOS_TO.COD_PREFIJO IS 'Codigo de prefijo.'
/
COMMENT ON COLUMN  GSM_RANGOS_PREFIJOS_TO.NUM_SECUENCIA IS 'Secuancial de rango'
/
COMMENT ON COLUMN  GSM_RANGOS_PREFIJOS_TO.NUM_VALOR_MINIMO IS 'Valor minimo de la secuencia.'
/
COMMENT ON COLUMN  GSM_RANGOS_PREFIJOS_TO.NUM_VALOR_MAXIMO IS 'Valor maximo de la secuencia.'
/
COMMENT ON COLUMN  GSM_RANGOS_PREFIJOS_TO.NUM_VALOR_INTERVALO IS 'Valor del intervalo NUM_VALOR_MAXIMO -NUM_VALOR_MINIMO.'
/
COMMENT ON COLUMN  GSM_RANGOS_PREFIJOS_TO.FEC_ACTUALIZACION IS 'Fecha de la ultima actualizacion.'
/
COMMENT ON COLUMN  GSM_RANGOS_PREFIJOS_TO.COD_USUARIO IS 'Codigo del usuario que actualizo.'
/
