CREATE TABLE GA_SUSCGRP(
    COD_GRUPO NUMBER(8,0) NOT NULL 
 ,  TS VARCHAR2(1) DEFAULT 'G' NOT NULL 
 ,  ID NUMBER(7,0) NOT NULL 
 ,  COB VARCHAR2(3) NOT NULL 
 ,  NOM VARCHAR2(50) NOT NULL 
 ,  STA VARCHAR2(1) DEFAULT 'S' NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_SUSCGRP IS 'Suscriptores Grupos Beeper de soporte Plataforma'
/
COMMENT ON COLUMN  GA_SUSCGRP.TS IS 'Tipo de suscripcion; (I)ndividual,(G)rupo'
/
COMMENT ON COLUMN  GA_SUSCGRP.ID IS 'Numero de Beeper'
/
COMMENT ON COLUMN  GA_SUSCGRP.COB IS 'Codigo de Cobertura'
/
COMMENT ON COLUMN  GA_SUSCGRP.NOM IS 'Nombre de Usuario'
/
COMMENT ON COLUMN  GA_SUSCGRP.STA IS 'Estatus Suscripcion; (S)Activo,(N)Desactivado'
/
COMMENT ON COLUMN  GA_SUSCGRP.COD_GRUPO IS 'Suscripcion Grupo'
/
