CREATE TABLE CMD_UNIVERSOS(
    COD_UNIVERSO VARCHAR2(6) NOT NULL 
 ,  DES_UNIVERSO VARCHAR2(50) NOT NULL 
 ,  FEC_CREACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMD_UNIVERSOS IS 'Definicion De Universos De Seleccion De Registros.'
/
COMMENT ON COLUMN  CMD_UNIVERSOS.COD_UNIVERSO IS 'Codigo del universo de seleccion..'
/
COMMENT ON COLUMN  CMD_UNIVERSOS.DES_UNIVERSO IS 'Descripcion del universo de seleccion..'
/
COMMENT ON COLUMN  CMD_UNIVERSOS.FEC_CREACION IS 'Fecha de insercion del registro en la tabla..'
/
COMMENT ON COLUMN  CMD_UNIVERSOS.NOM_USUARIO IS 'Usuario que hizo la creacion del registro en la tabla..'
/
