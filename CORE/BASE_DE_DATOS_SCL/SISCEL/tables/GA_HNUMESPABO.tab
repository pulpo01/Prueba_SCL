CREATE TABLE GA_HNUMESPABO(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  NUM_TELEFESP VARCHAR2(15) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  NUM_DIASNUM NUMBER(9,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_HNUMESPABO IS 'Numeros Especiales Abonados Celular'
/
COMMENT ON COLUMN  GA_HNUMESPABO.NUM_ABONADO IS 'Numero de Abonado'
/
COMMENT ON COLUMN  GA_HNUMESPABO.NUM_TELEFESP IS 'Telefono Especial'
/
COMMENT ON COLUMN  GA_HNUMESPABO.FEC_HISTORICO IS 'Fecha de Historico'
/
COMMENT ON COLUMN  GA_HNUMESPABO.NUM_DIASNUM IS 'Numero de asociacion con el periodo contratado de servicio'
/
