CREATE TABLE CO_HIST_MOV_CLIPRO(
    NUM_IDENT VARCHAR2(20) NOT NULL 
 ,  FEC_MODIF DATE NOT NULL 
 ,  USUARIO_MODIF VARCHAR2(30) NOT NULL 
 ,  DETALLE_MODIF VARCHAR2(100) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_HIST_MOV_CLIPRO IS 'REGISTRO HISTORICO DE LOS MOVIMIENTOS DE LOS CLIENTES-PROVEEDORES'
/
COMMENT ON COLUMN  CO_HIST_MOV_CLIPRO.NUM_IDENT IS 'RUT DEL CLIENTE-PROVEEDOR'
/
COMMENT ON COLUMN  CO_HIST_MOV_CLIPRO.FEC_MODIF IS 'FECHA DE LA MODIFICACION'
/
COMMENT ON COLUMN  CO_HIST_MOV_CLIPRO.USUARIO_MODIF IS 'USUARIO RESPONSABLE DE LA MODIFICACION'
/
COMMENT ON COLUMN  CO_HIST_MOV_CLIPRO.DETALLE_MODIF IS 'DETALLE DE LA MODIFICACION'
/
