 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  COD_CAT NUMBER(2,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_OPERADORA VARCHAR2(5) NOT NULL 
 ,  OBSERVACION VARCHAR2(50) NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
