 ,  DES_ESTADISTICA VARCHAR2(30) NOT NULL 
 ,  COD_MODULO VARCHAR2(2) NOT NULL 
 ,  ANCHO NUMBER(3,0) NOT NULL 
 ,  ALTO NUMBER(3,0) NOT NULL 
 ,  CAB_LINEA1 VARCHAR2(200) NULL 
 ,  CAB_LINEA2 VARCHAR2(200) NULL 
 ,  PIE_LINEA1 VARCHAR2(200) NULL 
 ,  PIE_LINEA2 VARCHAR2(200) NULL 
 ,  FIC_DOS VARCHAR2(50) NULL 
 ,  COD_CONSULTA NUMBER(5,0) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
