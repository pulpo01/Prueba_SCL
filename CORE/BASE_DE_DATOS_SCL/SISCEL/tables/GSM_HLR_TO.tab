CREATE TABLE GSM_HLR_TO(
    COD_HLR_SIMCARD VARCHAR2(10) NOT NULL 
 ,  COD_HLR VARCHAR2(10) NOT NULL 
 ,  FEC_ACTUALIZACION DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_HLR_TO IS 'Tabla de Relacion HLR.'
/
COMMENT ON COLUMN  GSM_HLR_TO.COD_HLR_SIMCARD IS 'Codigo HLR en de Centrales.'
/
COMMENT ON COLUMN  GSM_HLR_TO.COD_HLR IS 'Codigo HLR.'
/
COMMENT ON COLUMN  GSM_HLR_TO.FEC_ACTUALIZACION IS 'Fecha de la ultima actualizacion.'
/
COMMENT ON COLUMN  GSM_HLR_TO.COD_USUARIO IS 'Codigo del usuario que actualizo.'
/
