CREATE TABLE GA_DATCEN(
    NUM_MOVIMIENTO NUMBER(9,0) NOT NULL 
 ,  NUM_SERIECEN VARCHAR2(11) NULL 
 ,  IND_RESPUESTA NUMBER(1,0) NOT NULL 
 ,  NUM_MSNB VARCHAR2(12) NULL 
 ,  NUM_PERSONAL VARCHAR2(4) NULL 
 ,  STATUS VARCHAR2(30) NULL 
 ,  CADENA VARCHAR2(255) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_DATCEN IS 'Resultados de Consultas en Central'
/
COMMENT ON COLUMN  GA_DATCEN.NUM_MOVIMIENTO IS 'Numero de Movimiento'
/
COMMENT ON COLUMN  GA_DATCEN.NUM_SERIECEN IS 'Numero de Serie en Central'
/
COMMENT ON COLUMN  GA_DATCEN.IND_RESPUESTA IS 'Indicador de Respuesta ; (0) Exito, (1) Fallo'
/
COMMENT ON COLUMN  GA_DATCEN.NUM_MSNB IS 'Numero de MSNB'
/
COMMENT ON COLUMN  GA_DATCEN.NUM_PERSONAL IS 'PCODE'
/
COMMENT ON COLUMN  GA_DATCEN.STATUS IS 'Status en Central'
/
COMMENT ON COLUMN  GA_DATCEN.CADENA IS 'Cadena de Respuesta (en fallos) o Servicios (en exito)'
/
