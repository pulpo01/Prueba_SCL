CREATE TABLE CET_PERIODOSERV(
    COD_SERVICIO VARCHAR2(5) NOT NULL 
 ,  COD_PERIODO VARCHAR2(6) NOT NULL 
 ,  COD_EMPRESA VARCHAR2(5) NOT NULL 
 ,  COD_TIPPERIODO VARCHAR2(5) NOT NULL 
 ,  COD_TIPUSO VARCHAR2(2) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  IND_GRUPOSERV NUMBER(1,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CET_PERIODOSERV IS 'Periodos por empresa/servicio'
/
COMMENT ON COLUMN  CET_PERIODOSERV.COD_SERVICIO IS 'Codigo de servicio asociado al periodo'
/
COMMENT ON COLUMN  CET_PERIODOSERV.COD_PERIODO IS 'Codigo de periodo asociado'
/
COMMENT ON COLUMN  CET_PERIODOSERV.COD_EMPRESA IS 'Codigo empresa'
/
COMMENT ON COLUMN  CET_PERIODOSERV.COD_TIPPERIODO IS 'Codigo tipo de periodo'
/
COMMENT ON COLUMN  CET_PERIODOSERV.COD_TIPUSO IS 'Uso del periodo definido FA:Facturacion CO:Comisiones CB: Conciliacion'
/
COMMENT ON COLUMN  CET_PERIODOSERV.FEC_DESDE IS 'Fecha inicio vigencia del tipo de periodo'
/
COMMENT ON COLUMN  CET_PERIODOSERV.FEC_HASTA IS 'Fecha termino vigencia tipo periodo.'
/
COMMENT ON COLUMN  CET_PERIODOSERV.COD_ESTADO IS 'Estado Periodo:  AB: Abierto  CP: Cerrado Parcial  CT: Cerrado Total'
/
COMMENT ON COLUMN  CET_PERIODOSERV.FEC_ULTMOD IS 'Fecha ultima modificacion'
/
COMMENT ON COLUMN  CET_PERIODOSERV.NOM_USUARIO IS 'Responsable ultima modificacion'
/
COMMENT ON COLUMN  CET_PERIODOSERV.IND_GRUPOSERV IS 'Indicador de grupo de servicio 0:servicio 1:grupo'
/
