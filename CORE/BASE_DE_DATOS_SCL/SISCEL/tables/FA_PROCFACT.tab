CREATE TABLE FA_PROCFACT(
    COD_PROCESO NUMBER(4,0) NOT NULL 
 ,  DES_PROCESO VARCHAR2(30) NOT NULL 
 ,  COD_PROCPREC NUMBER(4,0) NULL 
 ,  COD_ESTAPREC NUMBER(1,0) NULL 
 ,  IND_REPROCESO NUMBER(1,0) NOT NULL 
 ,  IND_MULTIPLEX NUMBER(1,0) NULL 
 ,  IND_PROCCLIE NUMBER(1,0) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_PROCFACT IS 'Definicion de procesos de facturacion.'
/
COMMENT ON COLUMN  FA_PROCFACT.COD_PROCESO IS 'Codigo de proceso.'
/
COMMENT ON COLUMN  FA_PROCFACT.DES_PROCESO IS 'Descripcion de proceso.'
/
COMMENT ON COLUMN  FA_PROCFACT.COD_PROCPREC IS 'Codigo proceso precedente.'
/
COMMENT ON COLUMN  FA_PROCFACT.COD_ESTAPREC IS 'Codigo estado proceso precedente - 1: En proceso, 2: Terminado sin problemas, 3: Terminado con error.'
/
COMMENT ON COLUMN  FA_PROCFACT.IND_REPROCESO IS 'Indicador de reproceso automatico - 1: SI, 2: NO.'
/
