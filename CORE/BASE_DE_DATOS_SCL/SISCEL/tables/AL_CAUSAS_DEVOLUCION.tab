 ,  DES_CAUSA VARCHAR2(20) NOT NULL 
 ,  IND_RETORNO NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_CAUSAS_DEVOLUCION IS 'Motivos o causas de Devolucion'
/
COMMENT ON COLUMN  AL_CAUSAS_DEVOLUCION.COD_CAUSA IS 'Codigo causa devolucion'
/
COMMENT ON COLUMN  AL_CAUSAS_DEVOLUCION.DES_CAUSA IS 'Descripcion causa devolucion'
/
COMMENT ON COLUMN  AL_CAUSAS_DEVOLUCION.IND_RETORNO IS 'Retorno Si/No'
/
