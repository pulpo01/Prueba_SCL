CREATE TABLE CM_MATRIZRETENCIONES_TD(
    COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) NOT NULL 
 ,  COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  NUM_DESDE NUMBER(4,0) NOT NULL 
 ,  NUM_HASTA NUMBER(4,0) NOT NULL 
 ,  VAL_FACTOR NUMBER(14,4) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_MATRIZRETENCIONES_TD IS 'SE CONFIGURA EL DETALLE DE LA MATRIZ DE METAS PARA RETENCIONES'
/
COMMENT ON COLUMN  CM_MATRIZRETENCIONES_TD.COD_TIPORED IS 'CSDIGO DEL TIPO DE RED'
/
COMMENT ON COLUMN  CM_MATRIZRETENCIONES_TD.COD_PLANCOMIS IS 'CSDIGO DEL PLAN DE COMISIONES'
/
COMMENT ON COLUMN  CM_MATRIZRETENCIONES_TD.COD_CONCEPTO IS 'CSDIGO DEL CONCEPTO'
/
COMMENT ON COLUMN  CM_MATRIZRETENCIONES_TD.NUM_DESDE IS 'CANTIDAD DESDE DE LA MATRIZ DE METAS DE RETENCIONES'
/
COMMENT ON COLUMN  CM_MATRIZRETENCIONES_TD.NUM_HASTA IS 'CANTIDAD HASTA DE LA MATRIZ DE METAS DE RETENCIONES'
/
COMMENT ON COLUMN  CM_MATRIZRETENCIONES_TD.VAL_FACTOR IS 'FACTOR A APLICAR EN LA COMISISN BASE'
/
COMMENT ON COLUMN  CM_MATRIZRETENCIONES_TD.FEC_ULTMOD IS 'FECHA DE ZLTIMA MODIFICACISN'
/
COMMENT ON COLUMN  CM_MATRIZRETENCIONES_TD.NOM_USUARIO IS 'USUARIO QUE REALIZS ZLTIMA MODIFICACISN'
/
