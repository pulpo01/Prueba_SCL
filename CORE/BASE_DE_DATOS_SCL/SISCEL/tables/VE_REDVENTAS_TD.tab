CREATE TABLE VE_REDVENTAS_TD(
    COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  NUM_NIVEL NUMBER(2,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_VENDE_PADRE NUMBER(6,0) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_REDVENTAS_TD IS 'ESTRUCTURAS DE RED DE VENTAS'
/
COMMENT ON COLUMN  VE_REDVENTAS_TD.FEC_HASTA IS 'FECHA FINAL DE VIGENCIA'
/
COMMENT ON COLUMN  VE_REDVENTAS_TD.NOM_USUARIO IS 'USUARIO ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  VE_REDVENTAS_TD.COD_VENDE_PADRE IS 'CODIGO DEL AGENTE DE VENTAS PADRE PARA EL TIPO DE RED'
/
COMMENT ON COLUMN  VE_REDVENTAS_TD.COD_VENDEDOR IS 'CODIGO DEL AGENTE DE VENTAS O VENDEDOR'
/
COMMENT ON COLUMN  VE_REDVENTAS_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED'
/
COMMENT ON COLUMN  VE_REDVENTAS_TD.NUM_NIVEL IS 'NIVEL DE ASIGNACION'
/
COMMENT ON COLUMN  VE_REDVENTAS_TD.FEC_DESDE IS 'FECHA INICIO DE VIGENCIA'
/
