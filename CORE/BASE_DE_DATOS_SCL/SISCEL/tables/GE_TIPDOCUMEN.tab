CREATE TABLE GE_TIPDOCUMEN(
    COD_TIPDOCUM NUMBER(2,0) NOT NULL 
 ,  DES_TIPDOCUM VARCHAR2(40) NOT NULL 
 ,  COD_DOCUANUL NUMBER(2,0) NULL 
 ,  IND_LEGALIDAD NUMBER(1,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NULL 
 ,  DES_ABREVIADA VARCHAR2(3) NULL 
 ,  IND_NETEABLE NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  IND_PAGABLE NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  TIP_FOLIACION VARCHAR2(1) DEFAULT 'P' NOT NULL 
 ,  IND_PRIORIDAD VARCHAR2(2) DEFAULT '00' NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_TIPDOCUMEN IS 'Tipos de documentos emitibles- Retrofitted'
/
COMMENT ON COLUMN  GE_TIPDOCUMEN.COD_TIPDOCUM IS 'CODIGO DE TIPO DE DOC - Retrofitted'
/
COMMENT ON COLUMN  GE_TIPDOCUMEN.DES_TIPDOCUM IS 'DESCRIPCION DE TIPO D - Retrofitted'
/
COMMENT ON COLUMN  GE_TIPDOCUMEN.COD_DOCUANUL IS 'Documento que anula   - Retrofitted'
/
COMMENT ON COLUMN  GE_TIPDOCUMEN.IND_LEGALIDAD IS '1: Legal, 0: No legal - Retrofitted'
/
COMMENT ON COLUMN  GE_TIPDOCUMEN.COD_ARTICULO IS 'Articulo asociado.'
/
COMMENT ON COLUMN  GE_TIPDOCUMEN.DES_ABREVIADA IS 'Descripcion abreviada utilizada por los cajeros que identifica a un documento'
/
COMMENT ON COLUMN  GE_TIPDOCUMEN.IND_NETEABLE IS '0: No participa en un canje, 1: Puede participar en un canje'
/
COMMENT ON COLUMN  GE_TIPDOCUMEN.IND_PAGABLE IS 'indica si el documento esta afecto a pago (1) o si no lo está (0).'
/
COMMENT ON COLUMN  GE_TIPDOCUMEN.TIP_FOLIACION IS 'TIPO DE FOLIACION A:AUTOFOLIATIVA P:PREFOLIADA'
/
COMMENT ON COLUMN  GE_TIPDOCUMEN.IND_PRIORIDAD IS 'INDICE DE PRIORIDAD'
/
