CREATE TABLE AL_CODIGO_EXTERNO(
    COD_PLATAFORMA VARCHAR2(20) NOT NULL 
 ,  TIP_CODIGO VARCHAR2(5) NOT NULL 
 ,  CODIGO_EXTERNO VARCHAR2(20) NOT NULL 
 ,  CODIGO_INTERNO VARCHAR2(20) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_UTLMOD DATE NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  AL_CODIGO_EXTERNO.COD_PLATAFORMA IS 'Codigo de Plataforma'
/
COMMENT ON COLUMN  AL_CODIGO_EXTERNO.TIP_CODIGO IS 'Tipo de Codigo de Conversion'
/
COMMENT ON COLUMN  AL_CODIGO_EXTERNO.CODIGO_EXTERNO IS 'Codigo en Plataforma Externa'
/
COMMENT ON COLUMN  AL_CODIGO_EXTERNO.CODIGO_INTERNO IS 'Codigo Interno en SCL'
/
COMMENT ON COLUMN  AL_CODIGO_EXTERNO.NOM_USUARIO IS 'Usuario ultima alteracion'
/
COMMENT ON COLUMN  AL_CODIGO_EXTERNO.FEC_UTLMOD IS 'Fecha ultima alteracion'
/
