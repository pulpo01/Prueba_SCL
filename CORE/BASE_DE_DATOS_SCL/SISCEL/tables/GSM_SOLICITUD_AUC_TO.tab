CREATE TABLE GSM_SOLICITUD_AUC_TO(
    NUM_SEQ_SOLICITUD_AUC NUMBER(10,0) NOT NULL 
 ,  NUM_SEQ_SOLICITUD NUMBER(10,0) NOT NULL 
 ,  NUM_CANTIDAD NUMBER(18,0) NOT NULL 
 ,  NUM_CANTIDAD_AUC NUMBER(18,0) NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) NOT NULL 
 ,  FEC_ACTUALIZACION DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ,  NUM_INTENTOS NUMBER(18,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_SOLICITUD_AUC_TO IS 'Tabla de solicitud de AUC.'
/
COMMENT ON COLUMN  GSM_SOLICITUD_AUC_TO.NUM_SEQ_SOLICITUD_AUC IS 'Numero Secuencial de la Solicitud  de AUC'
/
COMMENT ON COLUMN  GSM_SOLICITUD_AUC_TO.NUM_SEQ_SOLICITUD IS 'Numero Secuencial de la Solicitud de Generacion de Simcard'
/
COMMENT ON COLUMN  GSM_SOLICITUD_AUC_TO.NUM_CANTIDAD IS 'Cantidad de la solicitud de AUC'
/
COMMENT ON COLUMN  GSM_SOLICITUD_AUC_TO.NUM_CANTIDAD_AUC IS 'Cantidad registros autentificados'
/
COMMENT ON COLUMN  GSM_SOLICITUD_AUC_TO.COD_ESTADO IS 'Estado de la solicitud de AUC'
/
COMMENT ON COLUMN  GSM_SOLICITUD_AUC_TO.FEC_ACTUALIZACION IS 'Fecha de actualizacion.'
/
COMMENT ON COLUMN  GSM_SOLICITUD_AUC_TO.COD_USUARIO IS 'Usuario que actualizo.'
/
COMMENT ON COLUMN  GSM_SOLICITUD_AUC_TO.NUM_INTENTOS IS 'Nzmero de Intentos'
/
