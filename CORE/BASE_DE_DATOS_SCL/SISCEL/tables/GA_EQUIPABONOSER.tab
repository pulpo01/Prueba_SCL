CREATE TABLE GA_EQUIPABONOSER(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ,  NUM_MOVIMIENTO NUMBER(10,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  NUM_UNIDADES NUMBER(6,0) NOT NULL 
 ,  COD_BODEGA NUMBER(6,0) NULL 
 ,  TIP_STOCK NUMBER(2,0) NULL 
 ,  COD_USO NUMBER(2,0) NULL 
 ,  COD_ESTADO NUMBER(2,0) NULL 
 ,  COD_PAQUETE NUMBER(6,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_EQUIPABONOSER IS 'Equipos no Seriados de Abonados de Productos'
/
COMMENT ON COLUMN  GA_EQUIPABONOSER.NUM_ABONADO IS 'Numero de Abonado/Producto'
/
COMMENT ON COLUMN  GA_EQUIPABONOSER.COD_ARTICULO IS 'Codigo de Articulo'
/
COMMENT ON COLUMN  GA_EQUIPABONOSER.FEC_ALTA IS 'Fecha de Insercion'
/
COMMENT ON COLUMN  GA_EQUIPABONOSER.NUM_MOVIMIENTO IS 'Numero de Movimiento de Stock'
/
COMMENT ON COLUMN  GA_EQUIPABONOSER.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_EQUIPABONOSER.NUM_UNIDADES IS 'Numero de Unidades'
/
COMMENT ON COLUMN  GA_EQUIPABONOSER.COD_BODEGA IS 'Codigo de Bodega o Almacen'
/
COMMENT ON COLUMN  GA_EQUIPABONOSER.TIP_STOCK IS 'Tipo de Stock del Almacen'
/
COMMENT ON COLUMN  GA_EQUIPABONOSER.COD_USO IS 'Uso al que se encuentra destinado el Equipo'
/
COMMENT ON COLUMN  GA_EQUIPABONOSER.COD_ESTADO IS 'Estado del Equipo'
/
COMMENT ON COLUMN  GA_EQUIPABONOSER.COD_PAQUETE IS 'Codigo de Paquete'
/
