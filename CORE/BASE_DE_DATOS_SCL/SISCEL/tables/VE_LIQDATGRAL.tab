CREATE TABLE VE_LIQDATGRAL(
    NUM_DIASDOCUM NUMBER(2,0) NOT NULL 
 ,  NUM_DIASFACTTRAF NUMBER(4,0) NOT NULL 
 ,  COD_ARTAMISTAR NUMBER(6,0) NOT NULL 
 ,  IND_CELAMISTAR NUMBER(2,0) NOT NULL 
 ,  NUM_DIASRENUNC NUMBER(3,0) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  NUM_MESPLANTARIF NUMBER(2,0) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_LIQDATGRAL IS 'Datos Generales para Liquidacion de Comisiones'
/
COMMENT ON COLUMN  VE_LIQDATGRAL.NUM_DIASDOCUM IS 'Numero de dias para hacer llegar documentos de venta'
/
COMMENT ON COLUMN  VE_LIQDATGRAL.NUM_DIASFACTTRAF IS 'Numero de dias para promediar facturacion por trafico'
/
COMMENT ON COLUMN  VE_LIQDATGRAL.COD_ARTAMISTAR IS 'Codigo del articulo AMISTAR'
/
COMMENT ON COLUMN  VE_LIQDATGRAL.IND_CELAMISTAR IS 'Numero de AMISTAR que representan la venta de un celular'
/
COMMENT ON COLUMN  VE_LIQDATGRAL.NUM_DIASRENUNC IS 'Numero de dias para considerar efectiva una renuncia'
/
COMMENT ON COLUMN  VE_LIQDATGRAL.COD_MONEDA IS 'Moneda de liquidacion.'
/
COMMENT ON COLUMN  VE_LIQDATGRAL.NUM_MESPLANTARIF IS 'Numero de meses para castigar / pagar por cambio de plan tarifario'
/
