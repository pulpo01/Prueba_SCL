CREATE TABLE ICG_SUSPREHAMOD(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_SUSPREHA VARCHAR2(3) NOT NULL 
 ,  COD_MODULO VARCHAR2(2) NOT NULL 
 ,  COD_ACTUACION_SUSP NUMBER(5,0) NOT NULL 
 ,  COD_ACTUACION_REHA NUMBER(5,0) NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICG_SUSPREHAMOD IS 'Suspensiones y Rehabilitaciones'
/
COMMENT ON COLUMN  ICG_SUSPREHAMOD.COD_PRODUCTO IS 'codigo de producto'
/
COMMENT ON COLUMN  ICG_SUSPREHAMOD.COD_SUSPREHA IS 'Codigo de Suspension/Rehabilitacion - Retrofitted'
/
COMMENT ON COLUMN  ICG_SUSPREHAMOD.COD_MODULO IS 'Codigo de Modulo que realiza la suspension - Retrofitted'
/
COMMENT ON COLUMN  ICG_SUSPREHAMOD.COD_ACTUACION_SUSP IS 'Codigo Actuacion de Suspension en Abonados - Retrofitted'
/
COMMENT ON COLUMN  ICG_SUSPREHAMOD.COD_ACTUACION_REHA IS 'Codigo Actuacion de Rehabilitacion en Abonados - Retrofitted'
/
