CREATE TABLE FA_PARAMDIROPERAD_TO (  
   COD_OPERAD                    VARCHAR2(5)   NOT NULL
 , COD_PARAMDIR                  NUMBER(3)     NOT NULL
 , ORDEN                         NUMBER(3)     NOT NULL
 , IND_OBLIGATORIO               NUMBER(1)     DEFAULT   0 NOT NULL 
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  FA_PARAMDIROPERAD_TO                      IS '[ FPOP ] Parámetros de dirección por OperadoraParámetros de dirección por Operadora'
/

COMMENT ON COLUMN FA_PARAMDIROPERAD_TO.COD_OPERAD                     IS 'Código de la operadora'
/
COMMENT ON COLUMN FA_PARAMDIROPERAD_TO.COD_PARAMDIR                   IS 'Código del parámetro de dirección'
/
COMMENT ON COLUMN FA_PARAMDIROPERAD_TO.ORDEN                          IS 'Orden dentro de la composición de la dirección'
/
COMMENT ON COLUMN FA_PARAMDIROPERAD_TO.IND_OBLIGATORIO                IS 'Indicador de obligatorio'
/

