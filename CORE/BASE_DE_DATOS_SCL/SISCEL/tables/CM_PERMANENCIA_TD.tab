CREATE TABLE CM_PERMANENCIA_TD(
    COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  TIP_PLAN VARCHAR2(5) NOT NULL 
 ,  NUM_PERIODOS NUMBER(8,0) NOT NULL 
 ,  IND_SALDO CHAR(1) NOT NULL 
 ,  TIP_BONIFICACION CHAR(1) NOT NULL 
 ,  COD_CONCEPTO_ASOC NUMBER(3,0) NOT NULL 
 ,  IMP_BONIFICACION NUMBER(14,4) NOT NULL 
 ,  FEC_CREACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) DEFAULT 'C' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_PERMANENCIA_TD IS 'Comisiones Por Permanencia.'
/
COMMENT ON COLUMN  CM_PERMANENCIA_TD.COD_CONCEPTO IS 'CONCEPTO DE PENALIZACION.'
/
COMMENT ON COLUMN  CM_PERMANENCIA_TD.TIP_PLAN IS 'TIPO DE PLAN A COMISIONAR.'
/
COMMENT ON COLUMN  CM_PERMANENCIA_TD.NUM_PERIODOS IS 'CANTIDAD DE PERIODOS.'
/
COMMENT ON COLUMN  CM_PERMANENCIA_TD.IND_SALDO IS 'INDICADOR DE SALDOS.'
/
COMMENT ON COLUMN  CM_PERMANENCIA_TD.TIP_BONIFICACION IS 'TIPO DE BONIFICACION.'
/
COMMENT ON COLUMN  CM_PERMANENCIA_TD.COD_CONCEPTO_ASOC IS 'CONCEPTO ASOCIADO.'
/
COMMENT ON COLUMN  CM_PERMANENCIA_TD.IMP_BONIFICACION IS 'IMPORTE BONIFICACION.'
/
COMMENT ON COLUMN  CM_PERMANENCIA_TD.FEC_CREACION IS 'FECHA DE CREACION.'
/
COMMENT ON COLUMN  CM_PERMANENCIA_TD.NOM_USUARIO IS 'NOMBRE USUARIO.'
/
COMMENT ON COLUMN  CM_PERMANENCIA_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
COMMENT ON COLUMN  CM_PERMANENCIA_TD.COD_PLANCOMIS IS 'PLAN DE COMISIONES QUE CONTIENEN AL CONCEPTO.'
/
