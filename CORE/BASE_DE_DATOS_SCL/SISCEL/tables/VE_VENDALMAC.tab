CREATE TABLE VE_VENDALMAC(
    COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_BODEGA NUMBER(6,0) NOT NULL 
 ,  FEC_ASIGNACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_DESASIGNAC DATE NULL 
 ) TABLESPACE    TS_CRM_GESTION_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_VENDALMAC IS 'Almacenes asociados a Comisionistas'
/
COMMENT ON COLUMN  VE_VENDALMAC.COD_VENDEDOR IS 'Csdigo de Comisionista'
/
COMMENT ON COLUMN  VE_VENDALMAC.COD_BODEGA IS 'Csdigo de Bodega'
/
COMMENT ON COLUMN  VE_VENDALMAC.FEC_ASIGNACION IS 'Fecha de asignacisn'
/
COMMENT ON COLUMN  VE_VENDALMAC.NOM_USUARIO IS 'Usuario que realiza la asignacisn'
/
COMMENT ON COLUMN  VE_VENDALMAC.FEC_DESASIGNAC IS 'Fecha de desasignacisn'
/
