CREATE TABLE VE_NOMBRAMIENTO_BURO_TO
(
  COD_REFERENCIA                 NUMBER(8)      NOT NULL,
  TIP_NOMBRAMIENTO               VARCHAR2(35 BYTE),
  NOMBRE_NOMBRAMIENTO            VARCHAR2(50 BYTE),
  APELLIDO_PATERNO_NOMBRAMIENTO  VARCHAR2(50 BYTE),
  APELLIDO_MATERNO_NOMBRAMIENTO  VARCHAR2(50 BYTE),
  TIP_IDENT                      VARCHAR2(30 BYTE),
  NUM_IDENT                      VARCHAR2(20 BYTE),
  NACIONALIDAD                   VARCHAR2(30 BYTE),
  ESTADO_CIVIL                   VARCHAR2(30 BYTE),
  OCUPACION                      VARCHAR2(50 BYTE),
  DOMICILIO                      VARCHAR2(350 BYTE),
  DIRECCION_OFICINA              VARCHAR2(350 BYTE)
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          1M
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING
/
