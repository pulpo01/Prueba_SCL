CREATE TABLE GE_CARGOS_SOBRESCRITOS_TO
(
  COD_CLIENTE             NUMBER(8)             NOT NULL,
  NUM_ABONADO             NUMBER(8)             NOT NULL,
  COD_ORIGEN_TRANSACCION  VARCHAR2(3 BYTE),
  TIPO_SERVICIO           VARCHAR2(3 BYTE)      NOT NULL,
  COD_PROD_CONTRATADO     NUMBER(38),
  COD_CARGO_CONTRATADO    NUMBER(8),
  COD_PLANTARIF           VARCHAR2(3 BYTE),
  COD_CARGOBASICO         VARCHAR2(3 BYTE),
  COD_SERVICIO            VARCHAR2(3 BYTE),
  IMPORTE_ORIGEN          NUMBER(14,4)          NOT NULL,
  IMPORTE_SOBRESCRITO     NUMBER(14,4)          NOT NULL,
  COD_MONEDA              VARCHAR2(3 BYTE)      NOT NULL,
  COD_CONCEPTO            NUMBER(4),
  NUM_VENTA               NUMBER(8),
  FEC_REGISTRO            DATE                  NOT NULL,
  NOM_USUARORA            VARCHAR2(30 BYTE)     NOT NULL
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          1M
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE GE_CARGOS_SOBRESCRITOS_TO IS '[ GCSE ] Cargos Sobre escritosCargos Sobre escritos';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.COD_CLIENTE IS 'Código de Cliente';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.NUM_ABONADO IS 'Numero de abonado';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.COD_ORIGEN_TRANSACCION IS 'Origen Transacción ("VT","PV")';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.TIPO_SERVICIO IS 'Tipo Servicio ("CB","SS","PA")';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.COD_PROD_CONTRATADO IS 'Código Producto contratado';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.COD_CARGO_CONTRATADO IS 'Código Cargo Contratado';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.COD_PLANTARIF IS 'Código Plan tarifario';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.COD_CARGOBASICO IS 'Código Cargo Básico';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.COD_SERVICIO IS 'Código Servicio';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.IMPORTE_ORIGEN IS 'Importe Origen';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.IMPORTE_SOBRESCRITO IS 'Importe Sobreescrito';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.COD_MONEDA IS 'Código de Moneda';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.COD_CONCEPTO IS 'Código de Concepto';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.NUM_VENTA IS 'Número de venta';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.FEC_REGISTRO IS 'Fecha de registro';

COMMENT ON COLUMN GE_CARGOS_SOBRESCRITOS_TO.NOM_USUARORA IS 'Nombre Usuario';
