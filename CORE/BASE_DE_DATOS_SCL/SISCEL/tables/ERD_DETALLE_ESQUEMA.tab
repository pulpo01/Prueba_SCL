CREATE TABLE ERD_DETALLE_ESQUEMA(
    COD_ESQUEMA NUMBER(6,0) NOT NULL 
 ,  COD_CAMPO NUMBER(6,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(6,0) NOT NULL 
 ,  NUM_ORDEN_SELECCION NUMBER(2,0) NOT NULL 
 ,  NOM_USUARIO_MODI VARCHAR2(30) NOT NULL 
 ,  FEC_MODI_H DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERD_DETALLE_ESQUEMA IS 'Definicion De Detalle De Los Esquemas De Evaluacion.'
/
COMMENT ON COLUMN  ERD_DETALLE_ESQUEMA.COD_ESQUEMA IS 'Identificador unico para el esquema de evaluacion. Ver tabla ERD_ESQUEMAS.'
/
COMMENT ON COLUMN  ERD_DETALLE_ESQUEMA.COD_CAMPO IS 'Identificador unico para el campo a seleccionar. Ver tabla ERD_CAMPOS_ACTIVOS.'
/
COMMENT ON COLUMN  ERD_DETALLE_ESQUEMA.COD_PRODUCTO IS 'Identificador unico para el producto/informe. Ver tabla ERD_CAMPOS_ACTIVOS.'
/
COMMENT ON COLUMN  ERD_DETALLE_ESQUEMA.NUM_ORDEN_SELECCION IS 'Orden de seleccion y/o prioridad.'
/
COMMENT ON COLUMN  ERD_DETALLE_ESQUEMA.NOM_USUARIO_MODI IS 'Nombre del usuario que creo/actualizo este registro.'
/
COMMENT ON COLUMN  ERD_DETALLE_ESQUEMA.FEC_MODI_H IS 'fecha de creacion/modificacion de este registro.'
/
