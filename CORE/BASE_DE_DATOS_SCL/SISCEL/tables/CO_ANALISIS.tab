CREATE TABLE CO_ANALISIS(
    FEC_ANALISIS DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  CNT_CLIENTE NUMBER(8,0) NOT NULL 
 ,  CNT_ABOCELU NUMBER(8,0) NOT NULL 
 ,  CNT_ABOBEEP NUMBER(8,0) NOT NULL 
 ,  DEU_VENCIDA NUMBER(14,4) NOT NULL 
 ,  IND_ESTADO CHAR(1) NOT NULL 
 ,  FEC_CIERRE DATE NULL 
 ,  SEC_ANALISIS NUMBER(8,0) NOT NULL 
 ,  CNT_NUM_IDENT NUMBER(8,0) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_ANALISIS IS 'Analisis De Clientes Morosos.'
/
COMMENT ON COLUMN  CO_ANALISIS.FEC_ANALISIS IS 'Fecha de analisis.'
/
COMMENT ON COLUMN  CO_ANALISIS.NOM_USUARIO IS 'Usuario.'
/
COMMENT ON COLUMN  CO_ANALISIS.CNT_CLIENTE IS 'Cantidad de Clientes.'
/
COMMENT ON COLUMN  CO_ANALISIS.CNT_ABOCELU IS 'Cantidad de abonados celulares.'
/
COMMENT ON COLUMN  CO_ANALISIS.CNT_ABOBEEP IS 'Cantidad de abonados beeper.'
/
COMMENT ON COLUMN  CO_ANALISIS.DEU_VENCIDA IS 'Deuda Vencida.'
/
COMMENT ON COLUMN  CO_ANALISIS.IND_ESTADO IS 'Estado del grupo.'
/
COMMENT ON COLUMN  CO_ANALISIS.FEC_CIERRE IS 'Fecha de cierre del grupo de analisis'
/
