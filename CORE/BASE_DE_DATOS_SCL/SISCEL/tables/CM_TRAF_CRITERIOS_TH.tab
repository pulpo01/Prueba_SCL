CREATE TABLE CM_TRAF_CRITERIOS_TH(
    NUM_GENERAL NUMBER(10,0) NOT NULL 
 ,  NUM_CRITERIO NUMBER(3,0) NOT NULL 
 ,  NUM_PERFIL NUMBER(3,0) NOT NULL 
 ,  NUM_PERIODO NUMBER(3,0) NOT NULL 
 ,  COD_PERIODO_HAB NUMBER(8,0) NOT NULL 
 ,  COD_PERIODO_EVAL NUMBER(8,0) NOT NULL 
 ,  TIP_UNITAS CHAR(1) NOT NULL 
 ,  UNI_ENTRADA NUMBER(10,0) NOT NULL 
 ,  UNI_SALIDA NUMBER(10,0) NOT NULL 
 ,  UNI_TOTAL NUMBER(10,0) NOT NULL 
 ,  IMP_COMISION NUMBER(14,4) NOT NULL 
 ,  COD_ORIGEN VARCHAR2(5) NULL 
 ,  MTO_RECARGA NUMBER(14,4) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_TRAF_CRITERIOS_TH IS 'Trafico Observado Para Cada Uno De Los Criterios.'
/
COMMENT ON COLUMN  CM_TRAF_CRITERIOS_TH.NUM_GENERAL IS 'IDENTIFICADOR GENERAL DE EVENTO COMISIONAL.'
/
COMMENT ON COLUMN  CM_TRAF_CRITERIOS_TH.NUM_CRITERIO IS 'NUMERO IDENTIFICADOR DEL CRITERIO DE VALORACION.'
/
COMMENT ON COLUMN  CM_TRAF_CRITERIOS_TH.NUM_PERFIL IS 'NUMERO IDENTIFICADOR DEL PERFIL DE EVALUACION.'
/
COMMENT ON COLUMN  CM_TRAF_CRITERIOS_TH.NUM_PERIODO IS 'PERIODO DE EVALUACION (0,1,2,3,...).'
/
COMMENT ON COLUMN  CM_TRAF_CRITERIOS_TH.COD_PERIODO_HAB IS 'CODIGO DE PERIODO DE HABILITACION.'
/
COMMENT ON COLUMN  CM_TRAF_CRITERIOS_TH.COD_PERIODO_EVAL IS 'CODIGO DE PERIODO DE EVALUACION.'
/
COMMENT ON COLUMN  CM_TRAF_CRITERIOS_TH.TIP_UNITAS IS 'TIPO DE UNIDAD DE TASACION.'
/
COMMENT ON COLUMN  CM_TRAF_CRITERIOS_TH.UNI_ENTRADA IS 'UNIDADES DE ENTRADA OBTENIDAS.'
/
COMMENT ON COLUMN  CM_TRAF_CRITERIOS_TH.UNI_SALIDA IS 'UNIDADES DE SALIDA OBTENIDAS.'
/
COMMENT ON COLUMN  CM_TRAF_CRITERIOS_TH.UNI_TOTAL IS 'UNIDADES  TOTALES OBTENIDAS.'
/
COMMENT ON COLUMN  CM_TRAF_CRITERIOS_TH.IMP_COMISION IS 'IMPORTE DE COMISION.'
/
COMMENT ON COLUMN  CM_TRAF_CRITERIOS_TH.COD_ORIGEN IS 'Codigo de Origen'
/
COMMENT ON COLUMN  CM_TRAF_CRITERIOS_TH.MTO_RECARGA IS 'Monto de la Recarga'
/
