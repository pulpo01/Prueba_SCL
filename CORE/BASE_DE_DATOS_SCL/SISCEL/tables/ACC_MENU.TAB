CREATE TABLE ACC_MENU(
    COD_MENU VARCHAR2(30) NOT NULL 
 ,  COD_APLI NUMBER(4,0) NOT NULL 
 ,  COD_MENU_PADRE VARCHAR2(30) NOT NULL 
 ,  GLS_MENU VARCHAR2(50) NULL 
 ,  FLG_TIPOPROG NUMBER(4,0) DEFAULT 0 NOT NULL 
 ,  FLG_PADRE NUMBER(4,0) DEFAULT 0 NOT NULL 
 ,  USER_LOG VARCHAR2(10) NOT NULL 
 ,  FEC_LOG DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  ACC_MENU.COD_MENU IS 'Codigo de la opcion de menu'
/
COMMENT ON COLUMN  ACC_MENU.COD_APLI IS 'Codigo de la aplicacion'
/
COMMENT ON COLUMN  ACC_MENU.COD_MENU_PADRE IS 'Codigo de la opcion de menu padre'
/
COMMENT ON COLUMN  ACC_MENU.GLS_MENU IS 'Descripcion de la opcion del menu'
/
COMMENT ON COLUMN  ACC_MENU.FLG_TIPOPROG IS 'Flag que identifica que tipo de programa es el que esta seleccionando el usuario'
/
COMMENT ON COLUMN  ACC_MENU.FLG_PADRE IS 'Flag indicador de opcion de ejecucion'
/
COMMENT ON COLUMN  ACC_MENU.USER_LOG IS 'Codigo del usuario logeado en el Sistema'
/
COMMENT ON COLUMN  ACC_MENU.FEC_LOG IS 'Fecha en que el Usuario realizo la transaccion'
/
