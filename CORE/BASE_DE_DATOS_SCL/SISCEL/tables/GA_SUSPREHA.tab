CREATE TABLE GA_SUSPREHA(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  DES_SERVICIO VARCHAR2(40) NOT NULL 
 ,  IND_AUTMAN VARCHAR2(1) NOT NULL 
 ,  COD_SERVSUPL NUMBER(2,0) NULL 
 ,  COD_NIVEL NUMBER(4,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_SUSPREHA IS 'Servicios de Suspension/Rehabilitacion'
/
COMMENT ON COLUMN  GA_SUSPREHA.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_SUSPREHA.COD_SERVICIO IS 'Codigo de Servicio'
/
COMMENT ON COLUMN  GA_SUSPREHA.DES_SERVICIO IS 'Descripcion de Servicio Suplementario'
/
COMMENT ON COLUMN  GA_SUSPREHA.IND_AUTMAN IS 'Indicativo de tipo de Cargo ; (A)utomatico, (M)anual'
/
COMMENT ON COLUMN  GA_SUSPREHA.COD_SERVSUPL IS 'Codigo de Servicio Suplementario'
/
COMMENT ON COLUMN  GA_SUSPREHA.COD_NIVEL IS 'Nivel de Servicio Suplementario'
/
