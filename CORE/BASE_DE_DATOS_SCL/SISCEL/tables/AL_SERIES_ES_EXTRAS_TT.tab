CREATE TABLE AL_SERIES_ES_EXTRAS_TT(
    NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  NUM_EXTRA NUMBER(9,0) NOT NULL 
 ,  NUM_LINEA NUMBER(10,0) NOT NULL 
 ,  COD_ESTADO NUMBER(2,0) NOT NULL 
 ,  MSG_ERROR VARCHAR2(100) NULL 
 ,  IND_PROCESO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NULL 
 ) TABLESPACE    INVENTARIO_TTAB
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_SERIES_ES_EXTRAS_TT IS '[ SEST ] Tabla temporal para el proceso de entradas o salidas extras'
/
COMMENT ON COLUMN  AL_SERIES_ES_EXTRAS_TT.NUM_SERIE IS 'Numero de Serie'
/
COMMENT ON COLUMN  AL_SERIES_ES_EXTRAS_TT.NUM_EXTRA IS 'Numero de Proceso de Entrada, Salida Extra'
/
COMMENT ON COLUMN  AL_SERIES_ES_EXTRAS_TT.NUM_LINEA IS 'Numero de Linea del Proceso'
/
COMMENT ON COLUMN  AL_SERIES_ES_EXTRAS_TT.COD_ESTADO IS 'Estado de error de la serie'
/
COMMENT ON COLUMN  AL_SERIES_ES_EXTRAS_TT.MSG_ERROR IS 'Descripción del error'
/
COMMENT ON COLUMN  AL_SERIES_ES_EXTRAS_TT.IND_PROCESO IS 'Tipo de Archivo a Procesar . 0: Archivo unico de proceso ; 1: Primer archivo de proceso; 2: Segundo archivo de proceso'
/
COMMENT ON COLUMN  AL_SERIES_ES_EXTRAS_TT.COD_ARTICULO IS 'Codigo de Articulo'
/
