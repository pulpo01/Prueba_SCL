CREATE TABLE GA_DETSINIE(
    NUM_SINIESTRO NUMBER(8,0) NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) NOT NULL 
 ,  FEC_DETALLE DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  OBS_DETALLE VARCHAR2(255) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_DETSINIE IS 'Detalles de Siniestro de Abonados de Producto'
/
COMMENT ON COLUMN  GA_DETSINIE.NUM_SINIESTRO IS 'Numero de Siniestro'
/
COMMENT ON COLUMN  GA_DETSINIE.COD_ESTADO IS 'Codigo de Estado del Siniestro'
/
COMMENT ON COLUMN  GA_DETSINIE.FEC_DETALLE IS 'Fecha Detalle Siniestro'
/
COMMENT ON COLUMN  GA_DETSINIE.NOM_USUARORA IS 'Usuario Oracle que Realiza el Detalle'
/
COMMENT ON COLUMN  GA_DETSINIE.OBS_DETALLE IS 'Observaciones del Detalle del Siniestro'
/
