CREATE TABLE AL_TECNOLOGIA(
    COD_TECNOLOGIA VARCHAR2(7) NOT NULL 
 ,  DES_TECNOLOGIA VARCHAR2(20) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTIMOD DATE NOT NULL 
 ,  COD_GRUPO VARCHAR2(7) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_TECNOLOGIA IS 'Tecnologias existentes'
/
COMMENT ON COLUMN  AL_TECNOLOGIA.COD_TECNOLOGIA IS 'Codigo de la tecnologia'
/
COMMENT ON COLUMN  AL_TECNOLOGIA.DES_TECNOLOGIA IS 'Descripcion'
/
COMMENT ON COLUMN  AL_TECNOLOGIA.NOM_USUARIO IS 'Usuario que realiza el ingreso/modificación de la tecnologia'
/
COMMENT ON COLUMN  AL_TECNOLOGIA.FEC_ULTIMOD IS 'Fecha en la que se realizó la  última inserción/modificación de la tecnologia'
/
COMMENT ON COLUMN  AL_TECNOLOGIA.COD_GRUPO IS 'CODIGO DE GRUPO'
/
