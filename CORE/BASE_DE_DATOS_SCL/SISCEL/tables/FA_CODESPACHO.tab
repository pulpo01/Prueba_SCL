CREATE TABLE FA_CODESPACHO(
    COD_DESPACHO VARCHAR2(5) NOT NULL 
 ,  DESC_DESPACHO VARCHAR2(30) NOT NULL 
 ,  COD_GENARCH VARCHAR2(2) NOT NULL 
 ,  COD_PRIORIDAD NUMBER(3,0) NOT NULL 
 ,  IND_VENTAS CHAR(1) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  FEC_ULTMOD DATE NULL 
 ,  IND_CORRESP CHAR(1) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_CODESPACHO IS 'Codigo de despacho facturacion'
/
COMMENT ON COLUMN  FA_CODESPACHO.COD_DESPACHO IS 'Codigo de despacho'
/
COMMENT ON COLUMN  FA_CODESPACHO.DESC_DESPACHO IS 'Descripcion secuencia despacho'
/
COMMENT ON COLUMN  FA_CODESPACHO.COD_GENARCH IS 'Codigo archivo asociado'
/
COMMENT ON COLUMN  FA_CODESPACHO.COD_PRIORIDAD IS 'Prioridad asignacion'
/
COMMENT ON COLUMN  FA_CODESPACHO.IND_VENTAS IS 'Indicador asignacion en ventas (1/0)'
/
COMMENT ON COLUMN  FA_CODESPACHO.IND_CORRESP IS 'Indicador de asignacion en correspondencia = 1 | 0 (por defecto, 0).'
/
