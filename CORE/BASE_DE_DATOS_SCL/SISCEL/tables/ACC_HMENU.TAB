CREATE TABLE ACC_HMENU(
    COD_MENU VARCHAR2(30) NOT NULL 
 ,  COD_MENU_PADRE VARCHAR2(30) NOT NULL 
 ,  COD_APLI NUMBER(4,0) NOT NULL 
 ,  GLS_MENU VARCHAR2(50) NULL 
 ,  FLG_TIPOPROG NUMBER(4,0) NULL 
 ,  FLG_PADRE NUMBER(4,0) NULL 
 ,  USER_LOG_ORIG VARCHAR2(10) NOT NULL 
 ,  FEC_LOG_ORIG DATE NOT NULL 
 ,  USER_LOG VARCHAR2(10) NOT NULL 
 ,  FEC_LOG DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  ACC_HMENU.COD_MENU IS 'Codigo de la opcion de menu'
/
COMMENT ON COLUMN  ACC_HMENU.COD_MENU_PADRE IS 'Codigo de la opcion de menu padre'
/
COMMENT ON COLUMN  ACC_HMENU.COD_APLI IS 'Codigo de la aplicacion'
/
COMMENT ON COLUMN  ACC_HMENU.GLS_MENU IS 'Descripcion de la opcion del menu'
/
COMMENT ON COLUMN  ACC_HMENU.FLG_TIPOPROG IS 'Flag que indica el tipo de programas a ejecutar'
/
COMMENT ON COLUMN  ACC_HMENU.FLG_PADRE IS 'Flag indicador de opcion de ejecucion'
/
COMMENT ON COLUMN  ACC_HMENU.USER_LOG_ORIG IS 'Codigo del usuario que realizo la transaccion original'
/
COMMENT ON COLUMN  ACC_HMENU.FEC_LOG_ORIG IS 'Fecha en que realizo la transaccion original'
/
COMMENT ON COLUMN  ACC_HMENU.USER_LOG IS 'Codigo del usuario logeado en el Sistema'
/
COMMENT ON COLUMN  ACC_HMENU.FEC_LOG IS 'Fecha en que el Usuario realizo la transaccion'
/
