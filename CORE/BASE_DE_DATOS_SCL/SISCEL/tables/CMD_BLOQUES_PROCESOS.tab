CREATE TABLE CMD_BLOQUES_PROCESOS(
    COD_BLOQUE VARCHAR2(15) NOT NULL 
 ,  COD_PROCESO VARCHAR2(15) NOT NULL 
 ,  ORD_PROCESO NUMBER(2,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMD_BLOQUES_PROCESOS IS 'Definicion De Procesos Por Bloque.'
/
COMMENT ON COLUMN  CMD_BLOQUES_PROCESOS.COD_BLOQUE IS 'Codigo de Bloque.'
/
COMMENT ON COLUMN  CMD_BLOQUES_PROCESOS.COD_PROCESO IS 'Codigo de Proceso.'
/
COMMENT ON COLUMN  CMD_BLOQUES_PROCESOS.ORD_PROCESO IS 'Orden de Ejecucion de Proceso dentro del bloque..'
/
COMMENT ON COLUMN  CMD_BLOQUES_PROCESOS.FEC_ULTMOD IS 'Fecha de ultima modificacion del registro..'
/
COMMENT ON COLUMN  CMD_BLOQUES_PROCESOS.NOM_USUARIO IS 'Usuario que hizo la insercion del registro en la tabla..'
/
