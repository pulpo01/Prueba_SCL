CREATE TABLE CMD_LOCALES_AUTORIZADOS(
    NUM_LOCAL NUMBER(6,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_COMISIONISTA NUMBER(6,0) NOT NULL 
 ,  DES_SUCURSAL VARCHAR2(60) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMD_LOCALES_AUTORIZADOS IS 'Locales Autorizados Para Bono Por Presencia  (Sellos Verdes).'
/
COMMENT ON COLUMN  CMD_LOCALES_AUTORIZADOS.NUM_LOCAL IS 'Correlativo indicador del numero de local.'
/
COMMENT ON COLUMN  CMD_LOCALES_AUTORIZADOS.COD_TIPCOMIS IS 'Codigo del tipo de comisionista (canal de ventas).'
/
COMMENT ON COLUMN  CMD_LOCALES_AUTORIZADOS.COD_COMISIONISTA IS 'Codigo del comisionista (master dealer).'
/
COMMENT ON COLUMN  CMD_LOCALES_AUTORIZADOS.DES_SUCURSAL IS 'Descripcion del local o sucursal autorizada.'
/
COMMENT ON COLUMN  CMD_LOCALES_AUTORIZADOS.FEC_ULTMOD IS 'Fecha de ultima modificacion del registro.'
/
COMMENT ON COLUMN  CMD_LOCALES_AUTORIZADOS.NOM_USUARIO IS 'Nomber del usuario que hizo la ultima modificacion del registro.'
/
COMMENT ON COLUMN  CMD_LOCALES_AUTORIZADOS.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
