CREATE TABLE VE_PLANDATGRAL(
    COD_CTODTOFAC NUMBER(2,0) NOT NULL 
 ,  COD_TIPSTKPAK NUMBER(2,0) NOT NULL 
 ,  COD_CELANALOG VARCHAR2(1) NOT NULL 
 ,  COD_CELDIGITAL VARCHAR2(1) NOT NULL 
 ,  COD_BPRALFA VARCHAR2(1) NOT NULL 
 ,  COD_BPRNUM VARCHAR2(1) NOT NULL 
 ,  COD_BPRTONO VARCHAR2(1) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_PLANDATGRAL IS 'Datos Generales de Planes Comerciales y Paquetes'
/
COMMENT ON COLUMN  VE_PLANDATGRAL.COD_CTODTOFAC IS 'Codigo del CONCEPTO DESCUENTO en Facturacion'
/
COMMENT ON COLUMN  VE_PLANDATGRAL.COD_TIPSTKPAK IS 'Csdigo de TIPO de STOCK para artmculos en Paquetes'
/
COMMENT ON COLUMN  VE_PLANDATGRAL.COD_CELANALOG IS 'Tipo de Terminal CELULAR ANALOGICO'
/
COMMENT ON COLUMN  VE_PLANDATGRAL.COD_CELDIGITAL IS 'Tipo de Terminal CELULAR DIGITAL'
/
COMMENT ON COLUMN  VE_PLANDATGRAL.COD_BPRALFA IS 'Tipo de Terminal BEEPER ALFANUMERICO'
/
COMMENT ON COLUMN  VE_PLANDATGRAL.COD_BPRNUM IS 'Tipo de Terminal BEEPER NUMERICO'
/
COMMENT ON COLUMN  VE_PLANDATGRAL.COD_BPRTONO IS 'Tipo de Terminal BEEPER TONO'
/
