CREATE TABLE VE_CTOLIQ(
    COD_CTOLIQ NUMBER(6,0) NOT NULL 
 ,  DES_CTOLIQ VARCHAR2(60) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CATCOMIS NUMBER(2,0) NOT NULL 
 ,  IND_BASLIMMETFC VARCHAR2(4) NOT NULL 
 ,  COD_TIPOCUAD VARCHAR2(4) NOT NULL 
 ,  IND_TIPOCTOLIQ NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CTOLIQ IS 'Conceptos de Liquidacion'
/
COMMENT ON COLUMN  VE_CTOLIQ.COD_CTOLIQ IS 'Codigo de Concepto Liquidacion'
/
COMMENT ON COLUMN  VE_CTOLIQ.DES_CTOLIQ IS 'Descripcion Concepto Liquidacion'
/
COMMENT ON COLUMN  VE_CTOLIQ.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  VE_CTOLIQ.COD_CATCOMIS IS 'Codigo de Categoria del Comisionista'
/
COMMENT ON COLUMN  VE_CTOLIQ.IND_BASLIMMETFC IS 'Indicativo de BASE-LIMITE-META-FACT. CORREC. (0000, 1000, 0010)'
/
COMMENT ON COLUMN  VE_CTOLIQ.COD_TIPOCUAD IS 'Codigo de Tipo de Cuadrante asociado (PPMC)'
/
COMMENT ON COLUMN  VE_CTOLIQ.IND_TIPOCTOLIQ IS 'Indicativo de tipo de concepto (0 - Especial/1 - Valores/2 - Cuadrante)'
/
