CREATE TABLE CO_INTERCOBAPLI
(
  NUM_SECUENCI         NUMBER(8)                NOT NULL,
  COD_TIPDOCUM         NUMBER(2)                NOT NULL,
  COD_VENDEDOR_AGENTE  NUMBER(6)                NOT NULL,
  LETRA                VARCHAR2(1 BYTE)         NOT NULL,
  COD_CENTREMI         NUMBER(4)                NOT NULL,
  NUM_SECUREL          NUMBER(8)                NOT NULL,
  COD_TIPDOCREL        NUMBER(2)                NOT NULL,
  COD_AGENTEREL        NUMBER(6)                NOT NULL,
  LETRA_REL            VARCHAR2(1 BYTE)         NOT NULL,
  COD_CENTRREL         NUMBER(4)                NOT NULL,
  NUM_CARGO            NUMBER(9)                NOT NULL,
  NUM_FOLIO            NUMBER(9)                NOT NULL,
  IMP_PAGO             NUMBER(14,4)             NOT NULL,
  IMP_CARGO            NUMBER(14,4)             NOT NULL,
  FACTOR_COBRO         NUMBER(8,5)              NOT NULL,
  FEC_PAGO             DATE                     NOT NULL,
  COD_CLIENTE          NUMBER(8)                NOT NULL,
  COD_CICLFACT         NUMBER(6)                NOT NULL,
  IND_FACTURADO        VARCHAR2(1 BYTE),
  NUM_CUOTA            NUMBER(8),
  SEC_CUOTA            NUMBER(3),
  NUM_IDENT            VARCHAR2(20 BYTE),
  FEC_VENCIMIE         DATE,
  COD_ENTIDAD          VARCHAR2(5 BYTE),
  MARCA                NUMBER(4),
  IMP_TRAMO            NUMBER(14,4),
  PREF_PLAZA           VARCHAR2(25 BYTE)
)
TABLESPACE TS_RECYCOB_SDAT
PCTUSED    0
PCTFREE    5
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          40K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON COLUMN CO_INTERCOBAPLI.NUM_IDENT IS 'RUT.';

COMMENT ON COLUMN CO_INTERCOBAPLI.FEC_VENCIMIE IS 'Fecha Vencimiento.';

COMMENT ON COLUMN CO_INTERCOBAPLI.COD_ENTIDAD IS 'Codigo Entidad.';

COMMENT ON COLUMN CO_INTERCOBAPLI.MARCA IS 'Marca Proceso.';

COMMENT ON COLUMN CO_INTERCOBAPLI.IMP_TRAMO IS 'Importe Tramo.';

COMMENT ON COLUMN CO_INTERCOBAPLI.PREF_PLAZA IS 'Prefijo Plaza.';

COMMENT ON COLUMN CO_INTERCOBAPLI.NUM_SECUENCI IS 'Numero de Secuencia';

COMMENT ON COLUMN CO_INTERCOBAPLI.COD_TIPDOCUM IS 'Codigo tipo Documento';

COMMENT ON COLUMN CO_INTERCOBAPLI.COD_VENDEDOR_AGENTE IS 'Codigo Vendedor';

COMMENT ON COLUMN CO_INTERCOBAPLI.LETRA IS 'Letra';

COMMENT ON COLUMN CO_INTERCOBAPLI.COD_CENTREMI IS 'Codigo Centro Emisor';

COMMENT ON COLUMN CO_INTERCOBAPLI.NUM_SECUREL IS 'Numero Secuencia Relacionada';

COMMENT ON COLUMN CO_INTERCOBAPLI.COD_TIPDOCREL IS 'Tipo Documento  Relacionado';

COMMENT ON COLUMN CO_INTERCOBAPLI.COD_AGENTEREL IS 'Codigo Agente Relacionado';

COMMENT ON COLUMN CO_INTERCOBAPLI.LETRA_REL IS 'Letra Relacionada';

COMMENT ON COLUMN CO_INTERCOBAPLI.COD_CENTRREL IS 'Codigo Centro Emisor Relacionado';

