CREATE TABLE ICC_MSGMASIVO_ESTADISTICA(
    MOVIMIENTO NUMBER(9,0) NOT NULL 
 ,  COD_MODULO VARCHAR2(2) NOT NULL 
 ,  COD_ACTUACION NUMBER(5,0) NOT NULL 
 ,  FECHA DATE NOT NULL 
 ,  CORRECTOS NUMBER(7,0) NOT NULL 
 ,  ERRONEOS NUMBER(7,0) NOT NULL 
 ,  ARCHIVO VARCHAR2(21) NOT NULL 
 ,  COD_MENSAJE NUMBER(3,0) NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICC_MSGMASIVO_ESTADISTICA IS 'Estadisticas De Envios Masivos De Mensajeria.'
/
COMMENT ON COLUMN  ICC_MSGMASIVO_ESTADISTICA.MOVIMIENTO IS 'Numero de movimiento.'
/
COMMENT ON COLUMN  ICC_MSGMASIVO_ESTADISTICA.COD_MODULO IS 'Codigo de Modulo.'
/
COMMENT ON COLUMN  ICC_MSGMASIVO_ESTADISTICA.COD_ACTUACION IS 'Codigo de Actuacion.'
/
COMMENT ON COLUMN  ICC_MSGMASIVO_ESTADISTICA.FECHA IS 'Fecha.'
/
COMMENT ON COLUMN  ICC_MSGMASIVO_ESTADISTICA.CORRECTOS IS 'Envios correctos.'
/
COMMENT ON COLUMN  ICC_MSGMASIVO_ESTADISTICA.ERRONEOS IS 'Envios Erroneos.'
/
COMMENT ON COLUMN  ICC_MSGMASIVO_ESTADISTICA.ARCHIVO IS 'Nombre de archivo de carga.'
/
COMMENT ON COLUMN  ICC_MSGMASIVO_ESTADISTICA.COD_MENSAJE IS '###PENDIENTE: Departamento de Soporte-Subgerencia de Desarrollo'
/
