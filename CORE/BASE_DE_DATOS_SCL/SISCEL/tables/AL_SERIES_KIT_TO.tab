CREATE TABLE AL_SERIES_KIT_TO(
    ID_SECUENCIA VARCHAR2(3) NOT NULL 
 ,  COD_PREFIJO VARCHAR2(3) NOT NULL 
 ,  VALOR_MINIMO FLOAT(126) NULL 
 ,  VALOR_MAXIMO FLOAT(126) NULL 
 ,  LARGO_SECUENCIA NUMBER(2,0) NULL 
 ,  VALOR_ACTUAL FLOAT(126) NULL 
 ,  FEC_CREACION DATE NULL 
 ,  NOM_USUARIO VARCHAR2(20) NULL 
 ,  FEC_ACTUALIZACION DATE NULL 
 ,  COD_VIGENCIA NUMBER NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_SERIES_KIT_TO IS 'Secuencias de series KIT'
/
COMMENT ON COLUMN  AL_SERIES_KIT_TO.ID_SECUENCIA IS 'Identificador de Secuencia'
/
COMMENT ON COLUMN  AL_SERIES_KIT_TO.COD_PREFIJO IS 'Codigo de Prefijo serie'
/
COMMENT ON COLUMN  AL_SERIES_KIT_TO.VALOR_MINIMO IS 'Valor Inicial de la secuencia'
/
COMMENT ON COLUMN  AL_SERIES_KIT_TO.VALOR_MAXIMO IS 'Valor Final de la Secuencia'
/
COMMENT ON COLUMN  AL_SERIES_KIT_TO.VALOR_ACTUAL IS 'Valor actual de la secuencia'
/
COMMENT ON COLUMN  AL_SERIES_KIT_TO.FEC_CREACION IS 'Fecha de creacion de la secuencia'
/
COMMENT ON COLUMN  AL_SERIES_KIT_TO.NOM_USUARIO IS 'Nombre de usuario creador secuencia'
/
COMMENT ON COLUMN  AL_SERIES_KIT_TO.FEC_ACTUALIZACION IS 'Fecha de actulizacion de la secuencia'
/
