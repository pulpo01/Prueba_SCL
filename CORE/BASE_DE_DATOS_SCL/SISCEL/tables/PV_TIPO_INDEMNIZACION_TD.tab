CREATE TABLE PV_TIPO_INDEMNIZACION_TD(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_INDEMNIZACION NUMBER(3,0) NOT NULL 
 ,  DES_INDEMNIZACION VARCHAR2(50) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_TIPO_INDEMNIZACION_TD IS 'Tipos de Indemnizacion por termino de Contrato.'
/
COMMENT ON COLUMN  PV_TIPO_INDEMNIZACION_TD.COD_PRODUCTO IS 'Codigo del Producto.'
/
COMMENT ON COLUMN  PV_TIPO_INDEMNIZACION_TD.COD_INDEMNIZACION IS 'Codigo del Tipo de Indemnizacion.'
/
COMMENT ON COLUMN  PV_TIPO_INDEMNIZACION_TD.DES_INDEMNIZACION IS 'Nombre del Tipo de Indemnizacion.'
/
COMMENT ON COLUMN  PV_TIPO_INDEMNIZACION_TD.FEC_DESDE IS 'Fecha desde para la vigencia'
/
COMMENT ON COLUMN  PV_TIPO_INDEMNIZACION_TD.FEC_HASTA IS 'Fecha hasta para la vigencia'
/
