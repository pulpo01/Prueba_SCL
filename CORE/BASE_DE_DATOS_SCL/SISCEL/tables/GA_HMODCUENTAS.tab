CREATE TABLE GA_HMODCUENTAS(
    COD_CUENTA NUMBER(8,0) NOT NULL 
 ,  FEC_MODIFICA DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  DES_CUENTA VARCHAR2(40) NULL 
 ,  TIP_CUENTA VARCHAR2(1) NULL 
 ,  NOM_RESPONSABLE VARCHAR2(40) NULL 
 ,  COD_TIPIDENT VARCHAR2(2) NULL 
 ,  NUM_IDENT VARCHAR2(20) NULL 
 ,  COD_DIRECCION NUMBER(20,0) NULL 
 ,  TEL_CONTACTO VARCHAR2(12) NULL 
 ,  IND_SECTOR VARCHAR2(1) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_HMODCUENTAS IS 'Cuentas de Clientes'
/
COMMENT ON COLUMN  GA_HMODCUENTAS.COD_CUENTA IS 'Codigo de Cuenta'
/
COMMENT ON COLUMN  GA_HMODCUENTAS.FEC_MODIFICA IS 'Fecha de Modificacion'
/
COMMENT ON COLUMN  GA_HMODCUENTAS.NOM_USUARORA IS 'Usuario que realiza el Cambio'
/
COMMENT ON COLUMN  GA_HMODCUENTAS.FEC_HISTORICO IS 'Fecha de Historico'
/
COMMENT ON COLUMN  GA_HMODCUENTAS.DES_CUENTA IS 'Descripcion de Cuenta'
/
COMMENT ON COLUMN  GA_HMODCUENTAS.TIP_CUENTA IS 'Tipo de Cuenta (P)articular/(E)mpresa'
/
COMMENT ON COLUMN  GA_HMODCUENTAS.NOM_RESPONSABLE IS 'Nombre del Responsable'
/
COMMENT ON COLUMN  GA_HMODCUENTAS.COD_TIPIDENT IS 'Tipo de Identificacion'
/
COMMENT ON COLUMN  GA_HMODCUENTAS.NUM_IDENT IS 'Numero de Identificacion'
/
COMMENT ON COLUMN  GA_HMODCUENTAS.COD_DIRECCION IS 'Codigo de Direccion'
/
COMMENT ON COLUMN  GA_HMODCUENTAS.TEL_CONTACTO IS 'Telefono de Contacto'
/
COMMENT ON COLUMN  GA_HMODCUENTAS.IND_SECTOR IS 'Indicativo de Sector (P)ublico/P(R)ivado'
/
