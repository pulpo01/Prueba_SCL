CREATE TABLE AL_ARTICULOS_BEEPER(
    COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  COD_PROTOCOLO VARCHAR2(2) NOT NULL 
 ,  NUM_VELOCIDAD NUMBER(4,0) NOT NULL 
 ,  COD_FRECUENCIA NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_ARTICULOS_BEEPER IS 'Datos especificos de Articulos Beeper'
/
COMMENT ON COLUMN  AL_ARTICULOS_BEEPER.COD_ARTICULO IS 'Csdigo de Artmculo'
/
COMMENT ON COLUMN  AL_ARTICULOS_BEEPER.COD_PROTOCOLO IS 'Csdigo de Protocolo'
/
COMMENT ON COLUMN  AL_ARTICULOS_BEEPER.NUM_VELOCIDAD IS 'Velocidad - Retrofitted'
/
COMMENT ON COLUMN  AL_ARTICULOS_BEEPER.COD_FRECUENCIA IS 'Csdigo de frecuencia'
/
