CREATE TABLE GE_TIPCUOTAS(
    COD_CUOTA VARCHAR2(2) NOT NULL 
 ,  DES_CUOTA VARCHAR2(30) NOT NULL 
 ,  NUM_CUOTAS NUMBER(2,0) NOT NULL 
 ,  POR_INTERES NUMBER(5,2) NOT NULL 
 ,  NUM_DIAS NUMBER(2,0) NOT NULL 
 ,  IND_FORMINTERES NUMBER(1,0) DEFAULT 1 NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_TIPCUOTAS IS 'Tipos de Financiamiento'
/
COMMENT ON COLUMN  GE_TIPCUOTAS.COD_CUOTA IS 'Codigo de Financiamiento'
/
COMMENT ON COLUMN  GE_TIPCUOTAS.DES_CUOTA IS 'Descripcion de Financiamiento'
/
COMMENT ON COLUMN  GE_TIPCUOTAS.NUM_CUOTAS IS 'Numero de Cuotas'
/
COMMENT ON COLUMN  GE_TIPCUOTAS.POR_INTERES IS 'Porcentaje de Interes'
/
COMMENT ON COLUMN  GE_TIPCUOTAS.NUM_DIAS IS 'Numero de Dias entre Cuotas'
/
COMMENT ON COLUMN  GE_TIPCUOTAS.IND_FORMINTERES IS 'Indicativo de Formula de Interes; (1) Simple, (0) Compuesto'
/
