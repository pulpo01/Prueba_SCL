CREATE TABLE AL_NUMERACION_MASIVA_TO(
    NUM_NUMERACION NUMBER(9,0) NOT NULL 
 ,  LIN_NUMERACION NUMBER(3,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  IND_SERIE NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  FEC_ACTUALIZACION DATE DEFAULT SYSDATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_NUMERACION_MASIVA_TO IS 'Tabla numeracion masiva'
/
COMMENT ON COLUMN  AL_NUMERACION_MASIVA_TO.NUM_NUMERACION IS 'Numero de orden de numeracion'
/
COMMENT ON COLUMN  AL_NUMERACION_MASIVA_TO.LIN_NUMERACION IS 'Linea de orden de numeracion'
/
COMMENT ON COLUMN  AL_NUMERACION_MASIVA_TO.NUM_SERIE IS 'Numero de serie'
/
COMMENT ON COLUMN  AL_NUMERACION_MASIVA_TO.IND_SERIE IS 'Indicador de estado de Serie'
/
COMMENT ON COLUMN  AL_NUMERACION_MASIVA_TO.FEC_ACTUALIZACION IS 'Fecha de Actualizacion'
/
COMMENT ON COLUMN  AL_NUMERACION_MASIVA_TO.NOM_USUARIO IS 'Nombre de usuario'
/
