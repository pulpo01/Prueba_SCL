CREATE TABLE GA_DATOSCLIENTE_TSE_TD
(
  NUM_IDENT      VARCHAR2(20 BYTE),
  NOM_CLIENTE    VARCHAR2(50 BYTE),
  NOM_APELLIDO1  VARCHAR2(20 BYTE),
  NOM_APELLIDO2  VARCHAR2(20 BYTE),
  COD_REGION     VARCHAR2(3 BYTE),
  COD_PROVINCIA  VARCHAR2(5 BYTE),
  COD_CIUDAD     VARCHAR2(5 BYTE)
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          4M
            NEXT             4M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON COLUMN GA_DATOSCLIENTE_TSE_TD.NUM_IDENT IS 'Número de Identificación del Cliente';

COMMENT ON COLUMN GA_DATOSCLIENTE_TSE_TD.NOM_CLIENTE IS 'Nombre Cliente';

COMMENT ON COLUMN GA_DATOSCLIENTE_TSE_TD.NOM_APELLIDO1 IS 'Primer Apellido del Cliente';

COMMENT ON COLUMN GA_DATOSCLIENTE_TSE_TD.NOM_APELLIDO2 IS 'Segundo Apellido del Cliente';

COMMENT ON COLUMN GA_DATOSCLIENTE_TSE_TD.COD_REGION IS 'Codigo de Region';

COMMENT ON COLUMN GA_DATOSCLIENTE_TSE_TD.COD_PROVINCIA IS 'Codigo de Provincia';

COMMENT ON COLUMN GA_DATOSCLIENTE_TSE_TD.COD_CIUDAD IS 'Codigo de Ciudad';