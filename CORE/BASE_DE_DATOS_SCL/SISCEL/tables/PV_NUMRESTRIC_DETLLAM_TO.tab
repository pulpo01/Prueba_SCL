CREATE TABLE PV_NUMRESTRIC_DETLLAM_TO (  
   NUM_ABONADO                   NUMBER(8)         NOT NULL  
 , NUM_CELULAR                   NUMBER(15)        NOT NULL  
 , NOM_USUARORA                  VARCHAR2(30)      NOT NULL  
 , FEC_INGRESO                   DATE                  NULL
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  PV_NUMRESTRIC_DETLLAM_TO                      IS '[ PV_NUMRESTDETLLAM ] Tabla de restricciones de numeros para la consulta de llamadasTabla de restricciones de numeros para la consulta de llamadas'
/

COMMENT ON COLUMN PV_NUMRESTRIC_DETLLAM_TO.NUM_ABONADO                    IS 'numero del abonado'
/
COMMENT ON COLUMN PV_NUMRESTRIC_DETLLAM_TO.NUM_CELULAR                    IS 'numero del movil'
/
COMMENT ON COLUMN PV_NUMRESTRIC_DETLLAM_TO.NOM_USUARORA                   IS 'usuario'
/
COMMENT ON COLUMN PV_NUMRESTRIC_DETLLAM_TO.FEC_INGRESO                    IS 'fecha ingreso movil'
/

