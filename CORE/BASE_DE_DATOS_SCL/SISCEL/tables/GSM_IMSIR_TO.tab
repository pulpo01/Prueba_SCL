CREATE TABLE GSM_IMSIR_TO(
    COD_PREFIJO VARCHAR2(10) NOT NULL 
 ,  NUM_IMSIR VARCHAR2(20) NOT NULL 
 ,  COD_ESTADO VARCHAR2(1) DEFAULT 'N' NOT NULL 
 ,  FEC_CREACION DATE DEFAULT SYSDATE NOT NULL 
 ,  FEC_ACTUALIZACION DATE DEFAULT SYSDATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_IMSIR_TO IS 'Tabla de IMSIR.'
/
COMMENT ON COLUMN  GSM_IMSIR_TO.COD_PREFIJO IS 'Codigo de prefijo'
/
COMMENT ON COLUMN  GSM_IMSIR_TO.NUM_IMSIR IS 'Numero de IMSIR.'
/
COMMENT ON COLUMN  GSM_IMSIR_TO.COD_ESTADO IS 'Estado del IMSIR'
/
COMMENT ON COLUMN  GSM_IMSIR_TO.FEC_CREACION IS 'Fecha de creacion del registro'
/
COMMENT ON COLUMN  GSM_IMSIR_TO.FEC_ACTUALIZACION IS 'Fecha de la ultima actualizacion.'
/
COMMENT ON COLUMN  GSM_IMSIR_TO.COD_USUARIO IS 'Codigo del usuario que actualizo.'
/
