CREATE TABLE CRD_ESTADOSEVE(
    COD_ESTADOEVE NUMBER(2,0) NOT NULL 
 ,  DES_ESTADOEVE VARCHAR2(60) NOT NULL 
 ,  COD_TIPDOCUM NUMBER(2,0) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CRD_ESTADOSEVE IS 'Tabla de codigos de los distintos estados por los que puede pasar un evento.'
/
COMMENT ON COLUMN  CRD_ESTADOSEVE.COD_ESTADOEVE IS 'Codigo de estado de un evento.'
/
COMMENT ON COLUMN  CRD_ESTADOSEVE.DES_ESTADOEVE IS 'Descripcion de un estado de un evento.'
/
COMMENT ON COLUMN  CRD_ESTADOSEVE.COD_TIPDOCUM IS 'Se usa cuando un estado en particular es solo para un tipo de documento.'
/
