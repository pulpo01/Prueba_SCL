CREATE TABLE AL_ESTADO_TRASPASO_TO
(
  NUM_TRASPASO     NUMBER(9)                    NOT NULL,
  ESTADO_TRASPASO  VARCHAR2(25 BYTE)            NOT NULL,
  MENSAJE_ERROR    VARCHAR2(100 BYTE)
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          1M
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING
/
COMMENT ON COLUMN AL_ESTADO_TRASPASO_TO.NUM_TRASPASO IS 'Numero de Traspaso'
/
COMMENT ON COLUMN AL_ESTADO_TRASPASO_TO.ESTADO_TRASPASO IS 'Estado del Traspaso'
/
COMMENT ON COLUMN AL_ESTADO_TRASPASO_TO.MENSAJE_ERROR IS 'Mensaje de Error del Traspaso'
/