CREATE TABLE AL_INGRESO_EXTERNO(
    ID NUMBER(10,0) NOT NULL 
 ,  NOM_ARCHIVO VARCHAR2(34) NOT NULL 
 ,  CODIGO_ESTADO VARCHAR2(20) NOT NULL 
 ,  FEC_ESTADO DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  NUM_PROCESO NUMBER(9,0) NULL 
 ,  PATH_PROCESO VARCHAR2(60) NULL 
 ,  CNT_TOTAL NUMBER(9,0) NULL 
 ,  CNT_PROCESADO NUMBER(9,0) NULL 
 ,  IND_ERROR NUMBER(1,0) DEFAULT 0 NULL 
 ,  NOM_ARCH_ERROR VARCHAR2(34) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  AL_INGRESO_EXTERNO.ID IS 'IDENTIFICADOR DE ARCHIVOS PARA PROCESO'
/
COMMENT ON COLUMN  AL_INGRESO_EXTERNO.NOM_ARCHIVO IS 'NOMBRE DE ARCHIVO PARA PROCESO'
/
COMMENT ON COLUMN  AL_INGRESO_EXTERNO.CODIGO_ESTADO IS 'ESTADO DEL PROCESO'
/
COMMENT ON COLUMN  AL_INGRESO_EXTERNO.FEC_ESTADO IS 'FECHA DE PROCESO'
/
COMMENT ON COLUMN  AL_INGRESO_EXTERNO.NOM_USUARIO IS 'USUARIO ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  AL_INGRESO_EXTERNO.NUM_PROCESO IS 'NUMERO DE PROCESO'
/
COMMENT ON COLUMN  AL_INGRESO_EXTERNO.PATH_PROCESO IS 'PATH CUENTA LOCAL DE ARCHIVOS'
/
COMMENT ON COLUMN  AL_INGRESO_EXTERNO.CNT_TOTAL IS 'CANTIDAD DE REGISTROS DENTRO DEL ARCHIVO DETALLE'
/
COMMENT ON COLUMN  AL_INGRESO_EXTERNO.CNT_PROCESADO IS 'CANTIDAD DE REGISTROS PROCESADOS'
/
COMMENT ON COLUMN  AL_INGRESO_EXTERNO.IND_ERROR IS 'INDICA ERROR EN PROCESAMIENTO DE REGISTROS'
/
COMMENT ON COLUMN  AL_INGRESO_EXTERNO.NOM_ARCH_ERROR IS 'NOMBRE DE ARCHIVO DE ERROR'
/
