CREATE TABLE CO_PAGOS_NA
(
  COD_MOTIVO_NA        NUMBER(2)                NOT NULL,
  COD_TIPDOCUM         NUMBER(2)                NOT NULL,
  COD_CENTREMI         NUMBER(4)                NOT NULL,
  NUM_SECUENCI         NUMBER(8)                NOT NULL,
  COD_VENDEDOR_AGENTE  NUMBER(6)                NOT NULL,
  LETRA                VARCHAR2(1 BYTE)         NOT NULL,
  COD_CLIENTE          NUMBER(8),
  IMP_PAGO             NUMBER(14,4)             NOT NULL,
  FEC_EFECTIVIDAD      DATE                     NOT NULL,
  COD_CAJA             NUMBER(4),
  COD_OFICINA          VARCHAR2(2 BYTE),
  FEC_VALOR            DATE                     NOT NULL,
  NOM_USUARORA         VARCHAR2(30 BYTE)        NOT NULL,
  COD_FORPAGO          NUMBER(1)                NOT NULL,
  COD_SISPAGO          NUMBER(2)                NOT NULL,
  COD_ORIPAGO          NUMBER(2)                NOT NULL,
  COD_CAUPAGO          NUMBER(2)                NOT NULL,
  COD_BANCO            VARCHAR2(15 BYTE),
  COD_TIPTARJETA       VARCHAR2(3 BYTE),
  COD_SUCURSAL         VARCHAR2(4 BYTE),
  CTA_CORRIENTE        VARCHAR2(18 BYTE),
  NUM_TARJETA          VARCHAR2(20 BYTE),
  DES_PAGO             VARCHAR2(150 BYTE),
  NUM_IDENT            VARCHAR2(20 BYTE)        NOT NULL,
  NUM_COMPAGO          NUMBER(12),
  PREF_PLAZA           VARCHAR2(25 BYTE),
  NUM_EJERCICIO        VARCHAR2(12 BYTE),
  COD_OPERADORA_SCL    VARCHAR2(5 BYTE),
  COD_PLAZA            VARCHAR2(5 BYTE)
)
TABLESPACE TS_RECYCOB_SDAT
PCTUSED    0
PCTFREE    5
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          40K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE CO_PAGOS_NA IS 'PAGOS NO APLICADOS';

COMMENT ON COLUMN CO_PAGOS_NA.COD_MOTIVO_NA IS 'Csdigo del motivo del pago no aplicado';

COMMENT ON COLUMN CO_PAGOS_NA.COD_TIPDOCUM IS 'TIPO DE DOCUMENTO';

COMMENT ON COLUMN CO_PAGOS_NA.COD_CENTREMI IS 'CODIGO DEL CENTRO EMISOR';

COMMENT ON COLUMN CO_PAGOS_NA.NUM_SECUENCI IS 'NUMERO DE SECUENCIA';

COMMENT ON COLUMN CO_PAGOS_NA.COD_VENDEDOR_AGENTE IS 'CODIGO DE AGENTE';

COMMENT ON COLUMN CO_PAGOS_NA.LETRA IS 'LETRA';

COMMENT ON COLUMN CO_PAGOS_NA.COD_CLIENTE IS 'CODIGO DE CLIENTE';

COMMENT ON COLUMN CO_PAGOS_NA.IMP_PAGO IS 'IMPORTE DEL PAGO';

COMMENT ON COLUMN CO_PAGOS_NA.FEC_EFECTIVIDAD IS 'FECHA DE EFECTIVIDAD';

COMMENT ON COLUMN CO_PAGOS_NA.COD_CAJA IS 'Codigo de Caja - Retrofitted';

COMMENT ON COLUMN CO_PAGOS_NA.COD_OFICINA IS 'Codigo de oficina';

COMMENT ON COLUMN CO_PAGOS_NA.FEC_VALOR IS 'FECHA VALOR';

COMMENT ON COLUMN CO_PAGOS_NA.NOM_USUARORA IS 'NOMBRE DE USUARIO';

COMMENT ON COLUMN CO_PAGOS_NA.COD_FORPAGO IS 'FORMA DE PAGO';

COMMENT ON COLUMN CO_PAGOS_NA.COD_SISPAGO IS 'CODIGO DE SISTEMA DE PAGO';

COMMENT ON COLUMN CO_PAGOS_NA.COD_ORIPAGO IS 'CODIGO DE ORIGEN DE PAGO';

COMMENT ON COLUMN CO_PAGOS_NA.COD_CAUPAGO IS 'CODIGO DE CAUSA DE PAGO';

COMMENT ON COLUMN CO_PAGOS_NA.COD_BANCO IS 'CODIGO DE BANCO';

COMMENT ON COLUMN CO_PAGOS_NA.COD_TIPTARJETA IS 'TIPO DE TARJETA';

COMMENT ON COLUMN CO_PAGOS_NA.COD_SUCURSAL IS 'SUCURSAL';

COMMENT ON COLUMN CO_PAGOS_NA.CTA_CORRIENTE IS 'CUENTA CORRIENTE';

COMMENT ON COLUMN CO_PAGOS_NA.NUM_TARJETA IS 'NUMERO DE TARJETA';

COMMENT ON COLUMN CO_PAGOS_NA.DES_PAGO IS 'DESCRIPCION DEL PAGO';

COMMENT ON COLUMN CO_PAGOS_NA.NUM_COMPAGO IS 'Numero de comprobante de pago';

COMMENT ON COLUMN CO_PAGOS_NA.PREF_PLAZA IS 'Codigo de prefijo plaza';

COMMENT ON COLUMN CO_PAGOS_NA.NUM_EJERCICIO IS 'Numero de Ejercicio';

COMMENT ON COLUMN CO_PAGOS_NA.COD_OPERADORA_SCL IS 'Codigo Operadora';

COMMENT ON COLUMN CO_PAGOS_NA.COD_PLAZA IS 'Plaza';

