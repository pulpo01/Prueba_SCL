CREATE TABLE FAD_IMPSUBGRUPOS(
    COD_SUBGRUPO NUMBER(4,0) NOT NULL 
 ,  DES_SUBGRUPO VARCHAR2(40) NULL 
 ,  DES_GLOSA VARCHAR2(50) NULL 
 ,  COD_GRUPO NUMBER(3,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(5,0) NOT NULL 
 ,  COD_TIPSUBGRUPO NUMBER(2,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  FEC_ULTMOD DATE NULL 
 ,  COD_REGISTRO VARCHAR2(5) NULL 
 ,  CRIT_ORDEN VARCHAR2(50) NULL 
 ,  COD_TIPLLAMADA NUMBER(2,0) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAD_IMPSUBGRUPOS IS 'Sub-Grupos De Impresion.'
/
COMMENT ON COLUMN  FAD_IMPSUBGRUPOS.COD_SUBGRUPO IS 'Codigo de subgrupo de impresion..'
/
COMMENT ON COLUMN  FAD_IMPSUBGRUPOS.DES_SUBGRUPO IS 'Descripcion del codigo de subgrupo de impresion..'
/
COMMENT ON COLUMN  FAD_IMPSUBGRUPOS.DES_GLOSA IS 'Glosa que aparecera fisicamente en el formulario..'
/
COMMENT ON COLUMN  FAD_IMPSUBGRUPOS.COD_GRUPO IS 'Codigo de grupo al cual va asociado el subgrupo..'
/
COMMENT ON COLUMN  FAD_IMPSUBGRUPOS.NUM_ORDEN IS 'Orden de aparicion del subgrupo dentro del bloque correspondiente al grupo asociado..'
/
COMMENT ON COLUMN  FAD_IMPSUBGRUPOS.COD_TIPSUBGRUPO IS 'Codigo de Grupo..'
/
COMMENT ON COLUMN  FAD_IMPSUBGRUPOS.NOM_USUARIO IS 'ultimo usuario que realizo algun cambio en el registro..'
/
COMMENT ON COLUMN  FAD_IMPSUBGRUPOS.FEC_ULTMOD IS 'Fecha de ultima modificacion..'
/
COMMENT ON COLUMN  FAD_IMPSUBGRUPOS.COD_REGISTRO IS 'CODIGO DE REGISTRO      '
/
COMMENT ON COLUMN  FAD_IMPSUBGRUPOS.CRIT_ORDEN IS 'CRITERIO DE ORDENAMIENTO'
/
COMMENT ON COLUMN  FAD_IMPSUBGRUPOS.COD_TIPLLAMADA IS 'TIPO DE LLAMADA         '
/
