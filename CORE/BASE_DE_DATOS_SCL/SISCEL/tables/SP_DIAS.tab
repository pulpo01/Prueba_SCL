CREATE TABLE SP_DIAS(
    NUM_DIA DATE NOT NULL 
 ,  IND_HABIL NUMBER(1,0) NOT NULL 
 ,  TIP_HORARIO NUMBER(2,0) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SP_DIAS IS 'Dias del anio laboral y sus horarios'
/
COMMENT ON COLUMN  SP_DIAS.NUM_DIA IS 'Numero de dia del a?o para el que se establece tipo de dia (habil o no) y tipo de horario'
/
COMMENT ON COLUMN  SP_DIAS.IND_HABIL IS 'indicativo de si es dia habil o no(1=si, 0=no)'
/
COMMENT ON COLUMN  SP_DIAS.TIP_HORARIO IS 'tipo de horario del dia en caso de ser habil'
/
