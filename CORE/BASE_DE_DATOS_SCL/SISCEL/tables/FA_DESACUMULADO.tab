CREATE TABLE FA_DESACUMULADO(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  DES_TOTALPROD VARCHAR2(60) NOT NULL 
 ,  DES_ACUMCARGO VARCHAR2(60) NOT NULL 
 ,  DES_ACUMIVA VARCHAR2(60) NOT NULL 
 ,  DES_ACUMDTO VARCHAR2(60) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_DESACUMULADO IS 'Descripcion para los acumulados por producto- Retrofitted'
/
COMMENT ON COLUMN  FA_DESACUMULADO.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  FA_DESACUMULADO.DES_TOTALPROD IS 'Descripcion del Total Producto'
/
COMMENT ON COLUMN  FA_DESACUMULADO.DES_ACUMCARGO IS 'Descripcion para acumulado de cargos'
/
COMMENT ON COLUMN  FA_DESACUMULADO.DES_ACUMIVA IS 'Descripcion para acumulado de Impuestos'
/
COMMENT ON COLUMN  FA_DESACUMULADO.DES_ACUMDTO IS 'Descripcion para acumulado de descuentos'
/
