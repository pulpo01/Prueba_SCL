CREATE TABLE ICH_CARGAS_AKEY(
    ID_CARGA NUMBER(5,0) NOT NULL 
 ,  FEC_CARGA DATE NULL 
 ,  NOM_USUARIO VARCHAR2(15) NULL 
 ,  NOM_ARCHIVO VARCHAR2(50) NULL 
 ,  FEC_ARCHIVO DATE NULL 
 ,  COD_FABRICANTE NUMBER(6,0) NULL 
 ,  COD_MODELO VARCHAR2(15) NULL 
 ,  CANTIDAD_SERIES NUMBER(8,0) NULL 
 ,  NUM_FACTURA NUMBER(8,0) NULL 
 ,  OBSERVACIONES VARCHAR2(200) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICH_CARGAS_AKEY IS 'Ich_Cargas_Akey.'
/
COMMENT ON COLUMN  ICH_CARGAS_AKEY.ID_CARGA IS 'Codigo de identificacion de la carga.'
/
COMMENT ON COLUMN  ICH_CARGAS_AKEY.FEC_CARGA IS 'Fecha de la carga.'
/
COMMENT ON COLUMN  ICH_CARGAS_AKEY.NOM_USUARIO IS 'Usuario responsable.'
/
COMMENT ON COLUMN  ICH_CARGAS_AKEY.NOM_ARCHIVO IS 'Nombre del archivo cargado.'
/
COMMENT ON COLUMN  ICH_CARGAS_AKEY.FEC_ARCHIVO IS 'Fecha de recepcion del archivo cargado.'
/
COMMENT ON COLUMN  ICH_CARGAS_AKEY.COD_FABRICANTE IS 'Codigo del proveedor de las series.'
/
COMMENT ON COLUMN  ICH_CARGAS_AKEY.COD_MODELO IS 'Codigo del modelo de las series (ind_equiacc = E y cod_fabricante=cod_fabricante).'
/
COMMENT ON COLUMN  ICH_CARGAS_AKEY.CANTIDAD_SERIES IS 'Cantidad de series cargadas.'
/
COMMENT ON COLUMN  ICH_CARGAS_AKEY.NUM_FACTURA IS 'Numero de factura para referencia.'
/
COMMENT ON COLUMN  ICH_CARGAS_AKEY.OBSERVACIONES IS 'Observaciones.'
/
