 ,  COD_ANOMALIA NUMBER(5,0) NOT NULL 
 ,  DESCRIPCION VARCHAR2(255) NOT NULL 
 ,  IND_REGULARIZA NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_DET_ANOMALIAS_TO IS '[ ANOM ] Anomalias de Pagos DealerAnomalias de Pagos Dealer'
/
