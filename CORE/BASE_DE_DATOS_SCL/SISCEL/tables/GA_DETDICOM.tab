CREATE TABLE GA_DETDICOM(
    NUM_CONSULTA NUMBER(8,0) NOT NULL 
 ,  TIP_DOCUMENTO VARCHAR2(2) NOT NULL 
 ,  TIP_MOVIMIENTO VARCHAR2(1) NULL 
 ,  TIP_MOTIVO VARCHAR2(1) NULL 
 ,  COD_TOMO VARCHAR2(1) NULL 
 ,  COD_MONEDA VARCHAR2(2) NOT NULL 
 ,  NUM_BOLETIN NUMBER(4,0) NOT NULL 
 ,  NUM_PAGINA NUMBER(3,0) NOT NULL 
 ,  FEC_BOLETIN DATE NOT NULL 
 ,  IMP_MONTO NUMBER(14,4) NOT NULL 
 ,  DES_BANCO VARCHAR2(18) NOT NULL 
 ,  NUM_ORDEN NUMBER(4,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_DETDICOM IS 'Detalles de Protestos de consultas del dicom'
/
COMMENT ON COLUMN  GA_DETDICOM.NUM_CONSULTA IS 'numero de consulta'
/
COMMENT ON COLUMN  GA_DETDICOM.TIP_DOCUMENTO IS 'tipo de documento'
/
COMMENT ON COLUMN  GA_DETDICOM.TIP_MOVIMIENTO IS 'tipo de movimientos'
/
COMMENT ON COLUMN  GA_DETDICOM.TIP_MOTIVO IS 'motivo de publicacion'
/
COMMENT ON COLUMN  GA_DETDICOM.COD_TOMO IS 'tomo'
/
COMMENT ON COLUMN  GA_DETDICOM.COD_MONEDA IS 'tipos de moneda'
/
COMMENT ON COLUMN  GA_DETDICOM.NUM_BOLETIN IS 'numero de boletin comercial'
/
COMMENT ON COLUMN  GA_DETDICOM.NUM_PAGINA IS 'pagina de boletin comercial'
/
COMMENT ON COLUMN  GA_DETDICOM.FEC_BOLETIN IS 'fecha del boletin comercial'
/
COMMENT ON COLUMN  GA_DETDICOM.IMP_MONTO IS 'monto'
/
COMMENT ON COLUMN  GA_DETDICOM.DES_BANCO IS 'banco o librador'
/
COMMENT ON COLUMN  GA_DETDICOM.NUM_ORDEN IS 'numero de orden de protesto  para la consulta'
/
