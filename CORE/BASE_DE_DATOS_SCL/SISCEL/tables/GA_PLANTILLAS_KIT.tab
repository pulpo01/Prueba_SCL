CREATE TABLE GA_PLANTILLAS_KIT(
    COD_KIT NUMBER(6,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  CARGA_INICIAL NUMBER(14,4) NOT NULL 
 ,  FEC_INICIO DATE NOT NULL 
 ,  FEC_TERMINO DATE NOT NULL 
 ,  DIAS_VIGENCIA NUMBER(3,0) DEFAULT 30 NOT NULL 
 ,  COD_USUA VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_PLANTILLAS_KIT IS 'Matriz De Cargas Iniciales De Kit Prepago.'
/
COMMENT ON COLUMN  GA_PLANTILLAS_KIT.COD_KIT IS 'Codigo de Articulo KIT.'
/
COMMENT ON COLUMN  GA_PLANTILLAS_KIT.COD_ARTICULO IS 'Codigo de Articulo de composicion.'
/
COMMENT ON COLUMN  GA_PLANTILLAS_KIT.CARGA_INICIAL IS 'Monto de carga inicial de los equipos del KIT.'
/
COMMENT ON COLUMN  GA_PLANTILLAS_KIT.FEC_INICIO IS 'Fecha Inicio de vigencia de Carga del KIT.'
/
COMMENT ON COLUMN  GA_PLANTILLAS_KIT.FEC_TERMINO IS 'Fecha Termino de vigencia de Carga del KIT.'
/
COMMENT ON COLUMN  GA_PLANTILLAS_KIT.DIAS_VIGENCIA IS 'Dias de vigencia de la Carga.'
/
COMMENT ON COLUMN  GA_PLANTILLAS_KIT.COD_USUA IS 'Usuario de modificacion del registro.'
/
COMMENT ON COLUMN  GA_PLANTILLAS_KIT.FEC_ULTMOD IS 'Fecha de ultima modificacion del registro.'
/
