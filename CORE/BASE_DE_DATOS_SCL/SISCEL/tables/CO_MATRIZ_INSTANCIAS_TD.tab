CREATE TABLE CO_MATRIZ_INSTANCIAS_TD(
    VAL_MIN NUMBER(3,0) NOT NULL 
 ,  VAL_MAX NUMBER(3,0) NOT NULL 
 ,  PORC_DESC NUMBER(3,0) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  CO_MATRIZ_INSTANCIAS_TD.VAL_MIN IS 'Valor Minimo en rango de % de cpu'
/
COMMENT ON COLUMN  CO_MATRIZ_INSTANCIAS_TD.VAL_MAX IS 'Valor Maximo en rango de % de cpu '
/
COMMENT ON COLUMN  CO_MATRIZ_INSTANCIAS_TD.PORC_DESC IS 'Porcentaje de descuento a utilizar en formula de calculo de instancias'
/
