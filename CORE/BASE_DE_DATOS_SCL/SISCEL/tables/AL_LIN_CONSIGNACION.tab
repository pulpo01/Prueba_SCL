CREATE TABLE AL_LIN_CONSIGNACION(
    NUM_LISTA NUMBER(6,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  PRC_ARTICULO NUMBER(14,4) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_LIN_CONSIGNACION IS 'Detalle de Listas de Precios de Articulos en Consignacion'
/
COMMENT ON COLUMN  AL_LIN_CONSIGNACION.PRC_ARTICULO IS 'Precio'
/
COMMENT ON COLUMN  AL_LIN_CONSIGNACION.NUM_LISTA IS 'Numero de lista'
/
COMMENT ON COLUMN  AL_LIN_CONSIGNACION.COD_ARTICULO IS 'Codigo de articulo'
/
