CREATE TABLE VE_MODCATEGORIAS(
    COD_CATEGORIA VARCHAR2(3) NOT NULL 
 ,  DES_CATEGORIA VARCHAR2(30) NOT NULL 
 ,  IND_ARTICPLAN NUMBER(1,0) NOT NULL 
 ,  IND_NIVELCAT NUMBER(1,0) NOT NULL 
 ,  FEC_MODIFICAC DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_MODCATEGORIAS IS 'Modificacion de las Categorias de Articulos, Planes Tarifarios y Sucursales'
/
COMMENT ON COLUMN  VE_MODCATEGORIAS.COD_CATEGORIA IS 'Codigo de Categoria'
/
COMMENT ON COLUMN  VE_MODCATEGORIAS.DES_CATEGORIA IS 'Descripcion de Categoria'
/
COMMENT ON COLUMN  VE_MODCATEGORIAS.IND_ARTICPLAN IS 'Indicativo de Articulo (0), Plan Tarifario (1) o Sucursal (2)'
/
COMMENT ON COLUMN  VE_MODCATEGORIAS.IND_NIVELCAT IS 'Indicativo de Nivel de la categoria'
/
COMMENT ON COLUMN  VE_MODCATEGORIAS.FEC_MODIFICAC IS 'Fecha de la modificacion'
/
COMMENT ON COLUMN  VE_MODCATEGORIAS.NOM_USUARIO IS 'Usuario que realizo la modificacion'
/
