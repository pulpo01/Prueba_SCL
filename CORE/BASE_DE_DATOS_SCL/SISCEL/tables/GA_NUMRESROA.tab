CREATE TABLE GA_NUMRESROA(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  COD_PAIS VARCHAR2(3) NOT NULL 
 ,  COD_ZONA VARCHAR2(2) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_NUMRESROA IS 'Numeracion Reservada a Roamers Permanentes'
/
COMMENT ON COLUMN  GA_NUMRESROA.NUM_ABONADO IS 'Numero de Abonado'
/
COMMENT ON COLUMN  GA_NUMRESROA.NUM_CELULAR IS 'Numero Celular'
/
COMMENT ON COLUMN  GA_NUMRESROA.COD_OPERADOR IS 'Codigo de Operador'
/
COMMENT ON COLUMN  GA_NUMRESROA.COD_PAIS IS 'Codigo de Pais'
/
COMMENT ON COLUMN  GA_NUMRESROA.COD_ZONA IS 'Codigo de Zona'
/
