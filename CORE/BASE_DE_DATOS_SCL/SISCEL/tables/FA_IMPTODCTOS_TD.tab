CREATE TABLE FA_IMPTODCTOS_TD(
    COD_CONCEPTO NUMBER(8,0) NOT NULL 
 ,  COD_ZONACARGO VARCHAR2(10) NOT NULL 
 ,  TIP_ZONACARGO NUMBER(1,0) NOT NULL 
 ,  TIP_EVALUACION NUMBER(1,0) NOT NULL 
 ,  COD_GRPSERVI NUMBER(3,0) NOT NULL 
 ,  COD_REGION VARCHAR2(5) NOT NULL 
 ,  COD_PROVINCIA VARCHAR2(5) DEFAULT ' ' NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  IMP_FACTURABLE NUMBER(14,4) NULL 
 ,  TIP_VALOR CHAR(1) NULL 
 ,  FEC_HASTA DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  COD_CATIMPOS NUMBER(2,0) DEFAULT 1 NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_IMPTODCTOS_TD IS 'Configuración de impuestos a documentos'
/
COMMENT ON COLUMN  FA_IMPTODCTOS_TD.COD_CONCEPTO IS 'Código del concepto de impuesto a generar'
/
COMMENT ON COLUMN  FA_IMPTODCTOS_TD.COD_ZONACARGO IS 'Código de la zona de cargo afecta al impuesto '
/
COMMENT ON COLUMN  FA_IMPTODCTOS_TD.TIP_ZONACARGO IS 'Tipo de zona de cargo: 0: zona impositiva; 1: provincia; 2:region; 3: ciudad ; 4:comuna'
/
COMMENT ON COLUMN  FA_IMPTODCTOS_TD.TIP_EVALUACION IS 'Tipo de evaluación del impuesto : 0: dirección del cliente y el abonado; 1: solo la del cliente; 2: solo la del abonado; 3: ninguna'
/
COMMENT ON COLUMN  FA_IMPTODCTOS_TD.COD_GRPSERVI IS 'Código del grupo de servicio de los conceptos afecto al impuesto al documento'
/
COMMENT ON COLUMN  FA_IMPTODCTOS_TD.COD_REGION IS 'Código de región a la que pertenece la zona de cargo'
/
COMMENT ON COLUMN  FA_IMPTODCTOS_TD.COD_PROVINCIA IS 'Código de provincia a la que pertenece la zona de cargo'
/
COMMENT ON COLUMN  FA_IMPTODCTOS_TD.FEC_DESDE IS 'Fecha de inicio del período de vigencia del impuesto'
/
COMMENT ON COLUMN  FA_IMPTODCTOS_TD.IMP_FACTURABLE IS 'Monto del importe del impuesto al documento'
/
COMMENT ON COLUMN  FA_IMPTODCTOS_TD.TIP_VALOR IS 'Tipo de valor ingresado en el campo IMP_FACTURABLE:  P: porcentaje; M: monto'
/
COMMENT ON COLUMN  FA_IMPTODCTOS_TD.FEC_HASTA IS 'Fecha de fin del período de vigencia del impuesto'
/
COMMENT ON COLUMN  FA_IMPTODCTOS_TD.NOM_USUARIO IS 'Nombre del usuario que actualiza el registro'
/
COMMENT ON COLUMN  FA_IMPTODCTOS_TD.COD_CATIMPOS IS 'Codigo de Categoria Impositiva'
/
