CREATE TABLE SISCEL.GA_EQUIPABOSER
(
  NUM_ABONADO     NUMBER(8)                     NOT NULL,
  NUM_SERIE       VARCHAR2(25 BYTE)             NOT NULL,
  COD_PRODUCTO    NUMBER(1)                     NOT NULL,
  IND_PROCEQUI    VARCHAR2(1 BYTE)              NOT NULL,
  FEC_ALTA        DATE                          NOT NULL,
  IND_PROPIEDAD   VARCHAR2(1 BYTE)              NOT NULL,
  COD_BODEGA      NUMBER(6),
  TIP_STOCK       NUMBER(2),
  COD_ARTICULO    NUMBER(6),
  IND_EQUIACC     VARCHAR2(1 BYTE),
  COD_MODVENTA    NUMBER(2),
  TIP_TERMINAL    VARCHAR2(1 BYTE),
  COD_USO         NUMBER(2),
  COD_CUOTA       VARCHAR2(2 BYTE),
  COD_ESTADO      NUMBER(2),
  CAP_CODE        NUMBER(7),
  COD_PROTOCOLO   VARCHAR2(2 BYTE),
  NUM_VELOCIDAD   NUMBER(4),
  COD_FRECUENCIA  NUMBER(1),
  COD_VERSION     NUMBER(2),
  NUM_SERIEMEC    VARCHAR2(20 BYTE),
  DES_EQUIPO      VARCHAR2(40 BYTE),
  COD_PAQUETE     NUMBER(6),
  NUM_MOVIMIENTO  NUMBER(10),
  COD_CAUSA       VARCHAR2(2 BYTE),
  IND_EQPRESTADO  CHAR(1 BYTE),
  NUM_IMEI        VARCHAR2(25 BYTE),
  COD_TECNOLOGIA  VARCHAR2(7 BYTE),
  IMP_CARGO       NUMBER(14,4),
  TIP_DTO         NUMBER(1),
  VAL_DTO         NUMBER(14,4),
  PRC_VENTA       NUMBER(14,4)
)   TABLESPACE TS_CRM_POSVENTA_LDAT
/

COMMENT ON TABLE SISCEL.GA_EQUIPABOSER IS 'Equipos Seriados de Abonados de Productos';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.PRC_VENTA IS 'Precio de lista de equipo';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.IND_PROCEQUI IS 'Indicativo de Procedencia Equipo; Interno (I), Externo (E), Subsidiado (S)';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.FEC_ALTA IS 'Fecha de Insercion';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.IND_PROPIEDAD IS 'Indicador de propiedad del equipo';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.COD_BODEGA IS 'Codigo de Bodega o Almacen';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.TIP_STOCK IS 'Tipo de Stock del Almacen';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.COD_ARTICULO IS 'Codigo de Articulo';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.IND_EQUIACC IS 'Indicativo de Equipo/Accesorio; E/A';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.COD_MODVENTA IS 'Codigo modalidad de venta';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.TIP_TERMINAL IS 'Tipo de Terminal; Celular - (A)nalogico o (D)igital, Beeper - (N)umerico o (A)lfanumerico';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.COD_USO IS 'Uso al que se encuentra destinado el Equipo';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.COD_CUOTA IS 'Codigo de cuota';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.COD_ESTADO IS 'Estado del Equipo';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.CAP_CODE IS 'Codigo de Cap Code Beeper';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.COD_PROTOCOLO IS 'Protocolo del equipo (beeper)';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.NUM_VELOCIDAD IS 'Velocidad (beeper)';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.COD_FRECUENCIA IS 'Frecuencia del equipo (beeper)';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.COD_VERSION IS 'Codigo de version (trek)';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.NUM_SERIEMEC IS 'Serie Mecanica del Equipo';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.DES_EQUIPO IS 'Propiedad del Equipo; (C)liente,(E)mpresa';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.COD_PAQUETE IS 'Codigo de Paquete';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.NUM_MOVIMIENTO IS 'Numero de Movimiento de Stock';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.COD_CAUSA IS 'Codigo de causa de cambio de equipo';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.IND_EQPRESTADO IS 'Indice de equipo en prestamo 1 prestado, 0 no.';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.NUM_IMEI IS 'Numero terminal GSM, al que esta asociada la SIMCARD';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.IMP_CARGO IS 'Importe del Cargo';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.TIP_DTO IS 'Tipo de Descuento a Aplicar; (0) Importe, (1) Porcentaje';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.VAL_DTO IS 'Valor del Descuento Aplicado';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.NUM_ABONADO IS 'Numero de Abonado/Producto';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.NUM_SERIE IS 'Numero de Serie del Equipo';

COMMENT ON COLUMN SISCEL.GA_EQUIPABOSER.COD_PRODUCTO IS 'Codigo de Producto';

