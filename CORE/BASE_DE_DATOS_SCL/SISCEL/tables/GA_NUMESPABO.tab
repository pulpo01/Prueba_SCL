CREATE TABLE GA_NUMESPABO(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  NUM_TELEFESP VARCHAR2(15) NOT NULL 
 ,  NUM_DIASNUM NUMBER(9,0) NOT NULL 
 ,  TIP_FRECUENTE VARCHAR2(1) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_NUMESPABO IS 'Numeros Especiales Abonados Celular'
/
COMMENT ON COLUMN  GA_NUMESPABO.NUM_ABONADO IS 'Numero de Abonado'
/
COMMENT ON COLUMN  GA_NUMESPABO.NUM_TELEFESP IS 'Numero especial Especial'
/
COMMENT ON COLUMN  GA_NUMESPABO.NUM_DIASNUM IS 'Numero de asociacion con el periodo de contratacion del servicio'
/
