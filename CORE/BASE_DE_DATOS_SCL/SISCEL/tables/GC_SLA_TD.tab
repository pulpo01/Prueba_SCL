CREATE TABLE GC_SLA_TD(
    COD_SLA NUMBER(6,0) NOT NULL 
 ,  TIEMPO_ESPERADO NUMBER(4,0) NOT NULL 
 ,  TIEMPO_RETRASO NUMBER(4,0) NOT NULL 
 ,  FEC_INI_VIGENCIA DATE NOT NULL 
 ,  FEC_FIN_VIGENCIA DATE NULL 
 ,  NOM_USUARORA VARCHAR2(30) DEFAULT USER NOT NULL 
 ,  FEC_ULTMOD DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GC_SLA_TD IS 'SLAs de GC'
/
COMMENT ON COLUMN  GC_SLA_TD.COD_SLA IS 'Codigo de SLA'
/
COMMENT ON COLUMN  GC_SLA_TD.TIEMPO_ESPERADO IS 'Tiempo de respuesta esperado para una tripleta'
/
COMMENT ON COLUMN  GC_SLA_TD.TIEMPO_RETRASO IS 'Tiempo de retraso para una tripleta'
/
COMMENT ON COLUMN  GC_SLA_TD.FEC_INI_VIGENCIA IS 'Fecha de inicio de vigencia del SLA'
/
COMMENT ON COLUMN  GC_SLA_TD.FEC_FIN_VIGENCIA IS 'Fecha de fin de vigencia del SLA'
/
COMMENT ON COLUMN  GC_SLA_TD.NOM_USUARORA IS 'Usuario Oracle'
/
COMMENT ON COLUMN  GC_SLA_TD.FEC_ULTMOD IS 'Fecha de ultima modificacion'
/
