CREATE TABLE CA_INCMAXIMAS(
    TIP_INCIDENCIA NUMBER(3,0) NOT NULL 
 ,  COD_CALCLIEN VARCHAR2(2) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  NUM_DIAS NUMBER(3,0) NOT NULL 
 ,  NUM_MAXIMO NUMBER(2,0) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NOT NULL 
 ,  IMPORTE NUMBER(14,4) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_INCMAXIMAS IS 'Maximo de incidencias por interlocutor'
/
COMMENT ON COLUMN  CA_INCMAXIMAS.TIP_INCIDENCIA IS 'Tipo Incidencia'
/
COMMENT ON COLUMN  CA_INCMAXIMAS.COD_CALCLIEN IS 'Calidad de Cliente'
/
COMMENT ON COLUMN  CA_INCMAXIMAS.COD_CONCEPTO IS 'Concepto Facturacion'
/
COMMENT ON COLUMN  CA_INCMAXIMAS.COD_PRODUCTO IS 'Codigo de producto'
/
COMMENT ON COLUMN  CA_INCMAXIMAS.NUM_DIAS IS 'Numero de Dias'
/
COMMENT ON COLUMN  CA_INCMAXIMAS.NUM_MAXIMO IS 'Maximo permitido'
/
COMMENT ON COLUMN  CA_INCMAXIMAS.COD_MONEDA IS 'Codigo de moneda'
/
COMMENT ON COLUMN  CA_INCMAXIMAS.IMPORTE IS 'Importe penalizacion'
/
