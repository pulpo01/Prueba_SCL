CREATE TABLE FA_MENSPROCESO(
    NUM_PROCESO NUMBER(8,0) NOT NULL 
 ,  COD_FORMULARIO NUMBER(2,0) NOT NULL 
 ,  COD_BLOQUE NUMBER(1,0) NOT NULL 
 ,  CORR_MENSAJE NUMBER(8,0) NOT NULL 
 ,  NUM_LINEAS NUMBER(2,0) NOT NULL 
 ,  COD_ORIGEN VARCHAR2(2) NOT NULL 
 ,  DESC_MENSAJE VARCHAR2(40) NULL 
 ,  IND_FACTURADO CHAR(1) NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_MENSPROCESO IS 'Mensajes por documento (no ciclico).'
/
COMMENT ON COLUMN  FA_MENSPROCESO.NUM_PROCESO IS 'Numero de proceso documento'
/
COMMENT ON COLUMN  FA_MENSPROCESO.COD_FORMULARIO IS 'Codigo de documento de destino'
/
COMMENT ON COLUMN  FA_MENSPROCESO.COD_BLOQUE IS 'Codigo de bloque destino mensaje'
/
COMMENT ON COLUMN  FA_MENSPROCESO.CORR_MENSAJE IS 'Correlativo creacion mensaje'
/
COMMENT ON COLUMN  FA_MENSPROCESO.NUM_LINEAS IS 'Numero de lineas del mensaje'
/
COMMENT ON COLUMN  FA_MENSPROCESO.COD_ORIGEN IS 'Codigo origen del mensaje'
/
COMMENT ON COLUMN  FA_MENSPROCESO.DESC_MENSAJE IS 'Descripcion corta del mensaje'
/
COMMENT ON COLUMN  FA_MENSPROCESO.IND_FACTURADO IS 'Indicador  incluido en la factura: I=ingresado | F=facturado incluido | N=facturado no incluido,'
/
COMMENT ON COLUMN  FA_MENSPROCESO.NOM_USUARIO IS 'Nombre usuario responsable modificacion'
/
COMMENT ON COLUMN  FA_MENSPROCESO.FEC_INGRESO IS 'Fecha ingreso mensaje'
/
