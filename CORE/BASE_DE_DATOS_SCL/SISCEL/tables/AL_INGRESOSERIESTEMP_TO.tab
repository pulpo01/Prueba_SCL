/
COMMENT ON COLUMN  AL_INGRESOSERIESTEMP_TO.CHK_DIGITS IS 'CHK_DIGITS'
/
COMMENT ON COLUMN  AL_INGRESOSERIESTEMP_TO.NUM_SEC_LOCA IS 'NUM_SEC_LOCA'
