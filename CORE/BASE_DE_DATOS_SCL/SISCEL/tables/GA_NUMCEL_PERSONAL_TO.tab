CREATE TABLE GA_NUMCEL_PERSONAL_TO(
    NUM_CEL_PERS NUMBER(15,0) NOT NULL 
 ,  NUM_CEL_CORP NUMBER(15,0) NOT NULL 
 ,  COD_ESTADO NUMBER(1,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_NUMCEL_PERSONAL_TO IS 'Números celulares activados como personal.'
/
COMMENT ON COLUMN  GA_NUMCEL_PERSONAL_TO.NUM_CEL_PERS IS 'Número celular personal.'
/
COMMENT ON COLUMN  GA_NUMCEL_PERSONAL_TO.NUM_CEL_CORP IS 'Número celular corporativo.'
/
COMMENT ON COLUMN  GA_NUMCEL_PERSONAL_TO.COD_ESTADO IS '0:Suspendido, 1: Activo, 2:Pendiente activación, 3: Pendiente modificación, 4:Pendiente desactivación'
/
COMMENT ON COLUMN  GA_NUMCEL_PERSONAL_TO.NOM_USUARIO IS 'Nombre de usuario que crea o modifica el registro.'
/
COMMENT ON COLUMN  GA_NUMCEL_PERSONAL_TO.FEC_ULTMOD IS 'Fecha de creación o última modificación.'
/
