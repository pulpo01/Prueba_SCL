CREATE TABLE CMT_METAS_PREPAGO(
    NUM_META NUMBER(6,0) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_COMISIONISTA NUMBER(6,0) NOT NULL 
 ,  VAL_META NUMBER(6,0) NOT NULL 
 ,  IMP_COMISION NUMBER(11,4) NOT NULL 
 ,  COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMT_METAS_PREPAGO IS 'Detalle De Metas Para Prepago.'
/
COMMENT ON COLUMN  CMT_METAS_PREPAGO.NUM_META IS 'Identificador del registro de meta al que detalla.'
/
COMMENT ON COLUMN  CMT_METAS_PREPAGO.COD_CONCEPTO IS 'Concepto de facturacion recuperado.'
/
COMMENT ON COLUMN  CMT_METAS_PREPAGO.COD_TIPCOMIS IS 'Cdigo del canal por el cual se hizo la venta (10, 26, 29, ...)..'
/
COMMENT ON COLUMN  CMT_METAS_PREPAGO.COD_COMISIONISTA IS 'Cdigo del comisionista que hizo la venta (Master dealer o vendedor raz)..'
/
COMMENT ON COLUMN  CMT_METAS_PREPAGO.VAL_META IS 'Valor de la meta a lograr.'
/
COMMENT ON COLUMN  CMT_METAS_PREPAGO.IMP_COMISION IS 'Importe logrado a partir de la meta..'
/
COMMENT ON COLUMN  CMT_METAS_PREPAGO.COD_PERIODO IS 'Periodo sobre el cual se aplica la meta..'
/
COMMENT ON COLUMN  CMT_METAS_PREPAGO.FEC_ULTMOD IS 'Fecha de ultima modificacion.'
/
COMMENT ON COLUMN  CMT_METAS_PREPAGO.NOM_USUARIO IS 'Usuario de ultima modificacion.'
/
