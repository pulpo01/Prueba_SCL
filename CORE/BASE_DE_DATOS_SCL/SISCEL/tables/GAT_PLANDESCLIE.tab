CREATE TABLE GAT_PLANDESCLIE(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_PLANDESC VARCHAR2(5) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAT_PLANDESCLIE IS 'Planes de Descuento por Cliente'
/
COMMENT ON COLUMN  GAT_PLANDESCLIE.COD_CLIENTE IS 'Codigo de cliente'
/
COMMENT ON COLUMN  GAT_PLANDESCLIE.COD_PLANDESC IS 'Codigo de plan de descuento asociado'
/
COMMENT ON COLUMN  GAT_PLANDESCLIE.FEC_DESDE IS 'Fecha Inicio de Vigencia'
/
COMMENT ON COLUMN  GAT_PLANDESCLIE.FEC_HASTA IS 'Fecha termino de Vigencia'
/
COMMENT ON COLUMN  GAT_PLANDESCLIE.FEC_ULTMOD IS 'Fecha ultima actualizacion'
/
COMMENT ON COLUMN  GAT_PLANDESCLIE.NOM_USUARIO IS 'Nombre usuario responsable'
/
