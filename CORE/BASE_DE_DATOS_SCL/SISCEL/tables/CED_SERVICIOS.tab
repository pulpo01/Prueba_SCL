CREATE TABLE CED_SERVICIOS(
    COD_SERVICIO VARCHAR2(5) NOT NULL 
 ,  DES_SERVICIO VARCHAR2(50) NOT NULL 
 ,  IND_PAGO VARCHAR2(1) NOT NULL 
 ,  IND_STOCK VARCHAR2(1) NOT NULL 
 ,  IND_DESPACHO VARCHAR2(1) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CED_SERVICIOS IS 'Definicisn de servicios electrsnicos'
/
COMMENT ON COLUMN  CED_SERVICIOS.COD_SERVICIO IS 'Codigo servicio'
/
COMMENT ON COLUMN  CED_SERVICIOS.DES_SERVICIO IS 'Descripcion de servicio'
/
COMMENT ON COLUMN  CED_SERVICIOS.IND_PAGO IS 'Indicador generacion movimiento de pago 1:SI, 0:NO'
/
COMMENT ON COLUMN  CED_SERVICIOS.IND_STOCK IS 'Indicador de control stock 1:Si, 0:NO'
/
COMMENT ON COLUMN  CED_SERVICIOS.IND_DESPACHO IS 'Indicador de despacho de producto 1:SI, 0:NO'
/
COMMENT ON COLUMN  CED_SERVICIOS.FEC_ULTMOD IS 'Fecha ultima modificacion'
/
COMMENT ON COLUMN  CED_SERVICIOS.NOM_USUARIO IS 'Responsable ultima modificacion'
/
