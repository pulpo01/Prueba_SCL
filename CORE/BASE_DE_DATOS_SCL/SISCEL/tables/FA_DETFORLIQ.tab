CREATE TABLE FA_DETFORLIQ(
    IND_ORDEN NUMBER(12,0) NOT NULL 
 ,  COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  NUM_TERMINAL VARCHAR2(15) NOT NULL 
 ,  FEC_CALL DATE NOT NULL 
 ,  HORA_CALL VARCHAR2(6) NOT NULL 
 ,  TIP_REG NUMBER(1,0) NOT NULL 
 ,  IND_ALQUILER NUMBER(1,0) NOT NULL 
 ,  IND_SALIDA VARCHAR2(10) NULL 
 ,  IND_ENTRADA VARCHAR2(20) NULL 
 ,  DES_ENTRADA VARCHAR2(20) NULL 
 ,  MOD_CALL NUMBER(1,0) NULL 
 ,  CLAVE_CALL VARCHAR2(2) NULL 
 ,  MINUTOS_TASADO NUMBER(5,0) NULL 
 ,  DUR_CALL NUMBER(9,0) NULL 
 ,  COD_SERVICIO VARCHAR2(2) NULL 
 ,  ACUM_NETOLLAM NUMBER(14,4) NULL 
 ,  ACUM_IVA NUMBER(14,4) NULL 
 ,  TOT_PAGAR NUMBER(14,4) NULL 
 ,  COD_TRAFICO VARCHAR2(2) NULL 
 ,  IND_REFACTURA VARCHAR2(1) NULL 
 ,  LOTE VARCHAR2(6) NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  SEQ_FORFAC NUMBER(12,0) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_DETFORLIQ IS 'Trafico carrier liquidado no facturado '
/
COMMENT ON COLUMN  FA_DETFORLIQ.IND_ORDEN IS 'Indicativo de Orden'
/
COMMENT ON COLUMN  FA_DETFORLIQ.COD_PERIODO IS 'Periodo de Facturacion o de alquiler'
/
COMMENT ON COLUMN  FA_DETFORLIQ.NUM_TERMINAL IS 'Num. telefono que paga'
/
COMMENT ON COLUMN  FA_DETFORLIQ.FEC_CALL IS 'Fecha de llamada'
/
COMMENT ON COLUMN  FA_DETFORLIQ.HORA_CALL IS 'Hora de la llamada'
/
COMMENT ON COLUMN  FA_DETFORLIQ.TIP_REG IS 'Tipo de Registro  9=Detalle'
/
COMMENT ON COLUMN  FA_DETFORLIQ.IND_ALQUILER IS 'Ind alquiler   S=1  N=0'
/
COMMENT ON COLUMN  FA_DETFORLIQ.IND_SALIDA IS 'Indicador de Salida (Area+Telefono)'
/
COMMENT ON COLUMN  FA_DETFORLIQ.IND_ENTRADA IS 'Numero discado '
/
COMMENT ON COLUMN  FA_DETFORLIQ.DES_ENTRADA IS 'Descripcion del punto de entrada de la llamada'
/
COMMENT ON COLUMN  FA_DETFORLIQ.MOD_CALL IS 'Modalidad de llamada 0=Contratado 1=Discado  '
/
COMMENT ON COLUMN  FA_DETFORLIQ.CLAVE_CALL IS 'Clave de la llamada'
/
COMMENT ON COLUMN  FA_DETFORLIQ.MINUTOS_TASADO IS 'Minutos tasados '
/
COMMENT ON COLUMN  FA_DETFORLIQ.DUR_CALL IS 'Duracion de la Llamada en segundos '
/
COMMENT ON COLUMN  FA_DETFORLIQ.COD_SERVICIO IS 'Csdigo de Servicio 01=Directo 02=Via Operadora '
/
COMMENT ON COLUMN  FA_DETFORLIQ.ACUM_NETOLLAM IS 'Valor neto de la LLamada '
/
COMMENT ON COLUMN  FA_DETFORLIQ.ACUM_IVA IS 'Valor de Iva de la Llamada '
/
COMMENT ON COLUMN  FA_DETFORLIQ.TOT_PAGAR IS 'Total a Pagar '
/
COMMENT ON COLUMN  FA_DETFORLIQ.COD_TRAFICO IS 'Codigo de Trafico  03=Nacional 04=Internacional '
/
COMMENT ON COLUMN  FA_DETFORLIQ.IND_REFACTURA IS 'Indicador de Refacturacion  R '
/
COMMENT ON COLUMN  FA_DETFORLIQ.LOTE IS 'Ano YYYY + Mes MM '
/
COMMENT ON COLUMN  FA_DETFORLIQ.NUM_ABONADO IS 'Numero de abonado correspondiente al terminal '
/
COMMENT ON COLUMN  FA_DETFORLIQ.COD_CLIENTE IS 'Cliente al que pertenece el abonado '
/
COMMENT ON COLUMN  FA_DETFORLIQ.COD_OPERADOR IS 'Codigo del operador '
/
COMMENT ON COLUMN  FA_DETFORLIQ.SEQ_FORFAC IS 'Secuencia FORFAC '
/
