CREATE TABLE CM_PORCENTAJE_BONOCARTERA_TD(
    COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) NOT NULL 
 ,  COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  VAL_PORCENTAJE NUMBER(3,0) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_PORCENTAJE_BONOCARTERA_TD IS 'PORCENTAJE DE ASIGNACION DE COMISION POR BONO CARTERA'
/
COMMENT ON COLUMN  CM_PORCENTAJE_BONOCARTERA_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS '
/
COMMENT ON COLUMN  CM_PORCENTAJE_BONOCARTERA_TD.COD_PLANCOMIS IS 'CODIGO DEL PLAN DE COMISIONES '
/
COMMENT ON COLUMN  CM_PORCENTAJE_BONOCARTERA_TD.COD_CONCEPTO IS 'CODIGO DEL CONCEPTO '
/
COMMENT ON COLUMN  CM_PORCENTAJE_BONOCARTERA_TD.VAL_PORCENTAJE IS 'VALOR DEL PORCENTAJE A COMISIONAR POR BONO CARTERA '
/
COMMENT ON COLUMN  CM_PORCENTAJE_BONOCARTERA_TD.FEC_ULTMOD IS 'FECHA ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  CM_PORCENTAJE_BONOCARTERA_TD.NOM_USUARIO IS 'USUARIO ULTIMA MODIFICACION'
/
