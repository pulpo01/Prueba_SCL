CREATE TABLE VE_EXCEPCION_VTA_PREPAGO_TO(
    COD_EXCEPCION_VTA NUMBER(12,0) NOT NULL 
 ,  NUM_VENTA NUMBER(8,0) NOT NULL 
 ,  COD_CAUSA NUMBER(3,0) NOT NULL 
 ,  FECHA_VENTA DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(50) NOT NULL 
 ,  COD_PLANTARIF VARCHAR2(3) NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(15,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_EXCEPCION_VTA_PREPAGO_TO IS '[ EXVP ] Ventas excepcionadas, superan el maximo de lineas de prepago permitidas'
/
COMMENT ON COLUMN  VE_EXCEPCION_VTA_PREPAGO_TO.COD_EXCEPCION_VTA IS 'Codigo de venta excepcionada.'
/
COMMENT ON COLUMN  VE_EXCEPCION_VTA_PREPAGO_TO.NUM_VENTA IS 'Numero de la venta.'
/
COMMENT ON COLUMN  VE_EXCEPCION_VTA_PREPAGO_TO.COD_CAUSA IS 'Codigo de causa de excepcion.'
/
COMMENT ON COLUMN  VE_EXCEPCION_VTA_PREPAGO_TO.FECHA_VENTA IS 'Fecha de la venta excepcionada.'
/
COMMENT ON COLUMN  VE_EXCEPCION_VTA_PREPAGO_TO.NOM_USUARIO IS 'Usuario que hace la excepcion.'
/
COMMENT ON COLUMN  VE_EXCEPCION_VTA_PREPAGO_TO.COD_PLANTARIF IS 'Codigo de plan tarifario del cliente.'
/
COMMENT ON COLUMN  VE_EXCEPCION_VTA_PREPAGO_TO.COD_CLIENTE IS 'Código del Cliente'
/
COMMENT ON COLUMN  VE_EXCEPCION_VTA_PREPAGO_TO.NUM_ABONADO IS 'Numero del Abonado'
/
