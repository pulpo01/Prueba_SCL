CREATE TABLE GE_CELDAS(
    COD_CELDA VARCHAR2(7) NOT NULL 
 ,  COD_SUBALM VARCHAR2(5) NOT NULL 
 ,  DES_CELDA VARCHAR2(50) NOT NULL 
 ,  COD_SUBALM2 VARCHAR2(5) NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_CELDAS IS 'Celdas de cobertura telefonica celular- Retrofitted'
/
COMMENT ON COLUMN  GE_CELDAS.COD_CELDA IS 'Codigo de celda - Retrofitted'
/
COMMENT ON COLUMN  GE_CELDAS.COD_SUBALM IS 'Codigo de SubALM al que pertenece la celda - Retrofitted'
/
COMMENT ON COLUMN  GE_CELDAS.DES_CELDA IS 'Descripcion de la celda - Retrofitted'
/
COMMENT ON COLUMN  GE_CELDAS.COD_SUBALM2 IS 'Codigo subalm alternativo cuando subalm pertenece a Plexsys'
/
