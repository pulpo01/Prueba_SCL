CREATE TABLE ICG_SERVICIO(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_SERVICIO NUMBER(2,0) NOT NULL 
 ,  DES_SERVICIO VARCHAR2(50) NOT NULL 
 ,  NUM_NIVELES NUMBER(4,0) NOT NULL 
 ,  IND_ESTADO NUMBER(1,0) NOT NULL 
 ,  IND_ABONADOS NUMBER(1,0) DEFAULT 1 NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICG_SERVICIO IS 'Servicios del Producto'
/
COMMENT ON COLUMN  ICG_SERVICIO.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  ICG_SERVICIO.COD_SERVICIO IS 'Codigo de Clase de Servicio'
/
COMMENT ON COLUMN  ICG_SERVICIO.DES_SERVICIO IS 'Descripcion de Clase de Servicio'
/
COMMENT ON COLUMN  ICG_SERVICIO.NUM_NIVELES IS 'numero de niveles posibles del servicio'
/
COMMENT ON COLUMN  ICG_SERVICIO.IND_ESTADO IS 'Estado del servicio'
/
COMMENT ON COLUMN  ICG_SERVICIO.IND_ABONADOS IS 'Indicador de si tiene repercusion en las centrales'
/
