CREATE TABLE CA_ACUMPENALES(
    TIP_INTER NUMBER(2,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_INTERLOCUTOR NUMBER(10,0) NOT NULL 
 ,  TIP_INCIDENCIA NUMBER(3,0) NOT NULL 
 ,  FEC_PRIMERA DATE NOT NULL 
 ,  FEC_ULTIMA DATE NOT NULL 
 ,  NUM_ACUMULADO NUMBER(3,0) NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CA_ACUMPENALES IS 'Acumulados penalizables'
/
COMMENT ON COLUMN  CA_ACUMPENALES.TIP_INTER IS 'Tipo interlocutor'
/
COMMENT ON COLUMN  CA_ACUMPENALES.COD_PRODUCTO IS 'Codigo de producto'
/
COMMENT ON COLUMN  CA_ACUMPENALES.COD_INTERLOCUTOR IS 'Codigo interlocutor'
/
COMMENT ON COLUMN  CA_ACUMPENALES.TIP_INCIDENCIA IS 'Tipo incidencia'
/
COMMENT ON COLUMN  CA_ACUMPENALES.FEC_PRIMERA IS 'Primera Fecha'
/
COMMENT ON COLUMN  CA_ACUMPENALES.FEC_ULTIMA IS 'Ultima Fecha'
/
COMMENT ON COLUMN  CA_ACUMPENALES.NUM_ACUMULADO IS 'Acumulado'
/
