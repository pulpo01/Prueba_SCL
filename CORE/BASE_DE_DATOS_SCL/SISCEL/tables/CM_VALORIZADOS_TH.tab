CREATE TABLE CM_VALORIZADOS_TH(
    NUM_GENERAL NUMBER(10,0) NOT NULL 
 ,  COD_UNIVERSO VARCHAR2(6) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_COMISIONISTA NUMBER(6,0) NOT NULL 
 ,  COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  ID_PERIODO VARCHAR2(10) NOT NULL 
 ,  IMP_CONCEPTO NUMBER(11,4) NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_VALORIZADOS_TH IS 'Tabla Historica De Valorizados.'
/
COMMENT ON COLUMN  CM_VALORIZADOS_TH.NUM_GENERAL IS 'CODIGO DEL COMISIONISTA.'
/
COMMENT ON COLUMN  CM_VALORIZADOS_TH.COD_UNIVERSO IS 'CODIGO DEL TIPO DE COMISIONISTA.'
/
COMMENT ON COLUMN  CM_VALORIZADOS_TH.COD_CONCEPTO IS 'CODIGO DEL PERIODO DE COMISIONES.'
/
COMMENT ON COLUMN  CM_VALORIZADOS_TH.COD_TIPCOMIS IS 'CODIGO DEL CONCEPTO DE COMISION.'
/
COMMENT ON COLUMN  CM_VALORIZADOS_TH.COD_COMISIONISTA IS 'VALOR DEL RANGO A EVALUAR.'
/
COMMENT ON COLUMN  CM_VALORIZADOS_TH.COD_PERIODO IS 'IMPORTE DE LA COMISION AL LOGRAR EL UMBRAR DE EVALUACION..'
/
COMMENT ON COLUMN  CM_VALORIZADOS_TH.ID_PERIODO IS 'INDICADOR SI SE CONSIDERA FECHA DE CORTE O NO..'
/
COMMENT ON COLUMN  CM_VALORIZADOS_TH.IMP_CONCEPTO IS '.'
/
COMMENT ON COLUMN  CM_VALORIZADOS_TH.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
