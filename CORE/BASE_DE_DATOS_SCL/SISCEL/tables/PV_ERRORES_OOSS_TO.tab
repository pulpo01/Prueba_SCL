CREATE TABLE PV_ERRORES_OOSS_TO(
    NUM_ERROR NUMBER(10,0) NOT NULL 
 ,  NUM_OS NUMBER(10,0) DEFAULT 1 NOT NULL 
 ,  COD_OS VARCHAR2(5) NOT NULL 
 ,  COD_ERROR VARCHAR2(8) NOT NULL 
 ,  GLS_ERROR VARCHAR2(250) NOT NULL 
 ,  NOM_PROC VARCHAR2(100) NULL 
 ,  FEC_ERROR DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  PV_ERRORES_OOSS_TO.NUM_ERROR IS 'NUMERO DE ERROR (SECUENCIA DE PV_ERRORES_SQ)'
/
COMMENT ON COLUMN  PV_ERRORES_OOSS_TO.NUM_OS IS 'NUMERO DE ORDEN DE SERVICIO.'
/
COMMENT ON COLUMN  PV_ERRORES_OOSS_TO.COD_OS IS 'CODIGO ORDEN DE SERVICIO'
/
COMMENT ON COLUMN  PV_ERRORES_OOSS_TO.COD_ERROR IS 'CODIGO DE ERROR'
/
COMMENT ON COLUMN  PV_ERRORES_OOSS_TO.GLS_ERROR IS 'GLOSA DE ERROR'
/
COMMENT ON COLUMN  PV_ERRORES_OOSS_TO.NOM_PROC IS 'NOMBRE DEL PROCEDIMIENTO QUE DA EL ERROR'
/
COMMENT ON COLUMN  PV_ERRORES_OOSS_TO.FEC_ERROR IS 'FECHA DE ERROR.'
/
COMMENT ON COLUMN  PV_ERRORES_OOSS_TO.NOM_USUARIO IS 'USUARIO CREADOR DE ORDEN DE SERVICIO'
/
