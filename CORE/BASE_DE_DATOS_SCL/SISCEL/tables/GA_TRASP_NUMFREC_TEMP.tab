CREATE TABLE GA_TRASP_NUMFREC_TEMP(
    COD_ACTABO VARCHAR2(2) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  NUM_CELULAR VARCHAR2(15) NOT NULL 
 ,  TIP_FRECUENTE VARCHAR2(1) NULL 
 ,  NUM_MOVIMIENTO NUMBER(9,0) NULL 
 ,  COD_ESTADO NUMBER(1,0) DEFAULT 0 NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  GA_TRASP_NUMFREC_TEMP.COD_ACTABO IS 'Codigo de actuacion'
/
COMMENT ON COLUMN  GA_TRASP_NUMFREC_TEMP.NUM_ABONADO IS 'Numero abonado'
/
COMMENT ON COLUMN  GA_TRASP_NUMFREC_TEMP.NUM_CELULAR IS 'SS: n? celular abonado, FI: n? frecuente'
/
COMMENT ON COLUMN  GA_TRASP_NUMFREC_TEMP.TIP_FRECUENTE IS 'F: frecuente fijo, M: frecuente movil'
/
COMMENT ON COLUMN  GA_TRASP_NUMFREC_TEMP.NUM_MOVIMIENTO IS 'N? movimiento central SS'
/
COMMENT ON COLUMN  GA_TRASP_NUMFREC_TEMP.COD_ESTADO IS '0: Inicio, 1: OK para ingresar movimiento.'
/
