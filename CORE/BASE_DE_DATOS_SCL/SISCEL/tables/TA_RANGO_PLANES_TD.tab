CREATE TABLE TA_RANGO_PLANES_TD(
    IMP_INICIAL NUMBER(14,4) NOT NULL 
 ,  IMP_FINAL NUMBER(14,4) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_PLANTARIF VARCHAR2(3) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  COD_RANGO VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TASACION_TTAB
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_RANGO_PLANES_TD IS '[ RGPL ] Rangos de cargos asociado a los planes tarifariosRangos de cargos asociado a los planes tarifarios'
/
COMMENT ON COLUMN  TA_RANGO_PLANES_TD.IMP_INICIAL IS 'Valor mínimo del importe para el plan'
/
COMMENT ON COLUMN  TA_RANGO_PLANES_TD.IMP_FINAL IS 'Valor máximo del importe para el plan'
/
COMMENT ON COLUMN  TA_RANGO_PLANES_TD.COD_PRODUCTO IS 'Producto (1=celular, 2=beep, etc)'
/
COMMENT ON COLUMN  TA_RANGO_PLANES_TD.COD_PLANTARIF IS 'Código de plan tarifario'
/
COMMENT ON COLUMN  TA_RANGO_PLANES_TD.FEC_DESDE IS 'Fecha de inicio de vigencia'
/
COMMENT ON COLUMN  TA_RANGO_PLANES_TD.FEC_HASTA IS 'Fecha término de vigencia'
/
COMMENT ON COLUMN  TA_RANGO_PLANES_TD.COD_RANGO IS 'Identificador del rango'
/
