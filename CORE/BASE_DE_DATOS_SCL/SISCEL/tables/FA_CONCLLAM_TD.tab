 ,  COD_CONCEPTO NUMBER(5,0) NOT NULL 
 ,  FEC_ULTMOD DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_CONCLLAM_TD IS 'Tabla de Tipos de Llamadas de Descuentos'
/
COMMENT ON COLUMN  FA_CONCLLAM_TD.TIP_DCTO IS 'Tipo de descuento'
/
COMMENT ON COLUMN  FA_CONCLLAM_TD.COD_CONCEPTO IS 'Codigo de Concepto'
/
COMMENT ON COLUMN  FA_CONCLLAM_TD.FEC_ULTMOD IS 'Fecha de ultima modificacion del registro'
/
COMMENT ON COLUMN  FA_CONCLLAM_TD.NOM_USUARIO IS 'Nombre de Usuario'
/
