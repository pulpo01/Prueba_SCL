CREATE TABLE FAD_CUADRANDESC(
    NUM_CUADRANTE NUMBER(6,0) NOT NULL 
 ,  VAL_DESDE NUMBER(12,2) NOT NULL 
 ,  VAL_HASTA NUMBER(12,2) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  TIP_DESCUENTO VARCHAR2(1) NOT NULL 
 ,  VAL_DESCUENTO NUMBER(8,2) NOT NULL 
 ,  TIP_MONEDA VARCHAR2(3) NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAD_CUADRANDESC IS 'Cuadrantes de Descuentos'
/
COMMENT ON COLUMN  FAD_CUADRANDESC.NUM_CUADRANTE IS 'Numero de cuadrante'
/
COMMENT ON COLUMN  FAD_CUADRANDESC.VAL_DESDE IS 'Valor inicio de rango'
/
COMMENT ON COLUMN  FAD_CUADRANDESC.VAL_HASTA IS 'Valor termino de rango'
/
COMMENT ON COLUMN  FAD_CUADRANDESC.FEC_DESDE IS 'Fecha Inicio de Vigencia'
/
COMMENT ON COLUMN  FAD_CUADRANDESC.FEC_HASTA IS 'Fecha termino de Vigencia'
/
COMMENT ON COLUMN  FAD_CUADRANDESC.TIP_DESCUENTO IS 'Tipo de descuento a aplicar M: Monto  P: Porcentaje'
/
COMMENT ON COLUMN  FAD_CUADRANDESC.VAL_DESCUENTO IS 'Valor del descuento'
/
COMMENT ON COLUMN  FAD_CUADRANDESC.TIP_MONEDA IS 'Tipo de moneda, descuento por monto'
/
COMMENT ON COLUMN  FAD_CUADRANDESC.FEC_ULTMOD IS 'Fecha ultima actualizacion'
/
COMMENT ON COLUMN  FAD_CUADRANDESC.NOM_USUARIO IS 'Nombre usuario responsable'
/
