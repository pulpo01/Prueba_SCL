CREATE TABLE FA_IMP_TIPFORMULARIO(
    COD_FORMULARIO NUMBER(3,0) NOT NULL 
 ,  DES_FORMULARIO VARCHAR2(50) NOT NULL 
 ,  IND_ABONADO NUMBER(1,0) NOT NULL 
 ,  NUM_COLUMNAS NUMBER(3,0) NOT NULL 
 ,  NUM_FILAS NUMBER(3,0) NOT NULL 
 ,  IND_RESUMENPAGO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_IMP_TIPFORMULARIO IS 'Formularios o tipos de hojas diponibles- Retrofitted'
/
COMMENT ON COLUMN  FA_IMP_TIPFORMULARIO.COD_FORMULARIO IS 'Codigo de Formulario - Retrofitted'
/
COMMENT ON COLUMN  FA_IMP_TIPFORMULARIO.DES_FORMULARIO IS 'Descripcion de Formulario - Retrofitted'
/
COMMENT ON COLUMN  FA_IMP_TIPFORMULARIO.IND_ABONADO IS '1 => Documento con n Abonados 0 => Documento con 1 Abonado - Retrofitted'
/
COMMENT ON COLUMN  FA_IMP_TIPFORMULARIO.NUM_COLUMNAS IS 'Numero de Columnas - Retrofitted'
/
COMMENT ON COLUMN  FA_IMP_TIPFORMULARIO.NUM_FILAS IS 'Numero de Filas - Retrofitted'
/
