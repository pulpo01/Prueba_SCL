CREATE TABLE CMT_TRAF_PREPAGO(
    NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  COD_PERIODO_HAB NUMBER(8,0) NOT NULL 
 ,  COD_PERIODO_EVAL NUMBER(8,0) NOT NULL 
 ,  UNI_ENTRADA NUMBER(5,0) NULL 
 ,  UNI_SALIDA NUMBER(5,0) NULL 
 ,  UNI_TOTALES NUMBER(6,0) NULL 
 ,  TIP_UNITAS CHAR(1) NOT NULL 
 ,  IMP_COMISION NUMBER(10,4) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMT_TRAF_PREPAGO IS 'Tabla De Anticipos De Comisiones.'
/
COMMENT ON COLUMN  CMT_TRAF_PREPAGO.NUM_CELULAR IS 'Numero de celular.'
/
COMMENT ON COLUMN  CMT_TRAF_PREPAGO.NUM_SERIE IS 'Numero de serie.'
/
COMMENT ON COLUMN  CMT_TRAF_PREPAGO.COD_PERIODO_HAB IS 'Periodo de habilitacion del celular.'
/
COMMENT ON COLUMN  CMT_TRAF_PREPAGO.COD_PERIODO_EVAL IS 'Periodo de evaluacion del celular.'
/
COMMENT ON COLUMN  CMT_TRAF_PREPAGO.UNI_ENTRADA IS 'Unidades de entrada del periodo.'
/
COMMENT ON COLUMN  CMT_TRAF_PREPAGO.UNI_SALIDA IS 'Unidades de salida del periodo.'
/
COMMENT ON COLUMN  CMT_TRAF_PREPAGO.UNI_TOTALES IS 'Unidades totales del periodo.'
/
COMMENT ON COLUMN  CMT_TRAF_PREPAGO.TIP_UNITAS IS 'Tipos de unidades consideradas.'
/
COMMENT ON COLUMN  CMT_TRAF_PREPAGO.IMP_COMISION IS 'Importe de comisiones logrado en el periodo.'
/
