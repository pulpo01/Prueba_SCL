CREATE TABLE SC_CONCEPTO(
    COD_DOMINIO_CTO NUMBER(2,0) NOT NULL 
 ,  COD_CONCEPTO VARCHAR2(15) NOT NULL 
 ,  IND_CONTABILIZA NUMBER(1,0) NULL 
 ,  DES_CONCEPTO VARCHAR2(60) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SC_CONCEPTO IS 'Conceptos Para Contabilizacion.'
/
COMMENT ON COLUMN  SC_CONCEPTO.COD_DOMINIO_CTO IS 'Codigo del dominio de conceptos.'
/
COMMENT ON COLUMN  SC_CONCEPTO.COD_CONCEPTO IS 'Codigo de concepto contable.'
/
COMMENT ON COLUMN  SC_CONCEPTO.IND_CONTABILIZA IS 'Indicador concepto: O=Contabiliza | 1=No Contabiliza.'
/
COMMENT ON COLUMN  SC_CONCEPTO.DES_CONCEPTO IS 'Descripcion de concepto.'
/
