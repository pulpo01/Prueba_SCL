CREATE TABLE ERD_CLIENTES_VETADOS(
    NOM_CLIENTE VARCHAR2(50) NOT NULL 
 ,  FEC_IMPEDIMENTO_H DATE NOT NULL 
 ,  COD_CAUSAL_IMPEDIMENTO NUMBER(6,0) NOT NULL 
 ,  DES_OBS_IMPEDIMENTO VARCHAR2(80) NULL 
 ,  NOM_USUARIO_MODI VARCHAR2(30) NOT NULL 
 ,  FEC_MODI_H DATE NOT NULL 
 ,  COD_TIPIDENT VARCHAR2(2) DEFAULT '01' NOT NULL 
 ,  NUM_IDENT_CLIENTE VARCHAR2(20) NOT NULL 
 ,  FEC_HASTA DATE DEFAULT TO_DATE('31/12/3000 23:59:59' , 'DD/MM/YYYY HH24:MI:SS' ) NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERD_CLIENTES_VETADOS IS 'Clientes No Recomendados .'
/
COMMENT ON COLUMN  ERD_CLIENTES_VETADOS.NOM_CLIENTE IS 'Nombre del cliente.'
/
COMMENT ON COLUMN  ERD_CLIENTES_VETADOS.FEC_IMPEDIMENTO_H IS 'Fecha desde la que rige el impedimento.'
/
COMMENT ON COLUMN  ERD_CLIENTES_VETADOS.COD_CAUSAL_IMPEDIMENTO IS 'Codigo de la causa de impedimento..'
/
COMMENT ON COLUMN  ERD_CLIENTES_VETADOS.DES_OBS_IMPEDIMENTO IS 'Descripcion de Observacion sobre impedimento.'
/
COMMENT ON COLUMN  ERD_CLIENTES_VETADOS.NOM_USUARIO_MODI IS 'Nombre del usuario que creo/actualizo este registro.'
/
COMMENT ON COLUMN  ERD_CLIENTES_VETADOS.FEC_MODI_H IS 'fecha de creacion/modificacion de este registro.'
/
COMMENT ON COLUMN  ERD_CLIENTES_VETADOS.COD_TIPIDENT IS 'Tipo de Identificacion.'
/
COMMENT ON COLUMN  ERD_CLIENTES_VETADOS.FEC_HASTA IS 'Fecha hasta del cliente vetado.'
/
