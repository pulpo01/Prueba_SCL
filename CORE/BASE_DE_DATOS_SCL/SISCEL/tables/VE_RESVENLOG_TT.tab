 ,  MAX_SEC NUMBER(9,0) NULL 
 ,  NIV_LOG NUMBER(2,0) DEFAULT 1 NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
