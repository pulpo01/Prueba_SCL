CREATE TABLE CI_ATENCION_CLIENTES(
    COD_ATENCION NUMBER(3,0) NOT NULL 
 ,  DES_ATENCION VARCHAR2(150) NOT NULL 
 ,  GRP_ATENCION NUMBER(3,0) NOT NULL 
 ,  NUM_NIVEL NUMBER(2,0) NOT NULL 
 ,  IND_VIGENTE VARCHAR2(1) NULL 
 ,  IND_OBSERV VARCHAR2(1) DEFAULT 'N' NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CI_ATENCION_CLIENTES IS ' Tabla que registra las necesidades del cliente'
/
COMMENT ON COLUMN  CI_ATENCION_CLIENTES.COD_ATENCION IS 'Codigo de la Descripcisn de la Atencisn'
/
COMMENT ON COLUMN  CI_ATENCION_CLIENTES.DES_ATENCION IS 'Nombre de la Atencisn'
/
COMMENT ON COLUMN  CI_ATENCION_CLIENTES.GRP_ATENCION IS 'Csdigo del Grupo al que pertenece'
/
COMMENT ON COLUMN  CI_ATENCION_CLIENTES.NUM_NIVEL IS 'Numero del nivel al que pertenece la atencisn'
/
COMMENT ON COLUMN  CI_ATENCION_CLIENTES.IND_VIGENTE IS 'Indica si la atencisn esta vigente s no esta vigente'
/
COMMENT ON COLUMN  CI_ATENCION_CLIENTES.IND_OBSERV IS 'OBLIGATORIEDAD DE LA OBSERVACION (S,N)'
/
