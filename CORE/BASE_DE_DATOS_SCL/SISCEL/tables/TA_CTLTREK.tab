CREATE TABLE TA_CTLTREK(
    NUM_BLOQUE VARCHAR2(9) NOT NULL 
 ,  NUM_TICKETS NUMBER(9,0) NOT NULL 
 ,  NUM_SALIENTES NUMBER(8,0) NOT NULL 
 ,  NUM_ANOMALIAS NUMBER(8,0) NOT NULL 
 ,  NUM_ENTRANTES NUMBER(8,0) NOT NULL 
 ,  NUM_CONGELADAS NUMBER(8,0) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_CTLTREK IS 'Control ficheros tasados trek'
/
COMMENT ON COLUMN  TA_CTLTREK.NUM_BLOQUE IS 'Numero de Bloque'
/
COMMENT ON COLUMN  TA_CTLTREK.NUM_TICKETS IS 'Numero de Tickets'
/
COMMENT ON COLUMN  TA_CTLTREK.NUM_SALIENTES IS 'Numero de tickets salientes'
/
COMMENT ON COLUMN  TA_CTLTREK.NUM_ANOMALIAS IS 'Numero de Llamadas  Anomalas'
/
COMMENT ON COLUMN  TA_CTLTREK.NUM_ENTRANTES IS 'Numero de llamadas entrantes'
/
COMMENT ON COLUMN  TA_CTLTREK.NUM_CONGELADAS IS 'Numero de Llamadas Congeladas'
/
