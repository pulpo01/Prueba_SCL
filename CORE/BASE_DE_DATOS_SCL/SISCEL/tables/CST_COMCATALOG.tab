CREATE TABLE CST_COMCATALOG(
    NOM_COMPONENTE VARCHAR2(60) NOT NULL 
 ,  TIP_COMPONENTE VARCHAR2(5) NOT NULL 
 ,  FEC_CREACION DATE NOT NULL 
 ,  NUM_VERSION NUMBER(2,0) NOT NULL 
 ,  NUM_SUBVERSION NUMBER(3,0) NOT NULL 
 ,  FEC_VERSION DATE NOT NULL 
 ,  NOM_COMORIGEN VARCHAR2(60) NOT NULL 
 ,  NOM_USUARESP VARCHAR2(50) NOT NULL 
 ,  COD_CLASSID VARCHAR2(60) NOT NULL 
 ,  NOM_CLASS VARCHAR2(60) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CST_COMCATALOG IS 'Catalogo De Componenetes De Apliaciones.'
/
COMMENT ON COLUMN  CST_COMCATALOG.NOM_COMPONENTE IS 'Nombre de la Componente del Sistema.'
/
COMMENT ON COLUMN  CST_COMCATALOG.TIP_COMPONENTE IS 'Tipo de Componente (Procedure, Dll, etc) Co_codigos.'
/
COMMENT ON COLUMN  CST_COMCATALOG.FEC_CREACION IS 'Fecha de la Creacion de la Componente.'
/
COMMENT ON COLUMN  CST_COMCATALOG.NUM_VERSION IS 'Numero de Version de la Componente.'
/
COMMENT ON COLUMN  CST_COMCATALOG.NUM_SUBVERSION IS 'Numero de Sub-Versin de la Componente.'
/
COMMENT ON COLUMN  CST_COMCATALOG.FEC_VERSION IS 'Fecha de la Generacion de Ultima Version .'
/
COMMENT ON COLUMN  CST_COMCATALOG.NOM_COMORIGEN IS 'Nombre de la Componente Origen del Sistema.'
/
COMMENT ON COLUMN  CST_COMCATALOG.NOM_USUARESP IS 'Nombre del Usuario de Desarrollo Responsable..'
/
COMMENT ON COLUMN  CST_COMCATALOG.COD_CLASSID IS 'Codigo Identificador de la Clase de la Componente (CLSID DLL).'
/
COMMENT ON COLUMN  CST_COMCATALOG.NOM_CLASS IS 'Nombre de la Classe del Objeto.'
/
