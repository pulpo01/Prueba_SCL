CREATE TABLE GA_DEFVENTA(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_MODVENTA NUMBER(2,0) NOT NULL 
 ,  COD_TIPCONTRATO VARCHAR2(3) NOT NULL 
 ,  COD_CICLO NUMBER(2,0) NOT NULL 
 ,  COD_GRPSERV VARCHAR2(2) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_TIPCONTRATORENT VARCHAR2(3) NULL 
 ,  COD_USORENT NUMBER(2,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_DEFVENTA IS 'Datos Defecto en la venta por producto'
/
COMMENT ON COLUMN  GA_DEFVENTA.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_DEFVENTA.COD_MODVENTA IS 'Codigo de Modalidad de Venta'
/
COMMENT ON COLUMN  GA_DEFVENTA.COD_TIPCONTRATO IS 'Codigo de tipo de contrato'
/
COMMENT ON COLUMN  GA_DEFVENTA.COD_CICLO IS 'Codigo de Ciclo de Facturacion'
/
COMMENT ON COLUMN  GA_DEFVENTA.COD_GRPSERV IS 'Codigo de grupo cobro Servicios'
/
COMMENT ON COLUMN  GA_DEFVENTA.COD_USO IS 'Codigo de Uso de Linea'
/
COMMENT ON COLUMN  GA_DEFVENTA.COD_TIPCONTRATORENT IS 'Codigo tipo contrato de Rentafhon'
/
COMMENT ON COLUMN  GA_DEFVENTA.COD_USORENT IS 'Codigo del uso en rentaphon'
/
