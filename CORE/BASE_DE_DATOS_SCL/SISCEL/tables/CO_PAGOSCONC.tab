CREATE TABLE CO_PAGOSCONC
(
  COD_TIPDOCUM         NUMBER(2)                NOT NULL,
  COD_CENTREMI         NUMBER(4)                NOT NULL,
  NUM_SECUENCI         NUMBER(8)                NOT NULL,
  COD_VENDEDOR_AGENTE  NUMBER(6)                NOT NULL,
  NUM_SECUREL          NUMBER(8),
  LETRA                VARCHAR2(1 BYTE)         NOT NULL,
  IMP_CONCEPTO         NUMBER(14,4)             NOT NULL,
  COD_PRODUCTO         NUMBER(1)                NOT NULL,
  COD_TIPDOCREL        NUMBER(2),
  COD_AGENTEREL        NUMBER(6),
  NUM_TRANSACCION      NUMBER(9),
  LETRA_REL            VARCHAR2(1 BYTE),
  COD_CENTRREL         NUMBER(4),
  COD_CONCEPTO         NUMBER(4),
  COLUMNA              NUMBER(4),
  NUM_ABONADO          NUMBER(8),
  NUM_FOLIO            NUMBER(9),
  NUM_CUOTA            NUMBER(8),
  SEC_CUOTA            NUMBER(3),
  NUM_VENTA            NUMBER(8),
  NUM_FOLIOCTC         VARCHAR2(11 BYTE)        DEFAULT '0',
  FEC_CANCELACION      DATE                     DEFAULT SYSDATE,
  COD_OPERADORA_SCL    VARCHAR2(5 BYTE),
  COD_PLAZA            VARCHAR2(5 BYTE),
  PREF_PLAZA           VARCHAR2(25 BYTE)
)
TABLESPACE TS_RECYCOB_LDAT
PCTUSED    0
PCTFREE    5
INITRANS   4
MAXTRANS   255
STORAGE    (
            INITIAL          128M
            NEXT             128M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE CO_PAGOSCONC IS 'Relaciones Entradas-Conceptos';

COMMENT ON COLUMN CO_PAGOSCONC.COD_TIPDOCUM IS 'CODIGO DE TIPO DE DOCUMENTO';

COMMENT ON COLUMN CO_PAGOSCONC.COD_CENTREMI IS 'CODIGO DEL CENTRO EMISOR';

COMMENT ON COLUMN CO_PAGOSCONC.NUM_SECUENCI IS 'Numero de secuencia del siguiente docume';

COMMENT ON COLUMN CO_PAGOSCONC.COD_VENDEDOR_AGENTE IS 'Codigo de Agente';

COMMENT ON COLUMN CO_PAGOSCONC.NUM_SECUREL IS 'Secuencia de documento Relacionado';

COMMENT ON COLUMN CO_PAGOSCONC.LETRA IS 'Letra de un documento';

COMMENT ON COLUMN CO_PAGOSCONC.IMP_CONCEPTO IS 'Importe del concepto';

COMMENT ON COLUMN CO_PAGOSCONC.COD_PRODUCTO IS 'Producto';

COMMENT ON COLUMN CO_PAGOSCONC.COD_TIPDOCREL IS 'Tipo de Documento Relacionado';

COMMENT ON COLUMN CO_PAGOSCONC.COD_AGENTEREL IS 'Agente de Documento Relacionado';

COMMENT ON COLUMN CO_PAGOSCONC.NUM_TRANSACCION IS 'Numero de transaccion';

COMMENT ON COLUMN CO_PAGOSCONC.LETRA_REL IS 'Letra de Documento Relacionado';

COMMENT ON COLUMN CO_PAGOSCONC.COD_CENTRREL IS 'Centro Emisor de Documento Relacionado';

COMMENT ON COLUMN CO_PAGOSCONC.COD_CONCEPTO IS 'Codigo de Concepto';

COMMENT ON COLUMN CO_PAGOSCONC.COLUMNA IS 'Secuencia';

COMMENT ON COLUMN CO_PAGOSCONC.NUM_ABONADO IS 'Numero de Abonado';

COMMENT ON COLUMN CO_PAGOSCONC.NUM_FOLIO IS 'Numero de Folio';

COMMENT ON COLUMN CO_PAGOSCONC.NUM_CUOTA IS 'Numero de Cuota';

COMMENT ON COLUMN CO_PAGOSCONC.SEC_CUOTA IS 'Secuencia de Cuota';

COMMENT ON COLUMN CO_PAGOSCONC.NUM_VENTA IS 'Numero de Venta';

COMMENT ON COLUMN CO_PAGOSCONC.FEC_CANCELACION IS 'FECHA DE CANCELACION DE DOCUMENTO.';

COMMENT ON COLUMN CO_PAGOSCONC.COD_OPERADORA_SCL IS 'Codigo de operadora';

COMMENT ON COLUMN CO_PAGOSCONC.COD_PLAZA IS 'Codigo de plaza';

COMMENT ON COLUMN CO_PAGOSCONC.PREF_PLAZA IS 'Prefijo Plaza.';

