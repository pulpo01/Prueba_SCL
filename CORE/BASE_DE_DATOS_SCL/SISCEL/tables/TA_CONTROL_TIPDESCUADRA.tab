CREATE TABLE TA_CONTROL_TIPDESCUADRA(
    TIP_DESCUADRATURA NUMBER(2,0) NOT NULL 
 ,  DES_DESCUADRATURA VARCHAR2(100) NOT NULL 
 ,  DES_COMPONENTE VARCHAR2(30) NOT NULL 
 ,  FEC_INSERCION DATE NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_CONTROL_TIPDESCUADRA IS 'DEFINICION DE TIPOS DE DESCUADRATURA.'
/
COMMENT ON COLUMN  TA_CONTROL_TIPDESCUADRA.TIP_DESCUADRATURA IS 'Tipo de descuadratura.'
/
COMMENT ON COLUMN  TA_CONTROL_TIPDESCUADRA.DES_DESCUADRATURA IS 'Descripcion del tipo de descuadratura.'
/
COMMENT ON COLUMN  TA_CONTROL_TIPDESCUADRA.DES_COMPONENTE IS 'Descripcion del componente al cual pertenece el tipo.'
/
COMMENT ON COLUMN  TA_CONTROL_TIPDESCUADRA.FEC_INSERCION IS 'Fecha de insercion de este tipo.'
/
COMMENT ON COLUMN  TA_CONTROL_TIPDESCUADRA.FEC_ULTMOD IS 'Fecha de ultima modificacisn de este tipo.'
/
COMMENT ON COLUMN  TA_CONTROL_TIPDESCUADRA.NOM_USUARIO IS 'Nombre del usuario que modifico / inserts este tipo.'
/
