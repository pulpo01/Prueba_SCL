CREATE TABLE IC_RUTINAS_TD(
    COD_RUTINA VARCHAR2(20) NOT NULL 
 ,  DES_RUTINA VARCHAR2(100) NOT NULL 
 ,  TIP_RUTINA CHAR(1) NOT NULL 
 ,  NOM_FISICO VARCHAR2(50) NOT NULL 
 ,  TIP_VALOR VARCHAR2(50) NULL 
 ,  NOM_USUARIO VARCHAR2(20) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  IC_RUTINAS_TD IS 'RUTINAS A EJECUTAR POR ACTUACION COMERCIAL'
/
COMMENT ON COLUMN  IC_RUTINAS_TD.COD_RUTINA IS 'CODIGO LOGICO DE LA RUTINA'
/
COMMENT ON COLUMN  IC_RUTINAS_TD.DES_RUTINA IS 'DESCRIPCION Y PROPOSITO DE LA RUTINA'
/
COMMENT ON COLUMN  IC_RUTINAS_TD.TIP_RUTINA IS 'F:FUNCION O P:PROCEDIMIENTO'
/
COMMENT ON COLUMN  IC_RUTINAS_TD.NOM_FISICO IS 'NOMBRE FISICO (PACKAGE.PROCEDURE)'
/
COMMENT ON COLUMN  IC_RUTINAS_TD.TIP_VALOR IS 'SI ES FUNCION: TABLA.COLUMNA%TYPE'
/
COMMENT ON COLUMN  IC_RUTINAS_TD.NOM_USUARIO IS 'USUARIO DE CREACION'
/
COMMENT ON COLUMN  IC_RUTINAS_TD.FEC_ULTMOD IS 'FECHA DE CREACION'
/
