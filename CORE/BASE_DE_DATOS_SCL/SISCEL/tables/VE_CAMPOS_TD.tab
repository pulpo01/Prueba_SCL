CREATE TABLE VE_CAMPOS_TD(
    COD_CAMPO NUMBER(2,0) NOT NULL 
 ,  DES_CAMPO VARCHAR2(50) NOT NULL 
 ,  NOM_ATRIBUTO VARCHAR2(30) NOT NULL 
 ,  NOM_OBJETO VARCHAR2(20) NOT NULL 
 ,  IND_OBJETO NUMBER(2,0) NOT NULL 
 ,  DES_OBJETO VARCHAR2(100) NOT NULL 
 ,  FEC_ULTMOD DATE DEFAULT SYSDATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CAMPOS_TD IS 'CAMPOS DE LA PLANTILLA DE DATOS PARA LAS CATEGORÍAS DE VENTA'
/
COMMENT ON COLUMN  VE_CAMPOS_TD.COD_CAMPO IS 'CÓDIGO DEL CAMPO'
/
COMMENT ON COLUMN  VE_CAMPOS_TD.DES_CAMPO IS 'DESCRIPCIÓN DEL CAMPO'
/
COMMENT ON COLUMN  VE_CAMPOS_TD.NOM_ATRIBUTO IS 'NOMBRE DEL ATRIBUTO'
/
COMMENT ON COLUMN  VE_CAMPOS_TD.NOM_OBJETO IS 'NOMBRE DE OBJETO VISUAL BASIC'
/
COMMENT ON COLUMN  VE_CAMPOS_TD.IND_OBJETO IS 'NÚMERO DE OBJETO VB'
/
COMMENT ON COLUMN  VE_CAMPOS_TD.DES_OBJETO IS 'DESCRIPCIÓN DEL OBJETO VB '
/
COMMENT ON COLUMN  VE_CAMPOS_TD.FEC_ULTMOD IS 'FECHA ÚLTIMA MODIFICACIÓN'
/
COMMENT ON COLUMN  VE_CAMPOS_TD.NOM_USUARIO IS 'USUARIO ÚLTIMA MODIFICACIÓN'
/
