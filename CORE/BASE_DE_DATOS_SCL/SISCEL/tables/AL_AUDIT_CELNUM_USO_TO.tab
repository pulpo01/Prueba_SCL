
CREATE TABLE SISCEL.AL_AUDIT_CELNUM_USO_TO
(
  DES_MODULO           VARCHAR2(48 BYTE),
  COD_USUARIO          VARCHAR2(30 BYTE),
  COD_OSUSUARIO        VARCHAR2(30 BYTE),
  DES_PROGRAM          VARCHAR2(48 BYTE),
  FEC_LOGIN            DATE,
  NUM_DESDE            NUMBER(15),
  NUM_HASTA            NUMBER(15),
  NUM_LIBRES_ORI       NUMBER(8),
  NUM_SIGUIENTE_ORI    NUMBER(15),
  NUM_LIBRES_DES       NUMBER(8),
  NUM_SIGUIENTE_DES    NUMBER(15),
  COD_OPERACION        VARCHAR2(1 BYTE),
  FEC_OPERACION        TIMESTAMP ,
  COD_SUBALM_OLD       VARCHAR2(5 BYTE),
  COD_PRODUCTO_OLD     NUMBER(1),
  COD_CENTRAL_OLD      NUMBER(3),
  COD_CAT_OLD          NUMBER(2),
  COD_USO_OLD          NUMBER(2),
  IND_CONTAMINADO_OLD  NUMBER(1)                DEFAULT 0,
  COD_SUBALM           VARCHAR2(5 BYTE),
  COD_PRODUCTO         NUMBER(1),
  COD_CENTRAL          NUMBER(3),
  COD_CAT              NUMBER(2),
  COD_USO              NUMBER(2),
  IND_CONTAMINADO      NUMBER(1)                DEFAULT 0
)
TABLESPACE TS_LOGISTICA_SDAT
PCTUSED    0
PCTFREE    3
INITRANS   16
MAXTRANS   255
STORAGE    (
            INITIAL          128K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE SISCEL.AL_AUDIT_CELNUM_USO_TO IS 'Tabla de auditoria sobre tablas ga_celnum_uso';

COMMENT ON COLUMN SISCEL.AL_AUDIT_CELNUM_USO_TO.DES_MODULO IS 'Descripcion de Modulo';

COMMENT ON COLUMN SISCEL.AL_AUDIT_CELNUM_USO_TO.COD_USUARIO IS 'Codigo de Usuario.';

COMMENT ON COLUMN SISCEL.AL_AUDIT_CELNUM_USO_TO.COD_OSUSUARIO IS 'Usuario Cliente de Oracle';

COMMENT ON COLUMN SISCEL.AL_AUDIT_CELNUM_USO_TO.DES_PROGRAM IS 'Nombre de programa ejecutable';

COMMENT ON COLUMN SISCEL.AL_AUDIT_CELNUM_USO_TO.FEC_LOGIN IS 'Fecha de conexion del usuario al sistema';

COMMENT ON COLUMN SISCEL.AL_AUDIT_CELNUM_USO_TO.NUM_DESDE IS 'Numero desde.';

COMMENT ON COLUMN SISCEL.AL_AUDIT_CELNUM_USO_TO.NUM_HASTA IS 'Ultimo Numero del Rango del Telefono';

COMMENT ON COLUMN SISCEL.AL_AUDIT_CELNUM_USO_TO.NUM_LIBRES_ORI IS 'Cantidad de Numeros Libres del Campo NUM_LIBRES Antes de la Actualizacion';

COMMENT ON COLUMN SISCEL.AL_AUDIT_CELNUM_USO_TO.NUM_SIGUIENTE_ORI IS 'Numero Siguiente  del Campo NUM_SIGUIENTE Antes de la Actualizacion';

COMMENT ON COLUMN SISCEL.AL_AUDIT_CELNUM_USO_TO.NUM_LIBRES_DES IS 'Cantidad de Numeros Libres de Destino del Campo NUM_LIBRES';

COMMENT ON COLUMN SISCEL.AL_AUDIT_CELNUM_USO_TO.NUM_SIGUIENTE_DES IS 'Numero Siguiente Libre de Destino del Campo NUM_SIGUIENTE';

COMMENT ON COLUMN SISCEL.AL_AUDIT_CELNUM_USO_TO.COD_OPERACION IS 'Codigo de operación (I=Insert,D=Delete,U=Update)';

COMMENT ON COLUMN SISCEL.AL_AUDIT_CELNUM_USO_TO.FEC_OPERACION IS 'Fecha Operación.';

/
