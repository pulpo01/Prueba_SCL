CREATE TABLE FA_CRITASIDESP(
    COD_CRITDESP VARCHAR2(3) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  VAL_PARAM VARCHAR2(20) NOT NULL 
 ,  VAL_OPERADOR VARCHAR2(2) NULL 
 ,  COD_DESPACHO VARCHAR2(5) NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  COD_CICLFACT NUMBER(8,0) NULL 
 ,  FEC_PROCESO DATE NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_CRITASIDESP IS 'Criterios de asignacion de secuencia despacho.'
/
COMMENT ON COLUMN  FA_CRITASIDESP.COD_CRITDESP IS 'Codigo de criterio asignacion. [PK 1/2]'
/
COMMENT ON COLUMN  FA_CRITASIDESP.FEC_DESDE IS 'Fecha inicio periodo vigencia. [PK 2/2]'
/
COMMENT ON COLUMN  FA_CRITASIDESP.VAL_PARAM IS 'Valor parametro a considerar.'
/
COMMENT ON COLUMN  FA_CRITASIDESP.VAL_OPERADOR IS 'Operando para busqueda de clientes.'
/
COMMENT ON COLUMN  FA_CRITASIDESP.COD_DESPACHO IS 'Codigo de despacho a asignar.'
/
COMMENT ON COLUMN  FA_CRITASIDESP.FEC_HASTA IS 'Fecha termino periodo vigencia.'
/
COMMENT ON COLUMN  FA_CRITASIDESP.NOM_USUARIO IS 'Nombre usuario responsable modificacion.'
/
COMMENT ON COLUMN  FA_CRITASIDESP.FEC_INGRESO IS 'Fecha ingreso mensaje.'
/
