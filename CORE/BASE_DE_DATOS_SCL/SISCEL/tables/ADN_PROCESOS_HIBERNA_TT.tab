CREATE TABLE ADN_PROCESOS_HIBERNA_TT (  
   IND_PROCESO                   NUMBER(3)               NOT NULL  
 , NUM_PROCESO                   NUMBER(9)               NOT NULL  
 , SEC_REG                       NUMBER(9)               NOT NULL  
 , IND_ERROR                     NUMBER(2)  DEFAULT   0   
 , NUM_CELULAR                   NUMBER(15)                                         
 , FECHA                         DATE                                         
 ) TABLESPACE     INSTALL_DATA
/


COMMENT ON TABLE  ADN_PROCESOS_HIBERNA_TT                      IS '[ ADN_PHIB ] TABLA TEMPORAL PARA PROCESO DE CARGA MASIVA'
/

COMMENT ON COLUMN ADN_PROCESOS_HIBERNA_TT.IND_PROCESO                    IS 'Indicador de Proceso'
/
COMMENT ON COLUMN ADN_PROCESOS_HIBERNA_TT.NUM_PROCESO                    IS 'Numero de Proceso'
/
COMMENT ON COLUMN ADN_PROCESOS_HIBERNA_TT.SEC_REG                        IS 'Secuencia del Registro'
/
COMMENT ON COLUMN ADN_PROCESOS_HIBERNA_TT.IND_ERROR                      IS 'Indicador del Error'
/
COMMENT ON COLUMN ADN_PROCESOS_HIBERNA_TT.NUM_CELULAR                    IS 'Numero de Celular a procesar'
/
COMMENT ON COLUMN ADN_PROCESOS_HIBERNA_TT.FECHA                          IS 'Fecha de Hibernacion sol.'
/

