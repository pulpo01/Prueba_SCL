CREATE TABLE GA_LNBEEP(
    NUM_SERIE VARCHAR2(14) NOT NULL 
 ,  IND_PROCEQUI VARCHAR2(1) NOT NULL 
 ,  COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  COD_FABRICANTE NUMBER(2,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  NUM_SERIEMEC VARCHAR2(20) NULL 
 ,  NUM_CAPCODE NUMBER(7,0) NULL 
 ,  NUM_ABONADO NUMBER(8,0) NULL 
 ,  NUM_BEEPER NUMBER(8,0) NULL 
 ,  COD_CLIENTE NUMBER(8,0) NULL 
 ,  FEC_ALTA DATE NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_LNBEEP IS 'Lista Negra de Equipos Beeper'
/
COMMENT ON COLUMN  GA_LNBEEP.NUM_SERIE IS 'Numero de Serie'
/
COMMENT ON COLUMN  GA_LNBEEP.IND_PROCEQUI IS 'Indicativo de Procedencia del Equipo (E)xterno/(I)nterno'
/
COMMENT ON COLUMN  GA_LNBEEP.COD_OPERADOR IS 'Operador de procedencia del Equipo'
/
COMMENT ON COLUMN  GA_LNBEEP.COD_FABRICANTE IS 'Fabricante del articulo'
/
COMMENT ON COLUMN  GA_LNBEEP.COD_ARTICULO IS 'Articulo'
/
COMMENT ON COLUMN  GA_LNBEEP.NUM_SERIEMEC IS 'Numero de Serie Mecanica'
/
COMMENT ON COLUMN  GA_LNBEEP.NUM_CAPCODE IS 'Numero de Cap Code'
/
COMMENT ON COLUMN  GA_LNBEEP.NUM_ABONADO IS 'Numero de Abonado STARTEL'
/
COMMENT ON COLUMN  GA_LNBEEP.NUM_BEEPER IS 'Numero de Abonado Beeper'
/
COMMENT ON COLUMN  GA_LNBEEP.COD_CLIENTE IS 'Cliente al que pertenece el Equipo'
/
COMMENT ON COLUMN  GA_LNBEEP.FEC_ALTA IS 'Fecha de ingreso en lista negra'
/
