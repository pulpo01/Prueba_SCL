CREATE TABLE FA_HOLDEMPRESA(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  TIP_PLANTARIF VARCHAR2(1) NOT NULL 
 ,  COD_GRUPO NUMBER(8,0) NOT NULL 
 ,  COD_CICLFACT NUMBER(6,0) NOT NULL 
 ,  COD_PLANTARIF VARCHAR2(3) NOT NULL 
 ,  COD_CARGOBASICO VARCHAR2(3) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_HOLDEMPRESA IS 'Cargos Basicos de Holding o Empresa-'
/
COMMENT ON COLUMN  FA_HOLDEMPRESA.COD_PRODUCTO IS 'Producto al que pertenece el Grupo'
/
COMMENT ON COLUMN  FA_HOLDEMPRESA.TIP_PLANTARIF IS 'Tipo de Plantarifario H/E'
/
COMMENT ON COLUMN  FA_HOLDEMPRESA.COD_GRUPO IS 'Grupo de Plantarifario'
/
COMMENT ON COLUMN  FA_HOLDEMPRESA.COD_CICLFACT IS 'Periodo de Facturacion -'
/
COMMENT ON COLUMN  FA_HOLDEMPRESA.COD_PLANTARIF IS 'Codigo de Plantarifario'
/
COMMENT ON COLUMN  FA_HOLDEMPRESA.COD_CARGOBASICO IS 'Codigo de Cargo Basico'
/
COMMENT ON COLUMN  FA_HOLDEMPRESA.COD_CLIENTE IS 'Codigo de Cliente'
/
