CREATE TABLE TA_TFBEEP(
    COD_PLANTARIF VARCHAR2(3) NOT NULL 
 ,  TIP_DIA VARCHAR2(2) NOT NULL 
 ,  COD_FRANHORA VARCHAR2(4) NOT NULL 
 ,  COD_FRANCONS VARCHAR2(3) NOT NULL 
 ,  COD_EFECTIVIDAD VARCHAR2(2) NOT NULL 
 ,  IMP_BEEPER NUMBER(12,4) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_TFBEEP IS 'Importes tarifas beeper'
/
COMMENT ON COLUMN  TA_TFBEEP.COD_PLANTARIF IS 'Csdigo plan tarifario'
/
COMMENT ON COLUMN  TA_TFBEEP.TIP_DIA IS 'Tipo de dma'
/
COMMENT ON COLUMN  TA_TFBEEP.COD_FRANHORA IS 'Csdigo franja horaria'
/
COMMENT ON COLUMN  TA_TFBEEP.COD_FRANCONS IS 'Csdigo franja de consumo'
/
COMMENT ON COLUMN  TA_TFBEEP.COD_EFECTIVIDAD IS 'Csdigo de efectividad'
/
COMMENT ON COLUMN  TA_TFBEEP.IMP_BEEPER IS 'Importe beeper'
/
