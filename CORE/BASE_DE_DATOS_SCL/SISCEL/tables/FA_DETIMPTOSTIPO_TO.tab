CREATE TABLE FA_DETIMPTOSTIPO_TO   (
    NUM_FOLIO NUMBER(12) NOT NULL,
    PREF_PLAZA VARCHAR2(25) NOT NULL,
    COD_CLIENTE NUMBER(8) NOT NULL,
    NUM_ABONADO NUMBER (8) NOT  NULL,
    COD_TIPIMPUES NUMBER(2) NOT NULL,
    ACUM_IMPUESTO_ORIG NUMBER(14,4) DEFAULT 0 NOT NULL ,
    ACUM_NETO_ORIGEN NUMBER(14,4) DEFAULT 0 NOT NULL ,
    ACUM_NETO_AFECTO_ORIGEN NUMBER(14,4) DEFAULT 0 NOT NULL ,
    ACUM_IMPUESTO_NCRED NUMBER(14,4) NULL,
    ACUM_NETO_NCRED NUMBER(14,4) NULL,
    ACUM_NETO_AFECTO_NCRED NUMBER(14,4) NULL
)
TABLESPACE INSTALL_DATA
/

COMMENT ON TABLE FA_DETIMPTOSTIPO_TO IS 'Acumulacion de impuestos para un documento, agrupado por abonado y tipo de impuesto'
/
COMMENT ON COLUMN FA_DETIMPTOSTIPO_TO.NUM_FOLIO IS 'Numero de Folio del documento'
/
COMMENT ON COLUMN FA_DETIMPTOSTIPO_TO.PREF_PLAZA  IS 'Prefijo de Plaza del documento'
/
COMMENT ON COLUMN FA_DETIMPTOSTIPO_TO.COD_CLIENTE  IS 'Codigo de cliente'
/
COMMENT ON COLUMN FA_DETIMPTOSTIPO_TO.NUM_ABONADO  IS 'Numero de Abonado'
/
COMMENT ON COLUMN FA_DETIMPTOSTIPO_TO.COD_TIPIMPUES  IS 'Codigo de tipo impuesto'
/
COMMENT ON COLUMN FA_DETIMPTOSTIPO_TO.ACUM_IMPUESTO_ORIG  IS 'Monto acumulado de impuesto del documento origen'
/
COMMENT ON COLUMN FA_DETIMPTOSTIPO_TO.ACUM_NETO_ORIGEN  IS 'Monto acumulado neto del documento origen'
/
COMMENT ON COLUMN FA_DETIMPTOSTIPO_TO.ACUM_NETO_AFECTO_ORIGEN  IS 'Monto acumulado afecto del documento origen'
/
COMMENT ON COLUMN FA_DETIMPTOSTIPO_TO.ACUM_IMPUESTO_NCRED  IS 'Monto acumulado de impuesto rebajado por notas de credito'
/
COMMENT ON COLUMN FA_DETIMPTOSTIPO_TO.ACUM_NETO_NCRED  IS 'Monto acumulado neto rebajado por notas de credito'
/
COMMENT ON COLUMN FA_DETIMPTOSTIPO_TO.ACUM_NETO_AFECTO_NCRED  IS 'Monto acumulado neto afecto rebajado por notas de credito'
/
