CREATE TABLE FAT_PLANVENCICLIE(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_TIPMOVIMIENT NUMBER(2,0) NOT NULL 
 ,  COD_PLANVENCIMIE VARCHAR2(5) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FAT_PLANVENCICLIE IS 'Plan De Vencimiento Por Cliente.'
/
COMMENT ON COLUMN  FAT_PLANVENCICLIE.COD_CLIENTE IS 'Codigo de Cliente.'
/
COMMENT ON COLUMN  FAT_PLANVENCICLIE.COD_TIPMOVIMIENT IS 'Codigo del tipo de movimiento.'
/
COMMENT ON COLUMN  FAT_PLANVENCICLIE.COD_PLANVENCIMIE IS 'Codigo de Plan de Vencimiento.'
/
COMMENT ON COLUMN  FAT_PLANVENCICLIE.FEC_DESDE IS 'Fecha de Inicio del periodo.'
/
COMMENT ON COLUMN  FAT_PLANVENCICLIE.FEC_HASTA IS 'Fecha de termino del periodo.'
/
COMMENT ON COLUMN  FAT_PLANVENCICLIE.FEC_ULTMOD IS 'Fecha de modificacion.'
/
COMMENT ON COLUMN  FAT_PLANVENCICLIE.NOM_USUARIO IS 'Nombre de Usuario.'
/
