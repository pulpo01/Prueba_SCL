CREATE TABLE AL_COMPRAS_MERCADERIA(
    COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  MES_COMPRA DATE NOT NULL 
 ,  PRC_COMPRA NUMBER(14,4) NOT NULL 
 ,  CAN_STOCK NUMBER(9,0) NOT NULL 
 ,  COD_OPERADORA_SCL VARCHAR2(5) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_COMPRAS_MERCADERIA IS 'Compras de los tipos de stock  Mercaderia.'
/
COMMENT ON COLUMN  AL_COMPRAS_MERCADERIA.COD_ARTICULO IS 'Artmculo'
/
COMMENT ON COLUMN  AL_COMPRAS_MERCADERIA.MES_COMPRA IS 'Mes de compra'
/
COMMENT ON COLUMN  AL_COMPRAS_MERCADERIA.PRC_COMPRA IS 'Precio de Compra Mas Alto'
/
COMMENT ON COLUMN  AL_COMPRAS_MERCADERIA.CAN_STOCK IS 'Cantidad en stock'
/
COMMENT ON COLUMN  AL_COMPRAS_MERCADERIA.COD_OPERADORA_SCL IS 'Codigo de operadora SCL.'
/
