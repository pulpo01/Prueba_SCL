CREATE TABLE GA_SERVSUPL_HOMTEC(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_SERVICIO_TDMA VARCHAR2(3) NOT NULL 
 ,  COD_SERVSUPL_TDMA NUMBER(2,0) NOT NULL 
 ,  COD_NIVEL_TDMA NUMBER(4,0) NOT NULL 
 ,  COD_SERVICIO_GSM VARCHAR2(3) NOT NULL 
 ,  COD_SERVSUPL_GSM NUMBER(2,0) NOT NULL 
 ,  COD_NIVEL_GSM NUMBER(4,0) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) DEFAULT USER NOT NULL 
 ,  FEC_ULTMOD DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_SERVSUPL_HOMTEC IS 'SERVICIOS SUPLEMENTARIOS HOMOLOGABLES POR TECNOLOGIA'
/
COMMENT ON COLUMN  GA_SERVSUPL_HOMTEC.COD_PRODUCTO IS 'CODIGO DEL PRODUCTO'
/
COMMENT ON COLUMN  GA_SERVSUPL_HOMTEC.COD_SERVICIO_TDMA IS 'CODIGO DE SERVICIO TDMA'
/
COMMENT ON COLUMN  GA_SERVSUPL_HOMTEC.COD_SERVSUPL_TDMA IS 'CODIGO DE SERVICIO SUPLEMENTARIO TDMA'
/
COMMENT ON COLUMN  GA_SERVSUPL_HOMTEC.COD_NIVEL_TDMA IS 'NIVEL DE SERVICIO SUPLEMENTARIO TDMA'
/
COMMENT ON COLUMN  GA_SERVSUPL_HOMTEC.COD_SERVICIO_GSM IS 'CODIGO DE SERVICIO GSM'
/
COMMENT ON COLUMN  GA_SERVSUPL_HOMTEC.COD_SERVSUPL_GSM IS 'CODIGO DE SERVICIO SUPLEMENTARIO GSM'
/
COMMENT ON COLUMN  GA_SERVSUPL_HOMTEC.COD_NIVEL_GSM IS 'NIVEL DE SERVICIO SUPLEMENTARIO GSM'
/
COMMENT ON COLUMN  GA_SERVSUPL_HOMTEC.NOM_USUARORA IS 'USUARIO QUE REALIZO LA MODIFICACION'
/
COMMENT ON COLUMN  GA_SERVSUPL_HOMTEC.FEC_ULTMOD IS 'FECHA ULTIMA MODIFICACION'
/
