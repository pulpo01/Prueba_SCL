CREATE TABLE FA_APLICADONC(
    NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  FEC_INICIO DATE NOT NULL 
 ,  FEC_FIN DATE NOT NULL 
 ,  TOT_APLICADO NUMBER(14,4) DEFAULT 0.0 NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_APLICADONC IS 'Importes Aplicados en forma de Nota de Cridito'
/
COMMENT ON COLUMN  FA_APLICADONC.FEC_INICIO IS 'Fecha de Inicio del Acumulado'
/
COMMENT ON COLUMN  FA_APLICADONC.FEC_FIN IS 'Fecha Fin del Acumulado'
/
COMMENT ON COLUMN  FA_APLICADONC.TOT_APLICADO IS 'Total Aplicado en Notas Cridito'
/
COMMENT ON COLUMN  FA_APLICADONC.NOM_USUARIO IS 'Nombre Usuario'
/
COMMENT ON COLUMN  FA_APLICADONC.COD_CLIENTE IS 'Codigo Cliente'
/
