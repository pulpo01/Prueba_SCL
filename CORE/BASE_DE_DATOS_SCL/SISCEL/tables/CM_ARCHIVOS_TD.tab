CREATE TABLE CM_ARCHIVOS_TD(
    COD_UNIVERSO VARCHAR2(6) NOT NULL 
 ,  COD_ARCHIVO VARCHAR2(10) NOT NULL 
 ,  NOM_FISICO VARCHAR2(20) NOT NULL 
 ,  CAR_SEPARADOR CHAR(1) NULL 
 ,  FEC_CREACION DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  IND_EJECUCION CHAR(1) NOT NULL 
 ,  USU_ULT_GENERACION VARCHAR2(30) NULL 
 ,  FEC_ULT_GENERACION DATE NULL 
 ,  NUM_REGISTROS NUMBER(8,0) NULL 
 ,  UBICACION VARCHAR2(100) NULL 
 ,  SEQ_PROCESO NUMBER(6,0) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_ARCHIVOS_TD IS '[ ARCH ] Tabla de configuracisn de ArchivosTabla de configuracisn de Archivos'
/
COMMENT ON COLUMN  CM_ARCHIVOS_TD.COD_UNIVERSO IS 'UNIVERSO SOBRE EL QUE SE CREA EL ARCHIVO'
/
COMMENT ON COLUMN  CM_ARCHIVOS_TD.COD_ARCHIVO IS 'ARCHIVO SOBRE EL QUE SE APLICA EL DETALLE'
/
COMMENT ON COLUMN  CM_ARCHIVOS_TD.NOM_FISICO IS 'NOMBRE FISICO DEL ARCHIVO'
/
COMMENT ON COLUMN  CM_ARCHIVOS_TD.CAR_SEPARADOR IS 'CARACTER SEPARADOR DE LOS CAMPOS DEL ARCHIVO (-, /, ;, |)'
/
COMMENT ON COLUMN  CM_ARCHIVOS_TD.FEC_CREACION IS 'FECHA DE CREACION DEL ARCHIVO'
/
COMMENT ON COLUMN  CM_ARCHIVOS_TD.NOM_USUARIO IS 'USUARIO DE CREACISN DEL ARCHIVO'
/
COMMENT ON COLUMN  CM_ARCHIVOS_TD.IND_EJECUCION IS 'INDICADOR DE EJECUCISN DEL ARCHIVO (S:SI  N:NO)'
/
COMMENT ON COLUMN  CM_ARCHIVOS_TD.USU_ULT_GENERACION IS 'USUARIO ULTIMA MODIFICACION DEL ARCHIVO'
/
COMMENT ON COLUMN  CM_ARCHIVOS_TD.FEC_ULT_GENERACION IS 'FECHA ULTIMA MODIFICACION DEL ARCHIVO'
/
COMMENT ON COLUMN  CM_ARCHIVOS_TD.NUM_REGISTROS IS 'NUMERO DE REGISTROS CREADOS POR EL ARCHIVO'
/
COMMENT ON COLUMN  CM_ARCHIVOS_TD.UBICACION IS 'UBICACISN O PATH DEL ARCHIVO'
/
COMMENT ON COLUMN  CM_ARCHIVOS_TD.SEQ_PROCESO IS 'SECUENCIA MANUAL DE PROCESO EN EJECUCISN'
/
