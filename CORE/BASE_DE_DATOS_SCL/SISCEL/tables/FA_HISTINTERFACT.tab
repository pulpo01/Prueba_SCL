CREATE TABLE SISCEL.FA_HISTINTERFACT
(
  NUM_PROCESO        NUMBER(8)                  NOT NULL,
  NUM_VENTA          NUMBER(8),
  COD_MODGENER       VARCHAR2(3 BYTE)           NOT NULL,
  COD_ESTADOC        NUMBER(3)                  NOT NULL,
  COD_ESTPROC        NUMBER(1)                  NOT NULL,
  COD_TIPMOVIMIEN    NUMBER(2)                  NOT NULL,
  COD_CATRIBUT       CHAR(1 BYTE)               NOT NULL,
  COD_CATIMPOSITIVA  NUMBER(1),
  COD_TIPDOCUM       NUMBER(2)                  NOT NULL,
  NUM_FOLIO          NUMBER(9),
  NUM_FOLIOREL       NUMBER(9),
  FEC_INGRESO        DATE,
  FEC_FACTURACION    DATE,
  FEC_IMPRESION      DATE,
  FEC_FOLIACION      DATE,
  FEC_VISACION       DATE,
  FEC_PASOCOBRO      DATE,
  FEC_CIERRE         DATE,
  FEC_VENCIMIENTO    DATE,
  NUM_CUOTAS         NUMBER(3)                  DEFAULT 0                     NOT NULL,
  COD_MODVENTA       NUMBER(2)                  DEFAULT 1                     NOT NULL,
  NOM_USUAELIM       VARCHAR2(30 BYTE),
  COD_CAUSAELIM      VARCHAR2(5 BYTE),
  PREF_PLAZA         VARCHAR2(25 BYTE)          DEFAULT 'TMG'                 NOT NULL,
  PREF_PLAZAREL      VARCHAR2(25 BYTE)          DEFAULT 'TMG'                 NOT NULL,
  TIP_FOLIACION      VARCHAR2(1 BYTE)           DEFAULT 'P'                   NOT NULL
)  TABLESPACE TS_FACTURACION_MDAT
/

COMMENT ON TABLE SISCEL.FA_HISTINTERFACT IS 'Historico de la Interfaz de Facturacion.';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.NUM_PROCESO IS 'Numero de proceso.  [PK 1/1]';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.NUM_VENTA IS 'Numero de venta';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.COD_MODGENER IS 'Indicador de generacion de documentos.';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.COD_ESTADOC IS 'Estado del documento.';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.COD_ESTPROC IS 'Estado del proceso: 0=sin procesar | 1=procesando | 2=terminado con error | 3=terminado correctamente';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.COD_TIPMOVIMIEN IS 'Tipo de movimiento: venta, miscelanea, NC, ciclo';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.COD_CATRIBUT IS 'Categoria tributaria: F=factura | B=boleta';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.COD_CATIMPOSITIVA IS 'Codigo de categoria impositiva del cliente: 1=afect | 0=exento';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.COD_TIPDOCUM IS 'Tipo documento a generar.';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.NUM_FOLIO IS 'Numero de folio.';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.NUM_FOLIOREL IS 'Numero de folio relacionado a NC.';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.FEC_INGRESO IS 'Fecha de Ingreso (HORA)';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.FEC_FACTURACION IS 'Fecha de Facturacion (HORA)';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.FEC_IMPRESION IS 'Fecha de impresion (HORA)';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.FEC_FOLIACION IS 'Fecha de Foliacion (HORA)';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.FEC_VISACION IS 'Fecha de visado (HORA)       ';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.FEC_PASOCOBRO IS 'Fecha de Paso a Cobros (HORA)';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.FEC_CIERRE IS 'Fecha de Cierre (HORA)';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.FEC_VENCIMIENTO IS 'Fecha de vencimiento de la factura.';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.NUM_CUOTAS IS 'Numero de cuotas de la venta.';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.COD_MODVENTA IS 'Modalidad de venta.';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.NOM_USUAELIM IS 'Usuario responsable de la eliminacisn del documento';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.COD_CAUSAELIM IS 'Codigo causa de eliminacisn E: Error de digitacion; D: duplicidad';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.PREF_PLAZA IS 'PREFIJO DE LA PLAZA';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.PREF_PLAZAREL IS 'PREFIJO DE LA PLAZA RELACIONADA';

COMMENT ON COLUMN SISCEL.FA_HISTINTERFACT.TIP_FOLIACION IS 'TIPO DE FOLIACION A:AUTOFOLIATIVA P:PREFOLIADA';

