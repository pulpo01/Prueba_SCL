CREATE TABLE FA_AJUSTES_TO
(
  IDE_AJUSTE           NUMBER(8)                NOT NULL,
  PROCESO              NUMBER(8)                NOT NULL,
  IND_TIPDOCUM         NUMBER(2)                NOT NULL,
  COD_OPERADORA        VARCHAR2(5 BYTE),
  PREF_FOLIO           VARCHAR2(25 BYTE),
  NUM_FOLIO            NUMBER(9),
  COD_CLIENTE          NUMBER(8)                NOT NULL,
  TOT_DOCUM            NUMBER(14,4),
  ESTADO               NUMBER(3),
  OBS_ESTADO           VARCHAR2(50 BYTE),
  NUM_PROCESO          NUMBER(10),
  FEC_VENCIMIE         DATE,
  FEC_ESTADO           DATE,
  USUARIO              VARCHAR2(30 BYTE),
  COD_TIPOLOGIA        VARCHAR2(5 BYTE),
  COD_AREAIMPUTABLE    VARCHAR2(5 BYTE),
  COD_AREASOLICITANTE  VARCHAR2(5 BYTE)
)
TABLESPACE TS_FACTURACION_SDAT
PCTUSED    0
PCTFREE    20
INITRANS   16
MAXTRANS   255
STORAGE    (
            INITIAL          80K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
CACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE FA_AJUSTES_TO IS 'DOCUMENTO AJUTE MASIVOS';

COMMENT ON COLUMN FA_AJUSTES_TO.COD_TIPOLOGIA IS 'Cod. Tipologia';

COMMENT ON COLUMN FA_AJUSTES_TO.COD_AREAIMPUTABLE IS 'Cod. Area Imputable';

COMMENT ON COLUMN FA_AJUSTES_TO.COD_AREASOLICITANTE IS 'Cod. Area Solicitante';

COMMENT ON COLUMN FA_AJUSTES_TO.IDE_AJUSTE IS 'IDENTIFICACION SOLICITUD DE AJUSTE';

COMMENT ON COLUMN FA_AJUSTES_TO.PROCESO IS 'IDENTIFICADOR DEL DETALLE DE AJUSTE';

COMMENT ON COLUMN FA_AJUSTES_TO.IND_TIPDOCUM IS 'INDICADOR DEL TIPO DE DOCUMENTO SOLICITADO';

COMMENT ON COLUMN FA_AJUSTES_TO.COD_OPERADORA IS 'OPERADORA PARA LA SOLICITUD';

COMMENT ON COLUMN FA_AJUSTES_TO.PREF_FOLIO IS 'PREFIJO DE FOLIO';

COMMENT ON COLUMN FA_AJUSTES_TO.NUM_FOLIO IS 'NUMERO DE ,FOLIO';

COMMENT ON COLUMN FA_AJUSTES_TO.COD_CLIENTE IS 'CODIGO DE CLIENTE';

COMMENT ON COLUMN FA_AJUSTES_TO.TOT_DOCUM IS 'TOTAL DOCUMENTO';

COMMENT ON COLUMN FA_AJUSTES_TO.ESTADO IS 'ESTADO DEL DOCUMENTO';

COMMENT ON COLUMN FA_AJUSTES_TO.OBS_ESTADO IS 'OBSERVACIONES ESTADO';

COMMENT ON COLUMN FA_AJUSTES_TO.NUM_PROCESO IS 'NUMERO PROCESO DOC. GENERADO';

COMMENT ON COLUMN FA_AJUSTES_TO.FEC_VENCIMIE IS 'FECHA VENCIMIENTO DOCUMENTO';

COMMENT ON COLUMN FA_AJUSTES_TO.FEC_ESTADO IS 'FECHA ACTUALIZACION ESTADO';

COMMENT ON COLUMN FA_AJUSTES_TO.USUARIO IS 'USUARIO';
