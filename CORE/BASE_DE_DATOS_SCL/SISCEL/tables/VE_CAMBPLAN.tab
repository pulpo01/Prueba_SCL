CREATE TABLE VE_CAMBPLAN(
    COD_PLANCOM_A NUMBER(6,0) NOT NULL 
 ,  COD_PLANCOM_N NUMBER(6,0) NOT NULL 
 ,  COD_CALCLIEN VARCHAR2(2) NOT NULL 
 ,  FEC_MODIFIC DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CAMBPLAN IS 'Cambios de Plan Comercial filtrados por Calidad de Cliente'
/
COMMENT ON COLUMN  VE_CAMBPLAN.COD_PLANCOM_A IS 'Codigo Plan Comercial Antiguo'
/
COMMENT ON COLUMN  VE_CAMBPLAN.COD_PLANCOM_N IS 'Codigo Plan Comercial Nuevo'
/
COMMENT ON COLUMN  VE_CAMBPLAN.COD_CALCLIEN IS 'Calidad de Cliente (filtro)'
/
COMMENT ON COLUMN  VE_CAMBPLAN.FEC_MODIFIC IS 'Fecha modificacion = lanzamiento del proceso'
/
COMMENT ON COLUMN  VE_CAMBPLAN.NOM_USUARIO IS 'Usuario que realiza el cambio'
/
