CREATE TABLE FA_RANGO_TABLA(
    COD_CICLFACT NUMBER(6,0) NOT NULL 
 ,  RANGO_INI NUMBER(8,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  RANGO_FIN NUMBER(8,0) NOT NULL 
 ,  NOM_TABLA VARCHAR2(20) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_RANGO_TABLA IS 'Rango por cliente para Partition View'
/
COMMENT ON COLUMN  FA_RANGO_TABLA.COD_CICLFACT IS 'Periodo de Facturacion'
/
COMMENT ON COLUMN  FA_RANGO_TABLA.RANGO_INI IS 'Rango inicial de cliente'
/
COMMENT ON COLUMN  FA_RANGO_TABLA.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  FA_RANGO_TABLA.RANGO_FIN IS 'Rango final de cliente'
/
COMMENT ON COLUMN  FA_RANGO_TABLA.NOM_TABLA IS 'Nombre de tabla para un rango y ciclfact'
/
