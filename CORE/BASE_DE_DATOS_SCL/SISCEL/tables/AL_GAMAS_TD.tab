CREATE TABLE SISCEL.AL_GAMAS_TD
(
  COD_GAMA           NUMBER(5)                  NOT NULL,
  DESC_GAMA          VARCHAR2(50 BYTE)          NOT NULL,
  VALOR_PRESTAMO     NUMBER(5)                  NOT NULL,
  FEC_CREA           DATE                       NOT NULL,
  USUARIO_CREA       VARCHAR2(30 BYTE)          NOT NULL,
  FEC_ACTUALIZA      DATE,
  USUARIO_ACTUALIZA  VARCHAR2(30 BYTE)
)  TABLESPACE INSTALL_DATA
/

COMMENT ON TABLE SISCEL.AL_GAMAS_TD IS '[ GAMA ] Contiene los atributos tipo GAMA de los articulos';

COMMENT ON COLUMN SISCEL.AL_GAMAS_TD.COD_GAMA IS 'Código de Gama asociado al artículo';

COMMENT ON COLUMN SISCEL.AL_GAMAS_TD.DESC_GAMA IS 'Descripción de la gama';

COMMENT ON COLUMN SISCEL.AL_GAMAS_TD.VALOR_PRESTAMO IS 'Orden de préstamo utilizado en STP';

COMMENT ON COLUMN SISCEL.AL_GAMAS_TD.FEC_CREA IS 'Fecha creación del registro';

COMMENT ON COLUMN SISCEL.AL_GAMAS_TD.USUARIO_CREA IS 'Usuario que crea el registro';

COMMENT ON COLUMN SISCEL.AL_GAMAS_TD.FEC_ACTUALIZA IS 'Fecha modificación del registro';

COMMENT ON COLUMN SISCEL.AL_GAMAS_TD.USUARIO_ACTUALIZA IS 'Usuario que modifica el registro';
