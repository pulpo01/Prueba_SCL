CREATE TABLE GA_ACTUASERV(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_ACTABO VARCHAR2(2) NOT NULL 
 ,  COD_TIPSERV VARCHAR2(1) NOT NULL 
 ,  COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_ACTUASERV IS 'Servicios x Actuaciones de Abonados'
/
COMMENT ON COLUMN  GA_ACTUASERV.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_ACTUASERV.COD_ACTABO IS 'Codigo de Actuacion de Abonados'
/
COMMENT ON COLUMN  GA_ACTUASERV.COD_TIPSERV IS 'Codigo Tipo de servicio'
/
COMMENT ON COLUMN  GA_ACTUASERV.COD_SERVICIO IS 'Codigo de Servicio'
/
COMMENT ON COLUMN  GA_ACTUASERV.COD_CONCEPTO IS 'Codigo de Concepto de Facturacion Asociado'
/
