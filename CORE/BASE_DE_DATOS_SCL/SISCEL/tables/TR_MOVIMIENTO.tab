CREATE TABLE TR_MOVIMIENTO(
    NUM_MOVIMIENTO NUMBER(9,0) NOT NULL 
 ,  NUM_INTENTOS NUMBER(3,0) NOT NULL 
 ,  DES_RESPUESTA VARCHAR2(80) NOT NULL 
 ,  COD_ACCION NUMBER(3,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  IND_BLOQUEO NUMBER(1,0) NOT NULL 
 ,  FEC_EJECUCION DATE NULL 
 ,  NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(11) NULL 
 ,  NUM_SERIE_NUE VARCHAR2(11) NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  TR_MOVIMIENTO.NUM_MOVIMIENTO IS 'Numero de movimiento'
/
COMMENT ON COLUMN  TR_MOVIMIENTO.NUM_INTENTOS IS 'Numero de intentos'
/
COMMENT ON COLUMN  TR_MOVIMIENTO.DES_RESPUESTA IS 'Descripcion de la respuesta del HLR'
/
COMMENT ON COLUMN  TR_MOVIMIENTO.COD_ACCION IS 'Codigo de Accion'
/
COMMENT ON COLUMN  TR_MOVIMIENTO.NOM_USUARIO IS 'Nombre usuario que genera movimiento'
/
COMMENT ON COLUMN  TR_MOVIMIENTO.FEC_INGRESO IS 'Fecha de ingreso del movimiento'
/
COMMENT ON COLUMN  TR_MOVIMIENTO.IND_BLOQUEO IS 'Estado de bloqueo'
/
COMMENT ON COLUMN  TR_MOVIMIENTO.FEC_EJECUCION IS 'Fecha de Ejecucisn del movimiento'
/
COMMENT ON COLUMN  TR_MOVIMIENTO.NUM_CELULAR IS 'Numero de Celular'
/
COMMENT ON COLUMN  TR_MOVIMIENTO.NUM_SERIE IS 'Numero de Serie (hexadecimal)'
/
COMMENT ON COLUMN  TR_MOVIMIENTO.NUM_SERIE_NUE IS 'Numero de Serie nuevo (Hexadecimal)'
/
