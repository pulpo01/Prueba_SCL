CREATE TABLE VE_TIPORED_TD(
    COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  DES_TIPORED VARCHAR2(30) NOT NULL 
 ,  COD_CICLOCOMIS NUMBER(2,0) NOT NULL 
 ,  IND_TIPO NUMBER(1,0) NOT NULL 
 ,  NUM_NIVELES NUMBER(2,0) NOT NULL 
 ,  IND_CANAL NUMBER(1,0) NOT NULL 
 ,  FEC_ULTMOD DATE DEFAULT SYSDATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ,  IND_APLIC_SUBORDINADA CHAR(1) DEFAULT 'N' NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_TIPORED_TD IS 'TIPOS DE RED'
/
COMMENT ON COLUMN  VE_TIPORED_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED'
/
COMMENT ON COLUMN  VE_TIPORED_TD.DES_TIPORED IS 'DESCRIPCION DEL TIPO DE RED'
/
COMMENT ON COLUMN  VE_TIPORED_TD.COD_CICLOCOMIS IS 'CODIGO DEL CICLO DE COMISIONES'
/
COMMENT ON COLUMN  VE_TIPORED_TD.IND_TIPO IS 'INDICADOR TIPOLOGIA (INTERNO = 0, EXTERNO = 1, HIBRIDO = 2) VER GED_CODIGOS'
/
COMMENT ON COLUMN  VE_TIPORED_TD.NUM_NIVELES IS 'NUMERO DE NIVELES DE LA RED'
/
COMMENT ON COLUMN  VE_TIPORED_TD.IND_CANAL IS 'INDICADOR DE ESTRUCTURA DE CANALIZACION (SUPER CANAL = 0, CANAL = 1, SUBCANAL = 2) VER GED_CODIGOS'
/
COMMENT ON COLUMN  VE_TIPORED_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  VE_TIPORED_TD.NOM_USUARIO IS 'USUARIO DE ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  VE_TIPORED_TD.IND_APLIC_SUBORDINADA IS 'INDICA APLICACION DE COMISION SUBORDINADA'
/
