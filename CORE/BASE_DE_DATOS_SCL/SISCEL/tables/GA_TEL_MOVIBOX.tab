CREATE TABLE GA_TEL_MOVIBOX(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  IND_PROCESADO VARCHAR2(2) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  NUM_TELEFONO1 VARCHAR2(10) NULL 
 ,  NUM_TELEFONO2 VARCHAR2(10) NULL 
 ,  NUM_TELEFONO3 VARCHAR2(10) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_TEL_MOVIBOX IS 'Numeros de telefono asociados al producto Celular Joven - MOVIBOX (maximo tres telefonos).'
/
COMMENT ON COLUMN  GA_TEL_MOVIBOX.COD_CLIENTE IS 'Codigo de cliente.'
/
COMMENT ON COLUMN  GA_TEL_MOVIBOX.NUM_ABONADO IS 'Numero de abonado.'
/
COMMENT ON COLUMN  GA_TEL_MOVIBOX.NUM_CELULAR IS 'Numero de celular.'
/
COMMENT ON COLUMN  GA_TEL_MOVIBOX.IND_PROCESADO IS 'Indicador de proceso: I = Ingresado, M = Modificado.'
/
COMMENT ON COLUMN  GA_TEL_MOVIBOX.FEC_ALTA IS 'Fecha de ingreso del numero de telefono MOVIBOX.'
/
COMMENT ON COLUMN  GA_TEL_MOVIBOX.NOM_USUARIO IS 'Usuario (Oracle) que efectuo el ingreso.'
/
COMMENT ON COLUMN  GA_TEL_MOVIBOX.NUM_TELEFONO1 IS 'Numero de telefono MOVIBOX 1'
/
COMMENT ON COLUMN  GA_TEL_MOVIBOX.NUM_TELEFONO2 IS 'Numero de telefono MOVIBOX 2'
/
COMMENT ON COLUMN  GA_TEL_MOVIBOX.NUM_TELEFONO3 IS 'Numero de telefono MOVIBOX 3'
/
