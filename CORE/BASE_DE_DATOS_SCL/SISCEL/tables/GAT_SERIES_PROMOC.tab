CREATE TABLE GAT_SERIES_PROMOC(
    NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  COD_PROMOCION NUMBER(5,0) NOT NULL 
 ,  COD_MONEDA VARCHAR2(3) NULL 
 ,  CARGA_TOTAL NUMBER(14,4) NULL 
 ,  CARGA_PERIODO NUMBER(14,4) NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ,  FEC_PROMOCION DATE NOT NULL 
 ,  NUM_ABONADO_ANT NUMBER(8,0) NULL 
 ,  NUM_ABONADO_ACT NUMBER(8,0) NULL 
 ,  ORIGEN CHAR(1) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GAT_SERIES_PROMOC IS 'Series En Promocion Prepago.'
/
COMMENT ON COLUMN  GAT_SERIES_PROMOC.NUM_SERIE IS 'Numero de serie.'
/
COMMENT ON COLUMN  GAT_SERIES_PROMOC.COD_PROMOCION IS 'codigo de promocion.'
/
COMMENT ON COLUMN  GAT_SERIES_PROMOC.COD_MONEDA IS 'codigo de moneda UF, dolar, etc.'
/
COMMENT ON COLUMN  GAT_SERIES_PROMOC.CARGA_TOTAL IS 'Carga total a celular.'
/
COMMENT ON COLUMN  GAT_SERIES_PROMOC.CARGA_PERIODO IS 'carga del periodo.'
/
COMMENT ON COLUMN  GAT_SERIES_PROMOC.FEC_ALTA IS 'fecha de alta.'
/
COMMENT ON COLUMN  GAT_SERIES_PROMOC.FEC_PROMOCION IS 'fecha de asignacion de promocion.'
/
COMMENT ON COLUMN  GAT_SERIES_PROMOC.NUM_ABONADO_ANT IS 'numero abonado anterior.'
/
COMMENT ON COLUMN  GAT_SERIES_PROMOC.NUM_ABONADO_ACT IS 'numero de abonado actual.'
/
COMMENT ON COLUMN  GAT_SERIES_PROMOC.ORIGEN IS 'origen de la venta.'
/
