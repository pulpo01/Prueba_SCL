CREATE TABLE ERD_PRODUCTOS(
    COD_PRODUCTO NUMBER(6,0) NOT NULL 
 ,  COD_FUENTE NUMBER(6,0) NOT NULL 
 ,  NOM_PRODUCTO VARCHAR2(30) NOT NULL 
 ,  NOM_USUARIO_MODI VARCHAR2(30) NOT NULL 
 ,  FEC_MODI_H DATE NOT NULL 
 ,  IND_ACTIVO CHAR(1) DEFAULT 'S' NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERD_PRODUCTOS IS 'Definicion De Productos Ofrecidos Por Las Fuentes.'
/
COMMENT ON COLUMN  ERD_PRODUCTOS.COD_PRODUCTO IS 'Identificador unico para el producto o informe.'
/
COMMENT ON COLUMN  ERD_PRODUCTOS.COD_FUENTE IS 'Identificador unico para fuentes de informacion. Ver tabla ERD_FUENTES.'
/
COMMENT ON COLUMN  ERD_PRODUCTOS.NOM_PRODUCTO IS 'Nombre del producto o informe.'
/
COMMENT ON COLUMN  ERD_PRODUCTOS.NOM_USUARIO_MODI IS 'Nombre del usuario que creo/actualizo este registro.'
/
COMMENT ON COLUMN  ERD_PRODUCTOS.FEC_MODI_H IS 'fecha de creacion/modificacion de este registro.'
/
