CREATE TABLE IC_PARAMRUTINAS_TD(
    COD_RUTINA VARCHAR2(20) NOT NULL 
 ,  NOM_PARAMETRO VARCHAR2(20) NOT NULL 
 ,  NUM_ORDEN NUMBER(2,0) NOT NULL 
 ,  MOD_PARAMETRO CHAR(1) NOT NULL 
 ,  VAL_INICIAL VARCHAR2(50) NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  IC_PARAMRUTINAS_TD IS 'ASOCIACION DE PARAMETROS POR RUTINA'
/
COMMENT ON COLUMN  IC_PARAMRUTINAS_TD.COD_RUTINA IS 'RUTINA QUE SE EJECUTA'
/
COMMENT ON COLUMN  IC_PARAMRUTINAS_TD.NOM_PARAMETRO IS 'CODIGO IDENTIFICADOR DEL PARAMETRO'
/
COMMENT ON COLUMN  IC_PARAMRUTINAS_TD.NUM_ORDEN IS 'ORDEN DE PASO DE PARAMETRO'
/
COMMENT ON COLUMN  IC_PARAMRUTINAS_TD.MOD_PARAMETRO IS 'PARAMETRO DE I:IN O:OUT B:BOTH'
/
COMMENT ON COLUMN  IC_PARAMRUTINAS_TD.VAL_INICIAL IS 'VALOR CON EL CUAL SE INICIALIZA LA VARIABLE.'
/
