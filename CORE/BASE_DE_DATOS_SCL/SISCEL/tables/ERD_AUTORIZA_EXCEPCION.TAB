CREATE TABLE ERD_AUTORIZA_EXCEPCION(
    COD_AUTORIZA_EXCEP VARCHAR2(5) NOT NULL 
 ,  NOM_AUTORIZA_EXCEP VARCHAR2(50) NOT NULL 
 ,  IND_VIGENCIA CHAR(1) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERD_AUTORIZA_EXCEPCION IS 'Personas que autorizan excepciones en Evaluacion.'
/
COMMENT ON COLUMN  ERD_AUTORIZA_EXCEPCION.COD_AUTORIZA_EXCEP IS 'Codigo de quien autoriza la excepcion.'
/
COMMENT ON COLUMN  ERD_AUTORIZA_EXCEPCION.NOM_AUTORIZA_EXCEP IS 'Nombre de quien autoriza la excepcion.'
/
COMMENT ON COLUMN  ERD_AUTORIZA_EXCEPCION.IND_VIGENCIA IS 'Indicador de Vigencia (S/N).'
/
COMMENT ON COLUMN  ERD_AUTORIZA_EXCEPCION.FEC_ULTMOD IS 'fecha de creacion/modificacion de este registro.'
/
COMMENT ON COLUMN  ERD_AUTORIZA_EXCEPCION.NOM_USUARIO IS 'Nombre del usuario que creo/actualizo este registro.'
/
