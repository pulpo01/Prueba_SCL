CREATE TABLE SP_ACCESORIOS_REEMPLAZO(
    NUM_ORDEN NUMBER(9,0) NOT NULL 
 ,  NUM_LINEA NUMBER(2,0) NOT NULL 
 ,  COD_BODEGA_REEM NUMBER(6,0) NOT NULL 
 ,  TIP_STOCK_REEM NUMBER(2,0) NOT NULL 
 ,  COD_ARTICULO_REEM NUMBER(6,0) NOT NULL 
 ,  COD_USO_REEM NUMBER(2,0) NOT NULL 
 ,  COD_ESTADO_REEM NUMBER(2,0) NOT NULL 
 ,  CANTIDAD NUMBER(2,0) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SP_ACCESORIOS_REEMPLAZO IS 'Accesorios que se dan en prestamo al cliente'
/
COMMENT ON COLUMN  SP_ACCESORIOS_REEMPLAZO.NUM_ORDEN IS 'Num. Orden de Reparacion a la que referencian los accesorios reemplazo'
/
COMMENT ON COLUMN  SP_ACCESORIOS_REEMPLAZO.NUM_LINEA IS 'Numero de la linea de accesorios de reemplazo de la orden de reparacion'
/
COMMENT ON COLUMN  SP_ACCESORIOS_REEMPLAZO.COD_BODEGA_REEM IS 'Codigo de la Bodega de donde se ha tomado el art. de reemplazo'
/
COMMENT ON COLUMN  SP_ACCESORIOS_REEMPLAZO.TIP_STOCK_REEM IS 'Tipo de Stock de los acceorios reemplazo'
/
COMMENT ON COLUMN  SP_ACCESORIOS_REEMPLAZO.COD_ARTICULO_REEM IS 'Codigo de Articulo del accesorio reemplazo'
/
COMMENT ON COLUMN  SP_ACCESORIOS_REEMPLAZO.COD_USO_REEM IS 'Codigo de Uso del Accesorio reemplazo'
/
COMMENT ON COLUMN  SP_ACCESORIOS_REEMPLAZO.COD_ESTADO_REEM IS 'Codigo de Estado del accesorio reemplazo'
/
COMMENT ON COLUMN  SP_ACCESORIOS_REEMPLAZO.CANTIDAD IS 'Cantidad de Accesorios reemplazo con iguales caracteristicas'
/
