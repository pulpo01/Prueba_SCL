CREATE TABLE TA_CTLBEEPLIM(
    NUM_BLOQUE VARCHAR2(9) NOT NULL 
 ,  NUM_TICKETS NUMBER(9,0) NOT NULL 
 ,  NUM_EXITOS NUMBER(8,0) NOT NULL 
 ,  NUM_NEXITOS NUMBER(8,0) NOT NULL 
 ,  NUM_FRAUDE NUMBER(5,0) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_CTLBEEPLIM IS 'Registros de Control de Producto Beeper en Limite Consumo'
/
COMMENT ON COLUMN  TA_CTLBEEPLIM.NUM_BLOQUE IS 'Nzmero de Bloque'
/
COMMENT ON COLUMN  TA_CTLBEEPLIM.NUM_TICKETS IS 'Numero de Tickets'
/
COMMENT ON COLUMN  TA_CTLBEEPLIM.NUM_EXITOS IS 'Numero de Exitos'
/
COMMENT ON COLUMN  TA_CTLBEEPLIM.NUM_NEXITOS IS 'Numero de No Exitos'
/
COMMENT ON COLUMN  TA_CTLBEEPLIM.NUM_FRAUDE IS 'Numero de tickets de fraude'
/
