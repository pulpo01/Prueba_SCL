 ,  COD_TIPDOCUM NUMBER(2,0) NOT NULL 
 ,  NUM_FOLIO NUMBER(9,0) NOT NULL 
 ,  SEC_CUOTA NUMBER(3,0) NOT NULL 
 ,  IMP_DOCUMENTO NUMBER(14,4) NOT NULL 
 ,  IMP_INTER_MORA NUMBER(14,4) NOT NULL 
 ,  IMP_GTOS_PROTESTO NUMBER(14,4) NOT NULL 
