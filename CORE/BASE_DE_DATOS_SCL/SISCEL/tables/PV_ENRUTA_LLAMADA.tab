 ,  FEC_ACTIVACION DATE NOT NULL 
 ,  FEC_TERMINO DATE NOT NULL 
 ,  FEC_MODIFICACION DATE NULL 
 ,  USUARIO VARCHAR2(30) NULL 
 ,  NUM_MIN_MDN_ORIG VARCHAR2(10) NULL 
 ,  NUM_MIN_MDN_DEST VARCHAR2(10) NULL 
 ,  NUM_OOSS_DES NUMBER(9,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
