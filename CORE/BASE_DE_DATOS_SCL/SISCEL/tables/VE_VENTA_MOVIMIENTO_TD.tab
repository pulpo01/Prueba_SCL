CREATE TABLE VE_VENTA_MOVIMIENTO_TD(
    TIPO_DOCUMENTO_WEB NUMBER(3,0) NOT NULL 
 ,  DESCRIPCION VARCHAR2(50) NOT NULL 
 ,  TIPO_VENTA VARCHAR2(10) NOT NULL 
 ,  TIP_MOVIMIENTO NUMBER(2,0) NOT NULL 
 ,  IND_ENTSAL VARCHAR2(1) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_OPERADORA VARCHAR2(5) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  FEC_INGRESO DATE DEFAULT SYSDATE NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_VENTA_MOVIMIENTO_TD IS 'Relacion entre tipos de documentos web v/s tipo de venta'
/
COMMENT ON COLUMN  VE_VENTA_MOVIMIENTO_TD.TIPO_DOCUMENTO_WEB IS 'Tipo de documento generado por Web en ventas Dealer'
/
COMMENT ON COLUMN  VE_VENTA_MOVIMIENTO_TD.DESCRIPCION IS 'Descripcion del documento'
/
COMMENT ON COLUMN  VE_VENTA_MOVIMIENTO_TD.TIPO_VENTA IS 'Tipo de venta realizada Factura/No Factura'
/
COMMENT ON COLUMN  VE_VENTA_MOVIMIENTO_TD.TIP_MOVIMIENTO IS 'Codigo de movimiento que realiza a inventario por devolucion'
/
COMMENT ON COLUMN  VE_VENTA_MOVIMIENTO_TD.IND_ENTSAL IS 'Tipo de Movimiento E/T'
/
COMMENT ON COLUMN  VE_VENTA_MOVIMIENTO_TD.COD_USO IS 'Codigo de Uso de numeracion'
/
COMMENT ON COLUMN  VE_VENTA_MOVIMIENTO_TD.COD_OPERADORA IS 'Codigo de Operadora'
/
COMMENT ON COLUMN  VE_VENTA_MOVIMIENTO_TD.NOM_USUARIO IS 'Nombre usuario Oracle que ingreso el registro'
/
COMMENT ON COLUMN  VE_VENTA_MOVIMIENTO_TD.FEC_INGRESO IS 'Fecha de ingreso del registro'
/
