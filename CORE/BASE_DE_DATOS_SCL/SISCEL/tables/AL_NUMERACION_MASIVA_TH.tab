CREATE TABLE AL_NUMERACION_MASIVA_TH(
    NUM_NUMERACION NUMBER(9,0) NOT NULL 
 ,  LIN_NUMERACION NUMBER(3,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  IND_SERIE NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  FEC_ACTUALIZACION DATE DEFAULT SYSDATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_NUMERACION_MASIVA_TH IS 'Tabla Historica de numeracion masiva'
/
COMMENT ON COLUMN  AL_NUMERACION_MASIVA_TH.NUM_NUMERACION IS 'Numero de orden de numeracion'
/
COMMENT ON COLUMN  AL_NUMERACION_MASIVA_TH.LIN_NUMERACION IS 'Linea de orden de numeracion'
/
COMMENT ON COLUMN  AL_NUMERACION_MASIVA_TH.NUM_SERIE IS 'Numero de serie'
/
COMMENT ON COLUMN  AL_NUMERACION_MASIVA_TH.IND_SERIE IS 'Indicador de estado de Serie'
/
COMMENT ON COLUMN  AL_NUMERACION_MASIVA_TH.FEC_ACTUALIZACION IS 'Fecha de Actualizacion'
/
COMMENT ON COLUMN  AL_NUMERACION_MASIVA_TH.NOM_USUARIO IS 'Nombre de usuario'
/
