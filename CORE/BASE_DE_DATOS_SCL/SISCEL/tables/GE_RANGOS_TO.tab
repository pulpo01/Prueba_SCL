CREATE TABLE GE_RANGOS_TO(
    COD_MODULO VARCHAR2(2) NOT NULL 
 ,  COD_CRITERIO NUMBER(3,0) NOT NULL 
 ,  ID_RANGO NUMBER(3,0) NOT NULL 
 ,  TIPO_RANGO NUMBER(2,0) NOT NULL 
 ,  MIN_RANGO NUMBER(12,0) NOT NULL 
 ,  MAX_RANGO NUMBER(12,0) NOT NULL 
 ,  VAL_RANGO NUMBER(4,0) NOT NULL 
 ,  OBSERVACION VARCHAR2(400) NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_RANGOS_TO IS 'RANGOS DE CRITERIOS'
/
COMMENT ON COLUMN  GE_RANGOS_TO.COD_MODULO IS 'CODIGO DE MODULO (APLICACION)'
/
COMMENT ON COLUMN  GE_RANGOS_TO.COD_CRITERIO IS 'CODIGO DE CRITERIO'
/
COMMENT ON COLUMN  GE_RANGOS_TO.ID_RANGO IS 'IDENTIFICADOR DE RANGO'
/
COMMENT ON COLUMN  GE_RANGOS_TO.TIPO_RANGO IS 'TIPO DE RANGO'
/
COMMENT ON COLUMN  GE_RANGOS_TO.MIN_RANGO IS 'VALOR MINIMO DEL RANGO'
/
COMMENT ON COLUMN  GE_RANGOS_TO.MAX_RANGO IS 'VALOR MAXIMO DEL RANGO'
/
COMMENT ON COLUMN  GE_RANGOS_TO.VAL_RANGO IS 'PUNTAJE ASOCIADO AL RANGO'
/
COMMENT ON COLUMN  GE_RANGOS_TO.OBSERVACION IS 'DETALLE DEL RANGO'
/
