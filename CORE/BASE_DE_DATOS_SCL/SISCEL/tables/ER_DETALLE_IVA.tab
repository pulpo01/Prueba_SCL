CREATE TABLE ER_DETALLE_IVA(
    NRO_SOLIC NUMBER(10,0) NOT NULL 
 ,  MES_DECLARACION NUMBER(2,0) NOT NULL 
 ,  ANO_DECLARACION NUMBER(4,0) NOT NULL 
 ,  FOLIO_DECLARACION NUMBER(12,0) NOT NULL 
 ,  MONTO_MES NUMBER(12,0) NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  ER_DETALLE_IVA.NRO_SOLIC IS 'Numero de solicitud.'
/
COMMENT ON COLUMN  ER_DETALLE_IVA.MES_DECLARACION IS 'Mes de declaracion del IVA.'
/
COMMENT ON COLUMN  ER_DETALLE_IVA.ANO_DECLARACION IS 'Ano de declaracion de IVA.'
/
COMMENT ON COLUMN  ER_DETALLE_IVA.FOLIO_DECLARACION IS 'Numero de folio de la declaracion de IVA.'
/
COMMENT ON COLUMN  ER_DETALLE_IVA.MONTO_MES IS 'Monto mensual de la declaracion de IVA.'
/
