CREATE TABLE GA_NUMROACOM(
    NUM_DESDE NUMBER(15,0) NOT NULL 
 ,  NUM_HASTA NUMBER(15,0) NOT NULL 
 ,  COD_SUBALM VARCHAR2(5) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CENTRAL NUMBER(3,0) NOT NULL 
 ,  COD_CAT NUMBER(2,0) NOT NULL 
 ,  COD_OPERADOR NUMBER(5,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_NUMROACOM IS 'Numeracion Roaming de la Compania x Operador'
/
COMMENT ON COLUMN  GA_NUMROACOM.NUM_DESDE IS 'Numero desde del rango'
/
COMMENT ON COLUMN  GA_NUMROACOM.NUM_HASTA IS 'Numero hasta del rango'
/
COMMENT ON COLUMN  GA_NUMROACOM.COD_SUBALM IS 'Codigo de Subalm'
/
COMMENT ON COLUMN  GA_NUMROACOM.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_NUMROACOM.COD_CENTRAL IS 'Codigo de Central'
/
COMMENT ON COLUMN  GA_NUMROACOM.COD_CAT IS 'Codigo de Categoria'
/
COMMENT ON COLUMN  GA_NUMROACOM.COD_OPERADOR IS 'Codigo de Operador Roaming'
/
