CREATE TABLE FA_GENCENTREMI(
    COD_CENTREMI NUMBER(4,0) NOT NULL 
 ,  COD_TIPMOVIMIEN NUMBER(2,0) NOT NULL 
 ,  COD_CATRIBUT CHAR(1) NOT NULL 
 ,  COD_MODGENER VARCHAR2(3) NOT NULL 
 ,  COD_MODVENTA NUMBER(2,0) DEFAULT 1 NOT NULL 
 ,  COD_APLIC VARCHAR2(3) DEFAULT 'FAC' NOT NULL 
 ,  TIP_FOLIACION VARCHAR2(1) DEFAULT 'P' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_GENCENTREMI IS 'Definicion de Modos de Generacion por Centros Emisores.'
/
COMMENT ON COLUMN  FA_GENCENTREMI.COD_CENTREMI IS 'Codigo de Centro Emisor.              [PK 1/3]'
/
COMMENT ON COLUMN  FA_GENCENTREMI.COD_TIPMOVIMIEN IS 'Tipo de Movimiento de Facturacion.    [PK 2/3]'
/
COMMENT ON COLUMN  FA_GENCENTREMI.COD_CATRIBUT IS 'Categoria Tributaria:  F=Factura | B=Boleta. [PK 3/3]'
/
COMMENT ON COLUMN  FA_GENCENTREMI.COD_MODGENER IS 'Modo de Generacion de Documentos de Facturacion.'
/
COMMENT ON COLUMN  FA_GENCENTREMI.COD_MODVENTA IS 'Codigo de Modalidad de Venta.'
/
COMMENT ON COLUMN  FA_GENCENTREMI.COD_APLIC IS 'Codigo de Interfaz.'
/
