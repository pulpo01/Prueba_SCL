CREATE TABLE CM_DIFCAMPANA_TD(
    COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  IND_CONSIDERA CHAR(1) DEFAULT 'S' NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_DIFCAMPANA_TD IS 'VALORACION DIFERENCIADA BONO CAMPANA.'
/
COMMENT ON COLUMN  CM_DIFCAMPANA_TD.COD_TIPORED IS 'TIPO DE RED SOBRE EL QUE SE APLICA EL CONCEPTOS.'
/
COMMENT ON COLUMN  CM_DIFCAMPANA_TD.COD_PLANCOMIS IS 'PLAN DE COMISIONES QUE CONTIENE EL CONCEPTO.'
/
COMMENT ON COLUMN  CM_DIFCAMPANA_TD.COD_CONCEPTO IS 'CONCEPTO DE COMISION.'
/
COMMENT ON COLUMN  CM_DIFCAMPANA_TD.COD_VENDEDOR IS 'VENDEDOR QUE VERA SUS VENTAS DIFERENCIADAS.'
/
COMMENT ON COLUMN  CM_DIFCAMPANA_TD.IND_CONSIDERA IS 'INDICADOR SI CONSIDERA (S) O NO CONSIDERA (N) LAS VENTAS DEL VENDEDOR.'
/
COMMENT ON COLUMN  CM_DIFCAMPANA_TD.NOM_USUARIO IS 'USUARIO QUE HIZO LA ULTIMA MODIFICACION '
/
COMMENT ON COLUMN  CM_DIFCAMPANA_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION '
/
