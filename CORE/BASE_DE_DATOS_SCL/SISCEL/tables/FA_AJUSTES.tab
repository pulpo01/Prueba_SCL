CREATE TABLE SISCEL.FA_AJUSTES
(
  NUM_FOLIO            NUMBER(9)                NOT NULL,
  COD_CLIENTE          NUMBER(8)                DEFAULT 1                     NOT NULL,
  ACUM_NETONOGRAVORIG  NUMBER(14,4)             NOT NULL,
  ACUM_NETONOGRAVNC    NUMBER(14,4)             NOT NULL,
  ACUM_NETONOGRAVND    NUMBER(14,4)             NOT NULL,
  ACUM_IVAORIG         NUMBER(14,4)             NOT NULL,
  ACUM_IVANC           NUMBER(14,4)             NOT NULL,
  ACUM_IVAND           NUMBER(14,4)             NOT NULL,
  COD_PLANCOM          NUMBER(6)                NOT NULL,
  ACUM_NETOGRAVORIG    NUMBER(14,4)             NOT NULL,
  COD_CATIMPOS         NUMBER(2)                NOT NULL,
  ACUM_NETOGRAVNC      NUMBER(14,4)             NOT NULL,
  ACUM_NETOGRAVND      NUMBER(14,4)             NOT NULL,
  COD_TIPDOCUM         VARCHAR2(2 BYTE)         DEFAULT '2'                   NOT NULL,
  PREF_PLAZA           VARCHAR2(25 BYTE)        DEFAULT 'TMG'                 NOT NULL,
  COD_TIPOLOGIA        VARCHAR2(5 BYTE),
  COD_AREAIMPUTABLE    VARCHAR2(5 BYTE),
  COD_AREASOLICITANTE  VARCHAR2(5 BYTE)
)
TABLESPACE TS_FACTURACION_SDAT
/

COMMENT ON TABLE SISCEL.FA_AJUSTES IS 'Datos generales de documentos que ha sido modificado por una Nota C/D';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.COD_TIPOLOGIA IS 'Cod. Tipologia';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.COD_AREAIMPUTABLE IS 'Cod. Area Imputable';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.COD_AREASOLICITANTE IS 'Cod. Area Solicitante';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.NUM_FOLIO IS 'Numero de Folio';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.ACUM_NETONOGRAVORIG IS 'Acumulado Neto No Gravado';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.ACUM_NETONOGRAVNC IS 'Acumulado Neto No Gravado (Nota Credito)';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.ACUM_NETONOGRAVND IS 'Acumulado Neto No Gravado (Nota Debito)';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.ACUM_IVAORIG IS 'Acumulado Iva Origen';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.ACUM_IVANC IS 'Acumulado Iva Nota Credito';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.ACUM_IVAND IS 'Acumulado Iva Nota Debito';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.COD_PLANCOM IS 'Codigo Plan Comercial';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.ACUM_NETOGRAVORIG IS 'Acumulado Neto Gravado de Origen';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.COD_CATIMPOS IS 'Codigo Categoria Impositiva';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.ACUM_NETOGRAVNC IS 'Acumulado Neto Gravado de NC';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.ACUM_NETOGRAVND IS 'Acumulado Neto Gravado de ND';

COMMENT ON COLUMN SISCEL.FA_AJUSTES.PREF_PLAZA IS 'PREFIJO DE LA PLAZA';
