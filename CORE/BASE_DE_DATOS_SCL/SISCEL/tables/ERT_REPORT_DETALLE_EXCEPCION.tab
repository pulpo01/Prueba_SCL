CREATE TABLE ERT_REPORT_DETALLE_EXCEPCION(
    NUM_SOLICITUD NUMBER(10,0) NOT NULL 
 ,  DES_TIPO_EXCEPCION VARCHAR2(60) NULL 
 ,  DES_EXCEPCION VARCHAR2(60) NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERT_REPORT_DETALLE_EXCEPCION IS 'Tabla de Paso para emitir detalle de excepciones de Reporte.'
/
COMMENT ON COLUMN  ERT_REPORT_DETALLE_EXCEPCION.NUM_SOLICITUD IS 'Numero de solicitud de Evaluacion de riesgo.'
/
COMMENT ON COLUMN  ERT_REPORT_DETALLE_EXCEPCION.DES_TIPO_EXCEPCION IS 'Descripcion del tipo de solicitud.'
/
COMMENT ON COLUMN  ERT_REPORT_DETALLE_EXCEPCION.DES_EXCEPCION IS 'Descripcion de la excepcion.'
/
COMMENT ON COLUMN  ERT_REPORT_DETALLE_EXCEPCION.NOM_USUARIO IS 'Usuario que realiza la excepcion.'
/
