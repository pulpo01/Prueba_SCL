CREATE TABLE FA_TRAZAPROC(
    COD_CICLFACT NUMBER(8,0) NOT NULL 
 ,  COD_PROCESO NUMBER(4,0) NOT NULL 
 ,  COD_ESTAPROC NUMBER(1,0) NOT NULL 
 ,  FEC_INICIO DATE NOT NULL 
 ,  FEC_TERMINO DATE NULL 
 ,  GLS_PROCESO VARCHAR2(50) NULL 
 ,  COD_CLIENTE NUMBER(8,0) NULL 
 ,  NUM_ABONADO NUMBER(8,0) NULL 
 ,  NUM_REGISTROS NUMBER(8,0) NULL 
 ,  HOST_ID VARCHAR2(20) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_TRAZAPROC IS 'Traza de procesos de facturacion.'
/
COMMENT ON COLUMN  FA_TRAZAPROC.COD_CICLFACT IS 'Ciclo en proceso.'
/
COMMENT ON COLUMN  FA_TRAZAPROC.COD_PROCESO IS 'Codigo de proceso.'
/
COMMENT ON COLUMN  FA_TRAZAPROC.COD_ESTAPROC IS 'Estado de proceso - 1: En proceso, 2: Terminado sin problemas, 3: Terminado con error.'
/
COMMENT ON COLUMN  FA_TRAZAPROC.FEC_INICIO IS 'Fecha inicio de proceso.'
/
COMMENT ON COLUMN  FA_TRAZAPROC.FEC_TERMINO IS 'Fecha termino de proceso.'
/
COMMENT ON COLUMN  FA_TRAZAPROC.GLS_PROCESO IS 'Observaciones.'
/
COMMENT ON COLUMN  FA_TRAZAPROC.COD_CLIENTE IS 'Ultimo cliente procesado.'
/
COMMENT ON COLUMN  FA_TRAZAPROC.NUM_ABONADO IS 'Ultimo abonado procesado.'
/
COMMENT ON COLUMN  FA_TRAZAPROC.NUM_REGISTROS IS 'Cantidad de registros procesados.'
/
