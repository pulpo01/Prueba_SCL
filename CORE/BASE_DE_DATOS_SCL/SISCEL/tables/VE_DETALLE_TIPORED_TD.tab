CREATE TABLE VE_DETALLE_TIPORED_TD(
    COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  NUM_NIVEL NUMBER(2,0) NOT NULL 
 ,  FEC_ULTMOD DATE DEFAULT SYSDATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_DETALLE_TIPORED_TD IS 'DETALLE DE CATEGORIAS DE VENTA ASIGNADAS POR NIVEL'
/
COMMENT ON COLUMN  VE_DETALLE_TIPORED_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED'
/
COMMENT ON COLUMN  VE_DETALLE_TIPORED_TD.COD_TIPCOMIS IS 'CODIGO DE LA CATEGORIA DE VENTA'
/
COMMENT ON COLUMN  VE_DETALLE_TIPORED_TD.NUM_NIVEL IS 'NUMERO DEL NIVEL'
/
COMMENT ON COLUMN  VE_DETALLE_TIPORED_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  VE_DETALLE_TIPORED_TD.NOM_USUARIO IS 'USUARIO ULTIMA MODIFICACION'
/
