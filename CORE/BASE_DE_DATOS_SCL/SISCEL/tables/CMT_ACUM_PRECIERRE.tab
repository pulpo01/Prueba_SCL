CREATE TABLE CMT_ACUM_PRECIERRE(
    COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  ID_PERIODO VARCHAR2(10) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_COMISIONISTA NUMBER(6,0) NOT NULL 
 ,  NUM_LOGRO NUMBER(6,0) NULL 
 ,  IMP_CONCEPTO NUMBER(14,4) NOT NULL 
 ,  NUM_REGISTROS NUMBER(6,0) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMT_ACUM_PRECIERRE IS 'Tabla De Acumulacion De Conceptos En Precierre.'
/
COMMENT ON COLUMN  CMT_ACUM_PRECIERRE.COD_CONCEPTO IS 'Concepto de facturacion recuperado.'
/
COMMENT ON COLUMN  CMT_ACUM_PRECIERRE.COD_PERIODO IS 'Codigo de Periodo. (YYYMM).'
/
COMMENT ON COLUMN  CMT_ACUM_PRECIERRE.ID_PERIODO IS 'Identificador de priodo (YYYYMM<N|D><M|Q|S><#SEQ>).'
/
COMMENT ON COLUMN  CMT_ACUM_PRECIERRE.COD_TIPCOMIS IS 'Codigo de canal de ventas       [MD].'
/
COMMENT ON COLUMN  CMT_ACUM_PRECIERRE.COD_COMISIONISTA IS 'Codigo del Comisionista          [MD].'
/
COMMENT ON COLUMN  CMT_ACUM_PRECIERRE.NUM_LOGRO IS 'Valor de entrada a la matriz de comisiones..'
/
COMMENT ON COLUMN  CMT_ACUM_PRECIERRE.IMP_CONCEPTO IS 'Comision lograda..'
/
COMMENT ON COLUMN  CMT_ACUM_PRECIERRE.NUM_REGISTROS IS 'Cantidad de registros sobre los que se aplica la comision lograda..'
/
