CREATE TABLE AL_DET_RESERVA_TO(
    NUM_SOLICITUD NUMBER(8,0) NOT NULL 
 ,  NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  FEC_BAJA DATE NOT NULL 
 ,  IND_EQUIPADO NUMBER(1,0) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_DET_RESERVA_TO IS 'Detalle auditoria Reserva de Numeros Almacen.'
/
COMMENT ON COLUMN  AL_DET_RESERVA_TO.NUM_SOLICITUD IS 'Numero de Solicitud asociado a una reserva o desreserva de numeros.'
/
COMMENT ON COLUMN  AL_DET_RESERVA_TO.NUM_CELULAR IS 'Numero de Celular a reservar o eliminar de almacen.'
/
COMMENT ON COLUMN  AL_DET_RESERVA_TO.FEC_BAJA IS 'Fecha en que fue dado de baja el numero celular.'
/
COMMENT ON COLUMN  AL_DET_RESERVA_TO.IND_EQUIPADO IS 'Indicador de equipo en central no(0), si(1).'
/
