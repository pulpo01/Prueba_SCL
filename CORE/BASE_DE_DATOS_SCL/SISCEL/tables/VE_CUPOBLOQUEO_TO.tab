CREATE TABLE VE_CUPOBLOQUEO_TO(
    COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  TIP_VENDEDOR VARCHAR2(1) NOT NULL 
 ,  NUM_DIAPAG NUMBER(3,0) NOT NULL 
 ,  NUM_DIALEG NUMBER(3,0) NOT NULL 
 ,  CAN_DIAS NUMBER(3,0) NOT NULL 
 ,  FEC_INI DATE NOT NULL 
 ,  FEC_FIN DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_MODIF DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_CUPOBLOQUEO_TO IS 'Configuración de Cupos y Bloqueos por Vendedor'
/
COMMENT ON COLUMN  VE_CUPOBLOQUEO_TO.NOM_USUARIO IS 'Nombre del usuario que realizo la modificacion'
/
COMMENT ON COLUMN  VE_CUPOBLOQUEO_TO.FEC_MODIF IS 'Fecha de la modificacion'
/
COMMENT ON COLUMN  VE_CUPOBLOQUEO_TO.COD_VENDEDOR IS 'Código del Vendedor o Vendealer'
/
COMMENT ON COLUMN  VE_CUPOBLOQUEO_TO.TIP_VENDEDOR IS 'Tipo del vendedor (V=Vendedor, D= Dealer)'
/
COMMENT ON COLUMN  VE_CUPOBLOQUEO_TO.NUM_DIAPAG IS 'Cantidad de dias de pago'
/
COMMENT ON COLUMN  VE_CUPOBLOQUEO_TO.NUM_DIALEG IS 'Cantidad de dias de legalizacion'
/
COMMENT ON COLUMN  VE_CUPOBLOQUEO_TO.CAN_DIAS IS 'Cantidad dÍas de morosidad'
/
COMMENT ON COLUMN  VE_CUPOBLOQUEO_TO.FEC_INI IS 'Fecha de inicio de bloqueo a vendedor'
/
COMMENT ON COLUMN  VE_CUPOBLOQUEO_TO.FEC_FIN IS 'Fecha de termino de bloqueo a vendedor'
/
