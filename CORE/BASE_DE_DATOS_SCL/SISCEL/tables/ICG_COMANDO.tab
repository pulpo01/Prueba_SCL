CREATE TABLE ICG_COMANDO(
    COD_LENGUAJE NUMBER(3,0) NOT NULL 
 ,  COD_COMANDO NUMBER(3,0) NOT NULL 
 ,  NOM_COMANDO VARCHAR2(6) NOT NULL 
 ,  DES_SINTAXIS VARCHAR2(512) NOT NULL 
 ,  DES_COMANDO VARCHAR2(50) NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICG_COMANDO IS 'Comandos del Lenguaje'
/
COMMENT ON COLUMN  ICG_COMANDO.COD_LENGUAJE IS 'Codigo de Lenguaje - Retrofitted'
/
COMMENT ON COLUMN  ICG_COMANDO.COD_COMANDO IS 'Codigo de Comando - Retrofitted'
/
COMMENT ON COLUMN  ICG_COMANDO.NOM_COMANDO IS 'Nombre del Comando - Retrofitted'
/
COMMENT ON COLUMN  ICG_COMANDO.DES_SINTAXIS IS 'Descripcion de la Sintaxis - Retrofitted'
/
COMMENT ON COLUMN  ICG_COMANDO.DES_COMANDO IS 'Descripcion del Comando - Retrofitted'
/
