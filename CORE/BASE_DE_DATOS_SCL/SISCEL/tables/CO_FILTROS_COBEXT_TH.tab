 ,  FEC_DESDE DATE NULL 
 ,  FEC_HASTA DATE NULL 
 ,  COD_CICLO NUMBER(2,0) NULL 
 ,  COD_OPERADORA_SCL VARCHAR2(5) NULL 
 ,  COD_PLAZA VARCHAR2(5) NULL 
 ,  ANTIGUEDAD NUMBER(5,0) NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_ESTADO VARCHAR2(3) NOT NULL 
 ,  FEC_ESTADO DATE NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
