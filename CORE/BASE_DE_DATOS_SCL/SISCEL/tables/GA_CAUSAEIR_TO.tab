CREATE TABLE GA_CAUSAEIR_TO
(
  NUM_SERIE         VARCHAR2(25 BYTE)           NOT NULL,
  COD_CAUSA         VARCHAR2(5 BYTE)            NOT NULL,
  COD_TIPOLISTA     VARCHAR2(1 BYTE)            NOT NULL,
  COD_OS            VARCHAR2(5 BYTE),
  FEC_ING           DATE                        NOT NULL,
  NOM_USUARIO       VARCHAR2(30 BYTE)           NOT NULL,
  ORIG_TRANS        VARCHAR2(1 BYTE)            NOT NULL,
  COD_ESTADO        VARCHAR2(1 BYTE)            DEFAULT '0',
  COD_CAUSA_OS      VARCHAR2(2 BYTE),
  COD_OS_REV        VARCHAR2(5 BYTE),
  COD_CAUSA_OS_REV  VARCHAR2(2 BYTE),
  COD_CAUSA_REV     VARCHAR2(5 BYTE)
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    5
INITRANS   16
MAXTRANS   255
STORAGE    (
            INITIAL          968K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE GA_CAUSAEIR_TO IS '[ GA_CAUEIIR_TO ] Causas del EIR asociadas a la serie de listas negras';

COMMENT ON COLUMN GA_CAUSAEIR_TO.NUM_SERIE IS 'numero de serie';

COMMENT ON COLUMN GA_CAUSAEIR_TO.COD_CAUSA IS 'codigo de causa en el eir';

COMMENT ON COLUMN GA_CAUSAEIR_TO.COD_TIPOLISTA IS 'tipo de lista';

COMMENT ON COLUMN GA_CAUSAEIR_TO.COD_OS IS 'codigo de la ooss';

COMMENT ON COLUMN GA_CAUSAEIR_TO.FEC_ING IS 'fecha de ingreso';

COMMENT ON COLUMN GA_CAUSAEIR_TO.NOM_USUARIO IS 'nombre del usuario';

COMMENT ON COLUMN GA_CAUSAEIR_TO.ORIG_TRANS IS '1:OOSS,2:Mant LN serie,3:Recepción LN serie interna,4:Recepción LN serie externa';

COMMENT ON COLUMN GA_CAUSAEIR_TO.COD_ESTADO IS '0: ingreso a LN , 1:egreso LN';

COMMENT ON COLUMN GA_CAUSAEIR_TO.COD_CAUSA_OS IS 'codigo de causa de la suspension de la OS';

COMMENT ON COLUMN GA_CAUSAEIR_TO.COD_OS_REV IS 'codigo ooss de reversa';

COMMENT ON COLUMN GA_CAUSAEIR_TO.COD_CAUSA_OS_REV IS 'codigo de causa de la reversa de la OS';

COMMENT ON COLUMN GA_CAUSAEIR_TO.COD_CAUSA_REV IS 'codigo de causa de reversa en el eir';

