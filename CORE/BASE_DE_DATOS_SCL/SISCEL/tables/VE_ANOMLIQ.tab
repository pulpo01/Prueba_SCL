CREATE TABLE VE_ANOMLIQ(
    NUM_ANOMALIA NUMBER(8,0) NOT NULL 
 ,  NUM_LIQUIDACION NUMBER(8,0) NOT NULL 
 ,  DES_MENSAJE VARCHAR2(120) NOT NULL 
 ,  NOM_PROCEDURE VARCHAR2(30) NOT NULL 
 ,  NOM_TABLA VARCHAR2(30) NOT NULL 
 ,  IND_ACCION VARCHAR2(1) NOT NULL 
 ,  COD_SQLCODE VARCHAR2(10) NULL 
 ,  DES_SQLERRM VARCHAR2(70) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_ANOMLIQ IS 'Anomalias en el proceso de liquidacion de comisiones'
/
COMMENT ON COLUMN  VE_ANOMLIQ.NUM_ANOMALIA IS 'Numero de la Anomalia'
/
COMMENT ON COLUMN  VE_ANOMLIQ.NUM_LIQUIDACION IS 'Numero del proceso de Liquidacion'
/
COMMENT ON COLUMN  VE_ANOMLIQ.DES_MENSAJE IS 'Descripcion del mensaje de anomalia'
/
COMMENT ON COLUMN  VE_ANOMLIQ.NOM_PROCEDURE IS 'Nombre del Procedimiento'
/
COMMENT ON COLUMN  VE_ANOMLIQ.NOM_TABLA IS 'Nombre de la Tabla'
/
COMMENT ON COLUMN  VE_ANOMLIQ.IND_ACCION IS 'Indicativo de Accion'
/
COMMENT ON COLUMN  VE_ANOMLIQ.COD_SQLCODE IS 'Codigo del Error Oracle'
/
COMMENT ON COLUMN  VE_ANOMLIQ.DES_SQLERRM IS 'Descripcion del mensaje de Error Oracle'
/
