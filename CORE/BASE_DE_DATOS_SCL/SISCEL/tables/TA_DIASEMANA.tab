CREATE TABLE TA_DIASEMANA(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_OPERADOR NUMBER(5,0) NOT NULL 
 ,  DES_DIASEMANA VARCHAR2(10) NOT NULL 
 ,  TIP_DIA VARCHAR2(2) NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_DIASEMANA IS 'Tipo de dia por operador para los dias de la semana'
/
COMMENT ON COLUMN  TA_DIASEMANA.COD_PRODUCTO IS 'Csdigo de Producto'
/
COMMENT ON COLUMN  TA_DIASEMANA.COD_OPERADOR IS 'Csdigo de Operador'
/
COMMENT ON COLUMN  TA_DIASEMANA.DES_DIASEMANA IS 'Descripcion Dma Semana'
/
COMMENT ON COLUMN  TA_DIASEMANA.TIP_DIA IS 'Tipo de Dma'
/
