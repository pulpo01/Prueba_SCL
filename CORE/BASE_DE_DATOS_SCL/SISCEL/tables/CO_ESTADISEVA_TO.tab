CREATE TABLE CO_ESTADISEVA_TO(
    COD_PROCESO VARCHAR2(5) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  SECUENCIA NUMBER(2,0) NOT NULL 
 ,  TIEMPO_PROCESO NUMBER(8,4) NULL 
 ,  CNT_CLIENTES_PROC NUMBER(8,0) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  CO_ESTADISEVA_TO.COD_PROCESO IS 'Identificador del Proceso'
/
COMMENT ON COLUMN  CO_ESTADISEVA_TO.FEC_INGRESO IS 'Fecha de Ingreso'
/
COMMENT ON COLUMN  CO_ESTADISEVA_TO.SECUENCIA IS 'Secuencia de los hilos'
/
COMMENT ON COLUMN  CO_ESTADISEVA_TO.TIEMPO_PROCESO IS 'Unidad de medida utilizada en campo valor. P: Porcentaje, V: Valor.'
/
COMMENT ON COLUMN  CO_ESTADISEVA_TO.CNT_CLIENTES_PROC IS 'Cantidad de Clientes procesados'
/
