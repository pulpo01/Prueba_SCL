CREATE TABLE CO_CICLO(
    COD_COBRADOR NUMBER(6,0) NOT NULL 
 ,  COD_CICLO NUMBER(3,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  IND_COMISION NUMBER(1,0) NOT NULL 
 ,  IND_ASIGNAR NUMBER(1,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(8,0) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_CICLO IS 'CICLO DE LA ASIGNACION DEL COBRADOR- Retrofitted'
/
COMMENT ON COLUMN  CO_CICLO.COD_COBRADOR IS 'CODIGO DE COBRADOR - Retrofitted'
/
COMMENT ON COLUMN  CO_CICLO.COD_CICLO IS 'CODIGO DE CICLO - Retrofitted'
/
COMMENT ON COLUMN  CO_CICLO.FEC_DESDE IS 'FECHA DE DESDE DEL CICLO - Retrofitted'
/
COMMENT ON COLUMN  CO_CICLO.FEC_HASTA IS 'FECHA DE HASTA DEL CICLO - Retrofitted'
/
COMMENT ON COLUMN  CO_CICLO.IND_COMISION IS 'INDICADOR DE CALSULO DE COMISION 1 (SI) 0 (NO) - Retrofitted'
/
COMMENT ON COLUMN  CO_CICLO.IND_ASIGNAR IS 'INDICADOR DE SI SE HA REALIZADO LA ASIGNACION DE CLIENTES 1 (SI) 0 (NO) - Retrofitted'
/
COMMENT ON COLUMN  CO_CICLO.NUM_ORDEN IS 'NUMERO DE ORDEN DE LA ASIGNACION - Retrofitted'
/
