CREATE TABLE CM_RESULTADOSRET_TD(
    COD_RESULTADO_RET NUMBER(3,0) NOT NULL 
 ,  COD_CATEGRESULTADO NUMBER(3,0) NOT NULL 
 ,  DES_RESULTADO_RET VARCHAR2(60) NOT NULL 
 ,  MTO_RETENCION NUMBER(14,4) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_RESULTADOSRET_TD IS 'VALOR DE LOS RESULTADOS DE RETENCIONES'
/
COMMENT ON COLUMN  CM_RESULTADOSRET_TD.COD_RESULTADO_RET IS 'CSDIGO RESULTADO ZLTIMO CONTACTO'
/
COMMENT ON COLUMN  CM_RESULTADOSRET_TD.COD_CATEGRESULTADO IS 'CSDIGO DE CLASIFICACISN DE RESULTADO DE LAS RETENCIONES PARA COMISIONES'
/
COMMENT ON COLUMN  CM_RESULTADOSRET_TD.DES_RESULTADO_RET IS 'DESCRIPCISN DEL RESULTADO DE RETENCIONES (GED_CODIGOS)'
/
COMMENT ON COLUMN  CM_RESULTADOSRET_TD.MTO_RETENCION IS 'MONTO DE RETENCISN POR CADA RESULTADO'
/
COMMENT ON COLUMN  CM_RESULTADOSRET_TD.FEC_ULTMOD IS 'FECHA DE ZLTIMA MODIFICACISN'
/
COMMENT ON COLUMN  CM_RESULTADOSRET_TD.NOM_USUARIO IS 'USUARIO QUE REALIZS ZLTIMA MODIFICACISN'
/
