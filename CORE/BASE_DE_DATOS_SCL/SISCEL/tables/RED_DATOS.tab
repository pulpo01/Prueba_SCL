CREATE TABLE RED_DATOS(
    COD_DATO NUMBER(4,0) NOT NULL 
 ,  DES_DATO VARCHAR2(60) NOT NULL 
 ,  MAX_SELECCIONADO NUMBER(1,0) NULL 
 ,  COD_SUBMODULO CHAR(1) NOT NULL 
 ,  IND_OBLIGATORIO NUMBER(4,0) NULL 
 ,  IND_DETALLE_OBLIGATORIO NUMBER(5,0) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  RED_DATOS.IND_OBLIGATORIO IS 'Obligatoriedad del dato (y en que circunstancia (sic)): 0=Obligatorio |  Null= No obligatorio.'
/
