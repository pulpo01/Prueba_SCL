CREATE TABLE SISCEL.GE_PROGRAMAS
(
  COD_PROGRAMA    VARCHAR2(3 BYTE)              NOT NULL,
  NUM_VERSION     NUMBER(2)                     NOT NULL,
  NUM_SUBVERSION  NUMBER(3)                     NOT NULL,
  NOM_EXE         VARCHAR2(20 BYTE),
  DES_PROGRAMA    VARCHAR2(30 BYTE)             NOT NULL,
  COD_MODULO      VARCHAR2(2 BYTE)              NOT NULL,
  FEC_DESDE_DH    DATE                          NOT NULL,
  FEC_HASTA_DH    DATE,
  IND_EMAIL       NUMBER(1)                     DEFAULT 0                     NOT NULL
)  TABLESPACE TS_GENERAL_SDAT
/

COMMENT ON TABLE SISCEL.GE_PROGRAMAS IS 'Programas de la aplicacion SISCEL';

COMMENT ON COLUMN SISCEL.GE_PROGRAMAS.COD_PROGRAMA IS 'Codigo de Programa';

COMMENT ON COLUMN SISCEL.GE_PROGRAMAS.NUM_VERSION IS 'Numero de Version';

COMMENT ON COLUMN SISCEL.GE_PROGRAMAS.NUM_SUBVERSION IS 'Numero de Subversion';

COMMENT ON COLUMN SISCEL.GE_PROGRAMAS.DES_PROGRAMA IS 'Descripcion del programa';

COMMENT ON COLUMN SISCEL.GE_PROGRAMAS.COD_MODULO IS 'Codigo del modulo - Retrofitted';

COMMENT ON COLUMN SISCEL.GE_PROGRAMAS.IND_EMAIL IS 'Indica si aplicación envía email, 1:si, 0:no';

