CREATE TABLE VE_SUCURCATEG(
    COD_CATEGORIA VARCHAR2(3) NOT NULL 
 ,  NUM_DESDE NUMBER(4,0) NOT NULL 
 ,  NUM_HASTA NUMBER(4,0) NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_SUCURCATEG IS 'Categorias asociadas a Sucursales'
/
COMMENT ON COLUMN  VE_SUCURCATEG.COD_CATEGORIA IS 'Codigo de Categoria'
/
COMMENT ON COLUMN  VE_SUCURCATEG.NUM_DESDE IS 'Limite inferior del rango'
/
COMMENT ON COLUMN  VE_SUCURCATEG.NUM_HASTA IS 'Limite superior del rango'
/
