CREATE TABLE CO_INMUNES(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  COD_ACCION VARCHAR2(5) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  COD_CAUSA VARCHAR2(2) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  CO_INMUNES.FEC_HASTA IS 'Fecha hasta la que el cliente es inmune.'
/
COMMENT ON COLUMN  CO_INMUNES.COD_CAUSA IS 'Causa por la que se le genero inmunidad.'
/
