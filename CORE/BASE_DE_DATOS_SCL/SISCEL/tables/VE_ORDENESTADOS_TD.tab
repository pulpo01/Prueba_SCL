CREATE TABLE VE_ORDENESTADOS_TD(
    COD_PROGRAMA VARCHAR2(3) NOT NULL 
 ,  IND_OFITER VARCHAR2(1) NOT NULL 
 ,  COD_PROCESO VARCHAR2(7) NOT NULL 
 ,  NIVEL VARCHAR2(2) NOT NULL 
 ,  COD_ESTADOORIGEN VARCHAR2(2) NOT NULL 
 ,  COD_ESTADODESTINO VARCHAR2(2) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_ORDENESTADOS_TD IS 'ORDEN DE ESTADO DE LA VENTA'
/
COMMENT ON COLUMN  VE_ORDENESTADOS_TD.COD_PROGRAMA IS 'PROGRAMA A EJECUTAR'
/
COMMENT ON COLUMN  VE_ORDENESTADOS_TD.IND_OFITER IS 'TIPO DE VENTA (O: OFICINA, T: TERRENO)'
/
COMMENT ON COLUMN  VE_ORDENESTADOS_TD.COD_PROCESO IS 'PROCESO A EJECUTAR'
/
COMMENT ON COLUMN  VE_ORDENESTADOS_TD.NIVEL IS 'NIVEL DEL ESTADO DE ORIGEN'
/
COMMENT ON COLUMN  VE_ORDENESTADOS_TD.COD_ESTADOORIGEN IS 'ESTADO ORIGEN'
/
COMMENT ON COLUMN  VE_ORDENESTADOS_TD.COD_ESTADODESTINO IS 'ESTADO DESTINO'
/
