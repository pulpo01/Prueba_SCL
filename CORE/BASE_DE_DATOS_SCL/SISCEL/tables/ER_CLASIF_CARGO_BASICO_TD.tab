CREATE TABLE ER_CLASIF_CARGO_BASICO_TD(
    COD_CLASIFICACION VARCHAR2(1) NOT NULL 
 ,  COD_GAMA VARCHAR2(5) NOT NULL 
 ,  VAL_RENTACB_INI NUMBER(14,4) NOT NULL 
 ,  VAL_RENTACB_FIN NUMBER(14,4) NOT NULL 
 ,  MTO_LIMCARGOBAS NUMBER(14,4) NOT NULL 
 ,  FEC_VIGENCIA_INI DATE NOT NULL 
 ,  FEC_VIGENCIA_FIN DATE NOT NULL 
 ,  NOM_USUARIO_MODI VARCHAR2(30) NOT NULL 
 ,  FEC_MODIFICACION DATE NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ER_CLASIF_CARGO_BASICO_TD IS '[ CLCB ] Relación Clasificación Clientes con planes tarifarios'
/
COMMENT ON COLUMN  ER_CLASIF_CARGO_BASICO_TD.COD_CLASIFICACION IS 'Clasificación Datacredit del cliente'
/
COMMENT ON COLUMN  ER_CLASIF_CARGO_BASICO_TD.COD_GAMA IS 'Gama informada de buró'
/
COMMENT ON COLUMN  ER_CLASIF_CARGO_BASICO_TD.VAL_RENTACB_INI IS 'Rango inicial de Cargo Básico'
/
COMMENT ON COLUMN  ER_CLASIF_CARGO_BASICO_TD.VAL_RENTACB_FIN IS 'Rango final de Cargo Básico'
/
COMMENT ON COLUMN  ER_CLASIF_CARGO_BASICO_TD.MTO_LIMCARGOBAS IS 'Monto Límite de Cargo Básico para optar a planes'
/
COMMENT ON COLUMN  ER_CLASIF_CARGO_BASICO_TD.FEC_VIGENCIA_INI IS 'Rango Inicial de vigencia'
/
COMMENT ON COLUMN  ER_CLASIF_CARGO_BASICO_TD.FEC_VIGENCIA_FIN IS 'Rango Final de vigencia'
/
COMMENT ON COLUMN  ER_CLASIF_CARGO_BASICO_TD.NOM_USUARIO_MODI IS 'Usuario que modifica el registro'
/
COMMENT ON COLUMN  ER_CLASIF_CARGO_BASICO_TD.FEC_MODIFICACION IS 'Fecha de última modificación'
/
