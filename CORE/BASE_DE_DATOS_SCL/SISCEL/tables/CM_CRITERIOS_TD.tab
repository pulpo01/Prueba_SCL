CREATE TABLE CM_CRITERIOS_TD(
    NUM_CRITERIO NUMBER(6,0) NOT NULL 
 ,  DES_CRITERIO VARCHAR2(30) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_CRITERIOS_TD IS 'Asignacion De Criterios De Evaluacion A Un Canal.'
/
COMMENT ON COLUMN  CM_CRITERIOS_TD.NUM_CRITERIO IS 'NUMERO IDENTIFICADOR DEL CRITERIO DE VALORACION.'
/
COMMENT ON COLUMN  CM_CRITERIOS_TD.DES_CRITERIO IS 'DESCRIPCION O NOMBRE DEL CRITERIO DE EVALUACION.'
/
COMMENT ON COLUMN  CM_CRITERIOS_TD.FEC_DESDE IS 'FECHA DESDE INICIO DE VIGENCIA (VENTAS).'
/
COMMENT ON COLUMN  CM_CRITERIOS_TD.FEC_HASTA IS 'FECHA HASTA TERMINO DE VIGENCIA (VENTAS).'
/
COMMENT ON COLUMN  CM_CRITERIOS_TD.NOM_USUARIO IS 'USUARIO DE ULTIMA MODIFICACION.'
/
COMMENT ON COLUMN  CM_CRITERIOS_TD.FEC_ULTMOD IS 'FECHA DE ULTMA MODIFICACION.'
/
