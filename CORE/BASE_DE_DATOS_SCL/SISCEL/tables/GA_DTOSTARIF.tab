CREATE TABLE GA_DTOSTARIF(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_CICLFACT NUMBER(6,0) NOT NULL 
 ,  NUM_MINUTOS NUMBER(6,0) NOT NULL 
 ,  TIP_PLANTARIF VARCHAR2(1) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_GRABACION DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_DTOSTARIF IS 'Numeros de minutos libres adiccionales por vendedor/abonado a los de su plan tarifario- Retrofitted'
/
COMMENT ON COLUMN  GA_DTOSTARIF.NUM_ABONADO IS 'Numero de Abonado - Retrofitted'
/
COMMENT ON COLUMN  GA_DTOSTARIF.COD_CICLFACT IS 'codigo de Ciclo de Facturaciondurante el cual se van aplicar los descuentos - Retrofitted'
/
COMMENT ON COLUMN  GA_DTOSTARIF.NUM_MINUTOS IS 'Numero de minutos Gratis para ese periodo de facturacion/abonado - Retrofitted'
/
COMMENT ON COLUMN  GA_DTOSTARIF.TIP_PLANTARIF IS 'Tipo de Plantarifario del Abonado - Retrofitted'
/
COMMENT ON COLUMN  GA_DTOSTARIF.COD_VENDEDOR IS 'Vendedor asociado al abonado - Retrofitted'
/
COMMENT ON COLUMN  GA_DTOSTARIF.NOM_USUARORA IS 'Ususario que realizo el cambio - Retrofitted'
/
COMMENT ON COLUMN  GA_DTOSTARIF.FEC_GRABACION IS 'Fecha en que se realizo la transaccion - Retrofitted'
/
