CREATE TABLE GA_CELNUM_SUBALM(
    NUM_DESDE NUMBER(15,0) NOT NULL 
 ,  NUM_HASTA NUMBER(15,0) NOT NULL 
 ,  COD_SUBALM VARCHAR2(5) NOT NULL 
 ,  NUM_MIN VARCHAR2(5) NULL 
 ,  NUM_NIR VARCHAR2(4) NULL 
 ,  IND_CONTAMINADO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  IND_PORTA_IN NUMBER(1,0) NULL 
 ,  IND_PORTA_OUT NUMBER(1,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_CELNUM_SUBALM IS 'Numeracion celular abierta por Subalm'
/
COMMENT ON COLUMN  GA_CELNUM_SUBALM.NUM_DESDE IS 'Numero desde del rango'
/
COMMENT ON COLUMN  GA_CELNUM_SUBALM.NUM_HASTA IS 'Numero hasta del rango'
/
COMMENT ON COLUMN  GA_CELNUM_SUBALM.COD_SUBALM IS 'Codigo de Subalm'
/
COMMENT ON COLUMN  GA_CELNUM_SUBALM.NUM_NIR IS 'Numero Identificador de la Region'
/
COMMENT ON COLUMN  GA_CELNUM_SUBALM.IND_CONTAMINADO IS 'Indicador Numero contaminado'
/
COMMENT ON COLUMN  GA_CELNUM_SUBALM.IND_PORTA_IN IS 'Indicador Portado In'
/
COMMENT ON COLUMN  GA_CELNUM_SUBALM.IND_PORTA_OUT IS 'Indicador Portado Out'
/
