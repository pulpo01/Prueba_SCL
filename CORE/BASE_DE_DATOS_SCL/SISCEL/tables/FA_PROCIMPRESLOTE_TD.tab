CREATE TABLE FA_PROCIMPRESLOTE_TD(
    NUM_SECUENCIAL NUMBER(8,0) NOT NULL 
 ,  NUM_PROCESO NUMBER(8,0) NOT NULL 
 ,  COD_TIPDOCUM VARCHAR2(5) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_PROCIMPRESLOTE_TD IS 'Genera conjunto de documentos a Imprimir.'
/
COMMENT ON COLUMN  FA_PROCIMPRESLOTE_TD.NUM_SECUENCIAL IS 'Numero secuencial Identifica el lote.'
/
COMMENT ON COLUMN  FA_PROCIMPRESLOTE_TD.NUM_PROCESO IS 'Numero de Proceso que identifica el documento.'
/
COMMENT ON COLUMN  FA_PROCIMPRESLOTE_TD.COD_TIPDOCUM IS 'Tipo de Documento a Procesar.'
/
COMMENT ON COLUMN  FA_PROCIMPRESLOTE_TD.FEC_ULTMOD IS 'Fecha de Actualizacion.'
/
COMMENT ON COLUMN  FA_PROCIMPRESLOTE_TD.NOM_USUARIO IS 'Nombre deUsuario.'
/
