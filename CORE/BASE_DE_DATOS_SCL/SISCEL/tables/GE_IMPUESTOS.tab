CREATE TABLE GE_IMPUESTOS(
    COD_CATIMPOS NUMBER(2,0) NOT NULL 
 ,  COD_ZONAIMPO NUMBER(2,0) NOT NULL 
 ,  COD_TIPIMPUES NUMBER(2,0) NOT NULL 
 ,  COD_GRPSERVI NUMBER(2,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  COD_CONCGENE NUMBER(4,0) NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  PRC_IMPUESTO NUMBER(15,6) NOT NULL 
 ,  COD_ZONAABON NUMBER(2,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_IMPUESTOS IS 'Porcentajes de impuestos'
/
COMMENT ON COLUMN  GE_IMPUESTOS.COD_CATIMPOS IS 'Categoria impositiva del centro emisor'
/
COMMENT ON COLUMN  GE_IMPUESTOS.COD_ZONAIMPO IS 'Division en zonas impositivas del pais'
/
COMMENT ON COLUMN  GE_IMPUESTOS.COD_TIPIMPUES IS 'Codigo de Tipo de Iva'
/
COMMENT ON COLUMN  GE_IMPUESTOS.COD_GRPSERVI IS 'Codigo grupo de servicio'
/
COMMENT ON COLUMN  GE_IMPUESTOS.FEC_DESDE IS 'Fecha de entrada en vigencia del impuesto.'
/
COMMENT ON COLUMN  GE_IMPUESTOS.COD_CONCGENE IS 'Concepto de facturacion que genera'
/
COMMENT ON COLUMN  GE_IMPUESTOS.FEC_HASTA IS 'Fecha Fin Periodo'
/
COMMENT ON COLUMN  GE_IMPUESTOS.PRC_IMPUESTO IS 'Porcentaje de impuesto'
/
COMMENT ON COLUMN  GE_IMPUESTOS.COD_ZONAABON IS 'Zona Impositiva del Abonado.'
/
