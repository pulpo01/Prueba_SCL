CREATE TABLE ICG_ENTSERVICIO(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_LENGUAJE NUMBER(3,0) NOT NULL 
 ,  COD_SERVICIO NUMBER(2,0) NOT NULL 
 ,  COD_NIVEL NUMBER(4,0) NOT NULL 
 ,  COD_SERVICIOCEN NUMBER(2,0) NOT NULL 
 ,  COD_NIVELCEN NUMBER(4,0) NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICG_ENTSERVICIO IS 'Asociacion de Servicios con Servicios en Central'
/
COMMENT ON COLUMN  ICG_ENTSERVICIO.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  ICG_ENTSERVICIO.COD_LENGUAJE IS 'codigo de lenguaje del servicio en central'
/
COMMENT ON COLUMN  ICG_ENTSERVICIO.COD_SERVICIO IS 'codigo de servicio'
/
COMMENT ON COLUMN  ICG_ENTSERVICIO.COD_NIVEL IS 'nivel del servicio'
/
COMMENT ON COLUMN  ICG_ENTSERVICIO.COD_SERVICIOCEN IS 'codigo de servicio en central'
/
COMMENT ON COLUMN  ICG_ENTSERVICIO.COD_NIVELCEN IS 'nivel de servicio en central'
/
