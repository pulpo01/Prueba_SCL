CREATE TABLE FA_REPPASOCOBROS(
    NUM_PROCPASOCOB NUMBER(8,0) NOT NULL 
 ,  COD_TIPDOCUM NUMBER(2,0) NOT NULL 
 ,  FEC_EFECTIVIDAD DATE NULL 
 ,  SALDO_DEBE NUMBER(16,4) NOT NULL 
 ,  SALDO_HABER NUMBER(16,4) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_MODGENER VARCHAR2(3) NULL 
 ,  COD_CICLFACT NUMBER(6,0) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_REPPASOCOBROS IS 'Reportes de Procesos de Paso a Cobros'
/
COMMENT ON COLUMN  FA_REPPASOCOBROS.NUM_PROCPASOCOB IS 'Numero de Proceso de Intercalacion'
/
COMMENT ON COLUMN  FA_REPPASOCOBROS.COD_TIPDOCUM IS 'Tipo de Documento'
/
COMMENT ON COLUMN  FA_REPPASOCOBROS.FEC_EFECTIVIDAD IS 'Fecha de Proceso'
/
COMMENT ON COLUMN  FA_REPPASOCOBROS.SALDO_DEBE IS 'Saldo DEBE de Cuenta Corriente Antes de Intercalar'
/
COMMENT ON COLUMN  FA_REPPASOCOBROS.SALDO_HABER IS 'Saldo HABER de Cuenta Corriente Antes de Intercalar'
/
COMMENT ON COLUMN  FA_REPPASOCOBROS.NOM_USUARIO IS 'Usuario'
/
COMMENT ON COLUMN  FA_REPPASOCOBROS.COD_CICLFACT IS 'Codigo de Ciclo de facturacion.'
/
