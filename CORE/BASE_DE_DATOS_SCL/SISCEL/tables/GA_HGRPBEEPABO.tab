 ,  FEC_ASIGNA DATE NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_DESASIGNA DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_HGRPBEEPABO IS 'Tabla de Abonados x Grupos de Beeper'
/
