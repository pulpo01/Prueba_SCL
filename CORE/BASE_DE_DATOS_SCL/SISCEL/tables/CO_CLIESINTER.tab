CREATE TABLE CO_CLIESINTER(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  COD_EXENCION VARCHAR2(5) NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_CLIESINTER IS '### Pendiente - Depto. Facturacion Tasacion y Cobros.'
/
COMMENT ON COLUMN  CO_CLIESINTER.COD_CLIENTE IS 'Identificacion Clientes.'
/
COMMENT ON COLUMN  CO_CLIESINTER.FEC_DESDE IS 'Fecha Inicio exencion.'
/
COMMENT ON COLUMN  CO_CLIESINTER.FEC_HASTA IS 'Fecha termino exencion.'
/
COMMENT ON COLUMN  CO_CLIESINTER.COD_EXENCION IS 'Codigo exencion interes.'
/
COMMENT ON COLUMN  CO_CLIESINTER.FEC_ULTMOD IS 'Fecha ultima modificacion.'
/
COMMENT ON COLUMN  CO_CLIESINTER.NOM_USUARIO IS 'Identificacion usuario.'
/
