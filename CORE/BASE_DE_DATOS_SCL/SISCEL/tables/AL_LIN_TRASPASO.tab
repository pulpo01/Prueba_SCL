CREATE TABLE AL_LIN_TRASPASO(
    NUM_TRASPASO NUMBER(9,0) NOT NULL 
 ,  LIN_TRASPASO NUMBER(3,0) NOT NULL 
 ,  TIP_STOCK NUMBER(2,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_ESTADO NUMBER(2,0) NOT NULL 
 ,  CAN_TRASPASO NUMBER(9,0) NOT NULL 
 ,  FEC_ENTRADA DATE NULL 
 ,  NUM_GUIA VARCHAR2(10) NULL 
 ,  NUM_SEC_LOCA_ORI NUMBER(10,0) NULL 
 ,  NUM_SEC_LOCA_DEST NUMBER(10,0) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_LIN_TRASPASO IS 'Lineas de Ordenes de Traspaso'
/
COMMENT ON COLUMN  AL_LIN_TRASPASO.NUM_TRASPASO IS 'Numero de orden de traspaso'
/
COMMENT ON COLUMN  AL_LIN_TRASPASO.LIN_TRASPASO IS 'Numero de linea'
/
COMMENT ON COLUMN  AL_LIN_TRASPASO.TIP_STOCK IS 'Tipo de stock al que corresponde el articulo'
/
COMMENT ON COLUMN  AL_LIN_TRASPASO.COD_ARTICULO IS 'Codigo de articulo a traspasar.'
/
COMMENT ON COLUMN  AL_LIN_TRASPASO.COD_USO IS 'Codigo de uso'
/
COMMENT ON COLUMN  AL_LIN_TRASPASO.COD_ESTADO IS 'Estado del articulo'
/
COMMENT ON COLUMN  AL_LIN_TRASPASO.CAN_TRASPASO IS 'Cantidad a traspasar'
/
COMMENT ON COLUMN  AL_LIN_TRASPASO.FEC_ENTRADA IS 'Fecha de ingreso en bodega'
/
COMMENT ON COLUMN  AL_LIN_TRASPASO.NUM_GUIA IS 'Numero de Guia de despacho'
/
COMMENT ON COLUMN  AL_LIN_TRASPASO.NUM_SEC_LOCA_ORI IS 'Secuencial de Localizacion Anterior.'
/
COMMENT ON COLUMN  AL_LIN_TRASPASO.NUM_SEC_LOCA_DEST IS 'Secuencial de Localizacion Destino.'
/
