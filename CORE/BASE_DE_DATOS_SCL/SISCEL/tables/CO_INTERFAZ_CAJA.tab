CREATE TABLE CO_INTERFAZ_CAJA(
    NUM_INTERFAZ NUMBER(8,0) NOT NULL 
 ,  TIP_MOVIMIENTO VARCHAR2(5) NOT NULL 
 ,  NUM_MOVIMIENTO NUMBER(8,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  IMPORTE NUMBER(14,4) NOT NULL 
 ,  IMPORTE_INTER NUMBER(14,4) NULL 
 ,  IND_PROCESADO NUMBER(1,0) NOT NULL 
 ,  IND_PASOCOB NUMBER(1,0) NULL 
 ,  FEC_MOVIMIENTO DATE NULL 
 ,  FEC_APLICAJA DATE NULL 
 ,  NOM_USUA_MOV VARCHAR2(20) NULL 
 ,  NOM_USUA_APLI VARCHAR2(20) NULL 
 ,  NUM_SECUENCI_PAGO NUMBER(8,0) NULL 
 ,  NUM_PROCESO NUMBER(8,0) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
