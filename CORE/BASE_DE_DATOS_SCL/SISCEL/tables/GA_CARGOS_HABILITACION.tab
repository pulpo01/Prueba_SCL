CREATE TABLE GA_CARGOS_HABILITACION(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_MODVENTA NUMBER(2,0) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  TIP_STOCK NUMBER(2,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  PRC_CARGO NUMBER(14,4) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  COD_CATEGORIA VARCHAR2(3) NOT NULL 
 ,  NUM_MESES NUMBER(2,0) NOT NULL 
 ,  VAL_MINIMO NUMBER(14,4) NULL 
 ,  VAL_MAXIMO NUMBER(14,4) NULL 
 ,  IND_VENTA NUMBER(1,0) NOT NULL 
 ,  COD_ANTIGUEDAD VARCHAR2(3) DEFAULT '01' NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
