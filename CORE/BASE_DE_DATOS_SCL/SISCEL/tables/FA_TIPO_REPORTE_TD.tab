 ,  DES_REPORTE VARCHAR2(25) NOT NULL 
 ,  COD_OPERADORA VARCHAR2(10) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_TIPO_REPORTE_TD IS '[ TPRE ] Indica los tipos de Reportes almacenados'
/
