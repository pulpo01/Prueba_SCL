CREATE TABLE SISCEL.PV_REGISTRA_RENOVACION_OS_TO
(
  NUM_OS              NUMBER(10)                DEFAULT 0                     NOT NULL,
  NUM_OS_RENOVA       NUMBER(10)                NOT NULL,
  NUM_ABONADO         NUMBER(8)                 NOT NULL,
  COD_OS              VARCHAR2(5 BYTE)          NOT NULL,
  FEC_INGRESO         DATE                      NOT NULL,
  NOM_USUARIO         VARCHAR2(30 BYTE)         NOT NULL,
  COD_ESTADO          VARCHAR2(2 BYTE)          DEFAULT 'PD'                  NOT NULL,
  IND_CARGO           NUMBER(1)                 DEFAULT 0,
  COD_VENDEDOR_COMIS  NUMBER(6)
)
TABLESPACE INSTALL_DATA
/
COMMENT ON TABLE SISCEL.PV_REGISTRA_RENOVACION_OS_TO IS '[ REGRENOVA ] Tabla que contienes la asociación de la OOSS de renovación versus la OOSS hijas';

COMMENT ON COLUMN SISCEL.PV_REGISTRA_RENOVACION_OS_TO.NUM_OS IS 'numero de orden de servicio hija';

COMMENT ON COLUMN SISCEL.PV_REGISTRA_RENOVACION_OS_TO.NUM_OS_RENOVA IS 'numero de orden de servicio de renovación';

COMMENT ON COLUMN SISCEL.PV_REGISTRA_RENOVACION_OS_TO.NUM_ABONADO IS 'numero de abonado';

COMMENT ON COLUMN SISCEL.PV_REGISTRA_RENOVACION_OS_TO.COD_OS IS 'codigo orden de servicio';

COMMENT ON COLUMN SISCEL.PV_REGISTRA_RENOVACION_OS_TO.FEC_INGRESO IS 'fecha de ingreso';

COMMENT ON COLUMN SISCEL.PV_REGISTRA_RENOVACION_OS_TO.NOM_USUARIO IS 'nombre de usuario';

COMMENT ON COLUMN SISCEL.PV_REGISTRA_RENOVACION_OS_TO.COD_ESTADO IS 'estado de la peticion';

COMMENT ON COLUMN SISCEL.PV_REGISTRA_RENOVACION_OS_TO.IND_CARGO IS 'indicador de cobro';

COMMENT ON COLUMN SISCEL.PV_REGISTRA_RENOVACION_OS_TO.COD_VENDEDOR_COMIS IS 'vendedor comisionista';
