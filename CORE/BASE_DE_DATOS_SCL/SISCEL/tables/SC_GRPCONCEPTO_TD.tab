CREATE TABLE SC_GRPCONCEPTO_TD(
    DOMINIO VARCHAR2(30) NOT NULL 
 ,  COD_GRPCONCEPTO NUMBER(6,0) NOT NULL 
 ,  DES_GRPCONCEPTO VARCHAR2(60) NOT NULL 
 ,  IND_ACTIVADO CHAR(1) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SC_GRPCONCEPTO_TD IS 'Dominios de conceptos de grupo'
/
COMMENT ON COLUMN  SC_GRPCONCEPTO_TD.DOMINIO IS 'Dominio asociado al concepto agrupado'
/
COMMENT ON COLUMN  SC_GRPCONCEPTO_TD.COD_GRPCONCEPTO IS 'Codigo del concepto facturable agrupado'
/
COMMENT ON COLUMN  SC_GRPCONCEPTO_TD.DES_GRPCONCEPTO IS 'Descripcion del concepto facturable agrupado'
/
COMMENT ON COLUMN  SC_GRPCONCEPTO_TD.IND_ACTIVADO IS 'Indicador de activado 0: contabilizable ;  1: No contabilizable'
/
