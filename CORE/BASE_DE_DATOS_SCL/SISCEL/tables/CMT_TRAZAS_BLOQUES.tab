CREATE TABLE CMT_TRAZAS_BLOQUES(
    SEC_BLOQUE NUMBER(6,0) NOT NULL 
 ,  COD_BLOQUE VARCHAR2(15) NOT NULL 
 ,  ID_PERIODO VARCHAR2(10) NULL 
 ,  COD_PERIODO NUMBER(8,0) NOT NULL 
 ,  IND_ESTADO CHAR(1) NOT NULL 
 ,  FEC_INICIO DATE NOT NULL 
 ,  FEC_TERMINO DATE NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMT_TRAZAS_BLOQUES IS 'Traza De Bloques.'
/
COMMENT ON COLUMN  CMT_TRAZAS_BLOQUES.SEC_BLOQUE IS 'Correlativo identificador unico de registro..'
/
COMMENT ON COLUMN  CMT_TRAZAS_BLOQUES.COD_BLOQUE IS 'Codigo de Bloque.'
/
COMMENT ON COLUMN  CMT_TRAZAS_BLOQUES.ID_PERIODO IS 'Identificador de periodo comisional..'
/
COMMENT ON COLUMN  CMT_TRAZAS_BLOQUES.COD_PERIODO IS 'Codigo de periodo comisional.'
/
COMMENT ON COLUMN  CMT_TRAZAS_BLOQUES.IND_ESTADO IS 'ESTADO DE EJECUCION [T]ERMINADO OK [I]NICIADO [F]ALLO.'
/
COMMENT ON COLUMN  CMT_TRAZAS_BLOQUES.FEC_INICIO IS 'Fecha de Inicio de Ejecucion.'
/
COMMENT ON COLUMN  CMT_TRAZAS_BLOQUES.FEC_TERMINO IS 'Fecha de Termino de Ejecucion.'
/
COMMENT ON COLUMN  CMT_TRAZAS_BLOQUES.NOM_USUARIO IS 'Usuario de ejecucion del bloque.'
/
