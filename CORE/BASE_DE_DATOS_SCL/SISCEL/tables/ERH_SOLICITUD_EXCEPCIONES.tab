CREATE TABLE ERH_SOLICITUD_EXCEPCIONES(
    NUM_SOLICITUD NUMBER(10,0) NOT NULL 
 ,  TIP_EXCEPCION VARCHAR2(2) NOT NULL 
 ,  COD_EXCEPCION VARCHAR2(5) NOT NULL 
 ,  COD_FUENTE NUMBER(6,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERH_SOLICITUD_EXCEPCIONES IS 'Detalle de Excepciones aplicadas a una solicitud.'
/
COMMENT ON COLUMN  ERH_SOLICITUD_EXCEPCIONES.NUM_SOLICITUD IS 'Numero de Solicitud.'
/
COMMENT ON COLUMN  ERH_SOLICITUD_EXCEPCIONES.TIP_EXCEPCION IS 'Tipo de Excepcion.'
/
COMMENT ON COLUMN  ERH_SOLICITUD_EXCEPCIONES.COD_EXCEPCION IS 'Codigo Excepcion.'
/
COMMENT ON COLUMN  ERH_SOLICITUD_EXCEPCIONES.COD_FUENTE IS 'Codigo Fuente.'
/
COMMENT ON COLUMN  ERH_SOLICITUD_EXCEPCIONES.NOM_USUARIO IS 'Usuario responsable.'
/
COMMENT ON COLUMN  ERH_SOLICITUD_EXCEPCIONES.FEC_ULTMOD IS 'Fecha movimiento.'
/
