CREATE TABLE SISCEL.AL_ACTIVACION_ALTAMIRA_TO
(
  ID_ACTIVACION   NUMBER(12)                    NOT NULL,
  FEC_ACTIVACION  DATE,
  NUM_CELULAR     NUMBER(15),
  NUM_SERIE       VARCHAR2(25 BYTE)             NOT NULL,
  FEC_CONTAB      DATE,
  FEC_FACT        DATE,
  COD_ARTICULO    NUMBER(6)                     NOT NULL,
  FEC_CREACION    DATE                          DEFAULT SYSDATE               NOT NULL
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          4M
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE SISCEL.AL_ACTIVACION_ALTAMIRA_TO IS '[ AL_ACTA ] Almacena información de los prepagos activados en la plataforma';

COMMENT ON COLUMN SISCEL.AL_ACTIVACION_ALTAMIRA_TO.FEC_CONTAB IS 'Fecha de contabilizacion';

COMMENT ON COLUMN SISCEL.AL_ACTIVACION_ALTAMIRA_TO.FEC_FACT IS 'Fecha de Facturacion ';

COMMENT ON COLUMN SISCEL.AL_ACTIVACION_ALTAMIRA_TO.FEC_CREACION IS 'Fecha de creacion';

COMMENT ON COLUMN SISCEL.AL_ACTIVACION_ALTAMIRA_TO.COD_ARTICULO IS 'Codigo de articulo';

COMMENT ON COLUMN SISCEL.AL_ACTIVACION_ALTAMIRA_TO.ID_ACTIVACION IS 'Indice de activacion';

COMMENT ON COLUMN SISCEL.AL_ACTIVACION_ALTAMIRA_TO.FEC_ACTIVACION IS 'Fecha de activacion en Altamira';

COMMENT ON COLUMN SISCEL.AL_ACTIVACION_ALTAMIRA_TO.NUM_CELULAR IS 'Numero de Celular activado en altamira';

COMMENT ON COLUMN SISCEL.AL_ACTIVACION_ALTAMIRA_TO.NUM_SERIE IS 'Número de Serie';




