CREATE TABLE CO_MOV_DOCUMENTOS(
    NUM_SECUENCIA_DOC NUMBER(8,0) NOT NULL 
 ,  NUM_DOCUMENTO VARCHAR2(20) NOT NULL 
 ,  NUM_SECUENCI NUMBER(8,0) NOT NULL 
 ,  COD_ESTADO VARCHAR2(5) NOT NULL 
 ,  COD_ESTADO_N VARCHAR2(5) NOT NULL 
 ,  FEC_CAMBIO_ESTADO DATE NOT NULL 
 ,  COD_PROTESTO VARCHAR2(5) NULL 
 ,  NUM_CUOTA NUMBER(8,0) NULL 
 ,  IMPORTE NUMBER(14,4) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_MOV_DOCUMENTOS IS 'Moviminetos De Los Documentos.'
/
COMMENT ON COLUMN  CO_MOV_DOCUMENTOS.NUM_SECUENCIA_DOC IS 'Numero de Secuencia del Documento.'
/
COMMENT ON COLUMN  CO_MOV_DOCUMENTOS.NUM_DOCUMENTO IS 'Numero del Documento.'
/
COMMENT ON COLUMN  CO_MOV_DOCUMENTOS.NUM_SECUENCI IS 'Secuencia de la Tabla.'
/
COMMENT ON COLUMN  CO_MOV_DOCUMENTOS.COD_ESTADO IS 'Codigo Estado del Documento.'
/
COMMENT ON COLUMN  CO_MOV_DOCUMENTOS.COD_ESTADO_N IS 'Nuevo Codigo de Estado del Documento.'
/
COMMENT ON COLUMN  CO_MOV_DOCUMENTOS.FEC_CAMBIO_ESTADO IS 'Fecha Cambio de Estado.'
/
COMMENT ON COLUMN  CO_MOV_DOCUMENTOS.COD_PROTESTO IS 'Codigo de Protesto.'
/
COMMENT ON COLUMN  CO_MOV_DOCUMENTOS.NUM_CUOTA IS 'Numero de Cuota.'
/
COMMENT ON COLUMN  CO_MOV_DOCUMENTOS.IMPORTE IS 'Monto del Documento.'
/
