CREATE TABLE CRH_DEVOLUCIONES
(
  COD_TIPDOCUM       NUMBER(2)                  NOT NULL,
  NUM_FOLIO          NUMBER(9)                  NOT NULL,
  NUM_EVENTO         NUMBER(5)                  NOT NULL,
  COD_MOTIVODEV      NUMBER(2)                  NOT NULL,
  NUM_GUIA           NUMBER(10)                 NOT NULL,
  FEC_INGRESO_DH     DATE                       NOT NULL,
  NOM_USUAINGRESO    VARCHAR2(30 BYTE)          NOT NULL,
  NUM_IDENT          VARCHAR2(20 BYTE),
  COD_TELEMARKETING  NUMBER(2),
  NUM_PROC_CMAS      NUMBER(30),
  NUM_CORR_CMAS      NUMBER(30),
  FEC_DEVOLUCION     DATE,
  COD_BARRA          VARCHAR2(12 BYTE),
  LOTE               NUMBER(6),
  POS_LOTE           NUMBER(6),
  COD_ESTADODEV      NUMBER(2),
  PREF_PLAZA         VARCHAR2(25 BYTE)          DEFAULT ' '
)
TABLESPACE TS_CRM_GENERAL_SDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          40K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE CRH_DEVOLUCIONES IS 'Tabla con el detalle de devoluciones por evento.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.COD_TIPDOCUM IS 'Tipo de documento.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.NUM_FOLIO IS 'Folio o numero de documento.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.NUM_EVENTO IS 'Numero de evento.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.COD_MOTIVODEV IS 'Motivo de la devolucion.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.NUM_GUIA IS 'Guia de despacho asociada a la devolucion.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.FEC_INGRESO_DH IS 'Fecha de registro de la devolucion.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.NOM_USUAINGRESO IS 'Usuario que registro la devolucion.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.NUM_IDENT IS 'RUT asociado a le devolucion, caso cartas.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.COD_TELEMARKETING IS 'Codigo de respuesta a la devolucion en telemarketing.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.NUM_PROC_CMAS IS 'Numero de proceso de carga masiva.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.NUM_CORR_CMAS IS 'Correlativo en proceso de carga masiva.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.FEC_DEVOLUCION IS 'Fecha de devolucion informada en archivo de correos.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.COD_BARRA IS 'Codigo de Barra.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.LOTE IS 'Lote.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.POS_LOTE IS 'Posicion dentro del lote.';

COMMENT ON COLUMN CRH_DEVOLUCIONES.PREF_PLAZA IS 'Codigo de Plaza.';

