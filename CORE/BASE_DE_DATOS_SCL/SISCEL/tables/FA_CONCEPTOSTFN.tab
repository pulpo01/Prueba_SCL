CREATE TABLE FA_CONCEPTOSTFN(
    COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COD_SUPERTFN NUMBER(5,0) NOT NULL 
 ,  COD_CONCTFN VARCHAR2(6) NOT NULL 
 ,  COD_CONCTFNDER VARCHAR2(6) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  FA_CONCEPTOSTFN.COD_CONCEPTO IS 'Codigo de Concepto'
/
COMMENT ON COLUMN  FA_CONCEPTOSTFN.COD_SUPERTFN IS 'Codigo de Super Telefono'
/
COMMENT ON COLUMN  FA_CONCEPTOSTFN.COD_CONCTFN IS 'Codigo Concepto Super Telefono'
/
COMMENT ON COLUMN  FA_CONCEPTOSTFN.COD_CONCTFNDER IS 'Codigo Concepto'
/
