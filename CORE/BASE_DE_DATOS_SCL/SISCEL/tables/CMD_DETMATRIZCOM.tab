CREATE TABLE CMD_DETMATRIZCOM(
    NUM_FILA NUMBER(6,0) NOT NULL 
 ,  NUM_MATRIZ NUMBER(6,0) NOT NULL 
 ,  NUM_DESDE NUMBER(10,0) NOT NULL 
 ,  NUM_HASTA NUMBER(10,0) NOT NULL 
 ,  IMP_COMISION NUMBER(14,4) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMD_DETMATRIZCOM IS 'Detalle De Las Matrices De Comisiones.'
/
COMMENT ON COLUMN  CMD_DETMATRIZCOM.NUM_FILA IS 'Identificador unico del registro de fila de la matriz de comisiones.'
/
COMMENT ON COLUMN  CMD_DETMATRIZCOM.NUM_MATRIZ IS 'Identificador de la matriz de comisiones a la que pertenece.'
/
COMMENT ON COLUMN  CMD_DETMATRIZCOM.NUM_DESDE IS 'umbral desde.'
/
COMMENT ON COLUMN  CMD_DETMATRIZCOM.NUM_HASTA IS 'Umbral hasta.'
/
COMMENT ON COLUMN  CMD_DETMATRIZCOM.IMP_COMISION IS 'Importe de comision asociado.'
/
COMMENT ON COLUMN  CMD_DETMATRIZCOM.FEC_ULTMOD IS 'Fecha de ultima modificacion del registro..'
/
COMMENT ON COLUMN  CMD_DETMATRIZCOM.NOM_USUARIO IS 'Usuario que hizo la ultima modificacion del registro en la tabla..'
/
