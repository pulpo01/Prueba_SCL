CREATE TABLE GED_PUNTOS_VENTA_AMISTAR(
    COD_PUNTO_VENTA NUMBER(10,0) NOT NULL 
 ,  DES_PUNTO_VENTA VARCHAR2(60) NOT NULL 
 ,  IND_VIGENCIA CHAR(1) NOT NULL 
 ,  COD_OFICINA VARCHAR2(2) NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NULL 
 ,  COD_COMUNA VARCHAR2(5) NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GED_PUNTOS_VENTA_AMISTAR IS 'Tabla de puntos de venta de Kits Amistar'
/
COMMENT ON COLUMN  GED_PUNTOS_VENTA_AMISTAR.COD_PUNTO_VENTA IS 'Codigo que identifica a un punto de venta de Kits Amistar'
/
COMMENT ON COLUMN  GED_PUNTOS_VENTA_AMISTAR.DES_PUNTO_VENTA IS 'Nombre del punto de venta de Kits Amistar'
/
COMMENT ON COLUMN  GED_PUNTOS_VENTA_AMISTAR.IND_VIGENCIA IS 'Vigencia del canal de venta'
/
COMMENT ON COLUMN  GED_PUNTOS_VENTA_AMISTAR.COD_OFICINA IS 'OFICINA ASOCIADA AL PUNTO DE VENTA'
/
COMMENT ON COLUMN  GED_PUNTOS_VENTA_AMISTAR.COD_VENDEDOR IS 'VENDEDOR ASOCIADO AL PUNTO DE VENTA'
/
COMMENT ON COLUMN  GED_PUNTOS_VENTA_AMISTAR.COD_COMUNA IS 'CODIGO DE COMUNA DEL PUNTO DE VENTA'
/
