CREATE TABLE AL_REP_CONSIGNACION(
    NUM_INFORME NUMBER(6,0) NOT NULL 
 ,  NUM_LINEA NUMBER(15,0) NOT NULL 
 ,  COD_BODEGA NUMBER(6,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  NUM_CANTIDAD NUMBER(9,0) NOT NULL 
 ,  TIP_MOVIMIENTO NUMBER(2,0) NOT NULL 
 ,  NUM_LISTA NUMBER(6,0) NULL 
 ,  PRC_ARTICULO NUMBER(14,4) NULL 
 ,  COD_PROVEEDOR NUMBER NULL 
 ,  COD_MONEDA VARCHAR2(3) NULL 
 ,  NUM_FACTURA NUMBER(9,0) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_REP_CONSIGNACION IS 'Tabla temporal con el informe de articulos en consignacion vendidos.'
/
COMMENT ON COLUMN  AL_REP_CONSIGNACION.NUM_INFORME IS 'Numero de Informe'
/
COMMENT ON COLUMN  AL_REP_CONSIGNACION.NUM_LINEA IS 'Numero de linea'
/
COMMENT ON COLUMN  AL_REP_CONSIGNACION.COD_BODEGA IS 'Codigo Bodega'
/
COMMENT ON COLUMN  AL_REP_CONSIGNACION.COD_ARTICULO IS 'Codigo de Articulo'
/
COMMENT ON COLUMN  AL_REP_CONSIGNACION.NUM_CANTIDAD IS 'Cantidad'
/
COMMENT ON COLUMN  AL_REP_CONSIGNACION.TIP_MOVIMIENTO IS 'Tipo de Movimiento'
/
COMMENT ON COLUMN  AL_REP_CONSIGNACION.NUM_LISTA IS 'Numero de Lista'
/
COMMENT ON COLUMN  AL_REP_CONSIGNACION.PRC_ARTICULO IS 'Precio Lista'
/
COMMENT ON COLUMN  AL_REP_CONSIGNACION.COD_PROVEEDOR IS 'Proveedor'
/
COMMENT ON COLUMN  AL_REP_CONSIGNACION.COD_MONEDA IS 'Moneda'
/
COMMENT ON COLUMN  AL_REP_CONSIGNACION.NUM_FACTURA IS 'Numero de Factura'
/
