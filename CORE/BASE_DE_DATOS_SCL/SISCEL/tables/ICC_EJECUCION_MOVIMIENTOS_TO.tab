CREATE TABLE ICC_EJECUCION_MOVIMIENTOS_TO(
    COD_MASIVO VARCHAR2(10) NOT NULL 
 ,  COD_TAREA VARCHAR2(10) NULL 
 ,  FEC_ASIGNACION DATE DEFAULT NULL NULL 
 ,  ESTADO VARCHAR2(1) NULL 
 ,  NUM_RANGO_PENDIENTES NUMBER(6,0) NULL 
 ,  NUM_RANGO_REINTENTABLES NUMBER(6,0) NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICC_EJECUCION_MOVIMIENTOS_TO IS 'Tabla para asignacion de las ejecuciones de procesamientos masivos'
/
COMMENT ON COLUMN  ICC_EJECUCION_MOVIMIENTOS_TO.COD_MASIVO IS 'Codigo de agrupamiento para procesos masivos'
/
COMMENT ON COLUMN  ICC_EJECUCION_MOVIMIENTOS_TO.COD_TAREA IS 'Identificador del Procesador que atiende la respuesta'
/
COMMENT ON COLUMN  ICC_EJECUCION_MOVIMIENTOS_TO.FEC_ASIGNACION IS 'Fecha de procesamiento'
/
COMMENT ON COLUMN  ICC_EJECUCION_MOVIMIENTOS_TO.ESTADO IS 'Estado  de la ejecucion'
/
COMMENT ON COLUMN  ICC_EJECUCION_MOVIMIENTOS_TO.NUM_RANGO_PENDIENTES IS 'Cantidad de registros para procesar en estado pendiente'
/
COMMENT ON COLUMN  ICC_EJECUCION_MOVIMIENTOS_TO.NUM_RANGO_REINTENTABLES IS 'Cantidad de registros para procesar en estado reintentable'
/
