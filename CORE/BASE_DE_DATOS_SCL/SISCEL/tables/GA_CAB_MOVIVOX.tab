CREATE TABLE GA_CAB_MOVIVOX(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  NUM_TELEFONOS NUMBER(5,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_CAB_MOVIVOX IS 'Cabecera que identifica a cada cliente y abonado que posee un telefono MoviVox'
/
COMMENT ON COLUMN  GA_CAB_MOVIVOX.COD_CLIENTE IS 'Codigo de cliente de contrato MoviVox'
/
COMMENT ON COLUMN  GA_CAB_MOVIVOX.NUM_ABONADO IS 'Numero de Abonado asociado a MoviVox'
/
COMMENT ON COLUMN  GA_CAB_MOVIVOX.FEC_ALTA IS 'Fecha de ingreso de venta'
/
COMMENT ON COLUMN  GA_CAB_MOVIVOX.NOM_USUARIO IS 'Usaurio que efectuo el ingreso'
/
COMMENT ON COLUMN  GA_CAB_MOVIVOX.NUM_CELULAR IS 'Numero celular asociado al abonado'
/
COMMENT ON COLUMN  GA_CAB_MOVIVOX.NUM_TELEFONOS IS 'Cantidad de telefonos MoviVox'
/
