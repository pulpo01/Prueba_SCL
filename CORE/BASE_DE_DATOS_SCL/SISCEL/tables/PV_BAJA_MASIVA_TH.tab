CREATE TABLE PV_BAJA_MASIVA_TH(
    NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  ID_PROCESO NUMBER(15,0) NOT NULL 
 ,  COD_ESTADO NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  COD_SQLCODE VARCHAR2(15) NULL 
 ,  COD_SQLERRM VARCHAR2(1000) NULL 
 ,  DESC_ERR VARCHAR2(200) NULL 
 ,  NOM_TABLA VARCHAR2(50) NULL 
 ,  COD_ACT VARCHAR2(1) NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  NUM_OS NUMBER(10,0) NULL 
 ,  FEC_HISTORICO DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_BAJA_MASIVA_TH IS '[ PV_BM_TH ] N de Celular para la Baja Masiva Batch Postpago HistoricoN de Celular para la Baja Masiva Batch Postpago Historico'
/
