/
COMMENT ON COLUMN  PV_BAJA_MASIVA_TH.COD_SQLERRM IS 'Error Sql Errm'
/
COMMENT ON COLUMN  PV_BAJA_MASIVA_TH.DESC_ERR IS 'Descripcion de Error'
