CREATE TABLE BP_CAMPANAS_TD(
    COD_CAMPANA VARCHAR2(5) NOT NULL 
 ,  DES_CAMPANA VARCHAR2(30) NOT NULL 
 ,  FEC_INICIO DATE NOT NULL 
 ,  FEC_TERMINO DATE NOT NULL 
 ,  COD_TIPLAN VARCHAR2(5) NOT NULL 
 ,  TIP_ENTIDAD VARCHAR2(5) NOT NULL 
 ,  IND_DEFAULT NUMBER(1,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_CRM_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  BP_CAMPANAS_TD IS 'Campa?as promocionales para la Venta.'
/
COMMENT ON COLUMN  BP_CAMPANAS_TD.COD_CAMPANA IS 'Codigo de campa?a.'
/
COMMENT ON COLUMN  BP_CAMPANAS_TD.DES_CAMPANA IS 'Descripcion de la campa?a.'
/
COMMENT ON COLUMN  BP_CAMPANAS_TD.FEC_INICIO IS 'Fecha de inicio de la campa?a.'
/
COMMENT ON COLUMN  BP_CAMPANAS_TD.FEC_TERMINO IS 'Fecha de termino de la campa?a.'
/
COMMENT ON COLUMN  BP_CAMPANAS_TD.COD_TIPLAN IS 'Tipo de plan (1:Prepago 2:Pospago 3:Hibrido).'
/
COMMENT ON COLUMN  BP_CAMPANAS_TD.TIP_ENTIDAD IS 'Nivel de la campa?a (A:Abonado C:Cliente).'
/
COMMENT ON COLUMN  BP_CAMPANAS_TD.IND_DEFAULT IS 'Indicador de campa?a por default en la venta.'
/
