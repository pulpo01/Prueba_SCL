CREATE TABLE VE_HISVENDEDORES
(
  COD_TIPCOMIS        VARCHAR2(2 BYTE)          NOT NULL,
  COD_VENDEDOR        NUMBER(6)                 NOT NULL,
  NOM_VENDEDOR        VARCHAR2(40 BYTE)         NOT NULL,
  COD_TIPIDENT        VARCHAR2(2 BYTE)          NOT NULL,
  NUM_IDENT           VARCHAR2(20 BYTE)         NOT NULL,
  FEC_CONTRATO        DATE                      NOT NULL,
  COD_DIRECCION       NUMBER(20)                NOT NULL,
  COD_OFICINA         VARCHAR2(2 BYTE)          NOT NULL,
  IND_INTERNO         NUMBER(1)                 NOT NULL,
  IND_TIPVENTA        NUMBER(1)                 NOT NULL,
  FEC_FINCONTRATO     DATE,
  COD_VENDE_RAIZ      NUMBER(6)                 NOT NULL,
  COD_CLIENTE         NUMBER(8),
  IND_DICOM           NUMBER(1),
  IND_DICOMCLEAR      NUMBER(1),
  NUM_TELEF1          VARCHAR2(15 BYTE),
  NUM_TELEF2          VARCHAR2(15 BYTE),
  NUM_FAX             VARCHAR2(15 BYTE),
  NUM_PIN             VARCHAR2(10 BYTE),
  NOM_RESPONSABLE     VARCHAR2(40 BYTE),
  COD_TIPIDRESP       VARCHAR2(2 BYTE),
  NUM_IDENTRESP       VARCHAR2(20 BYTE),
  COD_CUENTA          NUMBER(8),
  COD_TIPIDAPOD       VARCHAR2(2 BYTE),
  NUM_IDENTAPOD       VARCHAR2(20 BYTE),
  E_MAIL              VARCHAR2(100 BYTE),
  NOM_APODERADO       VARCHAR2(40 BYTE),
  NOM_CONTACTO        VARCHAR2(40 BYTE),
  NUM_TELEF3          VARCHAR2(15 BYTE),
  COD_ESTADO          NUMBER(2)                 DEFAULT 0                     NOT NULL,
  COD_PLAZA           VARCHAR2(5 BYTE)          DEFAULT '1'                   NOT NULL,
  IND_OFICINA_PROPIA  NUMBER(1)                 DEFAULT 0                     NOT NULL,
  COD_PASSWORD        VARCHAR2(11 BYTE),
  IND_PASSWORD        CHAR(1 BYTE),
  FEC_PASOHIST        DATE                      DEFAULT SYSDATE               NOT NULL,
  NOM_USUARIO         VARCHAR2(30 BYTE)         DEFAULT USER                  NOT NULL
)
TABLESPACE TS_CRM_GESTION_SDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          4120K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE VE_HISVENDEDORES IS 'Historico de movimiento de Vendedores comisionistas';

COMMENT ON COLUMN VE_HISVENDEDORES.COD_PLAZA IS 'Codigo de plaza del agente de ventas';

COMMENT ON COLUMN VE_HISVENDEDORES.IND_OFICINA_PROPIA IS 'Indicador de oficina propia o de la operadora';

COMMENT ON COLUMN VE_HISVENDEDORES.COD_PASSWORD IS 'Codigo de password';

COMMENT ON COLUMN VE_HISVENDEDORES.IND_PASSWORD IS 'Indicador de password';

COMMENT ON COLUMN VE_HISVENDEDORES.FEC_PASOHIST IS 'Fecha de paso a historicos';

COMMENT ON COLUMN VE_HISVENDEDORES.NOM_USUARIO IS 'Usuario paso a historicos';

COMMENT ON COLUMN VE_HISVENDEDORES.COD_TIPCOMIS IS 'Codigo del Tipo de Comisionista';

COMMENT ON COLUMN VE_HISVENDEDORES.COD_VENDEDOR IS 'Codigo del Vendedor';

COMMENT ON COLUMN VE_HISVENDEDORES.NOM_VENDEDOR IS 'Nombre del Vendedor';

COMMENT ON COLUMN VE_HISVENDEDORES.COD_TIPIDENT IS 'Codigo de Tipo de Identificacion';

COMMENT ON COLUMN VE_HISVENDEDORES.NUM_IDENT IS 'Numero de Identificacion';

COMMENT ON COLUMN VE_HISVENDEDORES.FEC_CONTRATO IS 'Fecha de contratacion';

COMMENT ON COLUMN VE_HISVENDEDORES.COD_DIRECCION IS 'Codigo de Direccion';

COMMENT ON COLUMN VE_HISVENDEDORES.COD_OFICINA IS 'Codigo de Oficina';

COMMENT ON COLUMN VE_HISVENDEDORES.IND_INTERNO IS 'Indicativo de Interno/Externo';

COMMENT ON COLUMN VE_HISVENDEDORES.IND_TIPVENTA IS 'Indicativo de Tipo de Venta = IND_RED (VE_CATCOMIS)';

COMMENT ON COLUMN VE_HISVENDEDORES.FEC_FINCONTRATO IS 'Fecha de fin de contrato';

COMMENT ON COLUMN VE_HISVENDEDORES.COD_VENDE_RAIZ IS 'Codigo del vendedor raiz para el vendedor en curso';

COMMENT ON COLUMN VE_HISVENDEDORES.COD_CLIENTE IS 'Codigo de Cliente';

COMMENT ON COLUMN VE_HISVENDEDORES.IND_DICOM IS 'Indicativo de DICOM';

COMMENT ON COLUMN VE_HISVENDEDORES.IND_DICOMCLEAR IS 'Indicativo de DICOM aclarado';

COMMENT ON COLUMN VE_HISVENDEDORES.NUM_TELEF1 IS 'Primer telefono';

COMMENT ON COLUMN VE_HISVENDEDORES.NUM_TELEF2 IS 'Segundo telefono';

COMMENT ON COLUMN VE_HISVENDEDORES.NUM_FAX IS 'Numero de fax';

COMMENT ON COLUMN VE_HISVENDEDORES.NUM_PIN IS 'Numero de PIN';

COMMENT ON COLUMN VE_HISVENDEDORES.NOM_RESPONSABLE IS 'Nombre del Responsable';

COMMENT ON COLUMN VE_HISVENDEDORES.COD_TIPIDRESP IS 'Tipo de Identificacion del Responsable';

COMMENT ON COLUMN VE_HISVENDEDORES.NUM_IDENTRESP IS 'Numero de Identificacion del Responsable';

COMMENT ON COLUMN VE_HISVENDEDORES.COD_CUENTA IS 'Codigo de Cuenta';

COMMENT ON COLUMN VE_HISVENDEDORES.COD_TIPIDAPOD IS 'Codigo Tipo identificacion apoderado';

COMMENT ON COLUMN VE_HISVENDEDORES.NUM_IDENTAPOD IS 'Numero Identificacion apoderado';

COMMENT ON COLUMN VE_HISVENDEDORES.E_MAIL IS 'correo electronico';

COMMENT ON COLUMN VE_HISVENDEDORES.NOM_APODERADO IS 'nombre apoderado';

COMMENT ON COLUMN VE_HISVENDEDORES.NOM_CONTACTO IS 'Nombre persona de contacto';

COMMENT ON COLUMN VE_HISVENDEDORES.NUM_TELEF3 IS 'Telefono de contacto';

COMMENT ON COLUMN VE_HISVENDEDORES.COD_ESTADO IS 'Codigo de estado (ve_estados_td)';

