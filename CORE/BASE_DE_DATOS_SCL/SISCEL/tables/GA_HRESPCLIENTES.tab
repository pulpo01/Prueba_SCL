CREATE TABLE GA_HRESPCLIENTES(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ORDEN NUMBER(1,0) NOT NULL 
 ,  COD_TIPIDENT VARCHAR2(2) NOT NULL 
 ,  NUM_IDENT VARCHAR2(20) NOT NULL 
 ,  NOM_RESPONSABLE VARCHAR2(50) NOT NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 CACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_HRESPCLIENTES IS 'Responsables Legales de Clientes'
/
COMMENT ON COLUMN  GA_HRESPCLIENTES.COD_CLIENTE IS 'Codigo de Cliente'
/
COMMENT ON COLUMN  GA_HRESPCLIENTES.NUM_ORDEN IS 'Numero de Orden del Responsable'
/
COMMENT ON COLUMN  GA_HRESPCLIENTES.COD_TIPIDENT IS 'Codigo Tipo Identificacion'
/
COMMENT ON COLUMN  GA_HRESPCLIENTES.NUM_IDENT IS 'Numero de Identificacion'
/
COMMENT ON COLUMN  GA_HRESPCLIENTES.NOM_RESPONSABLE IS 'Nombre de Responsable'
/
COMMENT ON COLUMN  GA_HRESPCLIENTES.FEC_HISTORICO IS 'Fecha de Historico'
/
