CREATE TABLE ICC_MOVBLOQ(
    NUM_MOVIMIENTO NUMBER(9,0) NOT NULL 
 ,  DES_PUESTO VARCHAR2(15) NOT NULL 
 ,  DES_USER VARCHAR2(30) DEFAULT 'USER' NOT NULL 
 ,  FEC_ENTRADA DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICC_MOVBLOQ IS 'Movimientos procesados por un operador'
/
COMMENT ON COLUMN  ICC_MOVBLOQ.NUM_MOVIMIENTO IS 'Numero de movimiento'
/
COMMENT ON COLUMN  ICC_MOVBLOQ.DES_PUESTO IS 'Puesto desde el que se bloqueo el movimiento'
/
COMMENT ON COLUMN  ICC_MOVBLOQ.DES_USER IS 'Usuario oracle que bloqueo el movimiento'
/
COMMENT ON COLUMN  ICC_MOVBLOQ.FEC_ENTRADA IS 'Fecha en que se bloqueo el movimiento'
/
