CREATE TABLE VE_RED_SOCIAL_TO
(
  COD_CLIENTE          NUMBER(8)                NOT NULL,
  DES_CUENTA_FACEBOOK  VARCHAR2(100 BYTE),
  DES_CUENTA_TWITTER   VARCHAR2(100 BYTE)
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          1M
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING
/
COMMENT ON COLUMN VE_RED_SOCIAL_TO.COD_CLIENTE IS 'Codigo Cliente';
/
COMMENT ON COLUMN VE_RED_SOCIAL_TO.DES_CUENTA_FACEBOOK IS 'Descripción Cuenta Facebook';
/
COMMENT ON COLUMN VE_RED_SOCIAL_TO.DES_CUENTA_TWITTER IS 'Descripcíon Cuenta Twitter';
/