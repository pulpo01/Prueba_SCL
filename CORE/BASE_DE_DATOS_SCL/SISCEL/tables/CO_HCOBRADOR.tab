CREATE TABLE CO_HCOBRADOR(
    FEC_VALOR DATE NOT NULL 
 ,  COD_COBRADOR NUMBER(6,0) NOT NULL 
 ,  NOM_COBRADOR VARCHAR2(30) NOT NULL 
 ,  NOM_APELL1COB VARCHAR2(30) NOT NULL 
 ,  NOM_APELL2COB VARCHAR2(30) NOT NULL 
 ,  FECHA_ALTA DATE NOT NULL 
 ,  COD_RIESGO NUMBER(1,0) NOT NULL 
 ,  COD_DISPER NUMBER(1,0) NOT NULL 
 ,  COD_TIPCOMISION NUMBER(2,0) NOT NULL 
 ,  NUM_CELULAR NUMBER(15,0) NOT NULL 
 ,  IND_JUDICIAL NUMBER(1,0) NOT NULL 
 ,  MONTO_DEUDA NUMBER(14,4) NOT NULL 
 ,  NUM_IDENT VARCHAR2(20) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
