CREATE TABLE IC_DETALLE_MOVIMIENTO_TH(
    NUM_MOVIMIENTO NUMBER(9,0) NOT NULL 
 ,  NOM_SUBPARAMETRO VARCHAR2(15) NOT NULL 
 ,  VAL_SUBPARAMETRO VARCHAR2(50) NULL 
 ,  FEC_HISTORICO DATE NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  IC_DETALLE_MOVIMIENTO_TH IS '[ IC_DETMOH ] Detalles de Movimientos Históricos'
/
COMMENT ON COLUMN  IC_DETALLE_MOVIMIENTO_TH.NUM_MOVIMIENTO IS 'Número Movimiento'
/
COMMENT ON COLUMN  IC_DETALLE_MOVIMIENTO_TH.NOM_SUBPARAMETRO IS 'Nombre del Subparámetro'
/
COMMENT ON COLUMN  IC_DETALLE_MOVIMIENTO_TH.VAL_SUBPARAMETRO IS 'Valor del subaparametro'
/
COMMENT ON COLUMN  IC_DETALLE_MOVIMIENTO_TH.FEC_HISTORICO IS 'Fecha de Paso a Histórico'
/
