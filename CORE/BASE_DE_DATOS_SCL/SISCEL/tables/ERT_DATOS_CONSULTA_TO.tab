CREATE TABLE ERT_DATOS_CONSULTA_TO(
    NUM_LINEA NUMBER(3,0) NOT NULL 
 ,  FECHA_ACTUALIZACION DATE NOT NULL 
 ,  NUM_IDENT VARCHAR2(20) NOT NULL 
 ,  TRAMA_EXTERNA VARCHAR2(1000) NOT NULL 
 ,  COD_TIPIDENT VARCHAR2(2) NULL 
 ,  IND_TIPO_SOLICITUD NUMBER(2,0) NULL 
 ) TABLESPACE    TS_CRM_GENERAL_MDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ERT_DATOS_CONSULTA_TO IS '[ DACO ] Información en trama de cliente recuperados de fuente externaInformación en trama de cliente recuperados de fuente externa'
/
COMMENT ON COLUMN  ERT_DATOS_CONSULTA_TO.NUM_LINEA IS 'Número de líneas que contiene la trama'
/
COMMENT ON COLUMN  ERT_DATOS_CONSULTA_TO.FECHA_ACTUALIZACION IS 'Fecha de actualización de datos de la fuente externa'
/
COMMENT ON COLUMN  ERT_DATOS_CONSULTA_TO.NUM_IDENT IS 'Número de identificación del cliente consultado a la fuente externa'
/
COMMENT ON COLUMN  ERT_DATOS_CONSULTA_TO.TRAMA_EXTERNA IS 'Trama que contiene la información de la fuente externa'
/
COMMENT ON COLUMN  ERT_DATOS_CONSULTA_TO.COD_TIPIDENT IS 'Tipo de identificacion'
/
COMMENT ON COLUMN  ERT_DATOS_CONSULTA_TO.IND_TIPO_SOLICITUD IS 'Indicador de Tipo de Solicitud (0= Prechequeo, 1=Evaluacion)'
/
