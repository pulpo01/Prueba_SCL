CREATE TABLE SISCEL.GA_HRESERVA
(
  NUM_CELULAR        NUMBER(15)                 NOT NULL,
  FEC_RESERVA        DATE                       NOT NULL,
  COD_CLIENTE        NUMBER(8),
  COD_VENDEDOR       NUMBER(6)                  NOT NULL,
  ORIGEN             VARCHAR2(12 BYTE)          NOT NULL,
  COD_SUBALM         VARCHAR2(5 BYTE)           NOT NULL,
  COD_PRODUCTO       NUMBER(1)                  NOT NULL,
  COD_CENTRAL        NUMBER(3)                  NOT NULL,
  COD_CAT            NUMBER(2)                  NOT NULL,
  COD_USO            NUMBER(2)                  NOT NULL,
  FEC_BAJA           DATE                       NOT NULL,
  IND_EQUIPADO       NUMBER(1)                  NOT NULL,
  USO_ANTERIOR       NUMBER(2),
  FECHA_DESRESERVA   DATE                       NOT NULL,
  CAUSAL_DESRESERVA  NUMBER(2)                  NOT NULL,
  NOM_USUARORA       VARCHAR2(30 BYTE)          NOT NULL,
  COD_VENDEALER      NUMBER(6)
)   TABLESPACE TS_CRM_POSVENTA_SDAT
/

COMMENT ON TABLE SISCEL.GA_HRESERVA IS 'Registro Historico De Reservas.';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.COD_VENDEALER IS 'Codigo de Vendedor Externo asociado al Número';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.NUM_CELULAR IS 'Numero de celular.';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.FEC_RESERVA IS 'Fecha de la reserva (sysdate).';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.COD_CLIENTE IS 'Identificador del cliente.';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.COD_VENDEDOR IS 'Identificador del vendedor.';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.ORIGEN IS 'Origen del numero que se reserva (nuevo, reciclado, reservado).';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.COD_SUBALM IS 'Subalm.';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.COD_PRODUCTO IS 'Producto.';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.COD_CENTRAL IS 'Central.';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.COD_CAT IS 'Categoria.';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.COD_USO IS 'Uso.';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.FEC_BAJA IS 'Fecha de Baja.';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.IND_EQUIPADO IS 'Indicador equipado.';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.USO_ANTERIOR IS 'Uso anterior.';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.FECHA_DESRESERVA IS 'Fecha de la desreserva.';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.CAUSAL_DESRESERVA IS 'Tipo de causa de la desreserva.';

COMMENT ON COLUMN SISCEL.GA_HRESERVA.NOM_USUARORA IS 'Identificador del usuario que realizo la desreserva.';

