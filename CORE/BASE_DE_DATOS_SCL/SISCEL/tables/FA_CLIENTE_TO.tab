CREATE TABLE FA_CLIENTE_TO(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ,  COD_DESPACHO VARCHAR2(5) NULL 
 ,  NOM_EMAIL VARCHAR2(70) NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_CLIENTE_TO IS '[ CLIE ] Tabla con datos de cliente para facturación'
/
COMMENT ON COLUMN  FA_CLIENTE_TO.COD_CLIENTE IS 'Código de cliente'
/
COMMENT ON COLUMN  FA_CLIENTE_TO.FEC_DESDE IS 'Fecha en que se modificó los datos del cliente'
/
COMMENT ON COLUMN  FA_CLIENTE_TO.FEC_HASTA IS 'Fecha que define el término de vigencia de los datos'
/
COMMENT ON COLUMN  FA_CLIENTE_TO.COD_DESPACHO IS 'Código de los tipos de despacho para la factura'
/
COMMENT ON COLUMN  FA_CLIENTE_TO.NOM_EMAIL IS 'Correo electrónico del cliente'
/
COMMENT ON COLUMN  FA_CLIENTE_TO.NOM_USUARIO IS 'Usuario que inserta/modifica el registro'
/
COMMENT ON COLUMN  FA_CLIENTE_TO.FEC_ULTMOD IS 'Fecha que se inserta/modifica el registro'
/
