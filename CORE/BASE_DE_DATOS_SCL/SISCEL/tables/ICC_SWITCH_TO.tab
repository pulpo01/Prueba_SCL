CREATE TABLE ICC_SWITCH_TO(
    COD_INFERIOR NUMBER(10,0) NOT NULL 
 ,  COD_SUPERIOR NUMBER(10,0) NOT NULL 
 ,  REGION NUMBER(3,0) NOT NULL 
 ,  SWITCHID NUMBER(3,0) NOT NULL 
 ,  NUM_SWITCH NUMBER(5,0) NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICC_SWITCH_TO IS 'SWITCH ASOCIADO AL ABONADO'
/
COMMENT ON COLUMN  ICC_SWITCH_TO.COD_INFERIOR IS 'CODIGO INFERIOR'
/
COMMENT ON COLUMN  ICC_SWITCH_TO.COD_SUPERIOR IS 'CODIGO SUPERIOR'
/
COMMENT ON COLUMN  ICC_SWITCH_TO.REGION IS 'REGION'
/
COMMENT ON COLUMN  ICC_SWITCH_TO.SWITCHID IS 'IDENTIFICADOR DEL SWITCH'
/
COMMENT ON COLUMN  ICC_SWITCH_TO.NUM_SWITCH IS 'NUMERO DE SWITCH'
/
