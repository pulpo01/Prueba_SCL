CREATE TABLE CO_FALLASCAJA(
    COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  COD_CAJA NUMBER(4,0) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  TIP_VALOR NUMBER(2,0) NOT NULL 
 ,  FEC_EFECTIVIDAD DATE NOT NULL 
 ,  IMP_FALLA_CAMBIO NUMBER(14,4) NULL 
 ,  IMP_FALLA_EGRESO NUMBER(14,4) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_FALLASCAJA IS 'FALLAS DE CAJA'
/
COMMENT ON COLUMN  CO_FALLASCAJA.COD_OFICINA IS 'CODIGO DE OFICINA'
/
COMMENT ON COLUMN  CO_FALLASCAJA.COD_CAJA IS 'CODIGO DE CAJA'
/
COMMENT ON COLUMN  CO_FALLASCAJA.NOM_USUARORA IS 'NOMBRE DE USUARIO'
/
COMMENT ON COLUMN  CO_FALLASCAJA.TIP_VALOR IS 'FORMA DE EFECTUAR EL PAGO'
/
COMMENT ON COLUMN  CO_FALLASCAJA.FEC_EFECTIVIDAD IS 'FECHA DE EFECTIVIDAD'
/
COMMENT ON COLUMN  CO_FALLASCAJA.IMP_FALLA_CAMBIO IS 'IMPORTE DE LA FALLA PARA CAMBIO'
/
COMMENT ON COLUMN  CO_FALLASCAJA.IMP_FALLA_EGRESO IS 'IMPORTE DE LA FALLA PARA EGRESOS'
/
