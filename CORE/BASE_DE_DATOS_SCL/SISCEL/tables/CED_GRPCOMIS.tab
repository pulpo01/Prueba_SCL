CREATE TABLE CED_GRPCOMIS(
    COD_GRPSERV NUMBER(5,0) NOT NULL 
 ,  COD_SERVICIO VARCHAR2(5) NOT NULL 
 ,  FEC_INICIO DATE NOT NULL 
 ,  FEC_TERMINO DATE NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(15) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CED_GRPCOMIS IS 'Definicisn de grupos de servicios para comisisn'
/
COMMENT ON COLUMN  CED_GRPCOMIS.COD_GRPSERV IS 'Grupo de servicios'
/
COMMENT ON COLUMN  CED_GRPCOMIS.COD_SERVICIO IS 'Servicio asociado'
/
COMMENT ON COLUMN  CED_GRPCOMIS.FEC_INICIO IS 'Fecha Inicio Vigencia grupo'
/
COMMENT ON COLUMN  CED_GRPCOMIS.FEC_TERMINO IS 'Fecha termino de vigencia Grupo'
/
COMMENT ON COLUMN  CED_GRPCOMIS.FEC_ULTMOD IS 'Fecha ultima modificacion'
/
COMMENT ON COLUMN  CED_GRPCOMIS.NOM_USUARIO IS 'Responsable ultima modificacion'
/
