CREATE TABLE SISCEL.PV_CAUSABLODESQ_TO
(
  COD_PRODUCTO      NUMBER(1)                   NOT NULL,
  COD_CAUSABLODES   VARCHAR2(2 BYTE)            NOT NULL,
  DES_CAUSABLODES   VARCHAR2(30 BYTE)           NOT NULL,
  FEC_INI_VIGENCIA  DATE                        NOT NULL,
  FEC_FIN_VIGENCIA  DATE                        NOT NULL,
  IND_DESBLOQ       VARCHAR2(2 BYTE)            NOT NULL,
  IND_LN            NUMBER(1),
  IND_COBRO         NUMBER(1),
  IND_PORTABLE      NUMBER(1),
  IND_FRAUDE        NUMBER(1)                   DEFAULT 0                     NOT NULL,
  COD_TIPFRAUDE     VARCHAR2(3 BYTE),
  IND_SMSPROMO      NUMBER(1)                   DEFAULT 0                     NOT NULL,
  TIP_SUSPENCION    NUMBER(1)                   DEFAULT 0
)  TABLESPACE TS_CRM_POSVENTA_SDAT
/

COMMENT ON COLUMN SISCEL.PV_CAUSABLODESQ_TO.IND_FRAUDE IS 'Indicador de Fraude';

COMMENT ON COLUMN SISCEL.PV_CAUSABLODESQ_TO.COD_TIPFRAUDE IS 'Codigo de Tipo deFraude';

COMMENT ON COLUMN SISCEL.PV_CAUSABLODESQ_TO.IND_SMSPROMO IS 'Indicador de SMS Promociones';

COMMENT ON COLUMN SISCEL.PV_CAUSABLODESQ_TO.TIP_SUSPENCION IS 'Tipo de suspención';

COMMENT ON COLUMN SISCEL.PV_CAUSABLODESQ_TO.IND_COBRO IS 'Indica si la causa es de cobro o no 1:SI, 0:NO.';

COMMENT ON COLUMN SISCEL.PV_CAUSABLODESQ_TO.IND_PORTABLE IS 'Indicador de portababilidad de numero 0:no portable, 1:es portable.';

COMMENT ON COLUMN SISCEL.PV_CAUSABLODESQ_TO.COD_PRODUCTO IS 'Codigo de Producto.';

COMMENT ON COLUMN SISCEL.PV_CAUSABLODESQ_TO.COD_CAUSABLODES IS 'Codigo de Causa de Bloqueo o Desbloqueo.';

COMMENT ON COLUMN SISCEL.PV_CAUSABLODESQ_TO.DES_CAUSABLODES IS 'Descripcion de Causa de Bloqueo o Desbloqueo.';

COMMENT ON COLUMN SISCEL.PV_CAUSABLODESQ_TO.FEC_INI_VIGENCIA IS 'Fecha inicio de la Vigencia de la causa.';

COMMENT ON COLUMN SISCEL.PV_CAUSABLODESQ_TO.FEC_FIN_VIGENCIA IS 'Fecha termino de la Vigencia de la causa.';

COMMENT ON COLUMN SISCEL.PV_CAUSABLODESQ_TO.IND_DESBLOQ IS 'Indicador B:bloqueo, D:desbloqueo.';

COMMENT ON COLUMN SISCEL.PV_CAUSABLODESQ_TO.IND_LN IS 'Indica si serie es o no ingresada a lista negra cuando se selecciona una causa 0:No, 1:Si.';

