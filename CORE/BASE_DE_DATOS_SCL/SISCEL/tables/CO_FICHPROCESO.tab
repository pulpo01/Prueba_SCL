 ,  COD_SUSPREHA VARCHAR2(3) NOT NULL 
 ,  COD_ACTABO VARCHAR2(2) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  NOM_FICH VARCHAR2(80) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_VALOR DATE NOT NULL 
 ,  TOT_REGISTROS NUMBER(8,0) NOT NULL 
 ,  TOT_SUSP NUMBER(8,0) NOT NULL 
 ,  TOT_REHA NUMBER(8,0) NOT NULL 
 ,  SUSP_ANOM NUMBER(8,0) NOT NULL 
 ,  REHA_ANOM NUMBER(8,0) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
