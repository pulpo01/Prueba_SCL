CREATE TABLE CO_FACPAGARE(
    FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  FACTOR_IND NUMBER(4,2) NOT NULL 
 ,  NUM_DIAS NUMBER(2,0) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_FACPAGARE IS 'Factor de porcentaje del pagare'
/
COMMENT ON COLUMN  CO_FACPAGARE.FEC_DESDE IS 'Fecha Inicio Periodo'
/
COMMENT ON COLUMN  CO_FACPAGARE.FEC_HASTA IS 'Fecha Fin Periodo'
/
COMMENT ON COLUMN  CO_FACPAGARE.FACTOR_IND IS 'Factor de porcentaje del pagare'
/
COMMENT ON COLUMN  CO_FACPAGARE.NUM_DIAS IS 'Numero de dias entre cuotas'
/
COMMENT ON COLUMN  CO_FACPAGARE.NOM_USUARORA IS 'Usuario de oracle'
/
