 ,  NUM_PAGARE NUMBER(8,0) NULL 
 ,  NUM_CUOTA NUMBER(2,0) NOT NULL 
 ,  FECHA_VENCTO DATE NOT NULL 
 ,  IMPORTE NUMBER(14,4) NOT NULL 
 ,  NUM_IDENT_CTACTE VARCHAR2(20) NULL 
 ,  IND_TITULAR VARCHAR2(5) NULL 
 ,  COD_BANCO VARCHAR2(15) NULL 
 ,  NUM_CTACTE VARCHAR2(18) NULL 
 ,  COD_SUCURSAL VARCHAR2(20) NULL 
 ,  COD_PLAZA VARCHAR2(20) NULL 
 ,  NUM_CHEQUE VARCHAR2(20) NULL 
 ,  COD_AUTORIZACION VARCHAR2(10) NULL 
 ,  COD_TIPIDENT VARCHAR2(2) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
