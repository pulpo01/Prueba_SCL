CREATE TABLE CMD_DIASFEST(
    FEC_DIAFEST DATE NOT NULL 
 ,  DES_DIAFEST VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMD_DIASFEST IS 'Tabla De Dias Considerados Festivos Para El Calculo De Comisiones.'
/
COMMENT ON COLUMN  CMD_DIASFEST.FEC_DIAFEST IS 'Fecha del dia festivo.'
/
COMMENT ON COLUMN  CMD_DIASFEST.DES_DIAFEST IS 'Motivo por el cual el dia es festivo.'
/
COMMENT ON COLUMN  CMD_DIASFEST.FEC_ULTMOD IS 'Fecha de la ultima modificacion del registro.'
/
COMMENT ON COLUMN  CMD_DIASFEST.NOM_USUARIO IS 'Usuario que realiza la ultima modificacion al registro..'
/
