CREATE TABLE VE_METAVEND(
    COD_CTOLIQ NUMBER(6,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  NUM_META NUMBER(12,4) NOT NULL 
 ,  FEC_VALOR DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_FINVALOR DATE NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_METAVEND IS 'Meta de Ventas asociada a cada Vendedor'
/
COMMENT ON COLUMN  VE_METAVEND.COD_CTOLIQ IS 'Codigo de Concepto Liquidacion'
/
COMMENT ON COLUMN  VE_METAVEND.COD_PRODUCTO IS 'Codigo de Producto para el concepto de liquidacion'
/
COMMENT ON COLUMN  VE_METAVEND.COD_TIPCOMIS IS 'Codigo de Tipo de Comisionista para Vendedor'
/
COMMENT ON COLUMN  VE_METAVEND.COD_VENDEDOR IS 'Codigo de Vendedor'
/
COMMENT ON COLUMN  VE_METAVEND.NUM_META IS 'Valor de la Meta/Bono'
/
COMMENT ON COLUMN  VE_METAVEND.FEC_VALOR IS 'Fecha de entrada en vigencia de la Meta/Bono'
/
COMMENT ON COLUMN  VE_METAVEND.NOM_USUARIO IS 'Usuario que realizo la asignacion'
/
COMMENT ON COLUMN  VE_METAVEND.FEC_FINVALOR IS 'Fecha de expiracion de la Meta/Bono'
/
