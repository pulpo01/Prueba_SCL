CREATE TABLE COT_EXT_CONVENIO(
    COD_ENTIDAD VARCHAR2(5) NOT NULL 
 ,  NUM_TRANSACCION NUMBER(10,0) NOT NULL 
 ,  TIPO_CONVENIO VARCHAR2(5) NOT NULL 
 ,  NUM_PAGARE NUMBER(8,0) NULL 
 ,  NUM_CUOTAS NUMBER(2,0) NOT NULL 
 ,  DES_CONVENIO VARCHAR2(5) NULL 
 ,  MTO_AFECTO_CONVENIO NUMBER(14,4) NOT NULL 
 ,  MTO_CONTADO NUMBER(14,4) NOT NULL 
 ,  MTO_INT_FINANCIERO NUMBER(14,4) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  COT_EXT_CONVENIO IS 'Convenio Del Pago.'
/
COMMENT ON COLUMN  COT_EXT_CONVENIO.COD_ENTIDAD IS 'Codigo de Entidad de Cobranza.'
/
COMMENT ON COLUMN  COT_EXT_CONVENIO.NUM_TRANSACCION IS 'NUMERO DE TRANSACCION.'
/
COMMENT ON COLUMN  COT_EXT_CONVENIO.TIPO_CONVENIO IS 'Tipo de Convenio.'
/
COMMENT ON COLUMN  COT_EXT_CONVENIO.NUM_PAGARE IS 'Numero del Pagare.'
/
COMMENT ON COLUMN  COT_EXT_CONVENIO.NUM_CUOTAS IS 'Numero de Cuotas.'
/
COMMENT ON COLUMN  COT_EXT_CONVENIO.DES_CONVENIO IS 'Descripcion del Convenio.'
/
COMMENT ON COLUMN  COT_EXT_CONVENIO.MTO_AFECTO_CONVENIO IS 'Monto Afecto al Convenio.'
/
COMMENT ON COLUMN  COT_EXT_CONVENIO.MTO_CONTADO IS 'Monto Contado Pagado.'
/
COMMENT ON COLUMN  COT_EXT_CONVENIO.MTO_INT_FINANCIERO IS 'Monto de Interes Financiero.'
/
