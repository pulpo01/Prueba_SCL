CREATE TABLE FA_PREFOLIADOS
(
  IND_ORDENTOTAL  NUMBER(12)                    NOT NULL,
  RESOLUCION      VARCHAR2(25 BYTE)             DEFAULT 'R'                   NOT NULL,
  SERIE           VARCHAR2(10 BYTE)             NOT NULL,
  COD_TIPDOCUM    NUMBER(5)                     NOT NULL,
  ETIQUETA        VARCHAR2(10 BYTE)             DEFAULT NULL,
  NUM_FOLIO       NUMBER(15)                    NOT NULL
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          1M
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE FA_PREFOLIADOS IS '[ PRFL ] Documentos PrefoliadosDocumentos Prefoliados';

COMMENT ON COLUMN FA_PREFOLIADOS.IND_ORDENTOTAL IS 'Indicador Orden Total';

COMMENT ON COLUMN FA_PREFOLIADOS.RESOLUCION IS 'Resolución';

COMMENT ON COLUMN FA_PREFOLIADOS.SERIE IS 'Serie de folios';

COMMENT ON COLUMN FA_PREFOLIADOS.COD_TIPDOCUM IS 'Cod. Tipo Documento';

COMMENT ON COLUMN FA_PREFOLIADOS.ETIQUETA IS 'Etiqueta de folios';

COMMENT ON COLUMN FA_PREFOLIADOS.NUM_FOLIO IS 'Número de folios';
