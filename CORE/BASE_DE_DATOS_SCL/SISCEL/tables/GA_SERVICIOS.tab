CREATE TABLE GA_SERVICIOS(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_SERVICIO VARCHAR2(3) NOT NULL 
 ,  DES_SERVICIO VARCHAR2(40) NOT NULL 
 ,  IND_AUTMAN VARCHAR2(1) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_SERVICIOS IS 'Servicios Ocasionales de Abonados'
/
COMMENT ON COLUMN  GA_SERVICIOS.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_SERVICIOS.COD_SERVICIO IS 'Codigo de Servicio'
/
COMMENT ON COLUMN  GA_SERVICIOS.DES_SERVICIO IS 'Descripcion de Servicio'
/
COMMENT ON COLUMN  GA_SERVICIOS.IND_AUTMAN IS 'Indicativo de tipo de Cargo ; (A)utomatico, (M)anual'
/
