CREATE TABLE GA_CONTCTA(
    COD_CUENTA NUMBER(8,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_TIPCONTRATO VARCHAR2(3) NOT NULL 
 ,  NUM_CONTRATO VARCHAR2(21) NOT NULL 
 ,  NUM_ANEXO VARCHAR2(21) NOT NULL 
 ,  NUM_MESES NUMBER(2,0) NOT NULL 
 ,  NUM_VENTA NUMBER(8,0) NULL 
 ,  FEC_CONTRATO DATE NULL 
 ,  NUM_ABONADOS NUMBER(5,0) NULL 
 ,  NUM_ABONADO NUMBER(8,0) NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_CONTCTA IS 'Tabla de Contratos por Cuenta'
/
COMMENT ON COLUMN  GA_CONTCTA.COD_CUENTA IS 'Codigo de Cuenta'
/
COMMENT ON COLUMN  GA_CONTCTA.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_CONTCTA.COD_TIPCONTRATO IS 'Codigo de Tipo de Contrato'
/
COMMENT ON COLUMN  GA_CONTCTA.NUM_CONTRATO IS 'Numero de Contrato'
/
COMMENT ON COLUMN  GA_CONTCTA.NUM_ANEXO IS 'Numero de Anexo de Contrato'
/
COMMENT ON COLUMN  GA_CONTCTA.NUM_MESES IS 'Numero de Meses de Contrato'
/
COMMENT ON COLUMN  GA_CONTCTA.NUM_VENTA IS 'Numero de Venta'
/
COMMENT ON COLUMN  GA_CONTCTA.FEC_CONTRATO IS 'Fecha de contrato'
/
COMMENT ON COLUMN  GA_CONTCTA.NUM_ABONADOS IS 'Numero de Abonados incluidos en el contrato o anexo'
/
COMMENT ON COLUMN  GA_CONTCTA.NUM_ABONADO IS 'Numero de Abonado cubierto por el seguro'
/
