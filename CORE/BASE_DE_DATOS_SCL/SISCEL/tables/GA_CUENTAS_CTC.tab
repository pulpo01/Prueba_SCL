CREATE TABLE GA_CUENTAS_CTC(
    COD_CUENTA NUMBER(8,0) NOT NULL 
 ,  DES_CUENTA VARCHAR2(40) NOT NULL 
 ,  COD_TIPIDENT VARCHAR2(2) NOT NULL 
 ,  NUM_IDENT VARCHAR2(20) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_CUENTAS_CTC IS 'Tablas Relaccionadas con CTC'
/
COMMENT ON COLUMN  GA_CUENTAS_CTC.COD_CUENTA IS 'codigo de cuenta'
/
COMMENT ON COLUMN  GA_CUENTAS_CTC.DES_CUENTA IS 'descripcion de la cuenta'
/
COMMENT ON COLUMN  GA_CUENTAS_CTC.COD_TIPIDENT IS 'tipo de identificacion (rut, dni.....)'
/
COMMENT ON COLUMN  GA_CUENTAS_CTC.NUM_IDENT IS 'numero de identificacion'
/
