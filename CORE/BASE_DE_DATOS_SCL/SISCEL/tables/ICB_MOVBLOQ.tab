CREATE TABLE ICB_MOVBLOQ(
    NUM_MOVIMIENTO NUMBER(9,0) NOT NULL 
 ,  DES_PUESTO VARCHAR2(15) NOT NULL 
 ,  DES_USER VARCHAR2(30) DEFAULT 'USER' NOT NULL 
 ,  FEC_ENTRADA DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICB_MOVBLOQ IS 'Movimientos Bloqueados Beeper'
/
COMMENT ON COLUMN  ICB_MOVBLOQ.NUM_MOVIMIENTO IS 'Numero de movimiento bloqueado'
/
COMMENT ON COLUMN  ICB_MOVBLOQ.DES_PUESTO IS 'Puesto desde donde se bloqueo el movimiento'
/
COMMENT ON COLUMN  ICB_MOVBLOQ.DES_USER IS 'Usuario oracle que bloqueo el movimiento'
/
COMMENT ON COLUMN  ICB_MOVBLOQ.FEC_ENTRADA IS 'Fecha en la que se bloqueo el movimiento'
/
