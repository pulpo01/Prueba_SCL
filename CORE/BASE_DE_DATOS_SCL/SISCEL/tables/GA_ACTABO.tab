CREATE TABLE GA_ACTABO(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_ACTABO VARCHAR2(2) NOT NULL 
 ,  DES_ACTABO VARCHAR2(30) NOT NULL 
 ,  COD_ACTCEN NUMBER(5,0) NULL 
 ,  IND_PREPAGO NUMBER(1,0) DEFAULT 0 NULL 
 ,  COD_TECNOLOGIA VARCHAR2(7) NULL 
 ,  COD_MODULO VARCHAR2(2) DEFAULT 'GA' NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_ACTABO IS 'Actuaciones del Modulo de Abonados'
/
COMMENT ON COLUMN  GA_ACTABO.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_ACTABO.COD_ACTABO IS 'Codigo de Actuacion de Abonados'
/
COMMENT ON COLUMN  GA_ACTABO.DES_ACTABO IS 'Descripcion de Actuacion de Abonados'
/
COMMENT ON COLUMN  GA_ACTABO.COD_ACTCEN IS 'Codigo de Actuacion en Central Asociado'
/
COMMENT ON COLUMN  GA_ACTABO.IND_PREPAGO IS 'Indicador de actuacion de Prpeago..'
/
COMMENT ON COLUMN  GA_ACTABO.COD_TECNOLOGIA IS 'Código Tecnología'
/
COMMENT ON COLUMN  GA_ACTABO.COD_MODULO IS 'Indicador de telefono de la tabla de series (0-7).'
/
