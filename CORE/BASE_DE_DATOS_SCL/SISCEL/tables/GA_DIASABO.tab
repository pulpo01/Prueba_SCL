CREATE TABLE GA_DIASABO(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  DES_DIAESP VARCHAR2(30) NOT NULL 
 ,  COD_TIPDIA VARCHAR2(3) NOT NULL 
 ,  FEC_DIAESP DATE NOT NULL 
 ,  NUM_DIASNUM NUMBER(9,0) NOT NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GA_DIASABO IS 'Dias Especiales de Abonados'
/
COMMENT ON COLUMN  GA_DIASABO.NUM_ABONADO IS 'Numero de Abonado'
/
COMMENT ON COLUMN  GA_DIASABO.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GA_DIASABO.DES_DIAESP IS 'Descripcion de dia especial de Abonado'
/
COMMENT ON COLUMN  GA_DIASABO.COD_TIPDIA IS 'Tipo de Dia Operador'
/
COMMENT ON COLUMN  GA_DIASABO.FEC_DIAESP IS 'Fecha del dia Especial'
/
COMMENT ON COLUMN  GA_DIASABO.NUM_DIASNUM IS 'Numero de asociacion al periodo de servicio contratado'
/
