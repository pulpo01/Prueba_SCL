 ,  NUM_CELULAR NUMBER(15,0) NULL 
 ,  FEC_ALTA DATE NULL 
 ,  COD_CLIENTE NUMBER(8,0) NULL 
 ,  NUM_ABONADO NUMBER(8,0) NULL 
 ,  FEC_RECARGA DATE NULL 
 ,  MTO_RECARGA NUMBER(14,4) NULL 
 ,  TIPO_TARJETA NUMBER(4,0) NULL 
 ,  MTO_PROMOCION NUMBER(14,4) NULL 
 ,  FEC_PROMOCION DATE NULL 
 ) TABLESPACE    TS_CRM_POSVENTA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  GA_PREPAGOS_CON_PROMOCION.TIPO_PROMOCION IS 'TIPO DE PROMOCION PREPAGO'
/
COMMENT ON COLUMN  GA_PREPAGOS_CON_PROMOCION.NUM_CELULAR IS 'CELULAR ASOCIADO'
/
COMMENT ON COLUMN  GA_PREPAGOS_CON_PROMOCION.FEC_ALTA IS 'FECHA DE ALTA DEL CELULAR'
