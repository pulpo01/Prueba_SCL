CREATE TABLE CM_VALCONTRATOS_TD(
    NUM_ACTUACION NUMBER(6,0) NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) NOT NULL 
 ,  COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  TIP_PLAN VARCHAR2(5) NOT NULL 
 ,  COD_CATEGCLIENTE VARCHAR2(10) NOT NULL 
 ,  COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  IND_ESCALONADO CHAR(1) DEFAULT 'N' NOT NULL 
 ,  IMP_COMISION NUMBER(10,4) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_VALCONTRATOS_TD IS 'ASOCIACION DE CONCEPTOS CON MODELO DE VALORACION DE CONTRATOS'
/
COMMENT ON COLUMN  CM_VALCONTRATOS_TD.NUM_ACTUACION IS 'IDENTIFICADOR DE REGISTRO UNICO'
/
COMMENT ON COLUMN  CM_VALCONTRATOS_TD.COD_PLANCOMIS IS 'CODIGO DEL PLAN DE COMISIONES'
/
COMMENT ON COLUMN  CM_VALCONTRATOS_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
COMMENT ON COLUMN  CM_VALCONTRATOS_TD.COD_CONCEPTO IS 'CODIGO DEL CONCEPTO'
/
COMMENT ON COLUMN  CM_VALCONTRATOS_TD.TIP_PLAN IS 'TIPO DE PLAN TARIFARIO DE LA VENTA'
/
COMMENT ON COLUMN  CM_VALCONTRATOS_TD.COD_CATEGCLIENTE IS 'CATEGORIA DEL CLIENTE, DESDE EL PUNTO DE VISTA TRIBUTARIO'
/
COMMENT ON COLUMN  CM_VALCONTRATOS_TD.COD_OFICINA IS 'CODIGO DE OFICINA DE LA VENTA.'
/
COMMENT ON COLUMN  CM_VALCONTRATOS_TD.FEC_DESDE IS 'FECHA DE INICIO DE LA VIGENCIA DEL REGISTRO'
/
COMMENT ON COLUMN  CM_VALCONTRATOS_TD.FEC_HASTA IS 'FECHA DE TERMINO DE LA VIGENCIA DEL REGISTRO'
/
COMMENT ON COLUMN  CM_VALCONTRATOS_TD.IND_ESCALONADO IS 'INDICADOR DE MATRIZ ESCALONADA (SOLO TIPO DE CALCULO V/M)'
/
COMMENT ON COLUMN  CM_VALCONTRATOS_TD.IMP_COMISION IS 'IMPORTE DE LA COMISION QUE SE APLICARA  TIPO DE CALCULO DIRECTO.'
/
COMMENT ON COLUMN  CM_VALCONTRATOS_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION '
/
COMMENT ON COLUMN  CM_VALCONTRATOS_TD.NOM_USUARIO IS 'USUARIO QUE HIZO LA ULTIMA MODIFICACION'
/
