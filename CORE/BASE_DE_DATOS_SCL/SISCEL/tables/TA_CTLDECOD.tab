CREATE TABLE TA_CTLDECOD(
    NUM_BLOQUE VARCHAR2(9) NOT NULL 
 ,  NUM_TICKETS NUMBER(9,0) NOT NULL 
 ,  NUM_TARIF NUMBER(8,0) NOT NULL 
 ,  NUM_NTARIF NUMBER(8,0) NOT NULL 
 ,  COD_ANOMALIA VARCHAR2(2) NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  TA_CTLDECOD IS 'Control de proceso de lectura de Tarificacion celular'
/
COMMENT ON COLUMN  TA_CTLDECOD.NUM_BLOQUE IS 'Numero de Bloque'
/
COMMENT ON COLUMN  TA_CTLDECOD.NUM_TICKETS IS 'Numero de Tickets'
/
COMMENT ON COLUMN  TA_CTLDECOD.NUM_TARIF IS 'Numero  de Tarificados'
/
COMMENT ON COLUMN  TA_CTLDECOD.NUM_NTARIF IS 'Numero de no tarificados'
/
COMMENT ON COLUMN  TA_CTLDECOD.COD_ANOMALIA IS 'Codigo de anomalia'
/
