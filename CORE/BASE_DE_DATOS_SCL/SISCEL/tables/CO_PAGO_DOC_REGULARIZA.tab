CREATE TABLE CO_PAGO_DOC_REGULARIZA(
    NUM_REGULARIZA NUMBER(9,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_SECUENCI NUMBER(9,0) NOT NULL 
 ,  COD_TIPDOCUM NUMBER(2,0) NOT NULL 
 ,  COD_VENDEDOR_AGENTE NUMBER(6,0) NOT NULL 
 ,  COD_LETRA CHAR(1) NOT NULL 
 ,  COD_CENTREMI NUMBER(4,0) NOT NULL 
 ,  COD_CONCEPTO NUMBER(4,0) NOT NULL 
 ,  COLUMNA NUMBER(4,0) NOT NULL 
 ,  NUM_CUOTA NUMBER(4,0) NULL 
 ,  SEC_CUOTA NUMBER(4,0) NULL 
 ,  IMPORTE_CONCEPTO NUMBER(14,4) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_PAGO_DOC_REGULARIZA IS 'Documentos Que Cancela El Pago Por Regularizacion.'
/
COMMENT ON COLUMN  CO_PAGO_DOC_REGULARIZA.NUM_REGULARIZA IS 'numero de regularizacion.'
/
COMMENT ON COLUMN  CO_PAGO_DOC_REGULARIZA.COD_CLIENTE IS 'codigo cliente regularizacion.'
/
COMMENT ON COLUMN  CO_PAGO_DOC_REGULARIZA.NUM_SECUENCI IS 'numero de secuencia del documento que se paga.'
/
COMMENT ON COLUMN  CO_PAGO_DOC_REGULARIZA.COD_TIPDOCUM IS 'tipo de documento que se paga.'
/
COMMENT ON COLUMN  CO_PAGO_DOC_REGULARIZA.COD_VENDEDOR_AGENTE IS 'vendedor agente del documento que se paga.'
/
COMMENT ON COLUMN  CO_PAGO_DOC_REGULARIZA.COD_LETRA IS 'letra del documento que se paga.'
/
COMMENT ON COLUMN  CO_PAGO_DOC_REGULARIZA.COD_CENTREMI IS 'centro emisor del documento que se paga.'
/
COMMENT ON COLUMN  CO_PAGO_DOC_REGULARIZA.COD_CONCEPTO IS 'codigo de concepto del documento que se paga (*).'
/
COMMENT ON COLUMN  CO_PAGO_DOC_REGULARIZA.COLUMNA IS 'columna del concepto del documento que se paga.'
/
COMMENT ON COLUMN  CO_PAGO_DOC_REGULARIZA.NUM_CUOTA IS 'numero de cuota del documento que se paga.'
/
COMMENT ON COLUMN  CO_PAGO_DOC_REGULARIZA.SEC_CUOTA IS 'sec cuota del documento que se paga.'
/
COMMENT ON COLUMN  CO_PAGO_DOC_REGULARIZA.IMPORTE_CONCEPTO IS 'importe del concepto que se paga.'
/
