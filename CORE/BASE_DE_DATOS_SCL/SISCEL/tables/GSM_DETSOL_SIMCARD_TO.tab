CREATE TABLE GSM_DETSOL_SIMCARD_TO(
    NUM_SEQ_SOLICITUD NUMBER(10,0) NOT NULL 
 ,  COD_ARCHIVO VARCHAR2(6) NOT NULL 
 ,  NUM_VERSION NUMBER(10,0) NOT NULL 
 ,  COD_CAMPO VARCHAR2(10) NOT NULL 
 ,  SEC_ARCHIVO_SECCION NUMBER NOT NULL 
 ,  VAL_CAMPO VARCHAR2(50) NOT NULL 
 ,  FEC_ACTUALIZACION DATE NOT NULL 
 ,  COD_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GSM_DETSOL_SIMCARD_TO IS 'Tabla detalle de la solicitud.'
/
COMMENT ON COLUMN  GSM_DETSOL_SIMCARD_TO.NUM_SEQ_SOLICITUD IS 'Numero secuencia de Solicitud.'
/
COMMENT ON COLUMN  GSM_DETSOL_SIMCARD_TO.COD_ARCHIVO IS 'Codigo de Archivo.'
/
COMMENT ON COLUMN  GSM_DETSOL_SIMCARD_TO.NUM_VERSION IS 'Numero de version del archivo.'
/
COMMENT ON COLUMN  GSM_DETSOL_SIMCARD_TO.COD_CAMPO IS 'Codigo de campo.'
/
COMMENT ON COLUMN  GSM_DETSOL_SIMCARD_TO.SEC_ARCHIVO_SECCION IS 'Codigo unico de relacion archivo y su secciones.'
/
COMMENT ON COLUMN  GSM_DETSOL_SIMCARD_TO.VAL_CAMPO IS 'Valor del campo.'
/
COMMENT ON COLUMN  GSM_DETSOL_SIMCARD_TO.FEC_ACTUALIZACION IS 'Fecha de la ultima actualizacion.'
/
COMMENT ON COLUMN  GSM_DETSOL_SIMCARD_TO.COD_USUARIO IS 'Codigo del usuario que actualizo.'
/
