CREATE TABLE FA_CICLO_SERVIDORES_TT(
    NOM_PROGRAMA VARCHAR2(40) NOT NULL 
 ,  HOST_ID VARCHAR2(20) NOT NULL 
 ,  COD_CICLFACT NUMBER(6,0) NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_CICLO_SERVIDORES_TT IS '[ CISE ] Registro de ciclos de facturación ejecutados por programas en los servidores.'
/
COMMENT ON COLUMN  FA_CICLO_SERVIDORES_TT.NOM_PROGRAMA IS 'Nombre de programa en ejecución'
/
COMMENT ON COLUMN  FA_CICLO_SERVIDORES_TT.HOST_ID IS 'Identificador de la máquina'
/
COMMENT ON COLUMN  FA_CICLO_SERVIDORES_TT.COD_CICLFACT IS 'Código de ciclo de facturación'
/
