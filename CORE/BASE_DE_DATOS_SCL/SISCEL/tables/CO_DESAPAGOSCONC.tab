CREATE TABLE CO_DESAPAGOSCONC
(
  COD_TIPDOCUM         NUMBER(2)                NOT NULL,
  COD_CENTREMI         NUMBER(4)                NOT NULL,
  NUM_SECUENCI         NUMBER(8)                NOT NULL,
  COD_VENDEDOR_AGENTE  NUMBER(6)                NOT NULL,
  LETRA                VARCHAR2(1 BYTE)         NOT NULL,
  IMP_CONCEPTO         NUMBER(14,4)             NOT NULL,
  COD_PRODUCTO         NUMBER(1)                NOT NULL,
  NUM_SECUREL          NUMBER(8),
  COD_TIPDOCREL        NUMBER(2),
  COD_AGENTEREL        NUMBER(6),
  NUM_TRANSACCION      NUMBER(9),
  LETRA_REL            VARCHAR2(1 BYTE),
  COD_CENTRREL         NUMBER(4),
  COD_CONCEPTO         NUMBER(4),
  COLUMNA              NUMBER(4),
  NUM_ABONADO          NUMBER(8),
  NUM_FOLIO            NUMBER(9),
  NUM_CUOTA            NUMBER(8),
  SEC_CUOTA            NUMBER(3),
  NUM_VENTA            NUMBER(8),
  PREF_PLAZA           VARCHAR2(25 BYTE),
  COD_OPERADORA_SCL    VARCHAR2(5 BYTE),
  COD_PLAZA            VARCHAR2(5 BYTE)
)
TABLESPACE TS_RECYCOB_SDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          520K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE CO_DESAPAGOSCONC IS 'RELACION DE PAGOS DESAPLICADOS CON CONCEPTOS';

COMMENT ON COLUMN CO_DESAPAGOSCONC.COD_TIPDOCUM IS ' - Retrofitted';

