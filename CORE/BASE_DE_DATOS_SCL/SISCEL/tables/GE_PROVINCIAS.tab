CREATE TABLE SISCEL.GE_PROVINCIAS
(
  COD_PROVINCIA  VARCHAR2(5 BYTE)               NOT NULL,
  COD_REGION     VARCHAR2(3 BYTE)               NOT NULL,
  DES_PROVINCIA  VARCHAR2(30 BYTE)              NOT NULL,
  TIP_ZONA       VARCHAR2(1 BYTE)               DEFAULT 'M'                   NOT NULL
)
TABLESPACE TS_GENERAL_SDAT
/

COMMENT ON TABLE SISCEL.GE_PROVINCIAS IS 'Provincias Geograficas'
/

COMMENT ON COLUMN SISCEL.GE_PROVINCIAS.COD_PROVINCIA IS 'Codigo de Provincia'
/

COMMENT ON COLUMN SISCEL.GE_PROVINCIAS.COD_REGION IS 'Codigo de Region'
/

COMMENT ON COLUMN SISCEL.GE_PROVINCIAS.DES_PROVINCIA IS 'Descripcion de Provincia'
/

COMMENT ON COLUMN SISCEL.GE_PROVINCIAS.TIP_ZONA IS 'M: Tipo zona metropolitana, P: Tipo Zona Provincia'
/


