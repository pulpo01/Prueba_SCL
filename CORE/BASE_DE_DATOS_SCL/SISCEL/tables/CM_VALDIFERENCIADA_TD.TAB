CREATE TABLE CM_VALDIFERENCIADA_TD(
    NUM_ACTUACION NUMBER(6,0) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  IMP_COMISION NUMBER(14,4) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_VALDIFERENCIADA_TD IS 'ESPECIFICACION DE FORMAS COMISIONALES PARA CONCEPTOS'
/
COMMENT ON COLUMN  CM_VALDIFERENCIADA_TD.NUM_ACTUACION IS 'ACTUACION SOBRE LA QUE SE DIFERENCIA.'
/
COMMENT ON COLUMN  CM_VALDIFERENCIADA_TD.COD_VENDEDOR IS 'VENDEDOR (PUNTO DE VENTA) QUE SE DIFERENCIA.'
/
COMMENT ON COLUMN  CM_VALDIFERENCIADA_TD.IMP_COMISION IS 'IMPORTE DIFERENCIADO (V-M: 0/1).'
/
COMMENT ON COLUMN  CM_VALDIFERENCIADA_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  CM_VALDIFERENCIADA_TD.NOM_USUARIO IS 'USUARIO QUE HIZO LA ULTIMA MODIFICACION'
/
