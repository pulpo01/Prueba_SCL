CREATE TABLE CO_PARAMETROS_UNIX_TO(
    COD_PROCESO VARCHAR2(5) NOT NULL 
 ,  FEC_INGRESO DATE NOT NULL 
 ,  CARGA_CPU NUMBER(5,2) NULL 
 ,  CNT_PROCESOS_EJECUTADOS NUMBER(8,0) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  CO_PARAMETROS_UNIX_TO.COD_PROCESO IS 'Identificador del Proceso'
/
COMMENT ON COLUMN  CO_PARAMETROS_UNIX_TO.FEC_INGRESO IS 'Fecha de Ingreso'
/
COMMENT ON COLUMN  CO_PARAMETROS_UNIX_TO.CARGA_CPU IS 'Carga Cpu durante el proceso'
/
COMMENT ON COLUMN  CO_PARAMETROS_UNIX_TO.CNT_PROCESOS_EJECUTADOS IS 'Cantidad de procesos ejecutados'
/
