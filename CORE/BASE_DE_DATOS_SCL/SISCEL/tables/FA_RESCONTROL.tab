CREATE TABLE FA_RESCONTROL(
    COD_INCONSIS NUMBER(2,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  DESC_INCONSIS VARCHAR2(75) NOT NULL 
 ,  CANT_INCONSIS NUMBER(6,0) NULL 
 ,  FEC_PROCESO DATE NULL 
 ,  TIP_CONSULTA CHAR(1) NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_RESCONTROL IS 'Resumen de Inconsistencias'
/
COMMENT ON COLUMN  FA_RESCONTROL.COD_INCONSIS IS 'Codigo Inconsistencia'
/
COMMENT ON COLUMN  FA_RESCONTROL.COD_PRODUCTO IS 'Codigo de negocio'
/
COMMENT ON COLUMN  FA_RESCONTROL.DESC_INCONSIS IS 'Descripcion inconsistencia'
/
COMMENT ON COLUMN  FA_RESCONTROL.CANT_INCONSIS IS 'Cantidad de Inconsistencias'
/
COMMENT ON COLUMN  FA_RESCONTROL.FEC_PROCESO IS 'Fecha ultimo proceso'
/
COMMENT ON COLUMN  FA_RESCONTROL.TIP_CONSULTA IS 'Tipo de consulta Interfaces'
/
