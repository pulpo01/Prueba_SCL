CREATE TABLE SP_CAUSUSP_VOLUN(
    COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_MODULO VARCHAR2(2) NOT NULL 
 ,  COD_CAUSUSP VARCHAR2(2) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SP_CAUSUSP_VOLUN IS 'Causas de Suspension Voluntarias por producto'
/
COMMENT ON COLUMN  SP_CAUSUSP_VOLUN.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  SP_CAUSUSP_VOLUN.COD_MODULO IS 'Codigo del modulo al que pertenecen las causas de suspension guardadas en esta tabla,siempre GA, abonados.'
/
COMMENT ON COLUMN  SP_CAUSUSP_VOLUN.COD_CAUSUSP IS 'Codigo de Causa de suspension'
/
