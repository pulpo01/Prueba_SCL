CREATE TABLE AL_USOS_MIN(
    COD_USO NUMBER(2,0) NOT NULL 
 ,  NUM_MIN VARCHAR2(3) NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_USOS_MIN IS 'Tabla de Min por Uso'
/
COMMENT ON COLUMN  AL_USOS_MIN.COD_USO IS 'Codigo de uso'
/
COMMENT ON COLUMN  AL_USOS_MIN.NUM_MIN IS 'Prefijo Min'
/
COMMENT ON COLUMN  AL_USOS_MIN.FEC_DESDE IS 'Fecha de Inicio de Vigencia del Min para el Uso'
/
COMMENT ON COLUMN  AL_USOS_MIN.FEC_HASTA IS 'Fecha de Fin de Vigencia del Min para el Uso'
/
