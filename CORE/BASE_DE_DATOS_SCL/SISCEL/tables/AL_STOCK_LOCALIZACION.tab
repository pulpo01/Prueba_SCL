CREATE TABLE AL_STOCK_LOCALIZACION(
    COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  NUM_SEC_LOCA NUMBER(10,0) NOT NULL 
 ,  FEC_CREACION DATE NULL 
 ,  CAN_STOCK NUMBER(9,0) NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_STOCK_LOCALIZACION IS 'Tabla De Stock Por Localizacion.'
/
COMMENT ON COLUMN  AL_STOCK_LOCALIZACION.COD_ARTICULO IS 'Codigo de articulo.'
/
COMMENT ON COLUMN  AL_STOCK_LOCALIZACION.NUM_SEC_LOCA IS 'Identificador de la Localizacion (Secuencial).'
/
COMMENT ON COLUMN  AL_STOCK_LOCALIZACION.FEC_CREACION IS 'Fecha de Creacion de Registro.'
/
COMMENT ON COLUMN  AL_STOCK_LOCALIZACION.CAN_STOCK IS 'Cantidad en Stock.'
/
