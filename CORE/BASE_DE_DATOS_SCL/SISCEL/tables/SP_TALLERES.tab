CREATE TABLE SP_TALLERES(
    COD_TALLER NUMBER(3,0) NOT NULL 
 ,  DES_TALLER VARCHAR2(40) NOT NULL 
 ,  NOM_CONTACTO VARCHAR2(40) NOT NULL 
 ,  COD_DIRECCION NUMBER(20,0) NOT NULL 
 ,  COD_PROVEEDOR NUMBER NOT NULL 
 ,  NUM_TELEF1 VARCHAR2(15) NULL 
 ,  NUM_TELEF2 VARCHAR2(15) NULL 
 ,  NUM_FAX VARCHAR2(15) NULL 
 ,  OBS_TALLER VARCHAR2(255) NULL 
 ,  COD_TIPIDENT VARCHAR2(2) NULL 
 ,  NUM_IDENT VARCHAR2(20) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SP_TALLERES IS 'Talleres de reparacion'
/
COMMENT ON COLUMN  SP_TALLERES.COD_TALLER IS 'codigo del taller'
/
COMMENT ON COLUMN  SP_TALLERES.DES_TALLER IS 'descripcion o nombre del taller'
/
COMMENT ON COLUMN  SP_TALLERES.NOM_CONTACTO IS 'nombre de contacto del taller o nombre de responsable del taller'
/
COMMENT ON COLUMN  SP_TALLERES.COD_DIRECCION IS 'codigo de direccion del taller'
/
COMMENT ON COLUMN  SP_TALLERES.COD_PROVEEDOR IS 'codigo de proveedor del taller'
/
COMMENT ON COLUMN  SP_TALLERES.NUM_TELEF1 IS 'numero del telefono primero del taller'
/
COMMENT ON COLUMN  SP_TALLERES.NUM_TELEF2 IS 'numero del telefono segundo del taller'
/
COMMENT ON COLUMN  SP_TALLERES.NUM_FAX IS 'numero del fax del taller'
/
COMMENT ON COLUMN  SP_TALLERES.OBS_TALLER IS 'observacion del taller'
/
COMMENT ON COLUMN  SP_TALLERES.COD_TIPIDENT IS 'Codigo del Tipo de Identificacion del taller'
/
COMMENT ON COLUMN  SP_TALLERES.NUM_IDENT IS 'Numero de Identificacion del taller'
/
