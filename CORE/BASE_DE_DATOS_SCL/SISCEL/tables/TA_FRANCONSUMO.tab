CREATE TABLE TA_FRANCONSUMO(
    COD_FRANCONS VARCHAR2(3) NOT NULL 
 ,  DES_FRANCONS VARCHAR2(30) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(40) NOT NULL 
 ,  FEC_ULTIMAMOD DATE NOT NULL 
 ) TABLESPACE    TS_TARIFICACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  TA_FRANCONSUMO.COD_FRANCONS IS 'Codigo Franja Consumo'
/
COMMENT ON COLUMN  TA_FRANCONSUMO.DES_FRANCONS IS 'Descripcion Franja Consumo'
/
COMMENT ON COLUMN  TA_FRANCONSUMO.NOM_USUARIO IS 'Almacena Nombre Usuario'
/
COMMENT ON COLUMN  TA_FRANCONSUMO.FEC_ULTIMAMOD IS 'Fecha Ultima Modificacion'
/
