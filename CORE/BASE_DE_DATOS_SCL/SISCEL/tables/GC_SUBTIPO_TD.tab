CREATE TABLE GC_SUBTIPO_TD(
    COD_SUBTIPO NUMBER(6,0) NOT NULL 
 ,  GLS_SUBTIPO VARCHAR2(70) NOT NULL 
 ,  COD_SUBTIPO_PADRE NUMBER(6,0) NULL 
 ,  COD_TIPO NUMBER(6,0) NOT NULL 
 ,  FEC_INI_VIGENCIA DATE NOT NULL 
 ,  FEC_FIN_VIGENCIA DATE NULL 
 ,  GLS_LINAJE_SUBTIPO VARCHAR2(200) NULL 
 ,  NOM_USUARORA VARCHAR2(30) DEFAULT USER NOT NULL 
 ,  FEC_ULTMOD DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GC_SUBTIPO_TD IS 'Subtipos de GC'
/
COMMENT ON COLUMN  GC_SUBTIPO_TD.COD_SUBTIPO IS 'Codigo de Subtipo'
/
COMMENT ON COLUMN  GC_SUBTIPO_TD.GLS_SUBTIPO IS 'Descripcion de Subtipo'
/
COMMENT ON COLUMN  GC_SUBTIPO_TD.COD_SUBTIPO_PADRE IS 'Codigo de subtipo padre'
/
COMMENT ON COLUMN  GC_SUBTIPO_TD.COD_TIPO IS 'Codigo de Tipo'
/
COMMENT ON COLUMN  GC_SUBTIPO_TD.FEC_INI_VIGENCIA IS 'Fecha de inicio vigencia del subtipo'
/
COMMENT ON COLUMN  GC_SUBTIPO_TD.FEC_FIN_VIGENCIA IS 'Fecha de fin de vigencia del subtipo'
/
COMMENT ON COLUMN  GC_SUBTIPO_TD.GLS_LINAJE_SUBTIPO IS 'Descripcion de linaje del subtipo'
/
COMMENT ON COLUMN  GC_SUBTIPO_TD.NOM_USUARORA IS 'Usuario Oracle'
/
COMMENT ON COLUMN  GC_SUBTIPO_TD.FEC_ULTMOD IS 'Fecha de ultima modificacion'
/
