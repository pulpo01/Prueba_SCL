CREATE TABLE PV_COMIS_OOSS_TD(
    COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_VENDEALER NUMBER(6,0) NULL 
 ,  IND_INTERNO NUMBER(1,0) NOT NULL 
 ,  COD_OFICINA VARCHAR2(2) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  NUM_OS NUMBER(10,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FECHA DATE NOT NULL 
 ,  COD_OS VARCHAR2(5) NOT NULL 
 ,  SUB_TIPO VARCHAR2(20) NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_COMIS_OOSS_TD IS '[ PCOS ] Comisiones Vendedores Externos'
/
COMMENT ON COLUMN  PV_COMIS_OOSS_TD.COD_VENDEDOR IS 'Código de Vendedor'
/
COMMENT ON COLUMN  PV_COMIS_OOSS_TD.COD_VENDEALER IS 'Código de Vendedor Dealer'
/
COMMENT ON COLUMN  PV_COMIS_OOSS_TD.IND_INTERNO IS 'Indicador de Vendedor Interno o Externo'
/
COMMENT ON COLUMN  PV_COMIS_OOSS_TD.COD_OFICINA IS 'Código de Oficina'
/
COMMENT ON COLUMN  PV_COMIS_OOSS_TD.COD_TIPCOMIS IS 'Código de Tipo Comisionista'
/
COMMENT ON COLUMN  PV_COMIS_OOSS_TD.NUM_OS IS 'Numero de Ordene de servicio'
/
COMMENT ON COLUMN  PV_COMIS_OOSS_TD.NOM_USUARIO IS 'Nombre de Usuario'
/
COMMENT ON COLUMN  PV_COMIS_OOSS_TD.FECHA IS 'Fecha de Inserción del Registro'
/
COMMENT ON COLUMN  PV_COMIS_OOSS_TD.COD_OS IS 'Código de Orden de Servicio'
/
COMMENT ON COLUMN  PV_COMIS_OOSS_TD.SUB_TIPO IS 'Sub Tipo'
/
