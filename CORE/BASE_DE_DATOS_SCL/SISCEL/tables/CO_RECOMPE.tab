CREATE TABLE CO_RECOMPE(
    COD_RECOMPE NUMBER(2,0) NOT NULL 
 ,  DES_RECOMPE VARCHAR2(30) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  IMP_RECOMPE NUMBER(14,4) NOT NULL 
 ,  IND_DESCUEN NUMBER(4,2) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_RECOMPE IS 'EGRESOS POR RECOMPENSA'
/
COMMENT ON COLUMN  CO_RECOMPE.COD_RECOMPE IS 'CODIGO DE EGRESO'
/
COMMENT ON COLUMN  CO_RECOMPE.DES_RECOMPE IS 'DESCRIPCION DE EGRESO'
/
COMMENT ON COLUMN  CO_RECOMPE.COD_PRODUCTO IS 'CODIGO DE PRODUCTO - Retrofitted'
/
COMMENT ON COLUMN  CO_RECOMPE.IMP_RECOMPE IS 'IMPORTE DE RECOMPENSA'
/
COMMENT ON COLUMN  CO_RECOMPE.IND_DESCUEN IS 'INDICADOR DE DECUENTO'
/
