 ,  FEC_VENTA DATE NOT NULL 
 ,  NUM_UNIDADES NUMBER(8,0) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CET_CTRSTOCKDIA IS 'Control stock de claves.'
