CREATE TABLE GE_PROCESOS_TD(
    COD_MODULO VARCHAR2(2) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  NUM_PROCESO NUMBER(5,0) NOT NULL 
 ,  TIP_PROCESO VARCHAR2(5) NOT NULL 
 ,  NOM_PROCESO VARCHAR2(100) NOT NULL 
 ,  DES_PROCESO VARCHAR2(150) NOT NULL 
 ,  PARAM_PROCESO VARCHAR2(200) NULL 
 ,  FEC_ULTMOD DATE DEFAULT SYSDATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_PROCESOS_TD IS 'Procesos comerciales por modulo'
/
COMMENT ON COLUMN  GE_PROCESOS_TD.COD_MODULO IS 'Codigo de Modulo'
/
COMMENT ON COLUMN  GE_PROCESOS_TD.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  GE_PROCESOS_TD.NUM_PROCESO IS 'Correlativo de proceso'
/
COMMENT ON COLUMN  GE_PROCESOS_TD.TIP_PROCESO IS 'Tipo de proceso, pl, fk, etc.'
/
COMMENT ON COLUMN  GE_PROCESOS_TD.NOM_PROCESO IS 'Nombre de proceso'
/
COMMENT ON COLUMN  GE_PROCESOS_TD.DES_PROCESO IS 'Descripcion de proceso'
/
COMMENT ON COLUMN  GE_PROCESOS_TD.PARAM_PROCESO IS 'Parametros de proceso'
/
COMMENT ON COLUMN  GE_PROCESOS_TD.FEC_ULTMOD IS 'Fecha ultima modificacion'
/
COMMENT ON COLUMN  GE_PROCESOS_TD.NOM_USUARIO IS 'Nombre de usuario'
/
