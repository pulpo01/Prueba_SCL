CREATE TABLE GA_MODABOCEL
(
  NUM_ABONADO            NUMBER(8)              NOT NULL,
  COD_TIPMODI            VARCHAR2(2 BYTE)       NOT NULL,
  FEC_MODIFICA           DATE                   NOT NULL,
  NOM_USUARORA           VARCHAR2(30 BYTE)      NOT NULL,
  NUM_CELULAR            NUMBER(15),
  COD_REGION             VARCHAR2(3 BYTE),
  COD_PROVINCIA          VARCHAR2(5 BYTE),
  COD_CIUDAD             VARCHAR2(5 BYTE),
  COD_CELDA              VARCHAR2(7 BYTE),
  COD_CENTRAL            NUMBER(3),
  COD_USO                NUMBER(2),
  TIP_PLANTARIF          VARCHAR2(1 BYTE),
  TIP_TERMINAL           VARCHAR2(1 BYTE),
  COD_PLANTARIF          VARCHAR2(3 BYTE),
  COD_CARGOBASICO        VARCHAR2(3 BYTE),
  COD_PLANSERV           VARCHAR2(3 BYTE),
  COD_LIMCONSUMO         VARCHAR2(5 BYTE),
  NUM_SERIE              VARCHAR2(25 BYTE),
  NUM_SERIEHEX           VARCHAR2(8 BYTE),
  NUM_SERIEMEC           VARCHAR2(20 BYTE),
  COD_HOLDING            NUMBER(8),
  COD_EMPRESA            NUMBER(8),
  COD_GRPSERV            VARCHAR2(2 BYTE),
  NUM_TELEFIJA           VARCHAR2(15 BYTE),
  COD_CARRIER            NUMBER(5),
  IND_PLEXSYS            NUMBER(1),
  COD_CENTRAL_PLEX       NUMBER(3),
  NUM_CELULAR_PLEX       NUMBER(15),
  COD_CREDMOR            NUMBER(3),
  COD_CREDCON            NUMBER(3),
  COD_CICLO              NUMBER(2),
  IND_FACTUR             NUMBER(1),
  IND_SUSPEN             NUMBER(1),
  IND_REHABI             NUMBER(1),
  IND_INSGUIAS           NUMBER(1),
  NUM_PERSONAL           VARCHAR2(4 BYTE),
  COD_CAUSA              VARCHAR2(2 BYTE),
  COD_OPREDFIJA          NUMBER(5),
  IND_SUPERTEL           NUMBER(1),
  NUM_MIN                VARCHAR2(3 BYTE)       DEFAULT '730',
  COD_TIPCONTRATO        VARCHAR2(3 BYTE),
  NUM_MESES              NUMBER(2),
  IND_EQPRESTADO         CHAR(1 BYTE),
  FEC_PRORROGA           DATE,
  NUM_OS                 NUMBER(10),
  NUM_CICLOS             NUMBER(10),
  NUM_MINUTOS            NUMBER(10),
  FEC_FINCONTRATO        DATE,
  COD_TECNOLOGIA         VARCHAR2(7 BYTE),
  NUM_IMEI               VARCHAR2(25 BYTE),
  NUM_MIN_MDN            VARCHAR2(10 BYTE),
  MOT_RENOVACION         VARCHAR2(5 BYTE),
  TIP_VISITA             NUMBER(2),
  UMBRAL                 NUMBER(14,4),
  CANT_AVISOS            NUMBER(5),
  COD_OFICINA_PRINCIPAL  VARCHAR2(2 BYTE),
  NUM_DIAS               NUMBER(5),
  COD_NOTIFICACION       NUMBER(2)
)
TABLESPACE TS_CRM_POSVENTA_MDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          220M
            NEXT             4M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE GA_MODABOCEL IS 'Modificaciones Abonados Red Celular';

COMMENT ON COLUMN GA_MODABOCEL.NUM_DIAS IS 'NUMERO DE DIAS CADUCIDAD SALDO';

COMMENT ON COLUMN GA_MODABOCEL.COD_NOTIFICACION IS 'CODIGO DE TIPO DE NOTIFICACION';

COMMENT ON COLUMN GA_MODABOCEL.NUM_ABONADO IS 'Secuencia Numero Abonado';

COMMENT ON COLUMN GA_MODABOCEL.COD_TIPMODI IS 'Codigo de Tipo de Modificacion';

COMMENT ON COLUMN GA_MODABOCEL.FEC_MODIFICA IS 'Fecha de Modificacion';

COMMENT ON COLUMN GA_MODABOCEL.NOM_USUARORA IS 'Usuario Creador Abonado';

COMMENT ON COLUMN GA_MODABOCEL.NUM_CELULAR IS 'Numero de Celular';

COMMENT ON COLUMN GA_MODABOCEL.COD_REGION IS 'Codigo de Region';

COMMENT ON COLUMN GA_MODABOCEL.COD_PROVINCIA IS 'Codigo de Provincia';

COMMENT ON COLUMN GA_MODABOCEL.COD_CIUDAD IS 'Codigo de Ciudad';

COMMENT ON COLUMN GA_MODABOCEL.COD_CELDA IS 'Codigo de celda';

COMMENT ON COLUMN GA_MODABOCEL.COD_CENTRAL IS 'Codigo de Central';

COMMENT ON COLUMN GA_MODABOCEL.COD_USO IS 'Codigo de Uso de Numeracion';

COMMENT ON COLUMN GA_MODABOCEL.TIP_PLANTARIF IS 'Tipo de Plan Tarifario ; Individual (I), Empresa (E), Holding (H)';

COMMENT ON COLUMN GA_MODABOCEL.TIP_TERMINAL IS 'Tipo de Terminal ; Analogico (A), Digital (D)';

COMMENT ON COLUMN GA_MODABOCEL.COD_PLANTARIF IS 'Codigo de Plan Tarifario';

COMMENT ON COLUMN GA_MODABOCEL.COD_CARGOBASICO IS 'Codigo Cargo Basico';

