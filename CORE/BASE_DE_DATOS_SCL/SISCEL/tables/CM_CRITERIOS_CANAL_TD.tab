CREATE TABLE CM_CRITERIOS_CANAL_TD(
    NUM_CRITERIO NUMBER(6,0) NOT NULL 
 ,  COD_CONCEPTO NUMBER(3,0) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  COD_TIPORED NUMBER(2,0) DEFAULT 0 NOT NULL 
 ,  COD_PLANCOMIS VARCHAR2(6) DEFAULT 'C' NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CM_CRITERIOS_CANAL_TD IS 'Asignacion De Criterios De Evaluacion A Un Canal.'
/
COMMENT ON COLUMN  CM_CRITERIOS_CANAL_TD.NUM_CRITERIO IS 'NUMERO IDENTIFICADOR DEL CRITERIO DE VALORACION.'
/
COMMENT ON COLUMN  CM_CRITERIOS_CANAL_TD.COD_CONCEPTO IS 'CONCEPTO DE COMISION ASOCIADO.'
/
COMMENT ON COLUMN  CM_CRITERIOS_CANAL_TD.NOM_USUARIO IS 'USUARIO DE ULTIMA MODIFICACION.'
/
COMMENT ON COLUMN  CM_CRITERIOS_CANAL_TD.FEC_ULTMOD IS 'FECHA DE ULTMA MODIFICACION.'
/
COMMENT ON COLUMN  CM_CRITERIOS_CANAL_TD.COD_TIPORED IS 'CODIGO DEL TIPO DE RED DE VENTAS'
/
COMMENT ON COLUMN  CM_CRITERIOS_CANAL_TD.COD_PLANCOMIS IS 'PLAN DE COMISIONES QUE CONTIENEN AL CONCEPTO.'
/
