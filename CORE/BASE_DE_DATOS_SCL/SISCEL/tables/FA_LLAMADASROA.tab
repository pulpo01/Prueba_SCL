CREATE TABLE FA_LLAMADASROA(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_CICLFACT NUMBER(6,0) NOT NULL 
 ,  NUM_MOVIL VARCHAR2(15) NOT NULL 
 ,  NUM_ASIGNADO VARCHAR2(15) NOT NULL 
 ,  FEC_LLAMADA DATE NOT NULL 
 ,  HOR_LLAMADA VARCHAR2(6) NOT NULL 
 ,  SEG_AIRE NUMBER(6,0) NOT NULL 
 ,  SEG_DURA NUMBER(6,0) NOT NULL 
 ,  IMP_MONTOLARGA NUMBER(11,4) NOT NULL 
 ,  IMP_MONTOAIRE NUMBER(12,4) NOT NULL 
 ,  NUM_DESTINO VARCHAR2(18) NOT NULL 
 ,  IND_ENTSAL NUMBER(1,0) NOT NULL 
 ,  IND_LARGA VARCHAR2(1) NOT NULL 
 ,  DES_DESTINO VARCHAR2(20) NOT NULL 
 ,  NUM_BLOQUE VARCHAR2(9) NOT NULL 
 ,  SEC_LLAMADAS NUMBER(8,0) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_LLAMADASROA IS 'Tabla de Historico de Llamadas Roaming'
/
COMMENT ON COLUMN  FA_LLAMADASROA.COD_CLIENTE IS 'Codigo de Cliente'
/
COMMENT ON COLUMN  FA_LLAMADASROA.NUM_ABONADO IS 'Numero de Abonado que efectua la Llamada'
/
COMMENT ON COLUMN  FA_LLAMADASROA.COD_CICLFACT IS 'Periodo de Facturacion'
/
COMMENT ON COLUMN  FA_LLAMADASROA.NUM_MOVIL IS 'Numero de Movil'
/
COMMENT ON COLUMN  FA_LLAMADASROA.NUM_ASIGNADO IS 'Numero Asignado'
/
COMMENT ON COLUMN  FA_LLAMADASROA.FEC_LLAMADA IS 'Fecha de Llamada'
/
COMMENT ON COLUMN  FA_LLAMADASROA.HOR_LLAMADA IS 'Hora de la Llamada'
/
COMMENT ON COLUMN  FA_LLAMADASROA.SEG_AIRE IS 'Segundos en el Aire de la Llamada'
/
COMMENT ON COLUMN  FA_LLAMADASROA.SEG_DURA IS 'Segundos de Duracion de la Llamada'
/
COMMENT ON COLUMN  FA_LLAMADASROA.IMP_MONTOLARGA IS 'Monto de Llamada Larga Distancia'
/
COMMENT ON COLUMN  FA_LLAMADASROA.IMP_MONTOAIRE IS 'Monto de Llamada en el Aire'
/
COMMENT ON COLUMN  FA_LLAMADASROA.NUM_DESTINO IS 'Numero Destino de la Llamada'
/
COMMENT ON COLUMN  FA_LLAMADASROA.IND_ENTSAL IS 'Indicativo de Llamada  Entrante/Saliente'
/
COMMENT ON COLUMN  FA_LLAMADASROA.IND_LARGA IS 'Indicativo de Larga Distancia'
/
COMMENT ON COLUMN  FA_LLAMADASROA.DES_DESTINO IS 'Localizacion Destino de la Llamada'
/
COMMENT ON COLUMN  FA_LLAMADASROA.NUM_BLOQUE IS 'Numero de Bloque'
/
COMMENT ON COLUMN  FA_LLAMADASROA.SEC_LLAMADAS IS 'Secuencia Llamadas'
/
