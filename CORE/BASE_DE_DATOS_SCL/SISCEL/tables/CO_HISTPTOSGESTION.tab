CREATE TABLE CO_HISTPTOSGESTION(
    COD_PTOGEST VARCHAR2(5) NOT NULL 
 ,  DES_PTOGEST VARCHAR2(30) NOT NULL 
 ,  NUM_DIAS NUMBER(3,0) NOT NULL 
 ,  ANT_PTOGEST VARCHAR2(5) NOT NULL 
 ,  IND_PRIORIDAD NUMBER(3,0) NOT NULL 
 ,  COD_GESTION VARCHAR2(2) NOT NULL 
 ,  COD_ESTADO CHAR(1) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_CATEGORIA VARCHAR2(5) NULL 
 ,  NUM_PROCESO NUMBER(4,0) NULL 
 ,  IND_PRORROGA VARCHAR2(1) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_HISTPTOSGESTION IS 'Historico Movimientos de Puntos de Gestion'
/
COMMENT ON COLUMN  CO_HISTPTOSGESTION.COD_PTOGEST IS 'Codigo del Punto de Gestion.'
/
