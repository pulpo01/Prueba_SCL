 ,  MTO_ASIGNADO NUMBER(14,4) NOT NULL 
 ,  MTO_RECAUDADO NUMBER(14,4) NOT NULL 
 ,  MTO_COMISION NUMBER(14,4) NOT NULL 
 ,  POR_RECUPERADO NUMBER(8,4) NOT NULL 
 ,  POR_APLICADO NUMBER(8,4) NOT NULL 
 ,  FEC_TRANSACCION DATE NOT NULL 
 ,  USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON COLUMN  CO_TRASPASO_COMISION_TO.NUM_TRANSACCION IS 'Secuencia de Proceso'
/
COMMENT ON COLUMN  CO_TRASPASO_COMISION_TO.MTO_ASIGNADO IS 'Monto Asignado'
/
COMMENT ON COLUMN  CO_TRASPASO_COMISION_TO.MTO_RECAUDADO IS 'Monto Recaudado'
/
COMMENT ON COLUMN  CO_TRASPASO_COMISION_TO.MTO_COMISION IS 'Monto Comision'
/
COMMENT ON COLUMN  CO_TRASPASO_COMISION_TO.POR_RECUPERADO IS 'Porcentaje de Recuperacion'
/
COMMENT ON COLUMN  CO_TRASPASO_COMISION_TO.POR_APLICADO IS 'Porcentaje de Calculo'
/
COMMENT ON COLUMN  CO_TRASPASO_COMISION_TO.FEC_TRANSACCION IS 'Fecha de Calculo'
/
COMMENT ON COLUMN  CO_TRASPASO_COMISION_TO.USUARIO IS 'Usuario'
/
