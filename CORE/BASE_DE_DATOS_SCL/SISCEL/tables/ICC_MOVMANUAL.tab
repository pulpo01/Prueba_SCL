CREATE TABLE ICC_MOVMANUAL(
    NUM_MOVIMIENTO NUMBER(9,0) NOT NULL 
 ,  DES_PUESTO VARCHAR2(15) NOT NULL 
 ,  DES_USER VARCHAR2(30) DEFAULT 'USER' NOT NULL 
 ,  FEC_ENTRADA DATE DEFAULT SYSDATE NOT NULL 
 ,  FEC_EJECUCION DATE NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICC_MOVMANUAL IS 'Movimientos Manuales Celular'
/
COMMENT ON COLUMN  ICC_MOVMANUAL.NUM_MOVIMIENTO IS 'Numero de movimiento'
/
COMMENT ON COLUMN  ICC_MOVMANUAL.DES_PUESTO IS 'puesto desde donde el operador confiramara la ejecucion del movimiento'
/
COMMENT ON COLUMN  ICC_MOVMANUAL.DES_USER IS 'usuario que confirmara la ejecucion del movimiento'
/
COMMENT ON COLUMN  ICC_MOVMANUAL.FEC_ENTRADA IS 'fecha de bloqueo del movimiento por parte del operador'
/
COMMENT ON COLUMN  ICC_MOVMANUAL.FEC_EJECUCION IS 'fecha de confirmacion de ejecucion manual del movimiento'
/
