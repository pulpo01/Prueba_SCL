CREATE TABLE GED_PARAMETROS_TD(
    NOM_PARAMETRO VARCHAR2(20) NOT NULL 
 ,  COD_MODULO VARCHAR2(2) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  VAL_PARAMETRO VARCHAR2(20) NOT NULL 
 ,  DES_PARAMETRO VARCHAR2(50) NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  FEC_ALTA DATE NOT NULL 
 ,  FEC_AUDITORIA DATE NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GED_PARAMETROS_TD IS '[ GPAT ] AUDITORIA DE PARAMETROS SUSPENSIÓN VOLUNTARIA PROGRAMADA'
/
COMMENT ON COLUMN  GED_PARAMETROS_TD.NOM_PARAMETRO IS 'Nombre del parametro'
/
COMMENT ON COLUMN  GED_PARAMETROS_TD.COD_MODULO IS 'Codigo del modulo'
/
COMMENT ON COLUMN  GED_PARAMETROS_TD.COD_PRODUCTO IS 'Codigo del producto'
/
COMMENT ON COLUMN  GED_PARAMETROS_TD.VAL_PARAMETRO IS 'Valor del parametro'
/
COMMENT ON COLUMN  GED_PARAMETROS_TD.DES_PARAMETRO IS 'Descripcion del parametro'
/
COMMENT ON COLUMN  GED_PARAMETROS_TD.NOM_USUARIO IS 'Usaurio que crea el parametro'
/
COMMENT ON COLUMN  GED_PARAMETROS_TD.FEC_ALTA IS 'Fecha de creacion del parametro'
/
COMMENT ON COLUMN  GED_PARAMETROS_TD.FEC_AUDITORIA IS 'Fecha de auditoria'
/
