CREATE TABLE ICC_NOTIFICACIONES_TO(
    ID_PROC NUMBER(12,0) NOT NULL 
 ,  EST_NOTIF NUMBER(3,0) NOT NULL 
 ,  NOTIFICACION VARCHAR2(512) NOT NULL 
 ,  FEC_NOTIF DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICC_NOTIFICACIONES_TO IS 'Tabla de asignacion de procesadores de respuestas'
/
COMMENT ON COLUMN  ICC_NOTIFICACIONES_TO.ID_PROC IS 'Identificador del Procesador que atiende la respuesta'
/
COMMENT ON COLUMN  ICC_NOTIFICACIONES_TO.EST_NOTIF IS 'Estado del procesamiento'
/
COMMENT ON COLUMN  ICC_NOTIFICACIONES_TO.NOTIFICACION IS 'Comando de la notioficacion'
/
COMMENT ON COLUMN  ICC_NOTIFICACIONES_TO.FEC_NOTIF IS 'Fecha de procesamiento'
/
