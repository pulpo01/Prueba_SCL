CREATE TABLE GE_PRESTACIONES_TD
(
  COD_PRESTACION            VARCHAR2(5 BYTE)    NOT NULL,
  DES_PRESTACION            VARCHAR2(50 BYTE)   NOT NULL,
  GRP_PRESTACION            VARCHAR2(5 BYTE)    NOT NULL,
  IND_ACTIVO                NUMBER(1)           NOT NULL,
  TIP_VENTA                 NUMBER(1)           NOT NULL,
  TIP_RED                   VARCHAR2(1 BYTE)    NOT NULL,
  IND_NUMERO                NUMBER(1)               NULL,
  IND_DIR_INSTALACION       NUMBER(1)               NULL,
  IND_INVENTARIO_FIJO       NUMBER(1)               NULL,
  IND_SS_INTERNET           NUMBER(1)               NULL,
  COD_PLANTARIF_DEFECTO     VARCHAR2(3 BYTE)        NULL,
  COD_CENTRAL_DEFECTO       NUMBER(3)               NULL,
  COD_CELDA_DEFECTO         VARCHAR2(500 BYTE)      NULL,
  IND_NUMPILOTO             NUMBER(1)               NULL,
  COD_SERVICIO_INSTALACION  VARCHAR2(3 BYTE)        NULL,
  IND_EXCEP_PROCESO         NUMBER(1)               NULL
)
TABLESPACE INSTALL_DATA
PCTUSED    0
PCTFREE    5
INITRANS   16
MAXTRANS   255
STORAGE    (
            INITIAL          40K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE GE_PRESTACIONES_TD IS '[ GEPRE ] Prestaciones comercializables en Ventas y posventas';

COMMENT ON COLUMN GE_PRESTACIONES_TD.COD_SERVICIO_INSTALACION IS 'Codigo de Servicio Ocasional para cobrar Cargo de Instalación';

COMMENT ON COLUMN GE_PRESTACIONES_TD.IND_EXCEP_PROCESO IS 'Indicador de excepcion de procesos de Clasificacion Cliente';

COMMENT ON COLUMN GE_PRESTACIONES_TD.COD_PRESTACION IS 'Código de la Prestación';

COMMENT ON COLUMN GE_PRESTACIONES_TD.DES_PRESTACION IS 'Descripción de la Prestación';

COMMENT ON COLUMN GE_PRESTACIONES_TD.GRP_PRESTACION IS 'Grupo de la Prestación (en GED_CODIGOS)';

COMMENT ON COLUMN GE_PRESTACIONES_TD.IND_ACTIVO IS 'Indicador Activo (1:Si) (0:No )';

COMMENT ON COLUMN GE_PRESTACIONES_TD.TIP_VENTA IS 'Tipo de Venta (0:Contrato) (1:Prepago)';

COMMENT ON COLUMN GE_PRESTACIONES_TD.TIP_RED IS 'Tipo de Red (F:Fija) (M:Movil)';

COMMENT ON COLUMN GE_PRESTACIONES_TD.IND_NUMERO IS 'Requiere Numero (0:No) (1:Si) (2:Nomeclatura) (3:Digito)';

COMMENT ON COLUMN GE_PRESTACIONES_TD.IND_DIR_INSTALACION IS 'Requiere Instalación (1:Si) (0:No)';

COMMENT ON COLUMN GE_PRESTACIONES_TD.IND_INVENTARIO_FIJO IS 'Requiere Inventario interno  (1:Si) (0:No)';

COMMENT ON COLUMN GE_PRESTACIONES_TD.IND_SS_INTERNET IS 'Requiere de Servicio Internet  (1:Si) (0:No)';

COMMENT ON COLUMN GE_PRESTACIONES_TD.COD_PLANTARIF_DEFECTO IS 'Codigo de Plan Tarifario por Defecto, NULL: seleccionable';

COMMENT ON COLUMN GE_PRESTACIONES_TD.COD_CENTRAL_DEFECTO IS 'Codigo de Central por Defecto';

COMMENT ON COLUMN GE_PRESTACIONES_TD.COD_CELDA_DEFECTO IS 'Codigo de Celda por defecto';

