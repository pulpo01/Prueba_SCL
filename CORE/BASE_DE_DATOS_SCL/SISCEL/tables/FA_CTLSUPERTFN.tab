CREATE TABLE FA_CTLSUPERTFN(
    SEQ_SUPERTFN NUMBER(12,0) NOT NULL 
 ,  NOM_FICHERO VARCHAR2(40) NOT NULL 
 ,  FEC_PROCESO DATE NOT NULL 
 ,  NUM_REGISTROS NUMBER(9,0) NOT NULL 
 ,  TOT_IMPORTE NUMBER(15,4) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  FA_CTLSUPERTFN IS 'Tabla de Control para el Proceso Super Telefono'
/
COMMENT ON COLUMN  FA_CTLSUPERTFN.SEQ_SUPERTFN IS 'Secuencia Super Telefono'
/
COMMENT ON COLUMN  FA_CTLSUPERTFN.NOM_FICHERO IS 'Nombre de Fichero'
/
COMMENT ON COLUMN  FA_CTLSUPERTFN.FEC_PROCESO IS 'Fecha de Proceso'
/
COMMENT ON COLUMN  FA_CTLSUPERTFN.NUM_REGISTROS IS 'Numero de Registros'
/
COMMENT ON COLUMN  FA_CTLSUPERTFN.TOT_IMPORTE IS 'Total Importe'
/
