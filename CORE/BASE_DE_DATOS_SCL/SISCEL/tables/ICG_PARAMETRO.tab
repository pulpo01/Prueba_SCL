CREATE TABLE ICG_PARAMETRO(
    COD_LENGUAJE NUMBER(3,0) NOT NULL 
 ,  NOM_PARAMETRO VARCHAR2(15) NOT NULL 
 ,  TIP_PARAMETRO VARCHAR2(15) NOT NULL 
 ,  DEF_PARAMETRO VARCHAR2(50) NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  ICG_PARAMETRO IS 'Parametros del Lenguaje'
/
COMMENT ON COLUMN  ICG_PARAMETRO.COD_LENGUAJE IS 'Codigo de Lenguaje - Retrofitted'
/
COMMENT ON COLUMN  ICG_PARAMETRO.NOM_PARAMETRO IS 'Nombre de Parametro - Retrofitted'
/
COMMENT ON COLUMN  ICG_PARAMETRO.TIP_PARAMETRO IS 'Codigo de Tipo - Retrofitted'
/
COMMENT ON COLUMN  ICG_PARAMETRO.DEF_PARAMETRO IS 'valor por defecto para el parametro'
/
