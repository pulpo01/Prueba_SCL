CREATE TABLE GED_ERR_PARAMETROS(
    COD_PARAMETRO NUMBER(4,0) NOT NULL 
 ,  DES_PARAMETRO VARCHAR2(60) NOT NULL 
 ,  TIP_DATO VARCHAR2(3) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GED_ERR_PARAMETROS IS 'Define Universo De Parametros Posibles.'
/
COMMENT ON COLUMN  GED_ERR_PARAMETROS.COD_PARAMETRO IS 'Codigo identificador del parametro.'
/
COMMENT ON COLUMN  GED_ERR_PARAMETROS.DES_PARAMETRO IS 'Descripcion del parametro.'
/
COMMENT ON COLUMN  GED_ERR_PARAMETROS.TIP_DATO IS 'Tipo de datos del Parametro.'
/
COMMENT ON COLUMN  GED_ERR_PARAMETROS.FEC_ULTMOD IS 'Fecha de ultima modificacion.'
/
COMMENT ON COLUMN  GED_ERR_PARAMETROS.NOM_USUARIO IS 'Usuario de creacion/modificacion.'
/
