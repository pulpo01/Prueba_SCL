CREATE TABLE CO_PL_COMISION(
    TIP_COMISION VARCHAR2(10) NOT NULL 
 ,  PL_CALCULO VARCHAR2(50) NOT NULL 
 ,  DESC_COMISION VARCHAR2(50) NOT NULL 
 ,  FORMA_CALCULO CHAR(1) DEFAULT 'P' NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_PL_COMISION IS 'Pl Calculo De Comisiones.'
/
COMMENT ON COLUMN  CO_PL_COMISION.TIP_COMISION IS 'Tipo de Comisión.'
/
COMMENT ON COLUMN  CO_PL_COMISION.PL_CALCULO IS 'Procedimiento (PL) asociado.'
/
COMMENT ON COLUMN  CO_PL_COMISION.DESC_COMISION IS 'Descripción Comisión.'
/
COMMENT ON COLUMN  CO_PL_COMISION.FORMA_CALCULO IS 'Tipo de Comision P:Puntual M:Masiva'
/
