 ,  TIP_REDONREDES VARCHAR2(5) NULL 
 ,  IND_ENTRADA NUMBER(1,0) DEFAULT '0' NOT NULL 
 ,  IND_COBROESPECIAL NUMBER(1,0) DEFAULT '0' NOT NULL 
 ,  IND_COBROSEPARADO NUMBER(1,0) DEFAULT '0' NOT NULL 
 ,  IND_TASARDURACION NUMBER(1,0) DEFAULT '0' NOT NULL 
 ,  COD_SERVICIO VARCHAR2(5) NULL 
