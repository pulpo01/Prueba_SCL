CREATE TABLE VE_PLANTILLA_TIPCOMIS_TD(
    COD_CAMPO NUMBER(2,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  IND_OBLIGATORIO NUMBER(1,0) NOT NULL 
 ,  IND_VISIBLE NUMBER(1,0) NOT NULL 
 ,  FEC_ULTMOD DATE DEFAULT SYSDATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) DEFAULT USER NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_PLANTILLA_TIPCOMIS_TD IS 'PLANTILLA DE CAMPOS POR CATEGORIA DE VENTA'
/
COMMENT ON COLUMN  VE_PLANTILLA_TIPCOMIS_TD.COD_CAMPO IS 'CODIGO DEL CAMPO'
/
COMMENT ON COLUMN  VE_PLANTILLA_TIPCOMIS_TD.COD_TIPCOMIS IS 'CODIGO DE CATEGEORIA DE VENTA'
/
COMMENT ON COLUMN  VE_PLANTILLA_TIPCOMIS_TD.IND_OBLIGATORIO IS 'INDICADOR DE CAMPO OBLIGATORIO (SI: 1, NO: 0)'
/
COMMENT ON COLUMN  VE_PLANTILLA_TIPCOMIS_TD.IND_VISIBLE IS 'INDICADOR DE CAMPO VISIBLE (SI:1, NO:0)'
/
COMMENT ON COLUMN  VE_PLANTILLA_TIPCOMIS_TD.FEC_ULTMOD IS 'FECHA DE ULTIMA MODIFICACION'
/
COMMENT ON COLUMN  VE_PLANTILLA_TIPCOMIS_TD.NOM_USUARIO IS 'USUARIO ULTIMA MODIFICACION'
/
