CREATE TABLE CET_AUDITORIA(
    NUM_TRANSACCION NUMBER(10,0) NOT NULL 
 ,  FEC_MOVIMIENTO DATE NOT NULL 
 ,  NUM_SERIE VARCHAR2(16) NOT NULL 
 ,  MTO_SERVICIO NUMBER(14,4) NOT NULL 
 ,  NUM_CELULAR VARCHAR2(15) NOT NULL 
 ,  COD_ESTCOMVERSE VARCHAR2(12) NOT NULL 
 ,  COD_ESTGCE VARCHAR2(5) NOT NULL 
 ,  COD_COMANDO VARCHAR2(5) NOT NULL 
 ,  COD_ESTFINAL VARCHAR2(5) NOT NULL 
 ,  COD_MODRECHA VARCHAR2(5) NULL 
 ,  GLS_OBSERVACION VARCHAR2(100) NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_SCL_OTROS_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CET_AUDITORIA IS 'Auditoria Gestion Clientes.'
/
COMMENT ON COLUMN  CET_AUDITORIA.NUM_TRANSACCION IS 'Numero de transaccion de venta.'
/
COMMENT ON COLUMN  CET_AUDITORIA.FEC_MOVIMIENTO IS 'Fecha de la consulta.'
/
COMMENT ON COLUMN  CET_AUDITORIA.NUM_SERIE IS 'Numero de serie pin.'
/
COMMENT ON COLUMN  CET_AUDITORIA.MTO_SERVICIO IS 'Valor servicio.'
/
COMMENT ON COLUMN  CET_AUDITORIA.NUM_CELULAR IS 'Numero de celular asignado.'
/
COMMENT ON COLUMN  CET_AUDITORIA.COD_ESTCOMVERSE IS 'Estado en COMVERSE.'
/
COMMENT ON COLUMN  CET_AUDITORIA.COD_ESTGCE IS 'Estado en gce.'
/
COMMENT ON COLUMN  CET_AUDITORIA.COD_COMANDO IS 'Codigo de comando a ejecutar.'
/
COMMENT ON COLUMN  CET_AUDITORIA.COD_ESTFINAL IS 'Estado final de la serie.'
/
COMMENT ON COLUMN  CET_AUDITORIA.COD_MODRECHA IS 'Motivo de rechazo.'
/
COMMENT ON COLUMN  CET_AUDITORIA.GLS_OBSERVACION IS 'Observacion.'
/
COMMENT ON COLUMN  CET_AUDITORIA.NOM_USUARIO IS 'Nombre usuario responsable.'
/
