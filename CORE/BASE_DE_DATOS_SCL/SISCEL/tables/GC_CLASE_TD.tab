CREATE TABLE GC_CLASE_TD(
    COD_CLASE NUMBER(6,0) NOT NULL 
 ,  GLS_CLASE VARCHAR2(70) NOT NULL 
 ,  FEC_INI_VIGENCIA DATE NOT NULL 
 ,  FEC_FIN_VIGENCIA DATE NULL 
 ,  NOM_USUARORA VARCHAR2(30) DEFAULT USER NOT NULL 
 ,  FEC_ULTMOD DATE DEFAULT SYSDATE NOT NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GC_CLASE_TD IS 'Clases de GC'
/
COMMENT ON COLUMN  GC_CLASE_TD.COD_CLASE IS 'Codigo de Clase'
/
COMMENT ON COLUMN  GC_CLASE_TD.GLS_CLASE IS 'Descripcion de Clase'
/
COMMENT ON COLUMN  GC_CLASE_TD.FEC_INI_VIGENCIA IS 'Fecha de inicio vigencia de la clase'
/
COMMENT ON COLUMN  GC_CLASE_TD.FEC_FIN_VIGENCIA IS 'Fecha de fin de vigencia de la clase'
/
COMMENT ON COLUMN  GC_CLASE_TD.NOM_USUARORA IS 'Usuario Oracle'
/
COMMENT ON COLUMN  GC_CLASE_TD.FEC_ULTMOD IS 'Fecha de ultima modificacion'
/
