CREATE TABLE VE_HISRESLIQUID(
    NUM_LIQUIDACION NUMBER(8,0) NOT NULL 
 ,  COD_TIPCOMIS VARCHAR2(2) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CTOLIQ NUMBER(6,0) NOT NULL 
 ,  IMP_COMISION NUMBER(12,4) NOT NULL 
 ,  COD_CUADRANTE NUMBER(8,0) NOT NULL 
 ,  NUM_META NUMBER(12,4) NULL 
 ,  IMP_BASE NUMBER(12,4) NULL 
 ,  NUM_LIMITE NUMBER(4,0) NULL 
 ) TABLESPACE    TS_CRM_GESTION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  VE_HISRESLIQUID IS 'Historico de Resultados de Liquidaciones'
/
COMMENT ON COLUMN  VE_HISRESLIQUID.NUM_LIQUIDACION IS 'Numero de Liquidacion'
/
COMMENT ON COLUMN  VE_HISRESLIQUID.COD_TIPCOMIS IS 'Codigo de Tipo de Comisionista'
/
COMMENT ON COLUMN  VE_HISRESLIQUID.COD_VENDEDOR IS 'Codigo de Vendedor'
/
COMMENT ON COLUMN  VE_HISRESLIQUID.COD_PRODUCTO IS 'Codigo de Producto'
/
COMMENT ON COLUMN  VE_HISRESLIQUID.COD_CTOLIQ IS 'Codigo de Concepto Liquidacion'
/
COMMENT ON COLUMN  VE_HISRESLIQUID.IMP_COMISION IS 'Importe de la Comision'
/
COMMENT ON COLUMN  VE_HISRESLIQUID.COD_CUADRANTE IS 'Codigo de Cuadrante'
/
COMMENT ON COLUMN  VE_HISRESLIQUID.NUM_META IS 'Valor de la Meta (si existe)'
/
COMMENT ON COLUMN  VE_HISRESLIQUID.IMP_BASE IS 'Importe Base (si existe)'
/
COMMENT ON COLUMN  VE_HISRESLIQUID.NUM_LIMITE IS 'Limite para importe base (si existe)'
/
