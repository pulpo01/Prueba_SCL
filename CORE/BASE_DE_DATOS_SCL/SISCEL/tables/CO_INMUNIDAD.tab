CREATE TABLE CO_INMUNIDAD(
    NUM_ABONADO NUMBER(8,0) NOT NULL 
 ,  COD_SUSPREHA VARCHAR2(3) NOT NULL 
 ,  COD_ACTABO VARCHAR2(2) NOT NULL 
 ,  COD_PRODUCTO NUMBER(1,0) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NOM_USUARORA VARCHAR2(30) NOT NULL 
 ,  FEC_VALOR DATE NOT NULL 
 ,  FEC_DESDE DATE NOT NULL 
 ,  FEC_HASTA DATE NOT NULL 
 ,  COD_TIPINMU NUMBER(3,0) NOT NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_INMUNIDAD IS 'Inmunidad de cobros al proceso de suspension- Retrofitted'
/
COMMENT ON COLUMN  CO_INMUNIDAD.NUM_ABONADO IS 'Codigo de abonado - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUNIDAD.COD_SUSPREHA IS 'Codigo de Suspension - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUNIDAD.COD_ACTABO IS 'Codigo de Actuacion Suspension - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUNIDAD.COD_PRODUCTO IS 'Codigo de Producto - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUNIDAD.COD_CLIENTE IS 'Codigo de cliente - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUNIDAD.NOM_USUARORA IS 'Usuario que realizo la inmunidad - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUNIDAD.FEC_VALOR IS 'Fecha de ingreso de la inmunidad - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUNIDAD.FEC_DESDE IS 'Fecha de comienzo de inmunidad - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUNIDAD.FEC_HASTA IS 'Fecha fin de inmunidad - Retrofitted'
/
COMMENT ON COLUMN  CO_INMUNIDAD.COD_TIPINMU IS 'Tipo de inmunidad - Retrofitted'
/
