CREATE TABLE AL_CAB_TIMBRADO(
    NUM_ORDEN NUMBER(9,0) NOT NULL 
 ,  FEC_CREACION DATE NOT NULL 
 ,  USU_CREACION VARCHAR2(30) NOT NULL 
 ,  COD_BODEGA NUMBER(6,0) NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) NOT NULL 
 ,  FEC_ENVIO DATE NULL 
 ,  USU_ENVIO VARCHAR2(30) NULL 
 ,  TIP_MOVIM_ENVIO NUMBER(2,0) NULL 
 ,  FEC_RECEPCION DATE NULL 
 ,  USU_RECEPCION VARCHAR2(30) NULL 
 ,  TIP_MOVIM_RECEP NUMBER(2,0) NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_CAB_TIMBRADO IS 'Cabecera Ordenes de Timbrado de Documentos'
/
COMMENT ON COLUMN  AL_CAB_TIMBRADO.NUM_ORDEN IS 'Numero de Orden de Timbrado'
/
COMMENT ON COLUMN  AL_CAB_TIMBRADO.FEC_CREACION IS 'Fecha en la que fue creada la orden de timbrado.'
/
COMMENT ON COLUMN  AL_CAB_TIMBRADO.USU_CREACION IS 'Usuario Creador'
/
COMMENT ON COLUMN  AL_CAB_TIMBRADO.COD_BODEGA IS 'Codigo de bodega'
/
COMMENT ON COLUMN  AL_CAB_TIMBRADO.COD_ESTADO IS 'Estado de la orden'
/
COMMENT ON COLUMN  AL_CAB_TIMBRADO.FEC_ENVIO IS 'Fecha de Envio a Impuestos Internos'
/
COMMENT ON COLUMN  AL_CAB_TIMBRADO.USU_ENVIO IS 'Usuario Envio'
/
COMMENT ON COLUMN  AL_CAB_TIMBRADO.TIP_MOVIM_ENVIO IS 'Tipo de Movimiento de Envio'
/
COMMENT ON COLUMN  AL_CAB_TIMBRADO.FEC_RECEPCION IS 'Fecha de Recepcion de Impuestos Internos'
/
COMMENT ON COLUMN  AL_CAB_TIMBRADO.USU_RECEPCION IS 'Usuario Recepcion'
/
COMMENT ON COLUMN  AL_CAB_TIMBRADO.TIP_MOVIM_RECEP IS 'Tipo de Movimiento de Recepcion'
/
