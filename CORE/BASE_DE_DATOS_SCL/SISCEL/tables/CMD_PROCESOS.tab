CREATE TABLE CMD_PROCESOS(
    COD_PROCESO VARCHAR2(15) NOT NULL 
 ,  DES_PROCESO VARCHAR2(50) NOT NULL 
 ,  TIP_PERIODO CHAR(1) NOT NULL 
 ,  IND_ARCHIVO CHAR(1) NOT NULL 
 ,  DIR_PROCESO VARCHAR2(50) NOT NULL 
 ,  NOM_EXE VARCHAR2(30) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ) TABLESPACE    TS_FACTURACION_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CMD_PROCESOS IS 'Definicion De Procesos De Comisiones.'
/
COMMENT ON COLUMN  CMD_PROCESOS.COD_PROCESO IS 'Codigo de Proceso.'
/
COMMENT ON COLUMN  CMD_PROCESOS.DES_PROCESO IS 'Descripcion de Proceso.'
/
COMMENT ON COLUMN  CMD_PROCESOS.TIP_PERIODO IS 'Tipo de periodo asociado a proceso [N]ormal [D]iferido [A]mistar.'
/
COMMENT ON COLUMN  CMD_PROCESOS.IND_ARCHIVO IS 'Indica si crea archivo en disco.'
/
COMMENT ON COLUMN  CMD_PROCESOS.DIR_PROCESO IS 'Variable de ambiente para ubicacion del proceso..'
/
COMMENT ON COLUMN  CMD_PROCESOS.NOM_EXE IS 'Nombre en disco de Proceso.'
/
COMMENT ON COLUMN  CMD_PROCESOS.FEC_ULTMOD IS 'Fecha de ultima modificacion del registro..'
/
COMMENT ON COLUMN  CMD_PROCESOS.NOM_USUARIO IS 'Usuario que hizo la insercion del registro en la tabla..'
/
