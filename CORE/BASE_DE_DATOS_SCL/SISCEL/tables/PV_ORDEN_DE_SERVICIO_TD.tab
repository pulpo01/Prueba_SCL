CREATE TABLE PV_ORDEN_DE_SERVICIO_TD(
    COD_OS VARCHAR2(5) NOT NULL 
 ,  ESTADO VARCHAR2(15) NOT NULL 
 ,  FEC_ACTUALIZACION DATE NOT NULL 
 ,  PARAMETROS BLOB NULL 
 ,  NUM_PROCESO NUMBER(38,0) NOT NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PV_ORDEN_DE_SERVICIO_TD IS '[ OOSS ] Estado de las Ordenes de Servicio ejecutadas'
/
COMMENT ON COLUMN  PV_ORDEN_DE_SERVICIO_TD.COD_OS IS 'Código de la Orden de Servicio'
/
COMMENT ON COLUMN  PV_ORDEN_DE_SERVICIO_TD.ESTADO IS 'Estado de la Ejecución'
/
COMMENT ON COLUMN  PV_ORDEN_DE_SERVICIO_TD.FEC_ACTUALIZACION IS 'Fecha del último estado'
/
COMMENT ON COLUMN  PV_ORDEN_DE_SERVICIO_TD.PARAMETROS IS 'Parámetros de Ejecución de la Orden de Servicio'
/
COMMENT ON COLUMN  PV_ORDEN_DE_SERVICIO_TD.NUM_PROCESO IS 'Número de Proceso de la Orden de Servicio'
/
