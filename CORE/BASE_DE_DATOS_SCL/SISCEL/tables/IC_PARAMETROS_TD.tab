CREATE TABLE IC_PARAMETROS_TD(
    NOM_PARAMETRO VARCHAR2(20) NOT NULL 
 ,  DES_PARAMETRO VARCHAR2(100) NOT NULL 
 ,  TIP_ORIGEN CHAR(1) NOT NULL 
 ,  NOM_ORIGEN VARCHAR2(50) NOT NULL 
 ,  TIP_VALOR VARCHAR2(50) NOT NULL 
 ,  TIP_DATO VARCHAR2(50) NOT NULL 
 ) TABLESPACE    TS_PROVISIONING_SDAT
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  IC_PARAMETROS_TD IS 'PARAMETROS ASOCIADOS A LA INTERFAZ COMERCIAL'
/
COMMENT ON COLUMN  IC_PARAMETROS_TD.NOM_PARAMETRO IS 'CODIGO IDENTIFICADOR DEL PARAMETRO'
/
COMMENT ON COLUMN  IC_PARAMETROS_TD.DES_PARAMETRO IS 'DESCRIPCION O PROPOSITO DEL PARAMETRO'
/
COMMENT ON COLUMN  IC_PARAMETROS_TD.TIP_ORIGEN IS 'ORIGEN. I:ICC_MOV, P:PROCESO, F:FUNCION ESPECIFICA'
/
COMMENT ON COLUMN  IC_PARAMETROS_TD.NOM_ORIGEN IS 'NOMBRE DEL ORIGEN (COLUMNA, NULL, RUTINA)'
/
COMMENT ON COLUMN  IC_PARAMETROS_TD.TIP_VALOR IS 'TABLA.COLUMNA%TYPE'
/
COMMENT ON COLUMN  IC_PARAMETROS_TD.TIP_DATO IS 'TIPO DE DATO GENERICO (NUMBER/VARCHAR2/DATE).'
/
