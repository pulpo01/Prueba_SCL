CREATE TABLE CO_HISTMOVCAJA
(
  COD_OFICINA          VARCHAR2(2 BYTE)         NOT NULL,
  COD_CAJA             NUMBER(4)                NOT NULL,
  NUM_SECUMOV          NUMBER(8)                NOT NULL,
  NUM_EJERCICIO        VARCHAR2(12 BYTE)        NOT NULL,
  FEC_EFECTIVIDAD      DATE                     NOT NULL,
  FEC_HISTORICO        DATE                     NOT NULL,
  NOM_USUARORA         VARCHAR2(30 BYTE)        NOT NULL,
  TIP_MOVCAJA          NUMBER(2)                NOT NULL,
  IND_DEPOSITO         NUMBER(1)                NOT NULL,
  IMPORTE              NUMBER(14,4)             NOT NULL,
  IND_ERRONEO          NUMBER(1)                DEFAULT 0                     NOT NULL,
  TIP_VALOR            NUMBER(2)                NOT NULL,
  IND_CIERRE           NUMBER(1)                DEFAULT 0                     NOT NULL,
  COD_CLIENTE          NUMBER(8),
  NUM_ABONADO          NUMBER(8),
  COD_PRODUCTO         NUMBER(1),
  TIP_DOCUMENT         NUMBER(2),
  COD_VENDEDOR_AGENTE  NUMBER(6),
  LETRA                VARCHAR2(1 BYTE),
  COD_CENTREMI         NUMBER(4),
  NUM_SECUENCI         NUMBER(8),
  LETRAC               VARCHAR2(1 BYTE),
  COD_CENTRC           NUMBER(4),
  NUM_SECUC            NUMBER(8),
  COD_BANCO            VARCHAR2(15 BYTE),
  COD_SUCURSAL         VARCHAR2(4 BYTE),
  CTA_CORRIENTE        VARCHAR2(18 BYTE),
  NUM_CHEQUE           VARCHAR2(20 BYTE),
  TIP_CLEARING         VARCHAR2(3 BYTE),
  FEC_CHEQUE           DATE,
  COD_TIPTARJETA       VARCHAR2(3 BYTE),
  NUM_TARJETA          VARCHAR2(20 BYTE),
  COD_AUTORIZA         VARCHAR2(15 BYTE),
  NUM_CUPON            VARCHAR2(20 BYTE),
  NUM_CUOTAS           NUMBER(2),
  FEC_CUPON            DATE,
  COD_MOVILI           NUMBER(2),
  DES_MOVILI           VARCHAR2(30 BYTE),
  COD_RECOMPE          NUMBER(2),
  NOM_CUSTODIA         VARCHAR2(50 BYTE),
  NUM_ORDEN            VARCHAR2(12 BYTE),
  FEC_EMISION          DATE,
  FEC_VENCIMIENTO      VARCHAR2(100 BYTE),
  COD_COBRADOR         NUMBER(6),
  NUM_INGMANUAL        NUMBER(8),
  NUM_RESPINGR         NUMBER(8),
  NUM_COMPAGO          NUMBER(9),
  NUM_IDENT            VARCHAR2(20 BYTE),
  COD_MONEDA           VARCHAR2(3 BYTE),
  COD_OPERADORA_SCL    VARCHAR2(5 BYTE),
  COD_PLAZA            VARCHAR2(5 BYTE),
  PREF_PLAZA           VARCHAR2(25 BYTE)
)
TABLESPACE TS_RECYCOB_SDAT
PCTUSED    0
PCTFREE    10
INITRANS   64
MAXTRANS   255
STORAGE    (
            INITIAL          20M
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

