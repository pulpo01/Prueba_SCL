CREATE TABLE CO_CODIGOS(
    NOM_TABLA VARCHAR2(30) NOT NULL 
 ,  NOM_COLUMNA VARCHAR2(50) NOT NULL 
 ,  COD_VALOR VARCHAR2(5) NOT NULL 
 ,  DES_VALOR VARCHAR2(512) NOT NULL 
 ,  FEC_ULTMOD DATE NOT NULL 
 ,  NOM_USUARIO VARCHAR2(30) NOT NULL 
 ,  COD_MODULO VARCHAR2(2) NULL 
 ) TABLESPACE    TS_RECYCOB_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CO_CODIGOS IS '### Pendiente - Depto. Facturacion, Tasacion y Cobros'
/
COMMENT ON COLUMN  CO_CODIGOS.NOM_TABLA IS 'Identificacion tabla asociada.'
/
COMMENT ON COLUMN  CO_CODIGOS.NOM_COLUMNA IS 'Identificacion columna asociada.'
/
COMMENT ON COLUMN  CO_CODIGOS.COD_VALOR IS 'Codigos asignados.'
/
COMMENT ON COLUMN  CO_CODIGOS.DES_VALOR IS 'Descripcion de codigos asignados.'
/
COMMENT ON COLUMN  CO_CODIGOS.FEC_ULTMOD IS 'Fecha ultima modificacion.'
/
COMMENT ON COLUMN  CO_CODIGOS.NOM_USUARIO IS 'Identificacion usuario.'
/
COMMENT ON COLUMN  CO_CODIGOS.COD_MODULO IS 'Codigo de modulo de SISCEL.'
/
