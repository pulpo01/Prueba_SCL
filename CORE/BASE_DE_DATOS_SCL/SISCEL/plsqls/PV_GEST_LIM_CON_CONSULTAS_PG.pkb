            || '   NVL(b.fec_hasta,SYSDATE) AND' 
            || '   b.cod_plantarif = '''|| LV_COD_PLANTARIF ||''' AND' 
            || '   b.ID_SUBSEGMENTO= '''|| LV_COD_SEGMENTO ||''' AND' 
            || '   b.IND_PRIORIDAD>= TO_NUMBER('''|| LV_IND_PRIORIDAD ||''') AND' 
            || '   rownum=1 ';    
    END IF;                                
        
OPEN SC_DATOS_LIM_CONSUMO FOR LV_SSQL;

EXCEPTION
