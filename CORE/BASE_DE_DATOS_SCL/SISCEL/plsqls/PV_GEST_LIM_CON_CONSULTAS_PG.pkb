                                    SV_MENS_RETORNO         OUT NOCOPY GE_ERRORES_TD.DET_MSGERROR%TYPE,
                                    SN_NUM_EVENTO           OUT NOCOPY GE_ERRORES_PG.EVENTO
)
 /*
<DOCUMENTACI흁
  TIPODOC = "PROCEDIMIENTO">
   <ELEMENTO
      NOMBRE = "PV_GET_DATOS_SINIESTRO_PR"
      LENGUAJE="PL/SQL"
