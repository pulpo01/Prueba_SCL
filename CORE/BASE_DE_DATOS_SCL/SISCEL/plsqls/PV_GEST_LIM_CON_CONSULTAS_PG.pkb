            <PARAM NOM="SV_MENS_RETORNO"  TIPO="VARCHAR" >MENSAJE DE RETORNO (C휷IGO DE ERROR)</PARAM>
            <PARAM NOM="SN_NUM_EVENTO"  TIPO="NUMERICO" >N＝ERO DE EVENTO</PARAM>
         </SALIDA>
      </PAR쳀ETROS>
   </ELEMENTO>
</DOCUMENTACI흁>
*/
IS
    LV_DES_ERROR               GE_ERRORES_PG.DESEVENT;
    LV_SSQL                    GE_ERRORES_PG.VQUERY;
    LV_NOMBRE_PROC             STRING(30)   := 'PV_CAMBIA_ESTADO_MOV_PR';
    LV_PKG_PROC                STRING(60)   := GV_PACKAGE||'.'||LV_NOMBRE_PROC;
    LN_NUM_TRANSACC            GA_TRANSACABO.NUM_TRANSACCION%TYPE;
    
BEGIN

    SN_COD_RETORNO  := 0;
    SV_MENS_RETORNO := 'OK';
    SN_NUM_EVENTO   := 0;

    UPDATE ICC_MOVIMIENTO SET
    COD_ESTADO = 1
    WHERE NUM_MOVIMIENTO = EN_NUM_MOVIMIENTO;

