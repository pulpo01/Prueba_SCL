    </ENTRADA>
    <SALIDA>
        <PARAM NOM="SV_DES_STOCK"  TIPO="VARCHAR"> </PARAM>
        <PARAM NOM="SV_NUM_SERIE"  TIPO="CURSOR"> </PARAM>
