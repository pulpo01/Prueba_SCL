<PAR쳀ETROS>
    <ENTRADA>
        <PARAM NOM="EN_CODUSO"  TIPO="NUMBER">CODIGO DE USO </PARAM>
        <PARAM NOM="EN_TIPTERMINAL"  TIPO="NUMBER">TIPO DEL TERMINAL </PARAM>
