CREATE OR REPLACE TYPE GE_SESION_QT AS OBJECT
(

  COD_CLIENTE        NUMBER(8),
  NUM_ABONADO        NUMBER(8),
  COD_ORDENSERVICIO  NUMBER(10),
  NUM_ORDENSERVICIO  NUMBER(10),
  NOM_USUARIO        VARCHAR2(50),
  CLAVE              VARCHAR2(50),
  FORWARD            VARCHAR2(50),
  COD_PROGRAMA       VARCHAR2(10),
  NUM_VERSION        NUMBER(10),
  ORIGEN_SERIE	     VARCHAR2(1),	 --- RRG 08-03-2009 COL 78629
