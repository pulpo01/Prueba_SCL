  CONSTRUCTOR FUNCTION GE_SESION_QT RETURN self AS result AS
  BEGIN
    COD_CLIENTE        := NULL;
    NUM_ABONADO        := NULL;
    COD_ORDENSERVICIO  := NULL;
    NUM_ORDENSERVICIO  := NULL;
    NOM_USUARIO        := NULL;
    CLAVE              := NULL;
    FORWARD            := NULL;
    COD_PROGRAMA       := NULL;
    NUM_VERSION        := NULL;
    ORIGEN_SERIE       := NULL;		--- RRG 08-03-2009 COL 78629
    RETURN;
  END;
END;
/
SHOW ERRORS
