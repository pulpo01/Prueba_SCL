  BEGIN
    NUM_SINIESTRO    := NULL;
    NUM_ABONADO      := NULL;
    COD_PRODUCTO     := NULL;
    COD_CAUSA        := NULL;
    COD_ESTADO       := NULL;
    NUM_TERMINAL     := NULL;
    NUM_SERIE        := NULL;
    FEC_SINIESTRO    := NULL;
    COD_SERVICIO     := NULL;
    COD_CARGOBASICO  := NULL;
    FEC_FORMALIZA    := NULL;
    FEC_ANULA        := NULL;
    FEC_RESTITUC     := NULL;
    NUM_CONSTPOL     := NULL;
    NUM_SOLLIQ       := NULL;
    NUM_SERIEREP     := NULL;
    IND_SUSP         := NULL;
    TIP_TERMINAL     := NULL;
    RETURN;
  END;
END;
/
SHOW ERRORS
