CREATE OR REPLACE TYPE GA_SINIESTROS_QT AS OBJECT
(
  NUM_SINIESTRO    NUMBER(8),
  NUM_ABONADO      NUMBER(8),
  COD_PRODUCTO     NUMBER(1),
  COD_CAUSA        VARCHAR2(2),
  COD_ESTADO       VARCHAR2(2),
  NUM_TERMINAL     VARCHAR2(15),
  NUM_SERIE        VARCHAR2(25),
  FEC_SINIESTRO    DATE,
  COD_SERVICIO     VARCHAR2(3),
  COD_CARGOBASICO  VARCHAR2(3),
