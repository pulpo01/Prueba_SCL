CREATE TABLE CU_BENEF_PROD_TO(
    COD_CLIENTE_CONTRATANTE NUMBER(10,0) NOT NULL 
 ,  NUM_ABONADO_CONTRATANTE NUMBER(10,0) NOT NULL 
 ,  TIPO_COMPORTAMIENTO VARCHAR2(5) NOT NULL 
 ,  FEC_INICIO_VIGENCIA DATE NOT NULL 
 ,  NUM_ABONADO_BENEFICIARIO NUMBER(10,0) NOT NULL 
 ,  FEC_TERMINO_VIGENCIA DATE NULL 
 ) TABLESPACE    MAS_CUSTOMER_DATA
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CU_BENEF_PROD_TO IS '[ BEPR ] Abonados beneficiarios de Modulos para el cliente'
/
COMMENT ON COLUMN  CU_BENEF_PROD_TO.COD_CLIENTE_CONTRATANTE IS 'Código Identificador del Cliente Contratante'
/
COMMENT ON COLUMN  CU_BENEF_PROD_TO.NUM_ABONADO_CONTRATANTE IS 'Número de Abonado Contratante'
/
COMMENT ON COLUMN  CU_BENEF_PROD_TO.TIPO_COMPORTAMIENTO IS 'Tipo de Comportamiento (Ver Dominio TIPO_COMPORTAMIENTO)'
/
COMMENT ON COLUMN  CU_BENEF_PROD_TO.FEC_INICIO_VIGENCIA IS 'Fecho de Entrada en Vigencia del Abonado Beneficiario'
/
COMMENT ON COLUMN  CU_BENEF_PROD_TO.NUM_ABONADO_BENEFICIARIO IS 'Número de Abonado Beneficiario'
/
COMMENT ON COLUMN  CU_BENEF_PROD_TO.FEC_TERMINO_VIGENCIA IS 'Fecho de Expiración de Vigencia del Abonado Beneficiario'
/
