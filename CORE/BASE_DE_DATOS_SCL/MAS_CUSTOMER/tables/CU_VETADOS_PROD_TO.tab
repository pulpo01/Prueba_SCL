CREATE TABLE CU_VETADOS_PROD_TO(
    COD_CLIENTE NUMBER(10,0) NOT NULL 
 ,  NUM_ABONADO NUMBER(10,0) NOT NULL 
 ,  TIPO_COMPORTAMIENTO VARCHAR2(5) NOT NULL 
 ,  FEC_INICIO_VIGENCIA DATE NOT NULL 
 ,  FEC_TERMINO_VIGENCIA DATE NULL 
 ) TABLESPACE    MAS_CUSTOMER_DATA
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  CU_VETADOS_PROD_TO IS '[ VEPR ] Abonados vetados de poder contratar productos'
/
COMMENT ON COLUMN  CU_VETADOS_PROD_TO.COD_CLIENTE IS 'Código Identificador del Cliente'
/
COMMENT ON COLUMN  CU_VETADOS_PROD_TO.NUM_ABONADO IS 'Número de Abonado vetado del cliente'
/
COMMENT ON COLUMN  CU_VETADOS_PROD_TO.TIPO_COMPORTAMIENTO IS 'Tipo de Comportamiento (Ver Dominio TIPO_COMPORTAMIENTO)'
/
COMMENT ON COLUMN  CU_VETADOS_PROD_TO.FEC_INICIO_VIGENCIA IS 'Fecha de Inicio de Veto'
/
COMMENT ON COLUMN  CU_VETADOS_PROD_TO.FEC_TERMINO_VIGENCIA IS 'Fechat de Termino del Veto'
/
