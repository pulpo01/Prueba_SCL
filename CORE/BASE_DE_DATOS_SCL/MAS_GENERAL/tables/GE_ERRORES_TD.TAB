CREATE TABLE GE_ERRORES_TD(
    COD_MSGERROR NUMBER(6,0) NOT NULL 
 ,  DET_MSGERROR VARCHAR2(3000) NOT NULL 
 ,  COD_GRUPO NUMBER(6,0) NULL 
 ,  COD_MSGCLIENTE NUMBER(6,0) NULL 
 ) TABLESPACE    TS_MAS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_ERRORES_TD IS 'TABLA QUE CONTIENE MENSAJES DE ERROR PARA SERVICIOS'
/
COMMENT ON COLUMN  GE_ERRORES_TD.COD_MSGERROR IS 'CODIGO MENSAJE DE ERROR'
/
COMMENT ON COLUMN  GE_ERRORES_TD.DET_MSGERROR IS 'TEXTO MENSAJE ERROR'
/
COMMENT ON COLUMN  GE_ERRORES_TD.COD_GRUPO IS 'CODIGO GRUPO ERROR'
/
COMMENT ON COLUMN  GE_ERRORES_TD.COD_MSGCLIENTE IS 'Codigo de Error del Cliente'
/
