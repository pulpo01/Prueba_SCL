CREATE TABLE GE_EVENTO_TO(
    EVENTO NUMBER(9,0) NOT NULL 
 ,  COD_PROGRAMA VARCHAR2(3) NOT NULL 
 ,  DESCRIPCION VARCHAR2(1000) NULL 
 ,  VERSION_PRG VARCHAR2(5) NULL 
 ,  NOM_USUARIO VARCHAR2(30) NULL 
 ,  NUM_PROCESO VARCHAR2(22) NULL 
 ,  FEC_EVENTO DATE DEFAULT SYSDATE NOT NULL 
 ,  MAQUINA VARCHAR2(50) NULL 
 ) TABLESPACE    TS_MAS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_EVENTO_TO IS 'TABLA QUE GUARDA LOS ELEMENTOS NECESARIOS PARA DETERMINAR Y GESTIONAR LOS ERRORES OCURRIDOS EN ALGUNA APLICACION'
/
COMMENT ON COLUMN  GE_EVENTO_TO.EVENTO IS 'VALOR NUMERICO QUE IDENTIFICA UN EVENTO'
/
COMMENT ON COLUMN  GE_EVENTO_TO.COD_PROGRAMA IS 'CODIGO DE LA APLICACION SCL QUE GENERA EL EVENTO'
/
COMMENT ON COLUMN  GE_EVENTO_TO.DESCRIPCION IS 'DESCRICION DEL EVENTO'
/
COMMENT ON COLUMN  GE_EVENTO_TO.VERSION_PRG IS 'VERSION DEL PROGRAMA SCL'
/
COMMENT ON COLUMN  GE_EVENTO_TO.NOM_USUARIO IS 'USUARIO ORACLE'
/
COMMENT ON COLUMN  GE_EVENTO_TO.NUM_PROCESO IS 'IDENTIFICADOR DEL PROCESO ORACLE'
/
COMMENT ON COLUMN  GE_EVENTO_TO.FEC_EVENTO IS 'FECHA EN QUE OCURRE EL EVENTO'
/
COMMENT ON COLUMN  GE_EVENTO_TO.MAQUINA IS 'NOMBRE ESTACION DE TRABAJO'
/
