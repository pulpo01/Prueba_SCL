CREATE TABLE GE_EVENTO_DETALLE_TO(
    DET_EVENTO NUMBER(9,0) NOT NULL 
 ,  EVENTO NUMBER(9,0) NOT NULL 
 ,  SERVICIO VARCHAR2(100) NULL 
 ,  QUERY VARCHAR2(3000) NULL 
 ,  COD_ERROR NUMBER NULL 
 ,  DESCRIPCION_ERROR LONG NULL 
 ) TABLESPACE    TS_MAS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_EVENTO_DETALLE_TO IS 'TABLA QUE CONTIENE EL DETALLE PARA UN EVENTO DADO'
/
COMMENT ON COLUMN  GE_EVENTO_DETALLE_TO.DET_EVENTO IS 'NUMERO IDENTIFICADOR DEL DETALLE DE EVENTO'
/
COMMENT ON COLUMN  GE_EVENTO_DETALLE_TO.EVENTO IS 'NUMERO DEL EVENTO AL QUE SE RELACIONA EL DETALLE DE EVENTO'
/
COMMENT ON COLUMN  GE_EVENTO_DETALLE_TO.SERVICIO IS 'SERVICIO DONDE OCURRE EL EVENTO Y AL QUE PERTENECE EL DETALLE'
/
COMMENT ON COLUMN  GE_EVENTO_DETALLE_TO.QUERY IS 'UNA DE LAS OPERACIËNES DML EJECUTADAS EN EL SERVICIO'
/
COMMENT ON COLUMN  GE_EVENTO_DETALLE_TO.COD_ERROR IS 'CODIGO DE ERROR'
/
COMMENT ON COLUMN  GE_EVENTO_DETALLE_TO.DESCRIPCION_ERROR IS 'DESCRIPCION DEL ERROR'
/
