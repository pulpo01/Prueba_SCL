CREATE TABLE GE_LOGTABLE_TO(
    NIVEL VARCHAR2(5) NULL 
 ,  MAQUINA VARCHAR2(30) NULL 
 ,  FECHA DATE NULL 
 ,  USUARIO VARCHAR2(30) NULL 
 ,  PROGRAMA VARCHAR2(30) NULL 
 ,  MENSAJE VARCHAR2(4000) NULL 
 ,  ID_LOG NUMBER NULL 
 ) TABLESPACE    INSTALL_DATA
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_LOGTABLE_TO IS ' Tabla de log detalle'
/
COMMENT ON COLUMN  GE_LOGTABLE_TO.NIVEL IS 'nivel de log'
/
COMMENT ON COLUMN  GE_LOGTABLE_TO.MAQUINA IS 'maquina'
/
COMMENT ON COLUMN  GE_LOGTABLE_TO.FECHA IS 'fecha del log'
/
COMMENT ON COLUMN  GE_LOGTABLE_TO.USUARIO IS 'usuario asociado al log'
/
COMMENT ON COLUMN  GE_LOGTABLE_TO.PROGRAMA IS 'aplicación en debug'
/
COMMENT ON COLUMN  GE_LOGTABLE_TO.MENSAJE IS 'detalle debug'
/
