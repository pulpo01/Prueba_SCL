CREATE TABLE GE_DOMINIOS_TD(
    COD_DOMINIO VARCHAR2(20) NOT NULL 
 ,  TIPO NUMBER(1,0) NOT NULL 
 ,  NOMBRE VARCHAR2(100) NOT NULL 
 ,  FEC_CREACION DATE DEFAULT SYSDATE NOT NULL 
 ,  USU_CREACION VARCHAR2(30) DEFAULT USER NOT NULL 
 ,  FEC_MODIFICACION DATE DEFAULT SYSDATE NOT NULL 
 ,  USU_MODIFICACION VARCHAR2(30) DEFAULT USER NOT NULL 
 ) TABLESPACE    TS_MAS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_DOMINIOS_TD IS 'TABLA QUE CONTIENE LA DEFINICION DE DOMINIOS DE DATOS'
/
COMMENT ON COLUMN  GE_DOMINIOS_TD.COD_DOMINIO IS 'CODIGO IDENTIFICADOR DEL DOMINIO'
/
COMMENT ON COLUMN  GE_DOMINIOS_TD.TIPO IS 'CLASIFICACION DE DOMINIO 1=VALOR'
/
COMMENT ON COLUMN  GE_DOMINIOS_TD.NOMBRE IS 'DESCRIPCION DEL DOMINIO'
/
COMMENT ON COLUMN  GE_DOMINIOS_TD.FEC_CREACION IS 'FECHA EN QUE SE GENERA EL REGISTRO'
/
COMMENT ON COLUMN  GE_DOMINIOS_TD.USU_CREACION IS 'USUARIO DE BD QUE INSERTA EL REGISTRO'
/
COMMENT ON COLUMN  GE_DOMINIOS_TD.FEC_MODIFICACION IS 'FECHA EN QUE SE MODIFICA EL REGISTRO'
/
COMMENT ON COLUMN  GE_DOMINIOS_TD.USU_MODIFICACION IS 'USUARIO DE BD QUE MODIFICA EL REGISTRO'
/
