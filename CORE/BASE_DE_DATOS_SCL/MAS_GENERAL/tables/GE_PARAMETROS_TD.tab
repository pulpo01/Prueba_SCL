CREATE TABLE GE_PARAMETROS_TD(
    COD_PARAMETRO NUMBER(6,0) NOT NULL 
 ,  NOM_PARAMETRO VARCHAR2(20) NULL 
 ,  DES_PARAMETRO VARCHAR2(150) DEFAULT '1' NULL 
 ,  COD_MODULO VARCHAR2(2) NOT NULL 
 ,  TIPO_PARAMETRO VARCHAR2(2) NOT NULL 
 ,  VALOR_NUMERICO NUMBER(20,6) NULL 
 ,  VALOR_TEXTO VARCHAR2(250) NULL 
 ,  VALOR_FECHA DATE NULL 
 ,  TIPO_VALOR VARCHAR2(1) NULL 
 ,  VIGENCIA_DESDE DATE NULL 
 ,  VIGENCIA_HASTA DATE NULL 
 ,  COD_PARAMETRO_PADRE NUMBER(6,0) NULL 
 ,  VALOR_DOMINIO VARCHAR2(50) NULL 
 ,  COD_DOMINIO VARCHAR2(20) NULL 
 ,  COD_OPERADORA VARCHAR2(5) NULL 
 ,  IND_HIJOS NUMBER(1,0) DEFAULT 0 NOT NULL 
 ,  COD_DOMINIO_PADRE VARCHAR2(20) NULL 
 ) TABLESPACE    TS_MAS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_PARAMETROS_TD IS 'REGISTRO DE PARAMETROS UTILIZADOS POR LOS PRODUCTOS'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.COD_PARAMETRO IS 'CODIGO CORRELATIVO DE PARAMETRO. SE OBTIENE DESDE SECUENCIA GE_PARA_SQ'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.NOM_PARAMETRO IS 'NOMBRE DEL PARAMETRO'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.DES_PARAMETRO IS 'DESCRIPCION DEL PARAMETRO'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.COD_MODULO IS 'CODIGO DE MODULO PROPIETARIO DEL PARAMETRO'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.TIPO_PARAMETRO IS 'TIPO DE PARAMETRO: SS=SISTEMA SIMPLE, SM=SISTEMA MULTIPLE, MS=MODIFICABLE SIMPLE, MM=MODIFICABLE MULTIPLE'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.VALOR_NUMERICO IS 'VALOR DEL PARAMETRO SI ES UN DATO DE TIPO NUMERICO'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.VALOR_TEXTO IS 'VALOR DEL PARAMETRO SI ES UN DATO DE TIPO STRING'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.VALOR_FECHA IS 'VALOR DEL PARAMETRO SI ES UN DATO DE TIPO FECHA'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.TIPO_VALOR IS 'DETERMINA QUE TIPO DE VALOR CONTIENE EL PARAMETRO: N=NUMERICO, C=CARACTER(STRING) ,  D=FECHA U O=DOMINIO'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.VIGENCIA_DESDE IS 'FECHA DE INICIO DE VIGENCIA'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.VIGENCIA_HASTA IS 'FECHA DE TERMINO DE VIGENCIA'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.COD_PARAMETRO_PADRE IS 'CODIGO DE PARAMETRO PADRE EXISTENTE ASOCIADO A ESTE PARAMETRO. ESTE SE SETEA CUANDO IND_HIJOS = 1'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.VALOR_DOMINIO IS 'VALOR QUE CORRESPONDE A UN DOMINIO'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.COD_DOMINIO IS 'CODIGO DE DOMINIO AL QUE PERTENECE VALOR_DOMINIO'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.COD_OPERADORA IS 'CODIGO DE OPERADORA'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.IND_HIJOS IS 'DETERMINA SI EL PARAMETRO TIENE HIJOS. 0=SIN HIJOS O 1=TIENE HIJOS'
/
COMMENT ON COLUMN  GE_PARAMETROS_TD.COD_DOMINIO_PADRE IS 'CODIGO DE DOMINIO AL QUE PERTENECE EL REGISTRO PADRE'
/
