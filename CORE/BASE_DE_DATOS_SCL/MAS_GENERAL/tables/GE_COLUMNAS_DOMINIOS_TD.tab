CREATE TABLE GE_COLUMNAS_DOMINIOS_TD(
    COD_DOMINIO VARCHAR2(20) NOT NULL 
 ,  NOMBRE_TABLA VARCHAR2(50) NOT NULL 
 ,  NOMBRE_COLUMNA VARCHAR2(50) NOT NULL 
 ) TABLESPACE    TS_MAS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_COLUMNAS_DOMINIOS_TD IS 'TABLA QUE CONTIENE TABLA-COLUMNA QUE USA UN DOMINIO ESPECIFICO Y ES USADA PARA GE_DOMINIOS_TD.TIPO_DOMINIO IN (VALOR, RANGO)'
/
COMMENT ON COLUMN  GE_COLUMNAS_DOMINIOS_TD.COD_DOMINIO IS 'CODIGO IDENTIFICADOR DEL DOMINIO'
/
COMMENT ON COLUMN  GE_COLUMNAS_DOMINIOS_TD.NOMBRE_TABLA IS 'NOMBRE DE LA TABLA PROPIETARIA DE LA COLUMNA QUE SE DECLARA EN CAMPO COLUMNA_DOMINIO'
/
COMMENT ON COLUMN  GE_COLUMNAS_DOMINIOS_TD.NOMBRE_COLUMNA IS 'NOMBRE DE LA COLUMNA QUE USARA UN DOMINIO ESPECIFICO'
/
