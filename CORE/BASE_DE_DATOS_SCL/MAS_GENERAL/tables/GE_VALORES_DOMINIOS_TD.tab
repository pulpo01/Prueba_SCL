CREATE TABLE GE_VALORES_DOMINIOS_TD(
    COD_DOMINIO VARCHAR2(20) NOT NULL 
 ,  VALOR VARCHAR2(50) NOT NULL 
 ,  DESCRIPCION_VALOR VARCHAR2(50) NOT NULL 
 ,  FEC_CREACION DATE DEFAULT SYSDATE NOT NULL 
 ,  USU_CREACION VARCHAR2(30) DEFAULT USER NOT NULL 
 ,  FEC_MODIFICACION DATE DEFAULT SYSDATE NOT NULL 
 ,  USU_MODIFICACION VARCHAR2(30) DEFAULT USER NOT NULL 
 ,  IND_ESTADO VARCHAR2(2) DEFAULT 'V' NOT NULL 
 ) TABLESPACE    TS_MAS_GENERAL_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_VALORES_DOMINIOS_TD IS 'TABLA QUE CONTIENE LOS VALORES DE UN DOMINIO.TABLA USADA CUANDO GE_DOMINIOS_TD.TIPO_DOMINIO = VALOR'
/
COMMENT ON COLUMN  GE_VALORES_DOMINIOS_TD.COD_DOMINIO IS 'CODIGO IDENTIFICADOR DEL DOMINIO'
/
COMMENT ON COLUMN  GE_VALORES_DOMINIOS_TD.VALOR IS 'VALOR ASIGNADO AL DOMINIO'
/
COMMENT ON COLUMN  GE_VALORES_DOMINIOS_TD.DESCRIPCION_VALOR IS 'DESCRIPCION VALOR DE DOMINIO'
/
COMMENT ON COLUMN  GE_VALORES_DOMINIOS_TD.FEC_CREACION IS 'FECHA EN QUE SE GENERA EL REGISTRO'
/
COMMENT ON COLUMN  GE_VALORES_DOMINIOS_TD.USU_CREACION IS 'USUARIO DE BD QUE INSERTA EL REGISTRO'
/
COMMENT ON COLUMN  GE_VALORES_DOMINIOS_TD.FEC_MODIFICACION IS 'FECHA EN QUE SE MODIFICA EL REGISTRO'
/
COMMENT ON COLUMN  GE_VALORES_DOMINIOS_TD.USU_MODIFICACION IS 'USUARIO DE BD QUE MODIFICA EL REGISTRO'
/
COMMENT ON COLUMN  GE_VALORES_DOMINIOS_TD.IND_ESTADO IS 'CODIGO PARA IDENTIFICAR EL ESTADO DEL VALOR DE DOMINIO V=vigente N=no vigente '
/
