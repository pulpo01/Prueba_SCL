CREATE OR REPLACE PROCEDURE            MSG_UPDATE_WGED_MENSAJERIA (
pr_NUM_IDENT_O      in WGED_MENSAJERIA.NUM_IDENT_O%TYPE,
pr_COD_ESTADO       in WGED_MENSAJERIA.COD_ESTADO%TYPE,
pr_MOTIVO      in WGED_MENSAJERIA.MOTIVO%TYPE,
pr_FECHA           varchar2
)
as
BEGIN
update WGED_MENSAJERIA
set LEIDO = '1', COD_ESTADO = pr_COD_ESTADO, MOTIVO = pr_MOTIVO
where NUM_IDENT_O = pr_NUM_IDENT_O
and fecha = to_date(pr_FECHA, 'dd-mm-yyyy hh24:mi:ss');
END;
/
SHOW ERRORS
