CREATE TABLE NPT_PEDIDO_HIST_TH
(
  COD_PEDIDO            NUMBER(12)              NOT NULL,
  COD_CLIENTE           NUMBER(8),
  COD_BODEGA            NUMBER(6),
  COD_USUARIO_CRE       NUMBER(6),
  COD_USUARIO_ING       NUMBER(6),
  COD_USUARIO_EJE       NUMBER(6),
  COD_TIPO_EMBALAJE     NUMBER(2),
  COD_TIPO_PAGO         NUMBER(2),
  OBS_PEDIDO            VARCHAR2(100 BYTE),
  FEC_CRE_PEDIDO        DATE                    NOT NULL,
  FEC_TEN_ENT_PEDIDO    DATE                    NOT NULL,
  MTO_PAGO_PEDIDO       NUMBER(16,4),
  COD_SISTEMA_DESPACHO  NUMBER(2),
  CAN_TOTAL_PEDIDO      NUMBER(8),
  MTO_NETO_PEDIDO       NUMBER(16,4),
  MTO_DES_PEDIDO        NUMBER(16,4),
  MTO_IVA_PEDIDO        NUMBER(16,4),
  MTO_TOT_PEDIDO        NUMBER(16,4),
  GUIA_DES_PEDIDO       NUMBER(8),
  TIP_DOC_PEDIDO        NUMBER(2),
  NUM_DOC_PEDIDO        NUMBER(8),
  CAN_CAJA_PEDIDO       NUMBER(4),
  PES_UNI_PEDIDO        NUMBER(5,2),
  PES_TOT_PEDIDO        NUMBER(7,2),
  TIP_DEV_PEDIDO        CHAR(1 BYTE),
  COD_ORIGEN_PEDIDO     NUMBER(12),
  NUM_ORDEN_COMPRA      NUMBER(9),
  COD_VENDEDOR          NUMBER(6),
  IND_PROC              NUMBER(1)               DEFAULT 0,
  COD_PLAZA             VARCHAR2(5 BYTE),
  IND_TRASPASO          NUMBER(1)               DEFAULT 0,
  NUM_GUIA              VARCHAR2(25 BYTE),
  SALDO_DISP            NUMBER(15,5),
  COD_PROMOCION         NUMBER(8),
  MTO_OTROS_IMPUESTOS   NUMBER(16,4)
)
TABLESPACE EBZ_SISCEL_TTAB
PCTUSED    0
PCTFREE    20
INITRANS   4
MAXTRANS   255
STORAGE    (
            INITIAL          1M
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.NUM_GUIA IS 'Folio para la guía en traspasos con bodega destino requiere guia remision';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.SALDO_DISP IS 'Saldo disponible del cliente al momento de aprobar el pedido';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.COD_PROMOCION IS 'Código de promoción';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.COD_PEDIDO IS 'CODIGO DEL PEDIDO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.COD_CLIENTE IS 'CODIGO DEL CLIENTE';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.COD_BODEGA IS 'CODIGO BODEGA DESTINO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.COD_USUARIO_CRE IS 'CODIGO DE USUARIO CREADOR';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.COD_USUARIO_ING IS 'CODIGO DE USUARIO INGRESO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.COD_USUARIO_EJE IS 'CODIGO DE USUARIO EJECUTIVO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.COD_TIPO_EMBALAJE IS 'CODIGO TIPO EMBALAJE';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.COD_TIPO_PAGO IS 'CODIGO TIPO PAGO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.OBS_PEDIDO IS 'OBSERVACION DEL PEDIDO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.FEC_CRE_PEDIDO IS 'FECHA CREACION PEDIDO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.FEC_TEN_ENT_PEDIDO IS 'FECHA TENTATIVA DE ENTREGA DEL PEDIDO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.MTO_PAGO_PEDIDO IS 'MONTO TOTAL DE PAGO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.COD_SISTEMA_DESPACHO IS 'CODIGO SISTEMA DESPACHO ';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.CAN_TOTAL_PEDIDO IS 'CANTIDAD TOTAL DE ARTICULOS SOLICITADOS';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.MTO_NETO_PEDIDO IS 'MONTO NETO DEL PEDIDO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.MTO_DES_PEDIDO IS 'MONTO DESCUENTO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.MTO_IVA_PEDIDO IS 'MONTO IMPUESTO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.MTO_TOT_PEDIDO IS 'MONTO TOTAL DEL PEDIDO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.GUIA_DES_PEDIDO IS 'NUMERO DE GUIA DESPACHO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.TIP_DOC_PEDIDO IS 'TIPO DOCUMENTO PAGO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.NUM_DOC_PEDIDO IS 'NUMERO DOCUMENTO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.CAN_CAJA_PEDIDO IS 'CANTIDAD DE CAJAS O EMBALAJES';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.PES_UNI_PEDIDO IS 'PESO UNITARIO POR CAJA';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.PES_TOT_PEDIDO IS 'PESO TOTAL CAJAS';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.TIP_DEV_PEDIDO IS 'TIPO DEVOLUCION';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.COD_ORIGEN_PEDIDO IS 'CODIGO ORIGEN PEDIDO';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.NUM_ORDEN_COMPRA IS 'NUMERO ORDEN COMPRA';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.COD_VENDEDOR IS 'CODIGO VENDEDOR';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.IND_PROC IS 'INDICADOR DE PROCEDENCIA';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.COD_PLAZA IS 'CODIGO PLAZA';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.IND_TRASPASO IS 'Indicador de tipo Traspaso (Traspaso o E/S EXtra)';

COMMENT ON COLUMN NPT_PEDIDO_HIST_TH.MTO_OTROS_IMPUESTOS IS 'Monto de otros impuestos distintos a iva';