CREATE TABLE NP_CONFIG_EVENTO_TD(
    COD_ACTABO VARCHAR2(2) NOT NULL 
 ,  ESTADO_INI VARCHAR2(30) NOT NULL 
 ,  ESTADO_FINAL VARCHAR2(30) NOT NULL 
 ,  COD_MOTIVO NUMBER(6,0) NOT NULL 
 ,  IND_NOTA_FACT VARCHAR2(2) NULL 
 ,  USUA_CREACION VARCHAR2(30) NOT NULL 
 ,  FEC_CREACION DATE NOT NULL 
 ,  COD_CAUCASER VARCHAR2(2) NOT NULL 
 ,  USU_ACTUALIZA VARCHAR2(30) NULL 
 ,  FEC_ACTUALIZA DATE NULL 
 ) TABLESPACE    INVENTARIO_TTAB
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  NP_CONFIG_EVENTO_TD IS 'Configuración de eventos/estado series/causas'
/
COMMENT ON COLUMN  NP_CONFIG_EVENTO_TD.COD_ACTABO IS 'Código de actuación'
/
COMMENT ON COLUMN  NP_CONFIG_EVENTO_TD.ESTADO_INI IS 'Estado inicial de la serie'
/
COMMENT ON COLUMN  NP_CONFIG_EVENTO_TD.ESTADO_FINAL IS 'Estado final de la serie'
/
COMMENT ON COLUMN  NP_CONFIG_EVENTO_TD.COD_MOTIVO IS 'Codigo del motivo de cambio de estado de la serie'
/
COMMENT ON COLUMN  NP_CONFIG_EVENTO_TD.IND_NOTA_FACT IS 'Indicador si aplica factura miscelanea (FM) o nota credito (NC)'
/
COMMENT ON COLUMN  NP_CONFIG_EVENTO_TD.USUA_CREACION IS 'Usuario que crea el registro'
/
COMMENT ON COLUMN  NP_CONFIG_EVENTO_TD.FEC_CREACION IS 'Fecha de creación del registro'
/
COMMENT ON COLUMN  NP_CONFIG_EVENTO_TD.COD_CAUCASER IS 'Causa de cambio serie/Simcard,restitucion o baja'
/
COMMENT ON COLUMN  NP_CONFIG_EVENTO_TD.USU_ACTUALIZA IS 'Usuario que actualiza el registro'
/
COMMENT ON COLUMN  NP_CONFIG_EVENTO_TD.FEC_ACTUALIZA IS 'Fecha de actualización del registro'
/
