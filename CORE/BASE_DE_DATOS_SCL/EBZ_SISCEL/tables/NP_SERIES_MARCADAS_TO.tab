CREATE TABLE NP_SERIES_MARCADAS_TO(
    IND_KEY NUMBER NOT NULL 
 ,  LIN_DET_PEDIDO NUMBER(2,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NULL 
 ,  COD_ARTICULO NUMBER(6,0) NULL 
 ,  TIPO_MARCA NUMBER(2,0) NULL 
 ,  MTO_DIFSERIE NUMBER(16,4) NULL 
 ,  FEC_CARGA DATE NULL 
 ) TABLESPACE    EBZ_SISCEL_TTAB
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  NP_SERIES_MARCADAS_TO IS ' Tabla que registra la información básica de una venta realizada para un mayorista.'
/
COMMENT ON COLUMN  NP_SERIES_MARCADAS_TO.IND_KEY IS 'Código para formar llave, Secuencia'
/
COMMENT ON COLUMN  NP_SERIES_MARCADAS_TO.LIN_DET_PEDIDO IS 'Línea de detalle del pedido'
/
COMMENT ON COLUMN  NP_SERIES_MARCADAS_TO.NUM_SERIE IS 'Número de Serie'
/
COMMENT ON COLUMN  NP_SERIES_MARCADAS_TO.COD_CLIENTE IS 'Código del Cliente Mayorista'
/
COMMENT ON COLUMN  NP_SERIES_MARCADAS_TO.COD_ARTICULO IS 'Código del artículo  '
/
COMMENT ON COLUMN  NP_SERIES_MARCADAS_TO.TIPO_MARCA IS 'Tipo de la marca  1: Serie Seleccionada '
/
COMMENT ON COLUMN  NP_SERIES_MARCADAS_TO.MTO_DIFSERIE IS 'Monto diferencia de la serie'
/
COMMENT ON COLUMN  NP_SERIES_MARCADAS_TO.FEC_CARGA IS 'Fecha de carga de la serie a descontar'
/
