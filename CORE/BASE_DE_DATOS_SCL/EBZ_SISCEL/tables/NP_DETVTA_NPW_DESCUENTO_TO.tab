CREATE TABLE NP_DETVTA_NPW_DESCUENTO_TO(
    COD_VTA_MAY NUMBER NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  COD_PEDIDO_ORIGEN NUMBER(12,0) NULL 
 ,  LIN_DET_PEDIDO NUMBER(2,0) NOT NULL 
 ,  MTO_NETO_PED NUMBER(16,4) NULL 
 ,  PJE_DESC_CLIE NUMBER(7,4) NULL 
 ,  MTO_NETO_CDES NUMBER(16,4) NULL 
 ,  MTO_NETO_SCL NUMBER(16,4) NULL 
 ,  MTO_DESC_MAY NUMBER(16,4) NULL 
 ) TABLESPACE    EBZ_SISCEL_TTAB
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  NP_DETVTA_NPW_DESCUENTO_TO IS 'Tabla de detalle para venta de NPW con descuento.'
/
COMMENT ON COLUMN  NP_DETVTA_NPW_DESCUENTO_TO.COD_VTA_MAY IS 'Código venta mayorista   '
/
COMMENT ON COLUMN  NP_DETVTA_NPW_DESCUENTO_TO.NUM_SERIE IS 'Número de serie del artículo'
/
COMMENT ON COLUMN  NP_DETVTA_NPW_DESCUENTO_TO.COD_PEDIDO_ORIGEN IS 'Código del pedido donde se realizó venta'
/
COMMENT ON COLUMN  NP_DETVTA_NPW_DESCUENTO_TO.LIN_DET_PEDIDO IS 'Línea de detalle del pedido'
/
COMMENT ON COLUMN  NP_DETVTA_NPW_DESCUENTO_TO.MTO_NETO_PED IS 'Valor neto del artículo ingresado en el pedido de origen'
/
COMMENT ON COLUMN  NP_DETVTA_NPW_DESCUENTO_TO.PJE_DESC_CLIE IS 'Porcentaje de descuento asociado al cliente en el pedido de origen'
/
COMMENT ON COLUMN  NP_DETVTA_NPW_DESCUENTO_TO.MTO_NETO_CDES IS 'Valor neto del artículo ingresado en el pedido de origen, aplicando los descuentos del cliente'
/
COMMENT ON COLUMN  NP_DETVTA_NPW_DESCUENTO_TO.MTO_NETO_SCL IS 'Valor unitario del artículo, matriz de SCL'
/
COMMENT ON COLUMN  NP_DETVTA_NPW_DESCUENTO_TO.MTO_DESC_MAY IS 'Valor de descuento por activaciones'
/
