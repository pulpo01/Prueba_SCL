CREATE TABLE NPT_REGISTRO_EXCEDENTE_TD
(
  COD_DISTRIBUIDOR           NUMBER(2)          NOT NULL,
  COD_ARTICULO               NUMBER(6)          NOT NULL,
  CANTIDAD_PROYECTADA        NUMBER(9)          DEFAULT 0,
  CANTIDAD_CONSUMIDA         NUMBER(9)          DEFAULT 0,
  CANTIDAD_PROYECTADA_CANAL  NUMBER(9)          DEFAULT 0,
  CANTIDAD_PROY_IND_CANAL    NUMBER(9)          DEFAULT 0,
  CANTIDAD_SOLICITADA        NUMBER(9)          DEFAULT 0                     NOT NULL,
  SALDO_INDIVIDUAL           NUMBER(9)          DEFAULT 0,
  SALDO_SOBRESTOCK           NUMBER(9)          DEFAULT 0,
  USU_CREACION               VARCHAR2(30 BYTE)  DEFAULT USER                  NOT NULL,
  FEC_CREACION               DATE               DEFAULT SYSDATE               NOT NULL
)
TABLESPACE EBZ_SISCEL_TTAB
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          128K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON COLUMN NPT_REGISTRO_EXCEDENTE_TD.COD_DISTRIBUIDOR IS 'Código de distribuidor';

COMMENT ON COLUMN NPT_REGISTRO_EXCEDENTE_TD.COD_ARTICULO IS 'Código de artículo';

COMMENT ON COLUMN NPT_REGISTRO_EXCEDENTE_TD.CANTIDAD_PROYECTADA IS 'Cantidad proyectada';

COMMENT ON COLUMN NPT_REGISTRO_EXCEDENTE_TD.CANTIDAD_CONSUMIDA IS 'Cantidad consumida';

COMMENT ON COLUMN NPT_REGISTRO_EXCEDENTE_TD.CANTIDAD_PROYECTADA_CANAL IS 'Cantidad proyectada por canal (sobre-stock por artículo y canal)';

COMMENT ON COLUMN NPT_REGISTRO_EXCEDENTE_TD.CANTIDAD_PROY_IND_CANAL IS 'Cantidad proyectada individual por canal (calculada en el momento)';

COMMENT ON COLUMN NPT_REGISTRO_EXCEDENTE_TD.CANTIDAD_SOLICITADA IS 'Cantidad solicitada';

COMMENT ON COLUMN NPT_REGISTRO_EXCEDENTE_TD.SALDO_INDIVIDUAL IS 'Saldo individual(por proyección de venta individual)';

COMMENT ON COLUMN NPT_REGISTRO_EXCEDENTE_TD.SALDO_SOBRESTOCK IS 'Saldo con sobre-stock (por proyección de venta individual + disponible individual por sobre-stock )';

COMMENT ON COLUMN NPT_REGISTRO_EXCEDENTE_TD.USU_CREACION IS 'Usuario creación';

COMMENT ON COLUMN NPT_REGISTRO_EXCEDENTE_TD.FEC_CREACION IS 'Fecha Actual';
/

