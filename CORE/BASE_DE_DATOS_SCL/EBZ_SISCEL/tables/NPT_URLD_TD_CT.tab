CREATE TABLE NPT_URL_TD
(
  DIR_URL      VARCHAR2(512 BYTE)               NOT NULL,
  HOST         VARCHAR2(100 BYTE)                NOT NULL,
  PORT         NUMBER(6)                        NOT NULL,
  METODO       VARCHAR2(100 BYTE)               NOT NULL,
  DESCRIPCION  VARCHAR2(200 BYTE)
)
TABLESPACE EBZ_SISCEL_TTAB
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          128K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOLOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING
/


COMMENT ON COLUMN EBZ_SISCEL.NPT_URL_TD.DIR_URL IS 'Direccion WSDL'
/
COMMENT ON COLUMN EBZ_SISCEL.NPT_URL_TD.HOST IS 'Servidor '
/
COMMENT ON COLUMN EBZ_SISCEL.NPT_URL_TD.PORT IS 'Puerto'
/
COMMENT ON COLUMN EBZ_SISCEL.NPT_URL_TD.METODO IS 'NOMBRE DE METODO'
/
COMMENT ON COLUMN EBZ_SISCEL.NPT_URL_TD.DESCRIPCION IS 'Descripcion del servicio'
/