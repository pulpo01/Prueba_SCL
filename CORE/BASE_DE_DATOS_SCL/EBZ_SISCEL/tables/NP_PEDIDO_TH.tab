 ,  COD_BODEGA NUMBER(6,0) NOT NULL 
 ,  COD_USUARIO_ING NUMBER(6,0) NOT NULL 
 ,  FECHA_CAMBIO DATE NOT NULL 
 ,  CAN_TOTAL_PEDIDO NUMBER(8,0) NOT NULL 
 ,  MTO_NETO_PEDIDO NUMBER(16,4) NOT NULL 
 ,  MTO_DES_PEDIDO NUMBER(16,4) NOT NULL 
 ,  MTO_IVA_PEDIDO NUMBER(16,4) NOT NULL 
 ,  MTO_TOT_PEDIDO NUMBER(16,4) NOT NULL 
 ,  CORRELATIVO_CAMBIO NUMBER(3,0) NOT NULL 
 ) TABLESPACE    EBZ_SISCEL_TTAB
 NOLOGGING
 NOCACHE
 NOPARALLEL
/
