CREATE TABLE NP_HIST_MOVIM_SERIES_TO(
    COD_CLIENTE NUMBER(8,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  FEC_MOVIMIENTO DATE NOT NULL 
 ,  COD_ESTADO VARCHAR2(10) NULL 
 ,  COD_MOTIVO NUMBER NULL 
 ,  COD_PEDIDO NUMBER(12,0) NULL 
 ,  COD_USUARIO VARCHAR2(20) NULL 
 ,  COD_ESTADOORIGEN VARCHAR2(10) NULL 
 ) TABLESPACE    EBZ_SISCEL_TTAB
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  NP_HIST_MOVIM_SERIES_TO IS 'Tabla histórica de los movimientos de las series asociadas a ventas de mayoristas'
/
COMMENT ON COLUMN  NP_HIST_MOVIM_SERIES_TO.COD_PEDIDO IS 'Código del Pedido.'
/
COMMENT ON COLUMN  NP_HIST_MOVIM_SERIES_TO.COD_USUARIO IS 'Valor neto del artículo según matriz de NPW'
/
COMMENT ON COLUMN  NP_HIST_MOVIM_SERIES_TO.COD_ESTADOORIGEN IS 'Código que identificará el origen del movimiento'
/
COMMENT ON COLUMN  NP_HIST_MOVIM_SERIES_TO.COD_CLIENTE IS 'Código del Cliente Mayorista'
/
COMMENT ON COLUMN  NP_HIST_MOVIM_SERIES_TO.NUM_SERIE IS 'Número de serie del artículo'
/
COMMENT ON COLUMN  NP_HIST_MOVIM_SERIES_TO.FEC_MOVIMIENTO IS 'Fecha del movimiento'
/
COMMENT ON COLUMN  NP_HIST_MOVIM_SERIES_TO.COD_ESTADO IS 'Código del estado NU: Nuevo, PE: Pendiente, AP: Aplicado, ND: No disponible, ER: En revisión'
/
COMMENT ON COLUMN  NP_HIST_MOVIM_SERIES_TO.COD_MOTIVO IS 'Código de motivo del cambio'
/
