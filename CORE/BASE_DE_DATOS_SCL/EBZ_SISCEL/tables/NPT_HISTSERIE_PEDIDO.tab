CREATE TABLE NPT_HISTSERIE_PEDIDO(
    COD_PEDIDO NUMBER(12,0) NOT NULL 
 ,  LIN_DET_PEDIDO NUMBER(2,0) NOT NULL 
 ,  COD_SERIE_PEDIDO VARCHAR2(25) NOT NULL 
 ,  COD_BAR_SER_PEDIDO VARCHAR2(20) NOT NULL 
 ,  FEC_TRASPASO DATE NOT NULL 
 ) TABLESPACE    EBZ_SISCEL_TTAB
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  NPT_HISTSERIE_PEDIDO IS 'Series Asociadas Al Pedido.'
/
COMMENT ON COLUMN  NPT_HISTSERIE_PEDIDO.COD_PEDIDO IS 'Codigo del pedido.'
/
COMMENT ON COLUMN  NPT_HISTSERIE_PEDIDO.LIN_DET_PEDIDO IS 'Linea detalle pedido.'
/
COMMENT ON COLUMN  NPT_HISTSERIE_PEDIDO.COD_SERIE_PEDIDO IS 'Codigo de serie pedido.'
/
COMMENT ON COLUMN  NPT_HISTSERIE_PEDIDO.COD_BAR_SER_PEDIDO IS 'Codigo de barras.'
/
COMMENT ON COLUMN  NPT_HISTSERIE_PEDIDO.FEC_TRASPASO IS 'Fecha de Traspaso a Historico.'
/
