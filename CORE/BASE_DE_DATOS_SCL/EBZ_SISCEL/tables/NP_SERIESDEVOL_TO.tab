CREATE TABLE EBZ_SISCEL.NP_SERIESDEVOL_TO
(
  COD_PEDIDO          NUMBER(12)                NOT NULL,
  COD_SERIES          VARCHAR2(4000 BYTE),
  COD_DEVPROEXI       VARCHAR2(2 BYTE)          DEFAULT '91',
  COD_INDKITACT       VARCHAR2(2 BYTE)          DEFAULT '06',
  DES_SERIEAUX        VARCHAR2(4000 BYTE),
  DES_SERIENOPEDIDO   VARCHAR2(4000 BYTE),
  DES_SERIEDEVUELTAS  VARCHAR2(4000 BYTE),
  DES_SERIEKITACTIVO  VARCHAR2(4000 BYTE),
  MSG_ERROR           VARCHAR2(200 BYTE),
  COD_CORRELATIVO     NUMBER(2)                 NOT NULL,
  DES_SERIEDUPLICADA  VARCHAR2(4000 BYTE),
  DES_SERABONOACTIVO  VARCHAR2(4000 BYTE),
  COD_CORR_DEVOL      NUMBER(4)                 DEFAULT 0                     NOT NULL
)  TABLESPACE EBZ_SISCEL_TTAB
/

COMMENT ON TABLE EBZ_SISCEL.NP_SERIESDEVOL_TO IS '[SERIESDEVOL] Tabla de paso de series devueltasTabla de paso de series devueltas';

COMMENT ON COLUMN EBZ_SISCEL.NP_SERIESDEVOL_TO.DES_SERABONOACTIVO IS 'Detalle de series del abonado no activo en la devolución';

COMMENT ON COLUMN EBZ_SISCEL.NP_SERIESDEVOL_TO.COD_CORR_DEVOL IS 'Correlativo de agrupación de series soportadas en la devolución';

COMMENT ON COLUMN EBZ_SISCEL.NP_SERIESDEVOL_TO.COD_PEDIDO IS 'codigo del pedido';

COMMENT ON COLUMN EBZ_SISCEL.NP_SERIESDEVOL_TO.COD_SERIES IS 'cadena de series';

COMMENT ON COLUMN EBZ_SISCEL.NP_SERIESDEVOL_TO.COD_DEVPROEXI IS 'codigo de devolucion exitosa';

COMMENT ON COLUMN EBZ_SISCEL.NP_SERIESDEVOL_TO.COD_INDKITACT IS 'codigo de indicador de kit activo';

COMMENT ON COLUMN EBZ_SISCEL.NP_SERIESDEVOL_TO.DES_SERIEAUX IS 'cadena de series auxiliar';

COMMENT ON COLUMN EBZ_SISCEL.NP_SERIESDEVOL_TO.DES_SERIENOPEDIDO IS 'descripcion de numero series devueltas';

COMMENT ON COLUMN EBZ_SISCEL.NP_SERIESDEVOL_TO.DES_SERIEDEVUELTAS IS 'descripcion de series devueltas';

COMMENT ON COLUMN EBZ_SISCEL.NP_SERIESDEVOL_TO.DES_SERIEKITACTIVO IS 'descripcion de kit activos';

COMMENT ON COLUMN EBZ_SISCEL.NP_SERIESDEVOL_TO.MSG_ERROR IS 'mensaje de error';

COMMENT ON COLUMN EBZ_SISCEL.NP_SERIESDEVOL_TO.COD_CORRELATIVO IS 'Detalle del correlativo del grupo a validar';

COMMENT ON COLUMN EBZ_SISCEL.NP_SERIESDEVOL_TO.DES_SERIEDUPLICADA IS 'Detalle de series duplicadas en la Devolución';

