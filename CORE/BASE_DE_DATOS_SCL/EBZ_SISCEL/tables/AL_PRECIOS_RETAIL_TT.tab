CREATE TABLE AL_PRECIOS_RETAIL_TT(
    NUM_PROCESO NUMBER(9,0) NOT NULL 
 ,  COD_VENDEDOR NUMBER(6,0) NOT NULL 
 ,  COD_ARTICULO NUMBER(6,0) NOT NULL 
 ,  PRECIO_ARTICULO NUMBER(14,4) NOT NULL 
 ,  COD_USO NUMBER(2,0) NOT NULL 
 ,  COD_CATEGORIA VARCHAR2(3) NOT NULL 
 ,  NUM_MESES NUMBER(2,0) NOT NULL 
 ,  IND_RECAMBIO NUMBER(1,0) NOT NULL 
 ,  COD_ESTADO NUMBER(2,0) NOT NULL 
 ,  COD_ERROR NUMBER(6,0) DEFAULT 0 NOT NULL 
 ) TABLESPACE    TS_LOGISTICA_SDAT
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  AL_PRECIOS_RETAIL_TT IS '[ AL_PRRETAIL ] Tabla temporal para el proceso de carga de precios retail'
/
COMMENT ON COLUMN  AL_PRECIOS_RETAIL_TT.NUM_PROCESO IS 'Número de Proceso'
/
COMMENT ON COLUMN  AL_PRECIOS_RETAIL_TT.COD_VENDEDOR IS 'Código Vendedor'
