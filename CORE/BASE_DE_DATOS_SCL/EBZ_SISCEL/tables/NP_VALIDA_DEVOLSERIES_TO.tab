CREATE TABLE NP_VALIDA_DEVOLSERIES_TO(
    COD_PEDIDO NUMBER(12,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  COD_ERROR NUMBER(2,0) NOT NULL 
 ,  DES_VALIDACION VARCHAR2(25) NULL 
 ,  FEC_VALIDA DATE NULL 
 ) TABLESPACE    EBZ_SISCEL_TTAB
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  NP_VALIDA_DEVOLSERIES_TO IS 'Validación de devolución de Series.'
/
COMMENT ON COLUMN  NP_VALIDA_DEVOLSERIES_TO.COD_PEDIDO IS 'Código del pedido a Validar'
/
COMMENT ON COLUMN  NP_VALIDA_DEVOLSERIES_TO.NUM_SERIE IS 'Número de serie a Validar'
/
COMMENT ON COLUMN  NP_VALIDA_DEVOLSERIES_TO.COD_ERROR IS 'Código de Error. 0:Sin errores, 1: No pertenece al pedido, 2: Serie ya Devuelta, 3: Kit Activo, 4:Serie Duplicada'
/
COMMENT ON COLUMN  NP_VALIDA_DEVOLSERIES_TO.DES_VALIDACION IS 'Descripción de la validación de la serie'
/
COMMENT ON COLUMN  NP_VALIDA_DEVOLSERIES_TO.FEC_VALIDA IS 'Fecha de validación.'
/
