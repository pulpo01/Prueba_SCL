CREATE TABLE NP_PARAM_ESTADOS_TO(
    COD_MOTIVO NUMBER NOT NULL 
 ,  DES_MOTIVO VARCHAR2(100) NULL 
 ,  TIP_ESTADO VARCHAR2(1) NULL 
 ) TABLESPACE    EBZ_SISCEL_TTAB
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  NP_PARAM_ESTADOS_TO IS '[ PAES ] Tabla que registra la información de los motivos de cambio de serie'
/
COMMENT ON COLUMN  NP_PARAM_ESTADOS_TO.COD_MOTIVO IS 'Código del motivo del parámetro'
/
COMMENT ON COLUMN  NP_PARAM_ESTADOS_TO.DES_MOTIVO IS 'Descripción del motivo del parámetro'
/
COMMENT ON COLUMN  NP_PARAM_ESTADOS_TO.TIP_ESTADO IS 'Estado de vigencia del parámetro  (V,N)'
/
