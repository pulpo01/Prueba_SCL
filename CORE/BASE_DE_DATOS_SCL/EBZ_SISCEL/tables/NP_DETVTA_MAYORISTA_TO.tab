CREATE TABLE NP_DETVTA_MAYORISTA_TO(
    COD_VTA_MAY NUMBER NOT NULL 
 ,  COD_CLIENTE NUMBER(8,0) NULL 
 ,  COD_PEDIDO NUMBER(12,0) NULL 
 ,  COD_ARTICULO NUMBER(6,0) NULL 
 ,  LIN_DET_PEDIDO NUMBER(2,0) NOT NULL 
 ,  CAN_ARTICULO NUMBER(6,0) NULL 
 ,  MTO_NETO_NPW NUMBER(16,4) NULL 
 ,  PJE_DESC_CLIE NUMBER(7,4) NULL 
 ,  MTO_NETO_PED NUMBER(16,4) NULL 
 ,  MTO_DESC_MAY NUMBER(16,4) NULL 
 ) TABLESPACE    EBZ_SISCEL_TTAB
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  NP_DETVTA_MAYORISTA_TO IS 'Tabla resumen para identificar venta de NPW con descuentos por venta de mayorista'
/
COMMENT ON COLUMN  NP_DETVTA_MAYORISTA_TO.COD_VTA_MAY IS 'Código venta mayorista   '
/
COMMENT ON COLUMN  NP_DETVTA_MAYORISTA_TO.COD_CLIENTE IS 'Código del Cliente Mayorista'
/
COMMENT ON COLUMN  NP_DETVTA_MAYORISTA_TO.COD_PEDIDO IS 'Código del Pedido.'
/
COMMENT ON COLUMN  NP_DETVTA_MAYORISTA_TO.COD_ARTICULO IS 'Código del artículo'
/
COMMENT ON COLUMN  NP_DETVTA_MAYORISTA_TO.LIN_DET_PEDIDO IS 'Línea de detalle del pedido'
/
COMMENT ON COLUMN  NP_DETVTA_MAYORISTA_TO.CAN_ARTICULO IS 'Cantidad de artículos vendidos'
/
COMMENT ON COLUMN  NP_DETVTA_MAYORISTA_TO.MTO_NETO_NPW IS 'Valor neto del artículo según matriz de NPW'
/
COMMENT ON COLUMN  NP_DETVTA_MAYORISTA_TO.PJE_DESC_CLIE IS 'Porcentaje de descuento asociado al cliente'
/
COMMENT ON COLUMN  NP_DETVTA_MAYORISTA_TO.MTO_NETO_PED IS 'Valor neto del artículo según matriz de NPW, menos los decuentos asociados al cliente'
/
COMMENT ON COLUMN  NP_DETVTA_MAYORISTA_TO.MTO_DESC_MAY IS 'Porcentaje de decuento asociado a las activaciones'
/
