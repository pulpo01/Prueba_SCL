CREATE TABLE NPT_PROM_CLIENTE_TH (  
   COD_PROMOCION                 NUMBER  (8)                                     NOT NULL  
 , COD_USUARIO                   NUMBER  (6)                                     NOT NULL  
 , FEC_CREACION                  DATE                                         NOT NULL  
 , USUA_CREACION                 VARCHAR2(30)                                     NOT NULL  
 , FEC_ACTUALIZA                 DATE                                           
 , USUA_ACTUALIZA                VARCHAR2(30)                                       
 , COD_ACCION                    VARCHAR2(1)                                     NOT NULL
 ) TABLESPACE  INSTALL_DATA   
/


COMMENT ON TABLE  NPT_PROM_CLIENTE_TH                      IS '[ PRCH ] Historico promociones por cliente'
/

COMMENT ON COLUMN NPT_PROM_CLIENTE_TH.COD_PROMOCION                  IS 'Código de promoción'
/
COMMENT ON COLUMN NPT_PROM_CLIENTE_TH.COD_USUARIO                    IS 'Código de usuario NPW'
/
COMMENT ON COLUMN NPT_PROM_CLIENTE_TH.FEC_CREACION                   IS 'Fecha de creación del registro'
/
COMMENT ON COLUMN NPT_PROM_CLIENTE_TH.USUA_CREACION                  IS 'Usuario que crea el registro'
/
COMMENT ON COLUMN NPT_PROM_CLIENTE_TH.FEC_ACTUALIZA                  IS 'Fecha de actualización del registro'
/
COMMENT ON COLUMN NPT_PROM_CLIENTE_TH.USUA_ACTUALIZA                 IS 'Usuario que actualiza el registro'
/
COMMENT ON COLUMN NPT_PROM_CLIENTE_TH.COD_ACCION                     IS 'Código de accion sobre registro: I=Insert,D=Delete,U=Update'
/