CREATE TABLE NP_VALIDACION_SERIES_TO(
    COD_PEDIDO NUMBER(12,0) NOT NULL 
 ,  LIN_DET_PEDIDO NUMBER(2,0) NOT NULL 
 ,  NUM_SERIE VARCHAR2(25) NOT NULL 
 ,  COD_BARRA_SERIE VARCHAR2(20) NULL 
 ,  ESTADO VARCHAR2(10) NULL 
 ,  COD_ERROR NUMBER(2,0) NULL 
 ) TABLESPACE    EBZ_SISCEL_TTAB
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  NP_VALIDACION_SERIES_TO IS 'TABLA DE VALIDACION DE SERIES'
/
COMMENT ON COLUMN  NP_VALIDACION_SERIES_TO.COD_PEDIDO IS 'CODIGO DEL PEDIDO'
/
COMMENT ON COLUMN  NP_VALIDACION_SERIES_TO.LIN_DET_PEDIDO IS 'LINEA DETALLE DEL PEDIDO'
/
COMMENT ON COLUMN  NP_VALIDACION_SERIES_TO.NUM_SERIE IS 'NUMERO DE LA SERIE DEL ARTICULO DEL PEDIDO'
/
COMMENT ON COLUMN  NP_VALIDACION_SERIES_TO.COD_BARRA_SERIE IS 'CODIGO DE BARRA'
/
COMMENT ON COLUMN  NP_VALIDACION_SERIES_TO.ESTADO IS 'ESTADO DE VALIDACION DE LA SERIE'
/
COMMENT ON COLUMN  NP_VALIDACION_SERIES_TO.COD_ERROR IS 'CODIGO DE ERROR DE VALIDACION'
/
