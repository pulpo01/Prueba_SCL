CREATE TABLE GE_COLA_PETICIONES_TO(
    NUM_TICKET NUMBER(10,0) NOT NULL 
 ,  COD_ESTADO VARCHAR2(2) NULL 
 ,  FECHA_CREACION DATE DEFAULT SYSDATE NOT NULL 
 ,  FECHA_CIERRE DATE NULL 
 ,  NUM_PROCESO NUMBER(9,0) NULL 
 ,  COD_SERVICIO VARCHAR2(10) NULL 
 ,  LINEA_COMANDO VARCHAR2(1024) NULL 
 ) TABLESPACE    FAC_SISCEL_DATA
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  GE_COLA_PETICIONES_TO IS 'Tabla de cola de peticiones para modelo de auditoria'
/
COMMENT ON COLUMN  GE_COLA_PETICIONES_TO.NUM_TICKET IS 'Numero de Ticket'
/
COMMENT ON COLUMN  GE_COLA_PETICIONES_TO.COD_ESTADO IS 'Codigo de estado del Ticket'
/
COMMENT ON COLUMN  GE_COLA_PETICIONES_TO.FECHA_CREACION IS 'Fecha de Creación del registro'
/
COMMENT ON COLUMN  GE_COLA_PETICIONES_TO.FECHA_CIERRE IS 'Fecha de cierre del registro'
/
COMMENT ON COLUMN  GE_COLA_PETICIONES_TO.NUM_PROCESO IS 'Numero de proceso asociado'
/
COMMENT ON COLUMN  GE_COLA_PETICIONES_TO.COD_SERVICIO IS 'Codigo de servicio'
/
COMMENT ON COLUMN  GE_COLA_PETICIONES_TO.LINEA_COMANDO IS 'Linea de commando a ejecutar'
/
