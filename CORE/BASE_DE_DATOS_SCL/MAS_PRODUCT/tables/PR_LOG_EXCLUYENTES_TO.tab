CREATE TABLE MAS_PRODUCT.PR_LOG_EXCLUYENTES_TO
(
  NUM_OS               NUMBER(10)               NOT NULL,
  COD_CLIENTE          NUMBER(8)                NOT NULL,
  NUM_ABONADO          NUMBER(8)                NOT NULL,
  USUARIO_OS           VARCHAR2(30 BYTE)        NOT NULL,
  FECHA                DATE                     NOT NULL,
  ID_PROD_OFERTADO     VARCHAR2(10 BYTE)        NOT NULL,
  TIPO_COD_EXCLUYENTE  NUMBER(2)                NOT NULL,
  COD_EXCLUYENTE       VARCHAR2(10 BYTE)        NOT NULL
)
TABLESPACE MAS_PRODUCT_DATA
PCTUSED    0
PCTFREE    5
INITRANS   16
MAXTRANS   255
STORAGE    (
            INITIAL          200K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
NOMONITORING;

COMMENT ON TABLE MAS_PRODUCT.PR_LOG_EXCLUYENTES_TO IS '[ LEXC ] tabla de registro inconsistencia de exclusion';

COMMENT ON COLUMN MAS_PRODUCT.PR_LOG_EXCLUYENTES_TO.NUM_OS IS 'numero orden de servicio';

COMMENT ON COLUMN MAS_PRODUCT.PR_LOG_EXCLUYENTES_TO.COD_CLIENTE IS 'codigo cliente';

COMMENT ON COLUMN MAS_PRODUCT.PR_LOG_EXCLUYENTES_TO.NUM_ABONADO IS 'numero abonado';

COMMENT ON COLUMN MAS_PRODUCT.PR_LOG_EXCLUYENTES_TO.USUARIO_OS IS 'usuario quien realizo la OS';

COMMENT ON COLUMN MAS_PRODUCT.PR_LOG_EXCLUYENTES_TO.FECHA IS 'Fecha de proceso';

COMMENT ON COLUMN MAS_PRODUCT.PR_LOG_EXCLUYENTES_TO.ID_PROD_OFERTADO IS 'id producto ofertado';

COMMENT ON COLUMN MAS_PRODUCT.PR_LOG_EXCLUYENTES_TO.TIPO_COD_EXCLUYENTE IS 'tipo de exclusion';

COMMENT ON COLUMN MAS_PRODUCT.PR_LOG_EXCLUYENTES_TO.COD_EXCLUYENTE IS 'id especificacion de producto';
