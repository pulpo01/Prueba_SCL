CREATE TABLE PS_ESPECIFICACIONES_PROD_TD(
    COD_ESPEC_PROD NUMBER(38,0) NOT NULL 
 ,  ID_ESPEC_PROD VARCHAR2(10) NOT NULL 
 ,  DES_ESPEC_PROD VARCHAR2(30) NULL 
 ,  FEC_INICIO_VIGENCIA DATE NOT NULL 
 ,  FEC_TERMINO_VIGENCIA DATE NULL 
 ,  TIPO_PLATAFORMA VARCHAR2(5) NOT NULL 
 ,  TIPO_COMPORTAMIENTO VARCHAR2(5) NOT NULL 
 ) TABLESPACE    MAS_PRODUCT_DATA
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PS_ESPECIFICACIONES_PROD_TD IS '[ ESPR ] Espeficaciones de Productos que se comercializarán'
/
COMMENT ON COLUMN  PS_ESPECIFICACIONES_PROD_TD.COD_ESPEC_PROD IS 'Código de Especificación de Producto'
/
COMMENT ON COLUMN  PS_ESPECIFICACIONES_PROD_TD.ID_ESPEC_PROD IS 'Nombre de Especificación de Producto'
/
COMMENT ON COLUMN  PS_ESPECIFICACIONES_PROD_TD.DES_ESPEC_PROD IS 'Descricpción de Especificación de Producto'
/
COMMENT ON COLUMN  PS_ESPECIFICACIONES_PROD_TD.FEC_INICIO_VIGENCIA IS 'Fecha de Entrada en Vigencia'
/
COMMENT ON COLUMN  PS_ESPECIFICACIONES_PROD_TD.FEC_TERMINO_VIGENCIA IS 'Fecha de Termino de Vigencia'
/
COMMENT ON COLUMN  PS_ESPECIFICACIONES_PROD_TD.TIPO_PLATAFORMA IS 'Tipo de Plataforma (Ver dominio TIPO_PLATAFORM_COMER)'
/
COMMENT ON COLUMN  PS_ESPECIFICACIONES_PROD_TD.TIPO_COMPORTAMIENTO IS 'Tipo de Comportamiento (Ver dominio TIPO_COMPORTAMIENTO)'
/
