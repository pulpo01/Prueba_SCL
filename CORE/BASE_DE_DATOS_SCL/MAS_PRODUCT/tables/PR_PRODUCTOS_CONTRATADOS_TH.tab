CREATE TABLE MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH
(
  COD_PROD_CONTRATADO         NUMBER(8)         NOT NULL,
  COD_CLIENTE_BENEFICIARIO    NUMBER(10)        NOT NULL,
  NUM_ABONADO_BENEFICIARIO    NUMBER(10)        NOT NULL,
  COD_PROD_OFERTADO           NUMBER(8)         NOT NULL,
  FEC_INICIO_VIGENCIA         DATE              NOT NULL,
  COD_CANAL                   VARCHAR2(20 BYTE) NOT NULL,
  NUM_PROCESO                 VARCHAR2(10 BYTE) NOT NULL,
  ORIGEN_PROCESO              VARCHAR2(5 BYTE)  NOT NULL,
  FEC_PROCESO                 DATE              NOT NULL,
  COD_ESTADO                  VARCHAR2(10 BYTE) NOT NULL,
  IND_CONDICION_CONTRATACION  VARCHAR2(1 BYTE)  NOT NULL,
  COD_CLIENTE_CONTRATANTE     NUMBER(10)        NOT NULL,
  NUM_ABONADO_CONTRATANTE     NUMBER(10)        NOT NULL,
  FEC_TERMINO_VIGENCIA        DATE                  NULL,
  IND_PRIORIDAD               NUMBER(5)             NULL,
  COD_PROD_CONTRA_PADRE       NUMBER(8)             NULL,
  COD_PERFIL_LISTA            NUMBER(8)             NULL,
  TIPO_COMPORTAMIENTO         VARCHAR2(5 BYTE)      NULL,
  FEC_DESCONTRATA             DATE              NOT NULL,
  NUM_PROC_DESCONTRATA        VARCHAR2(10 BYTE) NOT NULL,
  ORIGEN_PROCESO_DESCONTRATA  VARCHAR2(5 BYTE)  NOT NULL,
  COD_LIMCONS                 VARCHAR2(5 BYTE)      NULL,
  MTO_LIMCONS                 NUMBER(14,4)          NULL
)
TABLESPACE MAS_PRODUCT_DATA
PCTUSED    0
PCTFREE    5
INITRANS   16
MAXTRANS   255
STORAGE    (
            INITIAL          200M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING
/

COMMENT ON TABLE MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH IS '[ PRDS ] Historica de productos des-contratados por el Clientes/Abonado'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.MTO_LIMCONS IS 'Monto Limite de Consumo'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.COD_LIMCONS IS 'Código de Limite de Consumo (TOL_LIMITE_TD)'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.COD_PROD_CONTRATADO IS 'Código de Producto Contratado'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.COD_CLIENTE_BENEFICIARIO IS 'Código de Cliente que recibe el Producto Contratado'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.NUM_ABONADO_BENEFICIARIO IS 'Número de Abonado que recibe el Producto Contratado'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.COD_PROD_OFERTADO IS 'Código de Producto Ofertado que ha sido contratado'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.FEC_INICIO_VIGENCIA IS 'Fecha de Inicio de la Contratación'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.COD_CANAL IS 'Canal que originó la Contratación'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.NUM_PROCESO IS 'Número de Proceso que generó la Contratación'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.ORIGEN_PROCESO IS 'Origen del Proceso de Contratación'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.FEC_PROCESO IS 'Fecha del Proceso de Contratación'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.COD_ESTADO IS 'Estado del Producto Contratado'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.IND_CONDICION_CONTRATACION IS 'Condición de Contratación (D Defecto, O Opcional)'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.COD_CLIENTE_CONTRATANTE IS 'Código de Cliente que contrata el Producto Contratado'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.NUM_ABONADO_CONTRATANTE IS 'Número de Abonado que contrata el Producto Contratado'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.FEC_TERMINO_VIGENCIA IS 'Fecha de Termino de Vigencia de la Contratación'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.IND_PRIORIDAD IS 'Indicador de Prioridad de Contratación'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.COD_PROD_CONTRA_PADRE IS '0 indica Paquete, igual a COD_PROD_CONTRATADO es producto, <> COD_PROD_CONTRATADO es un hijo'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.COD_PERFIL_LISTA IS 'Código de Pefil de la Lista de Números del Producto'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.TIPO_COMPORTAMIENTO IS 'Tipo de Comportamiento (Ver dominio TIPO_COMPORTAMIENTO)'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.FEC_DESCONTRATA IS 'Fecha de Descontratación'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.NUM_PROC_DESCONTRATA IS 'Número del Proceso que generó la descontratación'
/

COMMENT ON COLUMN MAS_PRODUCT.PR_PRODUCTOS_CONTRATADOS_TH.ORIGEN_PROCESO_DESCONTRATA IS 'Origen del Proceso de Descontratación'
/

