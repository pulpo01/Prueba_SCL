CREATE TABLE PF_RESTRICCIONES_PROD_TD(
    TIPO_PLATAFORMA VARCHAR2(5) NOT NULL 
 ,  IND_NIVEL_APLICA VARCHAR2(1) NOT NULL 
 ,  TIPO_COMPORTAMIENTO VARCHAR2(5) NOT NULL 
 ,  FEC_INICIO_VIGENCIA DATE NOT NULL 
 ,  MAX_CANTIDAD NUMBER(10,0) NULL 
 ,  MAX_MONTO NUMBER(10,0) NULL 
 ,  MIN_CICLOS NUMBER(10,0) NULL 
 ,  FEC_TERMINO_VIGENCIA DATE NULL 
 ) TABLESPACE    MAS_PRODUCT_DATA
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PF_RESTRICCIONES_PROD_TD IS '[ RECO ] Restricciones para el modulo de Productos'
/
COMMENT ON COLUMN  PF_RESTRICCIONES_PROD_TD.TIPO_PLATAFORMA IS 'Tipo de Plataforma (Ver dominio TIPO_PLATAFORM_COMER)'
/
COMMENT ON COLUMN  PF_RESTRICCIONES_PROD_TD.IND_NIVEL_APLICA IS 'Indicador del Nivel de Aplicación (A Abonado, C Cliente)'
/
COMMENT ON COLUMN  PF_RESTRICCIONES_PROD_TD.TIPO_COMPORTAMIENTO IS 'Tipo de Comportamiento (Ver dominio TIPO_COMPORTAMIENTO)'
/
COMMENT ON COLUMN  PF_RESTRICCIONES_PROD_TD.FEC_INICIO_VIGENCIA IS 'Fecha de Inicio de Vigencia de la Restricción'
/
COMMENT ON COLUMN  PF_RESTRICCIONES_PROD_TD.MAX_CANTIDAD IS 'Cantidad Máxima de Contratación de un Producto'
/
COMMENT ON COLUMN  PF_RESTRICCIONES_PROD_TD.MAX_MONTO IS 'Cantidad Máxima en monto de Contratación de un Producto'
/
COMMENT ON COLUMN  PF_RESTRICCIONES_PROD_TD.MIN_CICLOS IS 'Cantidad Mínima de Ciclos que debe tener un cliente para contratar'
/
COMMENT ON COLUMN  PF_RESTRICCIONES_PROD_TD.FEC_TERMINO_VIGENCIA IS 'Fecha de Termino de Vigencia de la restricción'
/
