CREATE TABLE PF_CATALOGO_OFERTADO_TD(
    COD_PROD_OFERTADO NUMBER(38,0) NOT NULL 
 ,  COD_CANAL VARCHAR2(20) NOT NULL 
 ,  COD_CONCEPTO NUMBER(9,0) NOT NULL 
 ,  FEC_INICIO_VIGENCIA DATE NOT NULL 
 ,  COD_CARGO NUMBER(8,0) NOT NULL 
 ,  FEC_TERMINO_VIGENCIA DATE NULL 
 ) TABLESPACE    MAS_PRODUCT_DATA
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PF_CATALOGO_OFERTADO_TD IS '[ CAOF ] Canales y cargos para la comercialización de Producto.'
/
COMMENT ON COLUMN  PF_CATALOGO_OFERTADO_TD.COD_PROD_OFERTADO IS 'Código de Producto Ofertado'
/
COMMENT ON COLUMN  PF_CATALOGO_OFERTADO_TD.COD_CANAL IS 'Código de Canal que puede Ofertar el Producto (Ver dominio CANAL_DISTRIBUCION)'
/
COMMENT ON COLUMN  PF_CATALOGO_OFERTADO_TD.COD_CONCEPTO IS 'Código de Concepto Facturable con el que se factura el Producto Ofertado'
/
COMMENT ON COLUMN  PF_CATALOGO_OFERTADO_TD.FEC_INICIO_VIGENCIA IS 'Fecha de Inicio de la Oferta del Producto'
/
COMMENT ON COLUMN  PF_CATALOGO_OFERTADO_TD.COD_CARGO IS 'Código de Cargo que se cobra por el Producto Ofertado'
/
COMMENT ON COLUMN  PF_CATALOGO_OFERTADO_TD.FEC_TERMINO_VIGENCIA IS 'Fecha de Termino de Vigencia de la Oferta del Producto'
/
