CREATE TABLE PF_CARGOS_PRODUCTOS_TD(
    COD_CARGO NUMBER(38,0) NOT NULL 
 ,  MONTO_IMPORTE NUMBER(14,4) NOT NULL 
 ,  COD_MONEDA VARCHAR2(10) NOT NULL 
 ,  ID_CARGO VARCHAR2(10) NOT NULL 
 ) TABLESPACE    MAS_PRODUCT_DATA
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PF_CARGOS_PRODUCTOS_TD IS '[ CAPR ] Configuracion de los cargos que podrán asignarse a la Oferta de Productos'
/
COMMENT ON COLUMN  PF_CARGOS_PRODUCTOS_TD.COD_CARGO IS 'Codigo asignado'
/
COMMENT ON COLUMN  PF_CARGOS_PRODUCTOS_TD.MONTO_IMPORTE IS 'Valor del cargo'
/
COMMENT ON COLUMN  PF_CARGOS_PRODUCTOS_TD.COD_MONEDA IS 'Moneda definida del cargo'
/
COMMENT ON COLUMN  PF_CARGOS_PRODUCTOS_TD.ID_CARGO IS 'Recurrente/Ocasional'
/
