CREATE TABLE PF_PAQUETE_OFERTADO_TO(
    COD_PROD_PADRE NUMBER(38,0) NOT NULL 
 ,  COD_PROD_HIJO NUMBER(38,0) NOT NULL 
 ,  NUM_VECES_HIJO NUMBER(5,0) NULL 
 ) TABLESPACE    MAS_PRODUCT_DATA
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PF_PAQUETE_OFERTADO_TO IS '[ PAOF ] Conjunto de Productos a ser oferados'
/
COMMENT ON COLUMN  PF_PAQUETE_OFERTADO_TO.COD_PROD_PADRE IS 'Código del Producto Ofertado que hace de contenedor (Paquete) de Prodcutos'
/
COMMENT ON COLUMN  PF_PAQUETE_OFERTADO_TO.COD_PROD_HIJO IS 'Código del Producto Ofertado que pertenece al Paquete'
/
COMMENT ON COLUMN  PF_PAQUETE_OFERTADO_TO.NUM_VECES_HIJO IS 'Número de veces que se encuentra contenido un Producto Ofertado en un Paquete'
/
