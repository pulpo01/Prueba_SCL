CREATE TABLE PR_PRODUCTOS_CONTRATADOS_TO
(
  COD_PROD_CONTRATADO         NUMBER(38)        NOT NULL,
  COD_CLIENTE_BENEFICIARIO    NUMBER(38)        NOT NULL,
  NUM_ABONADO_BENEFICIARIO    NUMBER(38)        NOT NULL,
  COD_PROD_OFERTADO           NUMBER(38)        NOT NULL,
  FEC_INICIO_VIGENCIA         DATE              NOT NULL,
  COD_CANAL                   VARCHAR2(20 BYTE) NOT NULL,
  NUM_PROCESO                 VARCHAR2(10 BYTE) NOT NULL,
  ORIGEN_PROCESO              VARCHAR2(5 BYTE)  NOT NULL,
  FEC_PROCESO                 DATE              NOT NULL,
  COD_ESTADO                  VARCHAR2(10 BYTE) NOT NULL,
  IND_CONDICION_CONTRATACION  VARCHAR2(1 BYTE)  NOT NULL,
  COD_CLIENTE_CONTRATANTE     NUMBER(38)        NOT NULL,
  NUM_ABONADO_CONTRATANTE     NUMBER(38)        NOT NULL,
  FEC_TERMINO_VIGENCIA        DATE,
  IND_PRIORIDAD               NUMBER(5),
  COD_PROD_CONTRA_PADRE       NUMBER(38),
  COD_PERFIL_LISTA            NUMBER(38),
  TIPO_COMPORTAMIENTO         VARCHAR2(5 BYTE),
  COD_LIMCONS                 VARCHAR2(5 BYTE),
  MTO_LIMCONS                 NUMBER(14,4)
)
TABLESPACE MAS_PRODUCT_DATA
PCTUSED    0
PCTFREE    5
INITRANS   16
MAXTRANS   255
STORAGE    (
            INITIAL          200M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE PR_PRODUCTOS_CONTRATADOS_TO IS '[ PRCO ] Detalle de los productos contratados por el Clientes/Abonado';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.COD_LIMCONS IS 'Código de Limite de Consumo (TOL_LIMITE_TD)';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.MTO_LIMCONS IS 'Monto Limite de Consumo';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.COD_PROD_CONTRATADO IS 'Código de Producto Contratado';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.COD_CLIENTE_BENEFICIARIO IS 'Código de Cliente que recibe el Producto Contratado';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.NUM_ABONADO_BENEFICIARIO IS 'Número de Abonado que recibe el Producto Contratado';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.COD_PROD_OFERTADO IS 'Código de Producto Ofertado que ha sido contratado';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.FEC_INICIO_VIGENCIA IS 'Fecha de Inicio de la Contratación';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.COD_CANAL IS 'Canal que originó la Contratación';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.NUM_PROCESO IS 'Número de Proceso que generó la Contratación';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.ORIGEN_PROCESO IS 'Origen del Proceso de Contratación';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.FEC_PROCESO IS 'Fecha del Proceso de Contratación';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.COD_ESTADO IS 'Estado del Producto Contratado';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.IND_CONDICION_CONTRATACION IS 'Condición de Contratación (D Defecto, O Opcional)';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.COD_CLIENTE_CONTRATANTE IS 'Código de Cliente que contrata el Producto Contratado';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.NUM_ABONADO_CONTRATANTE IS 'Número de Abonado que contrata el Producto Contratado';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.FEC_TERMINO_VIGENCIA IS 'Fecha de Termino de Vigencia de la Contratación';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.IND_PRIORIDAD IS 'Indicador de Prioridad de Contratación';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.COD_PROD_CONTRA_PADRE IS '0 indica Paquete, igual a COD_PROD_CONTRATADO es producto, <> COD_PROD_CONTRATADO es un hijo';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.COD_PERFIL_LISTA IS 'Código de Pefil de la Lista de Números del Producto';

COMMENT ON COLUMN PR_PRODUCTOS_CONTRATADOS_TO.TIPO_COMPORTAMIENTO IS 'Tipo de Comportamiento (Ver dominio TIPO_COMPORTAMIENTO)';
