CREATE TABLE PS_CATEGORIAS_CARGO_TD(
    COD_CATEGORIA NUMBER(38,0) NOT NULL 
 ,  MIN_CARGO_REQUERIDO NUMBER(14,4) NOT NULL 
 ,  ID_CATEGORIA VARCHAR2(10) NOT NULL 
 ,  DES_CATEGORIA VARCHAR2(30) NULL 
 ) TABLESPACE    MAS_PRODUCT_DATA
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PS_CATEGORIAS_CARGO_TD IS '[ CCAR ] Categorias para grupar los cargos básicos de los planes tarifarios'
/
COMMENT ON COLUMN  PS_CATEGORIAS_CARGO_TD.COD_CATEGORIA IS 'Codigo de la categoria'
/
COMMENT ON COLUMN  PS_CATEGORIAS_CARGO_TD.MIN_CARGO_REQUERIDO IS 'Valor minimo del cargo basico'
/
COMMENT ON COLUMN  PS_CATEGORIAS_CARGO_TD.ID_CATEGORIA IS 'nombre asignado a la categoria'
/
COMMENT ON COLUMN  PS_CATEGORIAS_CARGO_TD.DES_CATEGORIA IS 'Descripción de la Categoría'
/
