COMMENT ON COLUMN PR_PROD_SS_EXCLUYENTES_TD.FEC_CREACION                   IS 'fecha cracion'
/
COMMENT ON COLUMN PR_PROD_SS_EXCLUYENTES_TD.NOM_USUARIO_MODIFICA           IS 'usuario ultima modificacion'
