CREATE TABLE PR_PROD_SS_EXCLUYENTES_TD (  
   TIPO_COD_EXCLUYENTE_ORIGEN    NUMBER(2)                                       NOT NULL  
 , COD_EXCLUYENTE_ORIGEN         VARCHAR2 (10)                                    NOT NULL  
 , TIPO_COD_EXCLUYENTE_DESTINO   NUMBER(2)                                       NOT NULL  
 , COD_EXCLUYENTE_DESTINO        VARCHAR2 (10)                                    NOT NULL  
 , FEC_INICIO_VIGENCIA           DATE                                         NOT NULL  
 , FEC_TERMINO_VIGENCIA          DATE                                         NULL  
 , NOM_USUARIO_CREACION          VARCHAR2 (30)                                    NULL  
 , FEC_CREACION                  DATE                                         NULL  
 , NOM_USUARIO_MODIFICA          VARCHAR2 (30)                                    NULL  
 , FEC_ULTIMA_MODIFICA           DATE                                         NULL 
 ) TABLESPACE     MAS_PRODUCT_DATA
 PCTFREE             5
 PCTUSED            60
 INITRANS           16
 MAXTRANS          255
 STORAGE (        
 INITIAL            200 K
 NEXT               100 K
 PCTINCREASE         0
 MINEXTENTS          1
 MAXEXTENTS       1024
 FREELISTS          32
 FREELIST GROUPS     1
 )
/


COMMENT ON TABLE  PR_PROD_SS_EXCLUYENTES_TD                      IS '[ PEXC ] tabla de exclusion entre planes adicionales y servicios suplementarios'
/

