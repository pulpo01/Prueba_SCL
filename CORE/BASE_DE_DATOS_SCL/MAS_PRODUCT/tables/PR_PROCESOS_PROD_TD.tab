CREATE TABLE PR_PROCESOS_PROD_TD
(
  COD_PROCESO        NUMBER(20)                 NOT NULL,
  NUM_PROCESO        VARCHAR2(20 BYTE)          NOT NULL,
  ORIGEN_PROCESO     VARCHAR2(10 BYTE)          NOT NULL,
  FEC_PROCESO        DATE                       NOT NULL,
  ESTADO_PROCESO     VARCHAR2(10 BYTE)          NOT NULL,
  PARAMETRO_PROCESO  BLOB,
  NUM_EVENTO         NUMBER(9),
  COD_OS             VARCHAR2(5 BYTE),
  COD_CLIENTE        NUMBER(8),
  NUM_ABONADO        NUMBER(8)
)
TABLESPACE MAS_PRODUCT_DATA
PCTUSED    0
PCTFREE    20
INITRANS   16
MAXTRANS   255
STORAGE    (
            INITIAL          200M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
LOB (PARAMETRO_PROCESO) STORE AS 
      ( TABLESPACE  MAS_PRODUCT_DATA 
        ENABLE      STORAGE IN ROW
        CHUNK       8192
        PCTVERSION  10
        NOCACHE
        INDEX       (
          TABLESPACE MAS_PRODUCT_DATA
          STORAGE    (
                      INITIAL          64K
                      NEXT             1
                      MINEXTENTS       1
                      MAXEXTENTS       UNLIMITED
                      PCTINCREASE      0
                      BUFFER_POOL      DEFAULT
                     ))
        STORAGE    (
                    INITIAL          64K
                    MINEXTENTS       1
                    MAXEXTENTS       UNLIMITED
                    PCTINCREASE      0
                    BUFFER_POOL      DEFAULT
                   )
      )
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE PR_PROCESOS_PROD_TD IS '[ PCPR ] Contiene la informacion para reprocesar';

COMMENT ON COLUMN PR_PROCESOS_PROD_TD.NUM_EVENTO IS 'Código de evento de error';

COMMENT ON COLUMN PR_PROCESOS_PROD_TD.COD_OS IS 'Código de orden de servicio';

COMMENT ON COLUMN PR_PROCESOS_PROD_TD.COD_CLIENTE IS 'Código de cliente';

COMMENT ON COLUMN PR_PROCESOS_PROD_TD.NUM_ABONADO IS 'Número de abonado';

COMMENT ON COLUMN PR_PROCESOS_PROD_TD.COD_PROCESO IS 'Código del Proceso Ejecutado';

COMMENT ON COLUMN PR_PROCESOS_PROD_TD.NUM_PROCESO IS 'Número del evento que ejecuta el proceso (ej. NUM_VENTA, NUM_OS)';

COMMENT ON COLUMN PR_PROCESOS_PROD_TD.ORIGEN_PROCESO IS 'Origen del Proceso';

COMMENT ON COLUMN PR_PROCESOS_PROD_TD.FEC_PROCESO IS 'Fecha del Proceso';

COMMENT ON COLUMN PR_PROCESOS_PROD_TD.ESTADO_PROCESO IS 'Estado del Proceso';

COMMENT ON COLUMN PR_PROCESOS_PROD_TD.PARAMETRO_PROCESO IS 'Parametro del Proceso';

