CREATE TABLE PF_DESCUENTOS_CONCEPTOS_TD(
    COD_DESCUENTO NUMBER(20,0) NOT NULL 
 ,  COD_CONCEPTO NUMBER(20,0) NOT NULL 
 ,  COD_CONCEPTO_DESCTO NUMBER(20,0) NOT NULL 
 ,  COD_CANAL VARCHAR2(20) NOT NULL 
 ,  TIPO_VENDEDOR VARCHAR2(5) NOT NULL 
 ,  COD_VENDEDOR NUMBER(38,0) NULL 
 ,  FEC_INICIO_VIGENCIA DATE NOT NULL 
 ,  FEC_TERMINO_VIGENCIA DATE NULL 
 ,  TIPO_DESCUENTO VARCHAR2(5) NOT NULL 
 ,  VALOR_DESCUENTO NUMBER(14,4) NOT NULL 
 ,  COD_MONEDA VARCHAR2(5) NULL 
 ) TABLESPACE    MAS_PRODUCT_DATA
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  PF_DESCUENTOS_CONCEPTOS_TD IS '[ DESCON ] Definición de descuentos para los conceptos disponibles'
/
COMMENT ON COLUMN  PF_DESCUENTOS_CONCEPTOS_TD.COD_DESCUENTO IS 'Código de Descuento'
/
COMMENT ON COLUMN  PF_DESCUENTOS_CONCEPTOS_TD.COD_CONCEPTO IS 'Código de Concepto Facturable'
/
COMMENT ON COLUMN  PF_DESCUENTOS_CONCEPTOS_TD.COD_CONCEPTO_DESCTO IS 'Código de Concepto de Descuento'
/
COMMENT ON COLUMN  PF_DESCUENTOS_CONCEPTOS_TD.COD_CANAL IS 'Código de Canal al que se asocia el descuento (Ver dominio CANAL_DISTRIBUCION)'
/
COMMENT ON COLUMN  PF_DESCUENTOS_CONCEPTOS_TD.TIPO_VENDEDOR IS 'Tipo de Vendedor que puede otorgar el descuento'
/
COMMENT ON COLUMN  PF_DESCUENTOS_CONCEPTOS_TD.COD_VENDEDOR IS 'Código de Vendedor que puede otorgar el descuento'
/
COMMENT ON COLUMN  PF_DESCUENTOS_CONCEPTOS_TD.FEC_INICIO_VIGENCIA IS 'Fecha de Inicio de Vigencia del Descuento'
/
COMMENT ON COLUMN  PF_DESCUENTOS_CONCEPTOS_TD.FEC_TERMINO_VIGENCIA IS 'Fecha de Termino de Vigencia del Descuento'
/
COMMENT ON COLUMN  PF_DESCUENTOS_CONCEPTOS_TD.TIPO_DESCUENTO IS 'Tipo de Descuento (M Monto, P Porcentaje)'
/
COMMENT ON COLUMN  PF_DESCUENTOS_CONCEPTOS_TD.VALOR_DESCUENTO IS 'Monto del Descuento'
/
COMMENT ON COLUMN  PF_DESCUENTOS_CONCEPTOS_TD.COD_MONEDA IS 'Código de Moneda del Descuento}'
/
