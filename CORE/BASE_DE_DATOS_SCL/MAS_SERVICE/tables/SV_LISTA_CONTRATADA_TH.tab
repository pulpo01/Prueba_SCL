CREATE TABLE SV_LISTA_CONTRATADA_TH
(
  COD_PROD_CONTRATADO      NUMBER(8)            NOT NULL,
  VALOR_ELEMENTO           VARCHAR2(18 BYTE)    NOT NULL,
  FEC_INICIO_VIGENCIA      DATE                 NOT NULL,
  FEC_TERMINO_VIGENCIA     DATE,
  COD_CATEGORIA_DESTINO    VARCHAR2(20 BYTE),
  NUM_PROCESO              VARCHAR2(10 BYTE)    NOT NULL,
  ORIGEN_PROCESO           VARCHAR2(5 BYTE)     NOT NULL,
  FEC_PROCESO              DATE                 NOT NULL,
  NUM_PROC_DESCONTRATA     VARCHAR2(10 BYTE)    NOT NULL,
  ORIGEN_PROC_DESCONTRATA  VARCHAR2(5 BYTE)     NOT NULL,
  FEC_DESCONTRATA          DATE                 NOT NULL
)
TABLESPACE MAS_SERVICE_DATA
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             128K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE SV_LISTA_CONTRATADA_TH IS '[ LIDS ] Historia elementos de las listas des-contratadas';

COMMENT ON COLUMN SV_LISTA_CONTRATADA_TH.COD_PROD_CONTRATADO IS 'Código del producto contratado';

COMMENT ON COLUMN SV_LISTA_CONTRATADA_TH.VALOR_ELEMENTO IS 'Valor del producto';

COMMENT ON COLUMN SV_LISTA_CONTRATADA_TH.FEC_INICIO_VIGENCIA IS 'Fecha Inicio Vigencia';

COMMENT ON COLUMN SV_LISTA_CONTRATADA_TH.FEC_TERMINO_VIGENCIA IS 'Fecha termino vigencia';

COMMENT ON COLUMN SV_LISTA_CONTRATADA_TH.COD_CATEGORIA_DESTINO IS 'Categoría destino OFN, ONN, RDF, INT';

COMMENT ON COLUMN SV_LISTA_CONTRATADA_TH.NUM_PROCESO IS 'numero del proceso de contratacion';

COMMENT ON COLUMN SV_LISTA_CONTRATADA_TH.ORIGEN_PROCESO IS 'Codigo del origen del prceso EJ: VTA, PVTA';

COMMENT ON COLUMN SV_LISTA_CONTRATADA_TH.FEC_PROCESO IS 'Fecha ejecucion del proceso';

COMMENT ON COLUMN SV_LISTA_CONTRATADA_TH.NUM_PROC_DESCONTRATA IS 'numero del proceso de descontratacion';

COMMENT ON COLUMN SV_LISTA_CONTRATADA_TH.ORIGEN_PROC_DESCONTRATA IS 'Codigo del origen del prceso EJ: VTA, PVTA';

COMMENT ON COLUMN SV_LISTA_CONTRATADA_TH.FEC_DESCONTRATA IS 'fecha de la descontratacion';

