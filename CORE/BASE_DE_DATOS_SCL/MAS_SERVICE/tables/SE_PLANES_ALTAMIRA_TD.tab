CREATE TABLE SE_PLANES_ALTAMIRA_TD(
    COD_PLAN_ALTAMIRA VARCHAR2(10) NOT NULL 
 ,  DES_PLAN_ALTAMIRA VARCHAR2(30) NULL 
 ,  TIPO_PLATAFORMA VARCHAR2(5) NOT NULL 
 ,  FEC_INICIO_VIGENCIA DATE NOT NULL 
 ,  FEC_TERMINO_VIGENCIA DATE NULL 
 ,  TIPO_PLAN_ALTAMIRA VARCHAR2(5) NOT NULL 
 ,  COD_LISTA_ALTAMIRA VARCHAR2(5) NULL 
 ,  CAN_BONIFICAR NUMBER(10,0) NULL 
 ,  TIPO_UNIDAD_BONIFICAR VARCHAR2(5) NULL 
 ) TABLESPACE    MAS_SERVICE_DATA
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SE_PLANES_ALTAMIRA_TD IS '[ ALTA ] Planes disponibles en Altamira'
/
COMMENT ON COLUMN  SE_PLANES_ALTAMIRA_TD.COD_PLAN_ALTAMIRA IS 'Código del Plan Altamira'
/
COMMENT ON COLUMN  SE_PLANES_ALTAMIRA_TD.DES_PLAN_ALTAMIRA IS 'Descripcion del Plan'
/
COMMENT ON COLUMN  SE_PLANES_ALTAMIRA_TD.TIPO_PLATAFORMA IS 'Postapago, Hibrido, Prepago, Atlantida'
/
COMMENT ON COLUMN  SE_PLANES_ALTAMIRA_TD.FEC_INICIO_VIGENCIA IS 'Fecha inicio de la vigencia del plan'
/
COMMENT ON COLUMN  SE_PLANES_ALTAMIRA_TD.FEC_TERMINO_VIGENCIA IS 'Fecha termino vigencia del plan'
/
COMMENT ON COLUMN  SE_PLANES_ALTAMIRA_TD.TIPO_PLAN_ALTAMIRA IS 'LISTA FRECUENTES/BONOS'
/
COMMENT ON COLUMN  SE_PLANES_ALTAMIRA_TD.COD_LISTA_ALTAMIRA IS 'indica Lista de Voz  o SMS '
/
COMMENT ON COLUMN  SE_PLANES_ALTAMIRA_TD.CAN_BONIFICAR IS 'Cantidad (msn-dinero) del bono '
/
COMMENT ON COLUMN  SE_PLANES_ALTAMIRA_TD.TIPO_UNIDAD_BONIFICAR IS 'mensaje/dinero'
/
