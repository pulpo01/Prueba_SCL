CREATE TABLE SE_PERFILES_LISTA_TD(
    COD_PERFIL_LISTA NUMBER(38,0) NOT NULL 
 ,  ID_PERFIL_LISTA VARCHAR2(10) NOT NULL 
 ,  DES_PERFIL_LISTA VARCHAR2(30) NULL 
 ,  TIPO_PLATAFORMA VARCHAR2(5) NOT NULL 
 ,  FEC_INICIO_VIGENCIA DATE NOT NULL 
 ,  FEC_TERMINO_VIGENCIA DATE NULL 
 ,  NUM_MAXIMO_LISTA NUMBER(5,0) NOT NULL 
 ,  TIPO_COMPORTAMIENTO VARCHAR2(5) NOT NULL 
 ,  FLG_AUTO_AFIN VARCHAR2(1) NULL 
 ) TABLESPACE    MAS_SERVICE_DATA
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SE_PERFILES_LISTA_TD IS '[ PELI ] Definicion para configuración de las  listas'
/
COMMENT ON COLUMN  SE_PERFILES_LISTA_TD.COD_PERFIL_LISTA IS 'Código Unico de Lista'
/
COMMENT ON COLUMN  SE_PERFILES_LISTA_TD.ID_PERFIL_LISTA IS 'Identificador de la lista'
/
COMMENT ON COLUMN  SE_PERFILES_LISTA_TD.DES_PERFIL_LISTA IS 'La descripcion de la lista'
/
COMMENT ON COLUMN  SE_PERFILES_LISTA_TD.TIPO_PLATAFORMA IS 'Postapago, Hibrido, Prepago, Atlantida'
/
COMMENT ON COLUMN  SE_PERFILES_LISTA_TD.FEC_INICIO_VIGENCIA IS 'Fecha Inicio vigencia Perfil lista'
/
COMMENT ON COLUMN  SE_PERFILES_LISTA_TD.FEC_TERMINO_VIGENCIA IS 'Fecha de termino vigencia del perfil lista'
/
COMMENT ON COLUMN  SE_PERFILES_LISTA_TD.NUM_MAXIMO_LISTA IS 'Cantidad máxima de números en la lista'
/
COMMENT ON COLUMN  SE_PERFILES_LISTA_TD.TIPO_COMPORTAMIENTO IS 'Frecuente, Afin'
/
COMMENT ON COLUMN  SE_PERFILES_LISTA_TD.FLG_AUTO_AFIN IS 'Indicador de autoafinidad'
/
