CREATE TABLE SE_DETALLE_PERFIL_TD(
    COD_PERFIL_LISTA NUMBER(8,0) NOT NULL 
 ,  COD_CATEGORIA_DESTINO VARCHAR2(20) NOT NULL 
 ,  NUM_MAXIMO NUMBER(5,0) NOT NULL 
 ) TABLESPACE    MAS_SERVICE_DATA
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SE_DETALLE_PERFIL_TD IS '[ DEPE ] Maximos permitidos para las distintas categorias de una lista'
/
COMMENT ON COLUMN  SE_DETALLE_PERFIL_TD.COD_PERFIL_LISTA IS 'Código Unico de Lista'
/
COMMENT ON COLUMN  SE_DETALLE_PERFIL_TD.COD_CATEGORIA_DESTINO IS 'OFN, ONN, RDF, INT'
/
COMMENT ON COLUMN  SE_DETALLE_PERFIL_TD.NUM_MAXIMO IS 'cantidad maxima de números de esta categoría en la lista'
/
