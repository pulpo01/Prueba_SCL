CREATE TABLE SE_DETALLES_ESPECIFICACION_TO(
    COD_ESPEC_PROD NUMBER(38,0) NOT NULL 
 ,  COD_SERVICIO_BASE VARCHAR2(10) NOT NULL 
 ,  IND_TIPO_SERVICIO VARCHAR2(20) NOT NULL 
 ,  COD_PROV_SERV NUMBER(38,0) NULL 
 ,  COD_PERFIL_LISTA NUMBER(38,0) NULL 
 ) TABLESPACE    MAS_SERVICE_DATA
 LOGGING
 NOCACHE
 NOPARALLEL
/
COMMENT ON TABLE  SE_DETALLES_ESPECIFICACION_TO IS '[ DEPR ] Servicios que componen una Especificacion de Productos'
/
COMMENT ON COLUMN  SE_DETALLES_ESPECIFICACION_TO.COD_ESPEC_PROD IS 'Código de la especificacion del prod'
/
COMMENT ON COLUMN  SE_DETALLES_ESPECIFICACION_TO.COD_SERVICIO_BASE IS 'Código de Plan de la Especificación de Servicio (Altamira, Atlantida, TOL)'
/
COMMENT ON COLUMN  SE_DETALLES_ESPECIFICACION_TO.IND_TIPO_SERVICIO IS 'Indicador del tipo de servicio'
/
COMMENT ON COLUMN  SE_DETALLES_ESPECIFICACION_TO.COD_PROV_SERV IS 'Codigo del prveedor de servicio'
/
COMMENT ON COLUMN  SE_DETALLES_ESPECIFICACION_TO.COD_PERFIL_LISTA IS 'Código Unico de Lista'
/
