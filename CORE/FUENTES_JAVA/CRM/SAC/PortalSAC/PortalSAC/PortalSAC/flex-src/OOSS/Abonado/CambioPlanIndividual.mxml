						numFrecuenteFi.text="";
						ComdataNumFreFijo.addItem(oItemNFijo);
						canNumFrecuenteFi.dataProvider=ComdataNumFreFijo;
						sacaNF.enabled=false;						
					}
					
				}
				
			}
			// --------------------------------------------------------------------------------
		
			private function agregarNM():void{
